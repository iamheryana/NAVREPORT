<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="04019_LapSalesRevenueV3" language="groovy" pageWidth="1200" pageHeight="595" orientation="Landscape" columnWidth="1160" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="d67d58b6-db98-43fc-b2fe-6c1d43bc5f9a">
	<property name="ireport.zoom" value="1.610510000000025"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="TglInvFrom" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="TglInvTo" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Company" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ProsesId" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT	1 AS GR_CRNXY
		,	CASE WHEN SRV.GROUP_NO = 4 THEN
				CASE WHEN  DATEPART(MONTH, $P{TglInvTo}) IN (5,6) THEN 4 ELSE 3 END
			ELSE
				SRV.GROUP_NO
			END AS GROUP_NO
		,SRV.GROUP_DESC
		,SRV.GROUP_ITEM_CAT
		,SRV.ITEM_CAT
		,ROUND((SRV.AMT_HO),0) AS AMT_HO
		,ROUND((SRV.AMT_SBY),0) AS AMT_SBY
		,ROUND((SRV.AMT_DPS),0) AS AMT_DPS
		,ROUND((SRV.AMT_CKR),0) AS AMT_CKR
		,ROUND((SRV.AMT_SMR),0) AS AMT_SMR
		,	(ROUND((SRV.AMT_HO),0) + ROUND((SRV.AMT_SBY),0) + ROUND((SRV.AMT_DPS),0)
			+ROUND((SRV.AMT_CKR),0) +ROUND((SRV.AMT_SMR),0) )AS TOTAL
		, ROUND((ISNULL(TRG.AMT_TARGET,0)),0)AS AMT_TARGET
		,TRG.LBL_TARGET
		,	ROUND((
			(
				SUM(SRV.AMT_HO) OVER (PARTITION BY SRV.GROUP_NO, SRV.GROUP_ITEM_CAT ORDER BY SRV.GROUP_NO, SRV.GROUP_ITEM_CAT) /
				CASE WHEN SUM(SRV.AMT_HO) OVER (PARTITION BY SRV.GROUP_NO ORDER BY SRV.GROUP_NO) = 0 THEN 1 ELSE SUM(SRV.AMT_HO) OVER (PARTITION BY SRV.GROUP_NO ORDER BY SRV.GROUP_NO) END
			) * 100), 2) AS PCN_HO
		,	ROUND((
			(
				SUM(SRV.AMT_SBY) OVER (PARTITION BY SRV.GROUP_NO, SRV.GROUP_ITEM_CAT ORDER BY SRV.GROUP_NO, SRV.GROUP_ITEM_CAT) /
				CASE WHEN SUM(SRV.AMT_SBY) OVER (PARTITION BY SRV.GROUP_NO ORDER BY SRV.GROUP_NO) = 0 THEN 1 ELSE SUM(SRV.AMT_SBY) OVER (PARTITION BY SRV.GROUP_NO ORDER BY SRV.GROUP_NO) END
			) * 100), 2) AS PCN_SBY
		,	ROUND((
			(
				SUM(SRV.AMT_DPS) OVER (PARTITION BY SRV.GROUP_NO, SRV.GROUP_ITEM_CAT ORDER BY SRV.GROUP_NO, SRV.GROUP_ITEM_CAT) /
				CASE WHEN SUM(SRV.AMT_DPS) OVER (PARTITION BY SRV.GROUP_NO ORDER BY SRV.GROUP_NO) = 0 THEN 1 ELSE SUM(SRV.AMT_DPS) OVER (PARTITION BY SRV.GROUP_NO ORDER BY SRV.GROUP_NO) END
			) * 100), 2) AS PCN_DPS
		,	ROUND((
			(
				SUM(SRV.AMT_CKR) OVER (PARTITION BY SRV.GROUP_NO, SRV.GROUP_ITEM_CAT ORDER BY SRV.GROUP_NO, SRV.GROUP_ITEM_CAT) /
				CASE WHEN SUM(SRV.AMT_CKR) OVER (PARTITION BY SRV.GROUP_NO ORDER BY SRV.GROUP_NO) = 0 THEN 1 ELSE SUM(SRV.AMT_CKR) OVER (PARTITION BY SRV.GROUP_NO ORDER BY SRV.GROUP_NO) END
			) * 100), 2) AS PCN_CKR
		,	ROUND((
			(
				SUM(SRV.AMT_SMR) OVER (PARTITION BY SRV.GROUP_NO, SRV.GROUP_ITEM_CAT ORDER BY SRV.GROUP_NO, SRV.GROUP_ITEM_CAT) /
				CASE WHEN SUM(SRV.AMT_SMR) OVER (PARTITION BY SRV.GROUP_NO ORDER BY SRV.GROUP_NO) = 0 THEN 1 ELSE SUM(SRV.AMT_SMR) OVER (PARTITION BY SRV.GROUP_NO ORDER BY SRV.GROUP_NO) END
			) * 100), 2) AS PCN_SMR
		,	ROUND((
			(
				SUM(SRV.AMT_HO + SRV.AMT_SBY + SRV.AMT_DPS + SRV.AMT_CKR + SRV.AMT_SMR) OVER (PARTITION BY SRV.GROUP_NO, SRV.GROUP_ITEM_CAT ORDER BY SRV.GROUP_NO, SRV.GROUP_ITEM_CAT) /
				CASE WHEN SUM(SRV.AMT_HO + SRV.AMT_SBY + SRV.AMT_DPS + SRV.AMT_CKR + SRV.AMT_SMR) OVER (PARTITION BY SRV.GROUP_NO ORDER BY SRV.GROUP_NO) = 0 THEN 1 ELSE SUM(SRV.AMT_HO + SRV.AMT_SBY + SRV.AMT_DPS + SRV.AMT_CKR + SRV.AMT_SMR) OVER (PARTITION BY SRV.GROUP_NO ORDER BY SRV.GROUP_NO) END
			) * 100), 2) AS PCN_TOTAL
FROM	(
			SELECT	X.GROUP_NO
					,X.GROUP_DESC
					,X.GROUP_ITEM_CAT
					,X.ITEM_CAT
					,(SUM(X.AMT_HO)) AS AMT_HO
					,(SUM(X.AMT_SBY)) AS AMT_SBY
					,(SUM(X.AMT_DPS)) AS AMT_DPS
					,(SUM(X.AMT_CKR)) AS AMT_CKR
					,(SUM(X.AMT_SMR)) AS AMT_SMR
			FROM	(
						SELECT	GROUP_NO
								,GROUP_DESC
								,CASE	WHEN GROUP_NO = 1 THEN GROUP_ITEM_CAT
										WHEN GROUP_NO = 2 THEN	'OS1'
								 END AS GROUP_ITEM_CAT
								,CASE	WHEN GROUP_NO = 1 THEN ITEM_CAT
										WHEN GROUP_NO = 2 THEN	'Uang Muka'
								 END AS ITEM_CAT
								,(CASE WHEN BRANCHES = '10' THEN AMOUNT ELSE 0 END) AS AMT_HO
								,(CASE WHEN BRANCHES = '15' THEN AMOUNT ELSE 0 END) AS AMT_SBY
								,(CASE WHEN BRANCHES = '19' THEN AMOUNT ELSE 0 END) AS AMT_DPS
								,(CASE WHEN BRANCHES = '16' THEN AMOUNT ELSE 0 END) AS AMT_CKR
								,(CASE WHEN BRANCHES = '17' THEN AMOUNT ELSE 0 END) AS AMT_SMR
						FROM [dbo].[TEMP08_SALES_REVENUE]
						WHERE	GROUP_NO = 1 AND
								PROSES_ID = $P{ProsesId}
					) X
			GROUP BY X.GROUP_NO, X.GROUP_DESC, X.GROUP_ITEM_CAT, X.ITEM_CAT
			UNION ALL
			SELECT	X.GROUP_NO
					,X.GROUP_DESC
					,X.GROUP_ITEM_CAT
					,X.ITEM_CAT
					,SUM(CASE WHEN X.BRANCHES = '10' THEN X.AMOUNT ELSE 0 END) AS AMT_HO
					,SUM(CASE WHEN X.BRANCHES = '15' THEN X.AMOUNT ELSE 0 END) AS AMT_SBY
					,SUM(CASE WHEN X.BRANCHES = '19' THEN X.AMOUNT ELSE 0 END) AS AMT_DPS
					,SUM(CASE WHEN X.BRANCHES = '16' THEN X.AMOUNT ELSE 0 END) AS AMT_CKR
					,SUM(CASE WHEN X.BRANCHES = '17' THEN X.AMOUNT ELSE 0 END) AS AMT_SMR
			FROM	(
						SELECT	GROUP_NO
								,	CASE WHEN (DATEPART(quarter, $P{TglInvTo}) IN (1,2)) THEN
										GROUP_DESC+' - Semester I'
									ELSE
										GROUP_DESC+' - ALL Semester'
									END AS GROUP_DESC
								, 'OS1' AS GROUP_ITEM_CAT
								, 'Uang Muka' AS ITEM_CAT
								, BRANCHES
								, CASE WHEN	(
												(DATEPART(quarter, $P{TglInvTo}) IN (1,2) AND Q_OUT_SO IN (1,2))
												 OR
												DATEPART(quarter, $P{TglInvTo}) IN (3,4)
											) THEN AMOUNT ELSE 0 END AS AMOUNT
						FROM [dbo].[TEMP08_SALES_REVENUE]
						WHERE	GROUP_NO  = 2 AND
								PROSES_ID = $P{ProsesId} AND
								(
									(
										SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) <= SUBSTRING(CONVERT(varchar, GETDATE(), 105), 7,4) AND
										YEAR_OUT_SO = SUBSTRING(CONVERT(varchar,  GETDATE(), 105), 7,4)
									)
										OR
									(
										SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) > SUBSTRING(CONVERT(varchar, GETDATE(), 105), 7,4) AND
										YEAR_OUT_SO <= SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4)
									)
								)
						UNION ALL
						SELECT	3 AS GROUP_NO
								, GROUP_DESC+' - Semester II' AS GROUP_DESC
								, 'OS1' AS GROUP_ITEM_CAT
								, 'Uang Muka' AS ITEM_CAT
								, BRANCHES
								, CASE WHEN	Q_OUT_SO IN (3,4) THEN AMOUNT ELSE 0 END AS AMOUNT
						FROM [dbo].[TEMP08_SALES_REVENUE]
						WHERE	GROUP_NO  = 2 AND
								PROSES_ID = $P{ProsesId} AND
								(
									(
										SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) <= SUBSTRING(CONVERT(varchar, GETDATE(), 105), 7,4) AND
										YEAR_OUT_SO = SUBSTRING(CONVERT(varchar,  GETDATE(), 105), 7,4)
									)
										OR
									(
										SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) > SUBSTRING(CONVERT(varchar, GETDATE(), 105), 7,4) AND
										YEAR_OUT_SO <= SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4)
									)
								)
					) X
			GROUP BY X.GROUP_NO, X.GROUP_DESC, X.GROUP_ITEM_CAT, X.ITEM_CAT
			UNION ALL
			SELECT	X.GROUP_NO
					,X.GROUP_DESC
					,X.GROUP_ITEM_CAT
					,X.ITEM_CAT
					,SUM(CASE WHEN X.BRANCHES = 'JAKARTA' THEN X.AMOUNT ELSE 0 END) AS AMT_HO
					,SUM(CASE WHEN X.BRANCHES = 'SURABAYA' THEN X.AMOUNT ELSE 0 END) AS AMT_SBY
					,SUM(CASE WHEN X.BRANCHES = 'BALI' THEN X.AMOUNT ELSE 0 END) AS AMT_DPS
					,SUM(CASE WHEN X.BRANCHES = 'CIKARANG' THEN X.AMOUNT ELSE 0 END) AS AMT_CKR
					,SUM(CASE WHEN X.BRANCHES = 'SEMARANG' THEN X.AMOUNT ELSE 0 END) AS AMT_SMR
			FROM	(
						SELECT	4 AS GROUP_NO
								,	CASE WHEN (DATEPART(quarter, $P{TglInvTo}) IN (1,2)) THEN
										'Outstanding SO (as per Today) - Semester I'
									ELSE
										'Outstanding SO (as per Today) - ALL Semester'
									END AS GROUP_DESC
								, 'OS2' AS GROUP_ITEM_CAT
								, 'Outstanding SO' AS ITEM_CAT
								, BRANCHES
								, CASE WHEN	(
												(DATEPART(quarter, $P{TglInvTo}) IN (1,2) AND Q_OUT_SO IN (1,2))
												 OR
												DATEPART(quarter, $P{TglInvTo}) IN (3,4)
											) THEN AMOUNT ELSE 0 END AS AMOUNT
						FROM [dbo].[TEMP08_SALES_REVENUE]
						WHERE	GROUP_NO  = 3 AND
								PROSES_ID = $P{ProsesId} AND
								(
									(
										SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) <= SUBSTRING(CONVERT(varchar, GETDATE(), 105), 7,4) AND
										YEAR_OUT_SO = SUBSTRING(CONVERT(varchar,  GETDATE(), 105), 7,4)
									)
										OR
									(
										SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) > SUBSTRING(CONVERT(varchar, GETDATE(), 105), 7,4) AND
										YEAR_OUT_SO <= SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4)
									)
								)
						UNION ALL
						SELECT	5 AS GROUP_NO
								, 'Outstanding SO (as per Today) - Semester II' AS GROUP_DESC
								, 'OS2' AS GROUP_ITEM_CAT
								, 'Outstanding SO' AS ITEM_CAT
								, BRANCHES
								, CASE WHEN	Q_OUT_SO IN (3,4) THEN AMOUNT ELSE 0 END AS AMOUNT
						FROM [dbo].[TEMP08_SALES_REVENUE]
						WHERE	GROUP_NO  = 3 AND
								PROSES_ID = $P{ProsesId} AND
								(
									(
										SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) <= SUBSTRING(CONVERT(varchar, GETDATE(), 105), 7,4) AND
										YEAR_OUT_SO = SUBSTRING(CONVERT(varchar,  GETDATE(), 105), 7,4)
									)
										OR
									(
										SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) > SUBSTRING(CONVERT(varchar, GETDATE(), 105), 7,4) AND
										YEAR_OUT_SO <= SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4)
									)
								)
					) X
			GROUP BY X.GROUP_NO, X.GROUP_DESC, X.GROUP_ITEM_CAT, X.ITEM_CAT
		) SRV
LEFT JOIN	(
				SELECT	GROUP_DESC AS LBL_TARGET
						, AMOUNT AS AMT_TARGET
				FROM [dbo].[TEMP08_SALES_REVENUE]
				WHERE	GROUP_NO  = 0 AND
						PROSES_ID = $P{ProsesId}
			) TRG ON 1 = 1
WHERE	(DATEPART(MONTH, $P{TglInvTo}) IN (5,6)) OR
		(DATEPART(MONTH, $P{TglInvTo}) NOT IN (5,6) AND SRV.GROUP_NO NOT IN (3,5))
UNION ALL
SELECT	2 AS GR_CRNXY
		,XX.GROUP_NO
		,XX.GROUP_DESC
		,XX.GROUP_ITEM_CAT
		,XX.ITEM_CAT
		,XX.AMT_HO
		,XX.AMT_SBY
		,XX.AMT_DPS
		,XX.AMT_CKR
		,XX.AMT_SMR
		,(XX.AMT_HO+XX.AMT_SBY+XX.AMT_DPS+XX.AMT_CKR+XX.AMT_SMR) AS TOTAL
		, 0 AS AMT_TARGET
		, '' AS LBL_TARGET
		, 0 AS PCN_HO
		, 0 AS PCN_SBY
		, 0 AS PCN_DPS
		, 0 AS PCN_CKR
		, 0 AS PCN_SMR
		, 0 AS PCN_TOTAL
FROM	(
			SELECT	X.GROUP_NO
					,X.GROUP_DESC
					,X.GROUP_ITEM_CAT
					,X.ITEM_CAT
					,ROUND(SUM(CASE WHEN X.BRANCHES = '10' THEN X.AMOUNT ELSE 0 END),0) AS AMT_HO
					,ROUND(SUM(CASE WHEN X.BRANCHES = '15' THEN X.AMOUNT ELSE 0 END),0) AS AMT_SBY
					,ROUND(SUM(CASE WHEN X.BRANCHES = '19' THEN X.AMOUNT ELSE 0 END),0) AS AMT_DPS
					,ROUND(SUM(CASE WHEN X.BRANCHES = '16' THEN X.AMOUNT ELSE 0 END),0) AS AMT_CKR
					,ROUND(SUM(CASE WHEN X.BRANCHES = '17' THEN X.AMOUNT ELSE 0 END),0) AS AMT_SMR
					,ROUND(SUM(X.AMOUNT),0) AS TOTAL
			FROM	(
						SELECT	NULL AS GROUP_NO
								, 'Uang Muka (Carried over to Semester II)' AS GROUP_DESC
								, 'OS1' AS GROUP_ITEM_CAT
								, 'Uang Muka' AS ITEM_CAT
								, BRANCHES
								, CASE WHEN	(	DATEPART(MONTH, $P{TglInvTo}) IN (1,2,3,4) AND
												Q_OUT_SO IN (3,4)) THEN AMOUNT ELSE 0 END AS AMOUNT
						FROM [dbo].[TEMP08_SALES_REVENUE]
						WHERE	GROUP_NO  = 2 AND
								PROSES_ID = $P{ProsesId} AND
								(
									(
										SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) <= SUBSTRING(CONVERT(varchar, GETDATE(), 105), 7,4) AND
										YEAR_OUT_SO = SUBSTRING(CONVERT(varchar,  GETDATE(), 105), 7,4)
									)
										OR
									(
										SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) > SUBSTRING(CONVERT(varchar, GETDATE(), 105), 7,4) AND
										YEAR_OUT_SO <= SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4)
									)
								)
					) X
			GROUP BY X.GROUP_NO, X.GROUP_DESC, X.GROUP_ITEM_CAT, X.ITEM_CAT
		) XX
WHERE XX.TOTAL <> 0
UNION ALL
SELECT	3 AS GR_CRNXY
		,XX.GROUP_NO
		,XX.GROUP_DESC
		,XX.GROUP_ITEM_CAT
		,XX.ITEM_CAT
		,XX.AMT_HO
		,XX.AMT_SBY
		,XX.AMT_DPS
		,XX.AMT_CKR
		,XX.AMT_SMR
		,(XX.AMT_HO+XX.AMT_SBY+XX.AMT_DPS+XX.AMT_CKR+XX.AMT_SMR) AS TOTAL
		, 0 AS AMT_TARGET
		, '' AS LBL_TARGET
		, 0 AS PCN_HO
		, 0 AS PCN_SBY
		, 0 AS PCN_DPS
		, 0 AS PCN_CKR
		, 0 AS PCN_SMR
		, 0 AS PCN_TOTAL
FROM	(
			SELECT	X.GROUP_NO
					,X.GROUP_DESC
					,X.GROUP_ITEM_CAT
					,X.ITEM_CAT
					,ROUND(SUM(CASE WHEN X.BRANCHES = 'JAKARTA' THEN X.AMOUNT ELSE 0 END),0) AS AMT_HO
					,ROUND(SUM(CASE WHEN X.BRANCHES = 'SURABAYA' THEN X.AMOUNT ELSE 0 END),0) AS AMT_SBY
					,ROUND(SUM(CASE WHEN X.BRANCHES = 'BALI' THEN X.AMOUNT ELSE 0 END),0) AS AMT_DPS
					,ROUND(SUM(CASE WHEN X.BRANCHES = 'CIKARANG' THEN X.AMOUNT ELSE 0 END),0) AS AMT_CKR
					,ROUND(SUM(CASE WHEN X.BRANCHES = 'SEMARANG' THEN X.AMOUNT ELSE 0 END),0) AS AMT_SMR
					,ROUND(SUM(X.AMOUNT),0) AS TOTAL
			FROM	(
						SELECT	NULL AS GROUP_NO
								, 'Outstanding SO (Carried over to Semester II)' AS GROUP_DESC
								, 'OS2' AS GROUP_ITEM_CAT
								, 'Outstanding SO' AS ITEM_CAT
								, BRANCHES
								, CASE WHEN	(	DATEPART(MONTH, $P{TglInvTo}) IN (1,2,3,4) AND
												Q_OUT_SO IN (3,4)) THEN AMOUNT ELSE 0 END AS AMOUNT
						FROM [dbo].[TEMP08_SALES_REVENUE]
						WHERE	GROUP_NO  = 3 AND
								PROSES_ID = $P{ProsesId} AND
								(
									(
										SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) <= SUBSTRING(CONVERT(varchar, GETDATE(), 105), 7,4) AND
										YEAR_OUT_SO = SUBSTRING(CONVERT(varchar,  GETDATE(), 105), 7,4)
									)
										OR
									(
										SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) > SUBSTRING(CONVERT(varchar, GETDATE(), 105), 7,4) AND
										YEAR_OUT_SO <= SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4)
									)
								)
					) X
			GROUP BY X.GROUP_NO, X.GROUP_DESC, X.GROUP_ITEM_CAT, X.ITEM_CAT
		) XX
WHERE XX.TOTAL <> 0
UNION ALL
SELECT	4 AS GR_CRNXY
		,X.GROUP_NO
		,X.GROUP_DESC
		,X.GROUP_ITEM_CAT
		,X.ITEM_CAT
		,X.AMT_HO
		,X.AMT_SBY
		,X.AMT_DPS
		,X.AMT_CKR
		,X.AMT_SMR
		,(X.AMT_HO+X.AMT_SBY+X.AMT_DPS+X.AMT_CKR+X.AMT_SMR) AS TOTAL
		, 0 AS AMT_TARGET
		, '' AS LBL_TARGET
		, 0 AS PCN_HO
		, 0 AS PCN_SBY
		, 0 AS PCN_DPS
		, 0 AS PCN_CKR
		, 0 AS PCN_SMR
		, 0 AS PCN_TOTAL
FROM	(
			SELECT	NULL AS GROUP_NO
					,'Uang Muka (Carried over to Next Year)' AS GROUP_DESC
					,'OS1' AS GROUP_ITEM_CAT
					,'Uang Muka' AS ITEM_CAT
					,ROUND(SUM(CASE WHEN BRANCHES = '10' THEN AMOUNT ELSE 0 END),0) AS AMT_HO
					,ROUND(SUM(CASE WHEN BRANCHES = '15' THEN AMOUNT ELSE 0 END),0) AS AMT_SBY
					,ROUND(SUM(CASE WHEN BRANCHES = '19' THEN AMOUNT ELSE 0 END),0) AS AMT_DPS
					,ROUND(SUM(CASE WHEN BRANCHES = '16' THEN AMOUNT ELSE 0 END),0) AS AMT_CKR
					,ROUND(SUM(CASE WHEN BRANCHES = '17' THEN AMOUNT ELSE 0 END),0) AS AMT_SMR
			FROM [dbo].[TEMP08_SALES_REVENUE]
			WHERE	GROUP_NO  = 2 AND
					PROSES_ID = $P{ProsesId} AND
					YEAR_OUT_SO = (SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) + 1)
		) X
UNION ALL
SELECT	5 AS GR_CRNXY
		,X.GROUP_NO
		,X.GROUP_DESC
		,X.GROUP_ITEM_CAT
		,X.ITEM_CAT
		,X.AMT_HO
		,X.AMT_SBY
		,X.AMT_DPS
		,X.AMT_CKR
		,X.AMT_SMR
		,(X.AMT_HO+X.AMT_SBY+X.AMT_DPS+X.AMT_CKR+X.AMT_SMR) AS TOTAL
		, 0 AS AMT_TARGET
		, '' AS LBL_TARGET
		, 0 AS PCN_HO
		, 0 AS PCN_SBY
		, 0 AS PCN_DPS
		, 0 AS PCN_CKR
		, 0 AS PCN_SMR
		, 0 AS PCN_TOTAL
FROM	(
			SELECT	NULL AS GROUP_NO
					,'Outstanding SO (Carried over to Next Year)' AS GROUP_DESC
					,'OS2' AS GROUP_ITEM_CAT
					,'Outstanding SO' AS ITEM_CAT
					,ROUND(SUM(CASE WHEN BRANCHES = 'JAKARTA' THEN AMOUNT ELSE 0 END),0) AS AMT_HO
					,ROUND(SUM(CASE WHEN BRANCHES = 'SURABAYA' THEN AMOUNT ELSE 0 END),0) AS AMT_SBY
					,ROUND(SUM(CASE WHEN BRANCHES = 'BALI' THEN AMOUNT ELSE 0 END),0) AS AMT_DPS
					,ROUND(SUM(CASE WHEN BRANCHES = 'CIKARANG' THEN AMOUNT ELSE 0 END),0) AS AMT_CKR
					,ROUND(SUM(CASE WHEN BRANCHES = 'SEMARANG' THEN AMOUNT ELSE 0 END),0) AS AMT_SMR
			FROM [dbo].[TEMP08_SALES_REVENUE]
			WHERE	GROUP_NO  = 3 AND
					PROSES_ID = $P{ProsesId} AND
					YEAR_OUT_SO = (SUBSTRING(CONVERT(varchar, $P{TglInvTo}, 105), 7,4) + 1)
		) X]]>
	</queryString>
	<field name="GR_CRNXY" class="java.lang.Integer"/>
	<field name="GROUP_NO" class="java.lang.Integer"/>
	<field name="GROUP_DESC" class="java.lang.String"/>
	<field name="GROUP_ITEM_CAT" class="java.lang.String"/>
	<field name="ITEM_CAT" class="java.lang.String"/>
	<field name="AMT_HO" class="java.math.BigDecimal"/>
	<field name="AMT_SBY" class="java.math.BigDecimal"/>
	<field name="AMT_DPS" class="java.math.BigDecimal"/>
	<field name="AMT_CKR" class="java.math.BigDecimal"/>
	<field name="AMT_SMR" class="java.math.BigDecimal"/>
