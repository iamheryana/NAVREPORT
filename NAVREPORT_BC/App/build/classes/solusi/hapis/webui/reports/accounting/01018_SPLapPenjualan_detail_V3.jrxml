	<queryString>
		<![CDATA[SELECT XX.REG
        , XX.NO_INV
		, XX.TGL_INV
		, XX.KODE_CUST
		, XX.NAMA_CUST
		, XX.NO_PO_CUST
		, XX.NO_FP
		, XX.NILAI_PENJUALAN
		, CASE WHEN XX.RN = 1 THEN XX.NILAI_PENJUALAN ELSE 0 END AS SUM_NILAI_PENJUALAN
		, XX.NILAI_DISC
		, CASE WHEN XX.RN = 1 THEN XX.NILAI_DISC ELSE 0 END AS SUM_NILAI_DISC
		, XX.NILAI_UM
		, CASE WHEN XX.RN = 1 THEN XX.NILAI_UM ELSE 0 END AS SUM_NILAI_UM
		, XX.NILAI_DPP
		, CASE WHEN XX.RN = 1 THEN XX.NILAI_DPP ELSE 0 END AS SUM_NILAI_DPP
		, XX.NILAI_PPN
		, CASE WHEN XX.RN = 1 THEN XX.NILAI_PPN ELSE 0 END AS SUM_NILAI_PPN
		, XX.NILAI_TOTAL
		, CASE WHEN XX.RN = 1 THEN XX.NILAI_TOTAL ELSE 0 END AS SUM_NILAI_TOTAL
		, XX.NILAI_COGS
		, CASE WHEN XX.RN = 1 THEN XX.NILAI_COGS ELSE 0 END AS SUM_NILAI_COGS
		, XX.MARGIN
		, CASE WHEN XX.RN = 1 THEN XX.MARGIN ELSE 0 END AS SUM_MARGIN
		, XX.MARGIN_PCN
		, CASE WHEN XX.RN = 1 THEN XX.MARGIN_PCN ELSE 0 END AS SUM_MARGIN_PCN
		, XX.CABANG
		, XX.CRM_NO
		, XX.ITEM_CAT
		, XX.ITEM_CAT_DESC
		, XX.NILAI_PENJUALAN_DTL
		, XX.NILAI_DISC_DTL
		, XX.NILAI_UM_DTL
		, XX.NILAI_DPP_DTL
		, XX.NILAI_PPN_DTL
		, XX.NILAI_TOTAL_DTL
		, XX.NILAI_COGS_DTL
FROM (
SELECT X.REG
        , X.NO_INV
		, X.TGL_INV
		, X.KODE_CUST
		, X.NAMA_CUST
		, X.NO_PO_CUST
		, X.NO_FP
		, X.NILAI_PENJUALAN
		, X.NILAI_DISC
		, X.NILAI_UM
		, X.NILAI_DPP
		, X.NILAI_PPN
		, X.NILAI_TOTAL
		, X.NILAI_COGS
		, X.MARGIN
		, X.MARGIN_PCN
		, CAB.[Name] AS CABANG
		, CRM.[No_] AS CRM_NO
		, ROW_NUMBER() OVER (PARTITION BY X.NO_INV ORDER BY X.NO_INV ) AS RN
		, DPJL.ITEM_CAT
		, DPJL.ITEM_CAT_DESC
		, DPJL.NILAI_PENJUALAN AS NILAI_PENJUALAN_DTL
		, DPJL.NILAI_DISC AS NILAI_DISC_DTL
		, DPJL.NILAI_UM AS NILAI_UM_DTL
		, DPJL.NILAI_DPP AS NILAI_DPP_DTL
		, DPJL.NILAI_PPN AS NILAI_PPN_DTL
		, DPJL.NILAI_TOTAL AS NILAI_TOTAL_DTL
		, DPJL.NILAI_COGS AS NILAI_COGS_DTL
FROM (
SELECT CASE WHEN SUBSTRING(SI.[No_],1,2) = 'UM' THEN 'INVOICE UANG MUKA' ELSE 'INVOICE' END AS REG
        , SI.[No_] AS NO_INV
		, SI.[Posting Date] AS TGL_INV
		, SI.[Shortcut Dimension 1 Code] AS CABANG
		, SI.[Bill-to Customer No_] AS KODE_CUST
		, SI.[Bill-to Name] AS NAMA_CUST
		, SI.[External Document No_] AS NO_PO_CUST
		, SI.[Faktur Pajak No_] AS NO_FP
		, DTL.NILAI_PENJUALAN
		, DTL.NILAI_DISC
		, DTL.NILAI_UM
		, DTL.NILAI_DPP
		, DTL.NILAI_PPN
		, DTL.NILAI_TOTAL
		, DTL.NILAI_COGS
		, DTL.NILAI_DPP - DTL.NILAI_COGS AS MARGIN
		,	CASE WHEN DTL.NILAI_DPP > 0 THEN
				((DTL.NILAI_DPP - DTL.NILAI_COGS)/DTL.NILAI_DPP)*100
			ELSE
				0
			END AS MARGIN_PCN
FROM [dbo].[SOLUSI$Sales Invoice Header] SI
INNER JOIN (
			SELECT	INV.NO_INVOICE
					,SUM(INV.NILAI_PENJUALAN) * -1 AS NILAI_PENJUALAN
					,SUM(INV.NILAI_DISC) * -1 AS NILAI_DISC
					,SUM(INV.NILAI_UM) * -1 AS NILAI_UM
					,(SUM(INV.NILAI_PENJUALAN) + SUM(INV.NILAI_DISC) + SUM(INV.NILAI_UM)) * -1 AS NILAI_DPP
					,SUM(INV.NILAI_PPN) * -1 AS NILAI_PPN
					,(SUM(INV.NILAI_PENJUALAN) + SUM(INV.NILAI_DISC) + SUM(INV.NILAI_UM) + SUM(INV.NILAI_PPN)) * -1 AS NILAI_TOTAL
					,SUM(INV.NILAI_COGS) AS NILAI_COGS
			FROM	(
						SELECT GLE.[Document No_] AS NO_INVOICE
								,CASE WHEN GLE.[G_L Account No_] IN ('400.110.00', '400.110.01', '400.110.04') THEN
									GLE.[Amount]
								ELSE
									0
								END AS NILAI_PENJUALAN
								,CASE WHEN GLE.[G_L Account No_] IN ('420.110.00', '420.110.01') THEN
									GLE.[Amount]
								ELSE
									0
								END AS NILAI_DISC
								,CASE WHEN GLE.[G_L Account No_] = ('400.110.03') THEN
									GLE.[Amount]
								ELSE
									0
								END AS NILAI_UM
								,CASE WHEN GLE.[G_L Account No_]= '230.190.00' THEN
									GLE.[Amount]
								ELSE
									0
								END AS NILAI_PPN
								,CASE WHEN GLE.[G_L Account No_]= '500.110.00' THEN
									CASE WHEN MIN(GLE.[Transaction No_]) OVER(PARTITION BY GLE.[Document No_]) = GLE.[Transaction No_] THEN GLE.[Debit Amount]
									ELSE GLE.[Amount]
									END
								ELSE
									0
								END AS NILAI_COGS
						FROM [dbo].[SOLUSI$G_L Entry] GLE
						INNER JOIN [dbo].[SOLUSI$Sales Invoice Header] SIA ON SIA.[No_] = GLE.[Document No_]
						WHERE GLE.[G_L Account No_] IN ('500.110.00' , '400.110.00', '230.190.00', '400.110.01', '420.110.00', '420.110.01', '400.110.03', '400.110.04')
					) INV
			GROUP BY INV.NO_INVOICE
		) DTL ON SI.[No_] = DTL.NO_INVOICE
UNION ALL
SELECT 'CREDIT MEMO' AS REG
		, SCI.[No_] AS NO_INV
		, SCI.[Posting Date] AS TGL_INV
		, SCI.[Shortcut Dimension 1 Code] AS CABANG
		, SCI.[Bill-to Customer No_] AS KODE_CUST
		, SCI.[Bill-to Name] AS NAMA_CUST
		, SCI.[External Document No_] AS NO_PO_CUST
		, SCI.[Faktur Pajak No_] AS NO_FP
		, DTL.NILAI_PENJUALAN
		, DTL.NILAI_DISC
		, DTL.NILAI_UM
		, DTL.NILAI_DPP
		, DTL.NILAI_PPN
		, DTL.NILAI_TOTAL
		, DTL.NILAI_COGS
		, DTL.NILAI_DPP - DTL.NILAI_COGS AS MARGIN
		,	CASE WHEN DTL.NILAI_DPP < 0 THEN
				((DTL.NILAI_DPP - DTL.NILAI_COGS)/DTL.NILAI_DPP)*100
			ELSE
				0
			END AS MARGIN_PCN
FROM [dbo].[SOLUSI$Sales Cr_Memo Header] SCI
INNER JOIN (
				SELECT	CINV.NO_INVOICE
						,SUM(CINV.NILAI_PENJUALAN) * -1 AS NILAI_PENJUALAN
						,SUM(CINV.NILAI_DISC) * -1 AS NILAI_DISC
						,SUM(CINV.NILAI_UM) * -1 AS NILAI_UM
						,(SUM(CINV.NILAI_PENJUALAN) + SUM(CINV.NILAI_DISC) + SUM(CINV.NILAI_UM)) * -1 AS NILAI_DPP
						,SUM(CINV.NILAI_PPN) * -1 AS NILAI_PPN
						,(SUM(CINV.NILAI_PENJUALAN) + SUM(CINV.NILAI_DISC) + SUM(CINV.NILAI_UM) + SUM(CINV.NILAI_PPN)) * -1 AS NILAI_TOTAL
						,SUM(CINV.NILAI_COGS) * -1 AS NILAI_COGS

				FROM	(
							SELECT GLE.[Document No_] AS NO_INVOICE
									,CASE WHEN GLE.[G_L Account No_] IN ('410.110.00', '410.110.01', '400.110.04') THEN
										GLE.[Amount]
									ELSE
										0
									END AS NILAI_PENJUALAN
									,CASE WHEN GLE.[G_L Account No_] IN ('420.110.00', '420.110.01' ) THEN
										GLE.[Amount]
									ELSE
										0
									END AS NILAI_DISC
									,CASE WHEN GLE.[G_L Account No_] = ('400.110.03') THEN
										GLE.[Amount]
									ELSE
										0
									END AS NILAI_UM
									,CASE WHEN GLE.[G_L Account No_]= '230.190.00' THEN
										GLE.[Amount]
									END AS NILAI_PPN
									,CASE WHEN GLE.[G_L Account No_]= '500.110.00' THEN
										CASE WHEN MIN(GLE.[Transaction No_]) OVER(PARTITION BY GLE.[Document No_]) = GLE.[Transaction No_] THEN GLE.[Credit Amount]
										ELSE GLE.[Amount]
										END
									ELSE
										0
									END AS NILAI_COGS
							FROM [dbo].[SOLUSI$G_L Entry] GLE
							INNER JOIN [dbo].[SOLUSI$Sales Cr_Memo Header] CIA ON CIA.[No_] = GLE.[Document No_]
							WHERE GLE.[G_L Account No_] IN ('500.110.00', '230.190.00', '410.110.00', '410.110.01', '420.110.00', '420.110.01', '400.110.03', '400.110.04')
						) CINV
				GROUP BY CINV.NO_INVOICE
		) DTL ON SCI.[No_] = DTL.NO_INVOICE
) X
LEFT JOIN	(
				SELECT [Applies-to Doc_ No_], MAX([No_]) AS [No_]
				FROM [dbo].[SOLUSI$Sales Cr_Memo Header]
				GROUP BY [Applies-to Doc_ No_]
			) CRM ON CRM.[Applies-to Doc_ No_] = X.NO_INV
LEFT JOIN [dbo].[SOLUSI$Responsibility Center] CAB ON CAB.[Code] = X.CABANG
LEFT JOIN	(
				SELECT DPJ.NO_INVOICE
						, DPJ.ITEM_CAT
						, IC.[Description] AS ITEM_CAT_DESC
						,DPJ.NILAI_PENJUALAN
						,DPJ.NILAI_DISC
						,DPJ.NILAI_UM
						,DPJ.NILAI_DPP
						,DPJ.NILAI_PPN
						,DPJ.NILAI_TOTAL
						,DPJ.NILAI_COGS
				FROM	(
							SELECT	INV.NO_INVOICE
									,INV.ITEM_CAT
									,SUM(INV.NILAI_PENJUALAN) * -1 AS NILAI_PENJUALAN
									,SUM(INV.NILAI_DISC) * -1 AS NILAI_DISC
									,SUM(INV.NILAI_UM) * -1 AS NILAI_UM
									,(SUM(INV.NILAI_PENJUALAN) + SUM(INV.NILAI_DISC) + SUM(INV.NILAI_UM)) * -1 AS NILAI_DPP
									,SUM(INV.NILAI_PPN) * -1 AS NILAI_PPN
									,(SUM(INV.NILAI_PENJUALAN) + SUM(INV.NILAI_DISC) + SUM(INV.NILAI_UM) + SUM(INV.NILAI_PPN)) * -1 AS NILAI_TOTAL
									,SUM(INV.NILAI_COGS) AS NILAI_COGS
							FROM	(
										SELECT GLE.[Document No_] AS NO_INVOICE
												, GLE.[Global Dimension 2 Code] AS ITEM_CAT
												,CASE WHEN GLE.[G_L Account No_] IN ('400.110.00', '400.110.01', '400.110.04') THEN
													GLE.[Amount]
												ELSE
													0
												END AS NILAI_PENJUALAN
												,CASE WHEN GLE.[G_L Account No_] IN ('420.110.00', '420.110.01') THEN
													GLE.[Amount]
												ELSE
													0
												END AS NILAI_DISC
												,CASE WHEN GLE.[G_L Account No_] = ('400.110.03') THEN
													GLE.[Amount]
												ELSE
													0
												END AS NILAI_UM
												,CASE WHEN GLE.[G_L Account No_]= '230.190.00' THEN
													GLE.[Amount]
												ELSE
													0
												END AS NILAI_PPN
												,CASE WHEN GLE.[G_L Account No_]= '500.110.00' THEN
													CASE WHEN MIN(GLE.[Transaction No_]) OVER(PARTITION BY GLE.[Document No_]) = GLE.[Transaction No_] THEN GLE.[Debit Amount]
													ELSE GLE.[Amount]
													END
												ELSE
													0
												END AS NILAI_COGS
										FROM [dbo].[SOLUSI$G_L Entry] GLE
										INNER JOIN [dbo].[SOLUSI$Sales Invoice Header] SIA ON SIA.[No_] = GLE.[Document No_]
										WHERE GLE.[G_L Account No_] IN ('500.110.00' , '400.110.00', '230.190.00', '400.110.01', '420.110.00', '420.110.01', '400.110.03', '400.110.04')
									) INV
							GROUP BY INV.NO_INVOICE, INV.ITEM_CAT
							UNION ALL
							SELECT	CINV.NO_INVOICE
									,CINV.ITEM_CAT
									,SUM(CINV.NILAI_PENJUALAN) * -1 AS NILAI_PENJUALAN
									,SUM(CINV.NILAI_DISC) * -1 AS NILAI_DISC
									,SUM(CINV.NILAI_UM) * -1 AS NILAI_UM
									,(SUM(CINV.NILAI_PENJUALAN) + SUM(CINV.NILAI_DISC) + SUM(CINV.NILAI_UM)) * -1 AS NILAI_DPP
									,SUM(CINV.NILAI_PPN) * -1 AS NILAI_PPN
									,(SUM(CINV.NILAI_PENJUALAN) + SUM(CINV.NILAI_DISC) + SUM(CINV.NILAI_UM) + SUM(CINV.NILAI_PPN)) * -1 AS NILAI_TOTAL
									,SUM(CINV.NILAI_COGS) * -1 AS NILAI_COGS
							FROM	(
										SELECT GLE.[Document No_] AS NO_INVOICE
												, GLE.[Global Dimension 2 Code] AS ITEM_CAT
												,CASE WHEN GLE.[G_L Account No_] IN ('410.110.00', '410.110.01', '400.110.04') THEN
													GLE.[Amount]
												ELSE
													0
												END AS NILAI_PENJUALAN
												,CASE WHEN GLE.[G_L Account No_] IN ('420.110.00', '420.110.01' ) THEN
													GLE.[Amount]
												ELSE
													0
												END AS NILAI_DISC
												,CASE WHEN GLE.[G_L Account No_] = ('400.110.03') THEN
													GLE.[Amount]
												ELSE
													0
												END AS NILAI_UM
												,CASE WHEN GLE.[G_L Account No_]= '230.190.00' THEN
													GLE.[Amount]
												END AS NILAI_PPN
												,CASE WHEN GLE.[G_L Account No_]= '500.110.00' THEN
													CASE WHEN MIN(GLE.[Transaction No_]) OVER(PARTITION BY GLE.[Document No_]) = GLE.[Transaction No_] THEN GLE.[Credit Amount]
													ELSE GLE.[Amount]
													END
												ELSE
													0
												END AS NILAI_COGS
										FROM [dbo].[SOLUSI$G_L Entry] GLE
										INNER JOIN [dbo].[SOLUSI$Sales Cr_Memo Header] CIA ON CIA.[No_] = GLE.[Document No_]
										WHERE GLE.[G_L Account No_] IN ('500.110.00', '230.190.00', '410.110.00', '410.110.01', '420.110.00', '420.110.01', '400.110.03', '400.110.04')
									) CINV
							GROUP BY CINV.NO_INVOICE, CINV.ITEM_CAT
						) DPJ
				LEFT JOIN [dbo].[SOLUSI$Item Category] IC ON IC.[Code] = DPJ.ITEM_CAT
			) DPJL ON DPJL.NO_INVOICE = X.NO_INV
) XX
WHERE (XX.TGL_INV >= $P{TglInvFrom} AND XX.TGL_INV <= $P{TglInvTo}) AND
	($P{Cabang} = 'ALL' OR XX.CABANG = $P{Cabang}) AND
  (XX.KODE_CUST >= $P{CustFrom} AND XX.KODE_CUST <= $P{CustUpto})]]>
	</queryString>
