<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="04038_ForecastPrincipal" language="groovy" pageWidth="1800" pageHeight="595" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="1760" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="b406b291-7ccc-4e85-8d33-6ed4d9da73cd">
	<property name="ireport.zoom" value="1.4641000000001414"/>
	<property name="ireport.x" value="587"/>
	<property name="ireport.y" value="0"/>
	<parameter name="Potensi" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Size" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ItemCategory" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Principal" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Margin" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Curr" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT	X.COMPANY
		,X.CUSTOMER
		,X.SALES_PRINCIPAL
		,X.DIM_SECTOR
		,X.DIM_SECTOR_NAME
		,X.PROJECT_TITLE
		,X.ITEM
		,X.ITEM_DESC
		,X.SALES
		,X.SIZE_PROJECT
		,X.POTENSIAL_PROJECT
		,X.DIM_APPLICATION
		,X.DIM_APPLICATION_NAME
		,X.DIM_PRINCIPAL
		,X.DIM_PRINCIPAL_NAME
		,X.ITEM_CAT
		,X.HW_PS
		,X.UNIT_PRICE
		,X.QTY_OUT
		,X.QTY_Q1
		,X.QTY_Q2
		,X.QTY_Q3
		,X.QTY_Q4
		,CASE WHEN  $P{Curr} = 'USD' THEN
			ROUND(((X.AMT_Q1 * (1 - CAST($P{Margin} AS DECIMAL(10,2)) / 100)) / ECX.EXC_RATE),-2)
		 ELSE
			ROUND((X.AMT_Q1 * (1 - CAST($P{Margin} AS DECIMAL(10,2)) / 100)),-3)
		 END AS AMT_Q1
		,CASE WHEN  $P{Curr} = 'USD' THEN
			ROUND(((X.AMT_Q2 * (1 - CAST($P{Margin} AS DECIMAL(10,2)) / 100)) / ECX.EXC_RATE),-2)
		 ELSE
			ROUND((X.AMT_Q2 * (1 - CAST($P{Margin} AS DECIMAL(10,2)) / 100)),-3)
		 END AS AMT_Q2
		,CASE WHEN  $P{Curr} = 'USD' THEN
			ROUND(((X.AMT_Q3 * (1 - CAST($P{Margin} AS DECIMAL(10,2)) / 100)) / ECX.EXC_RATE),-2)
		 ELSE
			ROUND((X.AMT_Q3 * (1 - CAST($P{Margin} AS DECIMAL(10,2)) / 100)),-3)
		 END AS AMT_Q3
		,CASE WHEN  $P{Curr} = 'USD' THEN
			ROUND(((X.AMT_Q4 * (1 - CAST($P{Margin} AS DECIMAL(10,2)) / 100)) / ECX.EXC_RATE),-2)
		 ELSE
			ROUND((X.AMT_Q4 * (1 - CAST($P{Margin} AS DECIMAL(10,2)) / 100)),-3)
		 END AS AMT_Q4
		,X.JUDUL_Q1
		,X.JUDUL_Q2
		,X.JUDUL_Q3
		,X.JUDUL_Q4
		,ECX.EXC_RATE
FROM	(
SELECT	MAM.COMPANY
		,MAM.CUSTOMER
		,MAM.SALES_PRINCIPAL
		,MAM.DIM_SECTOR
		,MAM.DIM_SECTOR_NAME
		,MAM.PROJECT_TITLE
		,MAM.ITEM
		,MAM.ITEM_DESC
		,MAM.SALES
		,MAM.SIZE_PROJECT
		,MAM.POTENSIAL_PROJECT
		,MAM.DIM_APPLICATION
		,MAM.DIM_APPLICATION_NAME
		,MAM.DIM_PRINCIPAL
		,MAM.DIM_PRINCIPAL_NAME
		,MAM.ITEM_CAT
		,MAM.HW_PS
		,MAM.UNIT_PRICE
		,MAM.QTY_OUT
		,MAM.QTY_Q1
		,(MAM.UNIT_PRICE * MAM.QTY_Q1) AS AMT_Q1
		,MAM.QTY_Q2
		,(MAM.UNIT_PRICE * MAM.QTY_Q2) AS AMT_Q2
		,MAM.QTY_Q3
		,(MAM.UNIT_PRICE * MAM.QTY_Q3) AS AMT_Q3
		,MAM.QTY_Q4
		,(MAM.UNIT_PRICE * MAM.QTY_Q4) AS AMT_Q4
		,MAM.JUDUL_Q1
		,MAM.JUDUL_Q2
		,MAM.JUDUL_Q3
		,MAM.JUDUL_Q4
FROM	(
			SELECT
					MAD.COMPANY
					,MAD.SALES
					,MAD.CUSTOMER
					,MAD.SALES_PRINCIPAL
					,MAD.PROJECT_TITLE
					,MAD.SIZE_PROJECT
					,MAD.POTENSIAL_PROJECT
					,MAD.DIM_SECTOR
					,MAD.DIM_SECTOR_NAME
					,MAD.DIM_APPLICATION
					,MAD.DIM_APPLICATION_NAME
					,MAD.DIM_PRINCIPAL
					,MAD.DIM_PRINCIPAL_NAME
					,MAD.ITEM
					,MAD.ITEM_DESC
					,MAD.ITEM_CAT
					,MAD.HW_PS
					,MAX(MAD.UNIT_PRICE) AS UNIT_PRICE
					,SUM(MAD.QTY_OUT) AS QTY_OUT
					,SUM(MAD.QTY_Q1) AS QTY_Q1
					,SUM(MAD.QTY_Q2) AS QTY_Q2
					,SUM(MAD.QTY_Q3) AS QTY_Q3
					,SUM(MAD.QTY_Q4) AS QTY_Q4
					,MAX(MAD.JUDUL_Q1) AS JUDUL_Q1
					,MAX(MAD.JUDUL_Q2) AS JUDUL_Q2
					,MAX(MAD.JUDUL_Q3) AS JUDUL_Q3
					,MAX(MAD.JUDUL_Q4) AS JUDUL_Q4
			FROM	(
						SELECT	CASE WHEN SOH.[No_ Series] = 'SBOM' THEN 'AJ' ELSE 'SP' END AS COMPANY
								,SOH.[Salesperson Code] AS SALES
								,CASE WHEN SOH.[External Document No_] = '' THEN
									SOH.[Sell-to Customer Name]
								 ELSE
									SOH.[Sell-to Customer Name]+' / '+ SOH.[External Document No_]
								 END AS CUSTOMER
								,	CASE WHEN SOL.[Warranty Code] = '' THEN
										CASE WHEN SUBSTRING(SOH.[Posting Description], 1,1) NOT IN ('L','R','M','H','A') THEN
											'R'
										ELSE
											SUBSTRING(SOH.[Posting Description], 1,1)
										END
									ELSE
										CASE WHEN SOL.[Warranty Code] = 'CLOSED' THEN 'H'
										ELSE
											CASE WHEN SUBSTRING(SOH.[Posting Description], 1,1) NOT IN ('L','R','M','H','A') THEN
												'R'
											ELSE
												SUBSTRING(SOH.[Posting Description], 1,1)
											END
										END
									END AS SIZE_PROJECT
								, CASE WHEN SUBSTRING(SOH.[Posting Description], 2,1) NOT IN ('1','0') THEN '0' ELSE SUBSTRING(SOH.[Posting Description], 2,1) END AS POTENSIAL_PROJECT
								,SOH.[Sell-to Contact] AS SALES_PRINCIPAL
								,SOH.[Your Reference] AS PROJECT_TITLE
								,DIMH.DIM_SECTOR
								,DIMH.DIM_SECTOR_NAME
								,DIMH.DIM_APPLICATION
								,DIMH.DIM_APPLICATION_NAME
								,DIMD.DIM_PRINCIPAL
								,DIMD.DIM_PRINCIPAL_NAME
								,SOL.[No_] AS ITEM
								,SOL.[Description] AS ITEM_DESC
								,SOL.[Shortcut Dimension 2 Code] AS ITEM_CAT
								,SUBSTRING(SOL.[Shortcut Dimension 2 Code] , 1,2) AS HW_PS
								,SOL.[Quantity] AS QTY_OUT
								,SOL.[Unit Price] AS UNIT_PRICE
								,CASE WHEN DATEPART(quarter, GETDATE())  >  DATEPART(quarter, SOL.[Shipment Date]) AND
										   DATEPART(year, GETDATE())  >=  DATEPART(year, SOL.[Shipment Date]) THEN
									SOL.[Qty_ to Ship]
								 ELSE
									CASE WHEN DATEPART(quarter, GETDATE())  =  DATEPART(quarter, SOL.[Shipment Date]) AND
											  DATEPART(year, GETDATE())  =  DATEPART(year, SOL.[Shipment Date]) THEN SOL.[Qty_ to Ship] ELSE 0 END
								 END AS QTY_Q1
								,CASE WHEN DATEPART(quarter, GETDATE()) < 4 THEN
									CASE WHEN DATEPART(quarter, GETDATE()) + 1	=  DATEPART(quarter, SOL.[Shipment Date]) AND
											  DATEPART(year, GETDATE())  =  DATEPART(year, SOL.[Shipment Date]) THEN SOL.[Qty_ to Ship] ELSE 0 END
								 ELSE
									CASE WHEN 1	=  DATEPART(quarter, SOL.[Shipment Date]) AND
											  DATEPART(year, GETDATE()) + 1  =  DATEPART(year, SOL.[Shipment Date]) THEN SOL.[Qty_ to Ship] ELSE 0 END
								 END AS QTY_Q2
								,CASE WHEN DATEPART(quarter, GETDATE()) < 3 THEN
									CASE WHEN DATEPART(quarter, GETDATE()) + 2	=  DATEPART(quarter, SOL.[Shipment Date]) AND
											  DATEPART(year, GETDATE())  =  DATEPART(year, SOL.[Shipment Date]) THEN SOL.[Qty_ to Ship] ELSE 0 END
								 ELSE
									CASE WHEN DATEPART(quarter, GETDATE()) < 4 THEN
										CASE WHEN 1	=  DATEPART(quarter, SOL.[Shipment Date]) AND
												  DATEPART(year, GETDATE()) + 1  =  DATEPART(year, SOL.[Shipment Date]) THEN SOL.[Qty_ to Ship] ELSE 0 END
									ELSE
										CASE WHEN 2	=  DATEPART(quarter, SOL.[Shipment Date]) AND
												  DATEPART(year, GETDATE()) + 1  =  DATEPART(year, SOL.[Shipment Date]) THEN SOL.[Qty_ to Ship] ELSE 0 END
									END
								 END AS QTY_Q3
								,CASE WHEN DATEPART(quarter, GETDATE()) < 2 THEN
									CASE WHEN DATEPART(quarter, GETDATE()) + 3	=  DATEPART(quarter, SOL.[Shipment Date]) AND
											  DATEPART(year, GETDATE())  =  DATEPART(year, SOL.[Shipment Date]) THEN SOL.[Qty_ to Ship] ELSE 0 END
								 ELSE
									CASE WHEN DATEPART(quarter, GETDATE()) < 3 THEN
										CASE WHEN 1	=  DATEPART(quarter, SOL.[Shipment Date]) AND
												  DATEPART(year, GETDATE()) + 1  =  DATEPART(year, SOL.[Shipment Date]) THEN SOL.[Qty_ to Ship] ELSE 0 END
									ELSE
										CASE WHEN DATEPART(quarter, GETDATE()) < 4 THEN
											CASE WHEN 2	=  DATEPART(quarter, SOL.[Shipment Date]) AND
													  DATEPART(year, GETDATE()) + 1  =  DATEPART(year, SOL.[Shipment Date]) THEN SOL.[Qty_ to Ship] ELSE 0 END
										ELSE
											CASE WHEN 3	=  DATEPART(quarter, SOL.[Shipment Date]) AND
													  DATEPART(year, GETDATE()) + 1  =  DATEPART(year, SOL.[Shipment Date]) THEN SOL.[Qty_ to Ship] ELSE 0 END
										END
									END
								 END AS QTY_Q4
								,'Q'+LTRIM(STR(DATEPART(quarter, GETDATE())))+' - '+LTRIM(STR(DATEPART(year, GETDATE()))) AS JUDUL_Q1
								,CASE WHEN DATEPART(quarter, GETDATE()) < 4 THEN
									'Q'+LTRIM(STR(DATEPART(quarter, GETDATE())+1))+' - '+LTRIM(STR(DATEPART(year, GETDATE())))
								 ELSE
									'Q1'+' - '+LTRIM(STR(DATEPART(year, GETDATE())+1))
								 END AS JUDUL_Q2
								,CASE WHEN DATEPART(quarter, GETDATE()) < 3 THEN
									'Q'+LTRIM(STR(DATEPART(quarter, GETDATE())+2))+' - '+LTRIM(STR(DATEPART(year, GETDATE())))
								 ELSE
									 CASE WHEN DATEPART(quarter, GETDATE()) < 4 THEN
										'Q1'+' - '+LTRIM(STR(DATEPART(year, GETDATE())+1))
									 ELSE
										'Q2'+' - '+LTRIM(STR(DATEPART(year, GETDATE())+1))
									 END
								 END AS JUDUL_Q3
								,CASE WHEN DATEPART(quarter, GETDATE()) < 2 THEN
									'Q'+LTRIM(STR(DATEPART(quarter, GETDATE())+3))+' - '+LTRIM(STR(DATEPART(year, GETDATE())))
								 ELSE
									 CASE WHEN DATEPART(quarter, GETDATE()) < 3 THEN
										'Q1'+' - '+LTRIM(STR(DATEPART(year, GETDATE())+1))
									 ELSE
										 CASE WHEN DATEPART(quarter, GETDATE()) < 4 THEN
											'Q2'+' - '+LTRIM(STR(DATEPART(year, GETDATE())+1))
										 ELSE
											'Q3'+' - '+LTRIM(STR(DATEPART(year, GETDATE())+1))
										 END
									 END
								 END AS JUDUL_Q4
						FROM [dbo].[BC_AUTOJAYA$Sales Header] SOH
						INNER JOIN [dbo].[BC_AUTOJAYA$Sales Line] SOL ON SOL.[Document Type] = SOH.[Document Type] AND SOL.[Document No_] = SOH.[No_]
						LEFT JOIN	(
										SELECT X.[Dimension Set ID]
											  ,MAX(CASE WHEN X.[Dimension Code] = 'SECTOR' THEN X.[Dimension Value Code] ELSE NULL END) AS DIM_SECTOR
											  ,MAX(CASE WHEN X.[Dimension Code] = 'APPLICATION' THEN X.[Dimension Value Code] ELSE NULL END) AS DIM_APPLICATION
											  ,MAX(CASE WHEN X.[Dimension Code] = 'SECTOR' THEN Y.[Name] ELSE NULL END) AS DIM_SECTOR_NAME
											  ,MAX(CASE WHEN X.[Dimension Code] = 'APPLICATION' THEN Y.[Name] ELSE NULL END) AS DIM_APPLICATION_NAME
										FROM [dbo].[BC_AUTOJAYA$Dimension Set Entry] X
										LEFT JOIN [dbo].[BC_AUTOJAYA$Dimension Value] Y ON X.[Dimension Code] = Y.[Dimension Code] AND X.[Dimension Value Code] = Y.[Code]
										GROUP BY [Dimension Set ID]
									) DIMH ON DIMH.[Dimension Set ID] = SOH.[Dimension Set ID]
						LEFT JOIN	(
										SELECT X.[Dimension Set ID]
											  ,MAX(CASE WHEN X.[Dimension Code] = 'PRINCIPAL' THEN X.[Dimension Value Code] ELSE NULL END) AS DIM_PRINCIPAL
											  ,MAX(CASE WHEN X.[Dimension Code] = 'PRINCIPAL' THEN Y.[Name] ELSE NULL END) AS DIM_PRINCIPAL_NAME
										FROM [dbo].[BC_AUTOJAYA$Dimension Set Entry] X
										LEFT JOIN [dbo].[BC_AUTOJAYA$Dimension Value] Y ON X.[Dimension Code] = Y.[Dimension Code] AND X.[Dimension Value Code] = Y.[Code]
										GROUP BY [Dimension Set ID]
									) DIMD ON DIMD.[Dimension Set ID] = SOL.[Dimension Set ID]
						WHERE SOH.[No_ Series] IN ('SBOM' , 'SBOMS')
					) MAD
			GROUP BY MAD.COMPANY
					,MAD.SALES
					,MAD.CUSTOMER
					,MAD.SALES_PRINCIPAL
					,MAD.PROJECT_TITLE
					,MAD.SIZE_PROJECT
					,MAD.POTENSIAL_PROJECT
					,MAD.DIM_SECTOR
					,MAD.DIM_SECTOR_NAME
					,MAD.DIM_APPLICATION
					,MAD.DIM_APPLICATION_NAME
					,MAD.DIM_PRINCIPAL
					,MAD.DIM_PRINCIPAL_NAME
					,MAD.ITEM
					,MAD.ITEM_DESC
					,MAD.ITEM_CAT
					,MAD.HW_PS
			) MAM
		) X
LEFT JOIN	(
				SELECT [Relational Exch_ Rate Amount] AS EXC_RATE
				FROM [dbo].[BC_AUTOJAYA$Currency Exchange Rate]
				WHERE [Currency Code] = 'USD' AND
					  [Starting Date] = (
											SELECT  MAX([Starting Date]) AS EXC_DATE
											FROM [dbo].[BC_AUTOJAYA$Currency Exchange Rate]
											WHERE	[Currency Code] = 'USD' AND
													[Starting Date] <= GETDATE()
										)
			) ECX ON 1 = 1
		WHERE	(X.SIZE_PROJECT = $P{Size} OR $P{Size} = 'ALL') AND
				(X.POTENSIAL_PROJECT = $P{Potensi} OR $P{Potensi} = 'ALL') AND
				( ($P{ItemCategory} IN ('HW', 'PS') AND X.HW_PS = $P{ItemCategory}) OR
				  ($P{ItemCategory} IN ('PS01', 'PS02', 'PS03', 'PS04', 'PS05') AND X.ITEM_CAT= $P{ItemCategory}) OR
				  (X.ITEM = $P{ItemCategory}) OR
				  ($P{ItemCategory} = 'ALL')
				) AND
				(X.DIM_PRINCIPAL = $P{Principal} OR  $P{Principal} = 'ALL')]]>
	</queryString>
