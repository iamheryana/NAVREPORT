<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="0105011_LapPembelianImportNonPPN" language="groovy" pageWidth="1500" pageHeight="595" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="1460" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="e4a9c5ca-8c3f-4842-92fe-fcbf6b01d302">
	<property name="ireport.zoom" value="1.7715610000000133"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="TglUpto" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="TglFrom" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Company" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT	TGL.TAHUN
		, TGL.BULAN
		, TGL.BULAN_SORT
		, DATEPART(YEAR, $P{TglUpto}) - DATEPART(YEAR, $P{TglFrom}) AS JML_TAHUN
		, YYY.COMPANY
		, YYY.TIPE
		, YYY.REG
		, YYY.JENIS_PEMBELIAN
		, YYY.JENIS
		, YYY.JENIS_SORT
		, YYY.NO_INV
		, YYY.TGL_INV
		, YYY.NO_INV_CANCEL
		, YYY.KODE_SUPP
		, ISNULL(YYY.NAMA_SUPP, 'TIDAK ADA DATA') AS NAMA_SUPP
		, YYY.NO_INV_SUPP
		, YYY.NO_FP
		, YYY.AMT_BELI_NON_INVTR
		, YYY.AMT_UM_NON_INVTR
		, YYY.AMT_SUBTOTAL_NON_INVTR
		, YYY.AMT_BIAYA_KIRIM
		, YYY.AMT_BELI_INVTR
		, YYY.AMT_BELI_IC
		, YYY.AMT_UM_INVTR
		, YYY.AMT_SUBTOTAL_INVTR
		, ISNULL(YYY.TOTAL_DPP, 0) AS TOTAL_DPP
		, ISNULL(YYY.AMOUNT_PPN, 0) AS AMOUNT_PPN
		, ISNULL(YYY.TOTAL, 0) AS TOTAL
		, ISNULL(YYY.PPN_YG_DIBAYAR, 0) AS PPN_YG_DIBAYAR
		, YYY.POT_PPH
		, ISNULL(YYY.PPH_YG_DIBAYAR, 0) AS PPH_YG_DIBAYAR
FROM	(
			SELECT	TAHUN
					,BULAN AS BULAN_SORT
					, CASE	WHEN BULAN = 1 THEN 'JANUARI'
							WHEN BULAN = 2 THEN 'FEBRUARI'
							WHEN BULAN = 3 THEN 'MARET'
							WHEN BULAN = 4 THEN 'APRIL'
							WHEN BULAN = 5 THEN 'MEI'
							WHEN BULAN = 6 THEN 'JUNI'
							WHEN BULAN = 7 THEN 'JULI'
							WHEN BULAN = 8 THEN 'AGUSTUS'
							WHEN BULAN = 9 THEN 'SEPTEMBER'
							WHEN BULAN = 10 THEN 'OKTOBER'
							WHEN BULAN = 11 THEN 'NOVEMBER'
							WHEN BULAN = 12 THEN 'DESEMBER'
							ELSE 'NA' END AS BULAN
			FROM [dbo].[M_TANGGAL]
			WHERE ([POSTING_DATE] >= $P{TglFrom} AND [POSTING_DATE] <= $P{TglUpto})
			GROUP BY [TAHUN], [BULAN]
		) TGL
LEFT JOIN	(
SELECT	XXX.COMPANY
		, XXX.TIPE
		, XXX.REG
		, XXX.JENIS_PEMBELIAN
		, XXX.JENIS
		, CASE	WHEN XXX.JENIS = 'PPN 11%' THEN 1
				WHEN XXX.JENIS = 'PPN 10%' THEN 2
				WHEN XXX.JENIS = 'PPN 1%' THEN 3
				WHEN XXX.JENIS = 'PPN' THEN 4
				WHEN XXX.JENIS = 'PPN - TS/MC' THEN 5
				WHEN XXX.JENIS = 'PIB' THEN 6
				WHEN XXX.JENIS = 'NON PKP' THEN 7
				WHEN XXX.JENIS = 'NON PPN' THEN 8
				WHEN XXX.JENIS = 'NON PPN - IMPORT' THEN 9
				WHEN XXX.JENIS = 'NON PPN - TS/MC' THEN 10
				WHEN XXX.JENIS = 'NON PPN - IMPORT - TS/MC' THEN 11
				ELSE 12 END AS JENIS_SORT
		, XXX.BULAN AS BULAN_SORT
		, XXX.TAHUN
		, XXX.NO_INV
		, XXX.TGL_INV
		, XXX.NO_INV_CANCEL
		, XXX.KODE_SUPP
		, XXX.NAMA_SUPP
		, XXX.NO_INV_SUPP
		, XXX.NO_FP
		, XXX.AMOUNT_OTHERS AS AMT_BELI_NON_INVTR
		, CASE WHEN XXX.JENIS_PEMBELIAN = 'NON INVENTORY' THEN XXX.AMOUNT_UM ELSE 0 END AS AMT_UM_NON_INVTR
		, CASE WHEN XXX.JENIS_PEMBELIAN = 'NON INVENTORY' THEN XXX.AMOUNT_OTHERS + XXX.AMOUNT_UM ELSE XXX.AMOUNT_OTHERS END AS AMT_SUBTOTAL_NON_INVTR
		, XXX.AMOUNT_BIAYA_FREIGHT AS AMT_BIAYA_KIRIM
		, XXX.AMOUNT_PEMBELIAN AS AMT_BELI_INVTR
		, XXX.AMOUNT_PEMBELIAN_IC AS AMT_BELI_IC
		, CASE WHEN XXX.JENIS_PEMBELIAN = 'INVENTORY' THEN XXX.AMOUNT_UM ELSE 0 END AS AMT_UM_INVTR
		,	CASE WHEN XXX.JENIS_PEMBELIAN = 'INVENTORY' THEN
				XXX.AMOUNT_BIAYA_FREIGHT + XXX.AMOUNT_PEMBELIAN + XXX.AMOUNT_PEMBELIAN_IC + XXX.AMOUNT_UM
			ELSE
				XXX.AMOUNT_BIAYA_FREIGHT + XXX.AMOUNT_PEMBELIAN + XXX.AMOUNT_PEMBELIAN_IC
			END AS AMT_SUBTOTAL_INVTR
		, XXX.SUBTOTAL AS TOTAL_DPP
		, XXX.AMOUNT_PPN
		, XXX.TOTAL
		, CASE WHEN  XXX.TGL_INV < CAST('04/01/2022' AS DATETIME) THEN ROUND(XXX.SUBTOTAL * 0.1, 0) ELSE ROUND(XXX.SUBTOTAL * 0.11, 0) END AS PPN_YG_DIBAYAR
		, XXX.POT_PPH
		, CASE WHEN  XXX.POT_PPH = 'Ya' THEN ROUND(XXX.SUBTOTAL * 0.2, 0) ELSE 0 END AS PPH_YG_DIBAYAR
FROM (
SELECT	XX.COMPANY
		, XX.TIPE
		, XX.REG
		,	CASE WHEN XX.JENIS = 'PIB' AND XX.JENIS_PEMBELIAN = 'PEMBELIAN NON INVENTORY' THEN
				'INVENTORY'
			ELSE
				CASE WHEN XX.JENIS_PEMBELIAN = 'PEMBELIAN NON INVENTORY' THEN  'NON INVENTORY' ELSE 'INVENTORY' END
			END AS JENIS_PEMBELIAN
		, XX.JENIS
		, XX.NO_INV
		, XX.TGL_INV
		, DATEPART(MONTH, XX.TGL_INV) AS BULAN
		, DATEPART(YEAR, XX.TGL_INV) AS TAHUN
		, XX.NO_INV_CANCEL
		, XX.KODE_SUPP
		, XX.NAMA_SUPP
		, XX.NO_INV_SUPP
		, XX.NO_FP
		, XX.AMOUNT_OTHERS
		, XX.AMOUNT_BIAYA_FREIGHT
		, XX.AMOUNT_PEMBELIAN
		, XX.AMOUNT_PEMBELIAN_IC
		, XX.AMOUNT_UM
		, XX.SUBTOTAL
		, (XX.AMOUNT_BIAYA_FREIGHT + XX.AMOUNT_PEMBELIAN + XX.AMOUNT_PEMBELIAN_IC) AS SUBTOTAL_PEMBELIAN
		, XX.AMOUNT_PPN
		, XX.TOTAL
		, XX.POT_PPH
FROM	(
			SELECT	X.COMPANY
					, X.TIPE
					, X.REG
					, CASE WHEN X.AMOUNT_BIAYA_FREIGHT = 0 AND X.AMOUNT_PEMBELIAN = 0 AND X.AMOUNT_PEMBELIAN_IC = 0 AND X.AMOUNT_UM = 0 THEN
						'PEMBELIAN NON INVENTORY'
					  ELSE
						'PEMBELIAN'
					  END AS JENIS_PEMBELIAN
					,
						CASE WHEN X.AMT_PPN_SUM = 0 THEN
							CASE WHEN SUP.SUPP_JENIS IS NOT NULL OR SUP.SUPP_JENIS <> '' THEN
								CASE WHEN SUP.SUPP_JENIS IN ('NON PPN - IMPORT', 'PIB') THEN
									CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'NON PPN - IMPORT - TS/MC' ELSE SUP.SUPP_JENIS END
								ELSE
									CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'NON PPN - TS/MC' ELSE SUP.SUPP_JENIS END
								END
							ELSE
								CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'NON PPN - TS/MC'
									 WHEN X.VAT_CODE = 'VAT' AND X.ADA_VAT1  = 0 AND X.ADA_VAT10  = 0 AND X.ADA_VAT11 <> 0 THEN 'PPN 11%'
									 WHEN X.VAT_CODE = 'VAT' AND X.ADA_VAT1  = 0 AND X.ADA_VAT10 <> 0 AND X.ADA_VAT11 =  0 THEN 'PPN 10%'
									 WHEN X.VAT_CODE = 'VAT' AND X.ADA_VAT1 <> 0 AND X.ADA_VAT10  = 0 AND X.ADA_VAT11 =  0 THEN 'PPN 1%'
									 ELSE 'NON PPN'
								END
							END
						ELSE
							CASE WHEN SUP.SUPP_JENIS IS NOT NULL OR SUP.SUPP_JENIS <> '' THEN
								CASE WHEN SUP.SUPP_JENIS = 'PIB' THEN
									SUP.SUPP_JENIS
								ELSE
									CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'PPN - TS/MC'
										 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_11 THEN 'PPN 11%'
										 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_10 THEN 'PPN 10%'
										 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_1 THEN 'PPN 1%'
										 ELSE 'PPN'
									END
								END
							ELSE
								CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'PPN - TS/MC'
									 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_11 THEN 'PPN 11%'
									 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_10 THEN 'PPN 10%'
									 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_1 THEN 'PPN 1%'
									 ELSE 'PPN'
								END
							END
						END AS JENIS
					, X.NO_INV
					, X.TGL_INV
					, X.NO_INV_CANCEL
					, X.KODE_SUPP
					, X.NAMA_SUPP
					, X.NO_INV_SUPP
					, X.NO_FP
					, X.AMOUNT_OTHERS
					, X.AMOUNT_BIAYA_FREIGHT
					, X.AMOUNT_PEMBELIAN
					, X.AMOUNT_PEMBELIAN_IC
					, X.AMOUNT_UM
					, X.SUBTOTAL
					, X.AMOUNT_PPN
					, X.TOTAL
					, X.POT_PPH
			FROM	(
						SELECT	'AUTOJAYA' AS COMPANY
								, HDR.TIPE
								, HDR.REG
								, CASE WHEN HDR.LICENSE > 0 OR HDR.TIER_MAINTENANCE > 0 OR HDR.KODE_SUPP IN ('SUPP289' , 'SUPP424') THEN 'Ya' ELSE 'Tidak' END AS POT_PPH
								, HDR.TIER_MAINTENANCE
								, HDR.ADA_VAT1
								, HDR.ADA_VAT10
								, HDR.ADA_VAT11
								, HDR.VAT_CODE
								, HDR.NO_INV
								, HDR.TGL_INV
								, HDR.NO_INV_CANCEL
								, HDR.KODE_SUPP
								, HDR.NAMA_SUPP
								, HDR.NO_INV_SUPP
								, HDR.NO_FP
								, HDR.AMOUNT_OTHERS
								, (HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST) AS AMOUNT_BIAYA_FREIGHT
								, HDR.AMOUNT_PEMBELIAN
								, HDR.AMOUNT_PEMBELIAN_IC
								, HDR.AMOUNT_UM
								, (HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) AS SUBTOTAL
								, HDR.AMOUNT_PPN
								, (HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM + HDR.AMOUNT_PPN) AS TOTAL
								,ROUND(HDR.AMOUNT_PPN,0) AS AMT_PPN_SUM
								,ROUND(((HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) * 0.11 ),0) AS  AMT_PPN_CAL_11
								,ROUND(((HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) * 0.1 ),0) AS  AMT_PPN_CAL_10
								,ROUND(((HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) * 0.01 ),0) AS  AMT_PPN_CAL_1
						FROM	(
									SELECT	CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' THEN 'INVOICE UM' ELSE 'INVOICE' END AS REG
											, CASE WHEN SUBSTRING(GLE.[Document No_],4,1) = 'T' OR SUBSTRING(GLE.[Document No_],1,2) = 'PP' OR PIL.ITEM > 0 THEN 'TRADE' ELSE 'NON TRADE' END AS TIPE
											, PIL.LICENSE
											, PIL.TIER_MAINTENANCE
											, PIL.ADA_VAT1
											, PIL.ADA_VAT10
											, PIL.ADA_VAT11
											, PIH.[VAT Bus_ Posting Group] AS VAT_CODE
											, GLE.[Document No_] AS NO_INV
											, PIH.[Posting Date] AS TGL_INV
											, '' AS NO_INV_CANCEL
											, PIH.[Pay-to Vendor No_] AS KODE_SUPP
											, PIH.[Pay-to Name] AS NAMA_SUPP
											, PIH.[Vendor Invoice No_] AS NO_INV_SUPP
											, PIH.[Vendor Tax Invoice No_] AS NO_FP
											, SUM(CASE WHEN GLE.[G_L Account No_] NOT IN ('210.111.00', '510.200.00', '116.199.00','510.100.00','118.150.00', '118.290.00', '510.100.01')  THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_OTHERS
											, SUM(CASE WHEN GLE.[G_L Account No_] = '116.199.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_FGOODS
											, SUM(CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_DCOST
											, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_PEMBELIAN
											, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.01' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_PEMBELIAN_IC
											, SUM(CASE WHEN GLE.[G_L Account No_] = '118.150.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_UM
											, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_PPN
									FROM [dbo].[BC_AUTOJAYA$G_L Entry] GLE
									INNER JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH ON PIH.[No_] = GLE.[Document No_]
									INNER JOIN	(
													SELECT	PLD.[Document No_]
															,SUM(CASE WHEN PLD.[Item Category Code] = 'PS01' THEN 1 ELSE 0 END) AS  LICENSE
															,SUM(CASE WHEN ISNULL(DIMD.DIM_PRODUCT,'') IN ('S401','S404','S405') THEN 1 ELSE 0 END) AS  TIER_MAINTENANCE
															,SUM(CASE WHEN PLD.[Type] = 2 THEN 1 ELSE 0 END) AS ITEM
															,SUM(CASE WHEN PLD.[Type] = 4 THEN 1 ELSE 0 END) AS FA
															,SUM(CASE WHEN PLD.[Type] = 1 THEN 1 ELSE 0 END) AS GL_ACCOUNT
															,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT1' THEN 1 ELSE 0 END) AS ADA_VAT1
															,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT10' THEN 1 ELSE 0 END) AS ADA_VAT10
															,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT11' THEN 1 ELSE 0 END) AS ADA_VAT11
													FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PLD
													LEFT JOIN	(
																	SELECT X.[Dimension Set ID]
																		  ,MAX(CASE WHEN X.[Dimension Code] = 'PRODUCT' THEN X.[Dimension Value Code] ELSE NULL END) AS DIM_PRODUCT
																	FROM [dbo].[BC_AUTOJAYA$Dimension Set Entry] X
																	GROUP BY [Dimension Set ID]
																) DIMD ON DIMD.[Dimension Set ID] = PLD.[Dimension Set ID]
													GROUP BY [Document No_]
												) PIL ON PIL.[Document No_] = PIH.[No_]
									WHERE GLE.[G_L Account No_] NOT IN ('210.110.00', '220.130.00')
									GROUP BY GLE.[Document No_], PIH.[Pay-to Vendor No_], PIH.[Pay-to Name] , PIH.[Vendor Invoice No_],
											 PIH.[Vendor Tax Invoice No_], PIH.[Posting Date], PIL.ITEM, PIL.LICENSE, PIL.TIER_MAINTENANCE,
											 PIH.[VAT Bus_ Posting Group], PIL.ADA_VAT1, PIL.ADA_VAT10, PIL.ADA_VAT11
									UNION ALL
									SELECT	CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' THEN 'CREDIT MEMO UM' ELSE 'CREDIT MEMO' END AS REG
											, CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' OR CIL.ITEM > 0 THEN 'TRADE' ELSE 'NON TRADE' END AS TIPE
											, CIL.LICENSE
											, CIL.TIER_MAINTENANCE
											, CIL.ADA_VAT1
											, CIL.ADA_VAT10
											, CIL.ADA_VAT11
											, CIH.[VAT Bus_ Posting Group] AS VAT_CODE
											, GLE.[Document No_] AS NO_INV
											, CIH.[Posting Date] AS TGL_INV
											, CIH.[Applies-to Doc_ No_]  AS NO_INV_CANCEL
											, CIH.[Pay-to Vendor No_] AS KODE_SUPP
											, CIH.[Pay-to Name] AS NAMA_SUPP
											, '' AS NO_INV_SUPP
											, CIH.[Vendor Tax Invoice No_] AS NO_FP
											, SUM(CASE WHEN GLE.[G_L Account No_] NOT IN ('210.111.00', '510.200.00', '116.199.00','510.100.00','118.150.00', '118.290.00', '510.100.01')  THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_OTHERS
											, SUM(CASE WHEN GLE.[G_L Account No_] = '116.199.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_FGOODS
											, SUM(CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_DCOST
											, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_PEMBELIAN
											, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.01' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_PEMBELIAN_IC
											, SUM(CASE WHEN GLE.[G_L Account No_] = '118.150.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_UM
											, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_PPN
									FROM [dbo].[BC_AUTOJAYA$G_L Entry] GLE
									INNER JOIN [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Hdr_] CIH ON CIH.[No_] = GLE.[Document No_]
									INNER JOIN	(
													SELECT	PLD.[Document No_]
															,SUM(CASE WHEN PLD.[Item Category Code] = 'PS01' THEN 1 ELSE 0 END) AS  LICENSE
															,SUM(CASE WHEN ISNULL(DIMD.DIM_PRODUCT,'') IN ('S401','S404','S405') THEN 1 ELSE 0 END) AS  TIER_MAINTENANCE
															,SUM(CASE WHEN PLD.[Type] = 2 THEN 1 ELSE 0 END) AS ITEM
															,SUM(CASE WHEN PLD.[Type] = 4 THEN 1 ELSE 0 END) AS FA
															,SUM(CASE WHEN PLD.[Type] = 1 THEN 1 ELSE 0 END) AS GL_ACCOUNT
															,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT1' THEN 1 ELSE 0 END) AS ADA_VAT1
															,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT10' THEN 1 ELSE 0 END) AS ADA_VAT10
															,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT11' THEN 1 ELSE 0 END) AS ADA_VAT11
													FROM [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Line] PLD
													LEFT JOIN	(
																	SELECT X.[Dimension Set ID]
																		  ,MAX(CASE WHEN X.[Dimension Code] = 'PRODUCT' THEN X.[Dimension Value Code] ELSE NULL END) AS DIM_PRODUCT
																	FROM [dbo].[BC_AUTOJAYA$Dimension Set Entry] X
																	GROUP BY [Dimension Set ID]
																) DIMD ON DIMD.[Dimension Set ID] = PLD.[Dimension Set ID]
													GROUP BY [Document No_]
												) CIL ON CIL.[Document No_] = CIH.[No_]
									WHERE GLE.[G_L Account No_] NOT IN ( '210.110.00', '220.130.00')
									GROUP BY GLE.[Document No_],  CIH.[Pay-to Vendor No_], CIH.[Pay-to Name] , CIH.[Vendor Tax Invoice No_],
											 CIH.[Applies-to Doc_ No_] , CIH.[Posting Date], CIL.ITEM, CIL.LICENSE, CIL.TIER_MAINTENANCE,
											 CIH.[VAT Bus_ Posting Group], CIL.ADA_VAT1, CIL.ADA_VAT10, CIL.ADA_VAT11
								) HDR
					) X
			LEFT JOIN	(
							SELECT [No_] AS KODE_SUPP, [Dimension Value Code] AS SUPP_JENIS
							FROM [dbo].[BC_AUTOJAYA$Default Dimension]
							WHERE	[Dimension Code] = 'JENIS PEMBELIAN'
									AND [Table ID] = 23
						) SUP ON SUP.KODE_SUPP = X.KODE_SUPP
			WHERE	($P{Company} = 'AUTOJAYA')
					AND (X.TGL_INV >= $P{TglFrom} AND X.TGL_INV <= $P{TglUpto})
			UNION ALL
			SELECT	X.COMPANY
					, X.TIPE
					, X.REG
					, CASE WHEN X.AMOUNT_BIAYA_FREIGHT = 0 AND X.AMOUNT_PEMBELIAN = 0 AND X.AMOUNT_PEMBELIAN_IC = 0 THEN
						'PEMBELIAN NON INVENTORY'
					  ELSE
						'PEMBELIAN'
					  END AS JENIS_PEMBELIAN
					,
						CASE WHEN X.AMT_PPN_SUM = 0 THEN
							CASE WHEN SUP.SUPP_JENIS IS NOT NULL OR SUP.SUPP_JENIS <> '' THEN
								CASE WHEN SUP.SUPP_JENIS IN ('NON PPN - IMPORT', 'PIB') THEN
									CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'NON PPN - IMPORT - TS/MC' ELSE SUP.SUPP_JENIS END
								ELSE
									CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'NON PPN - TS/MC' ELSE SUP.SUPP_JENIS END
								END
							ELSE
								CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'NON PPN - TS/MC'
									 WHEN X.VAT_CODE = 'VAT' AND X.ADA_VAT1  = 0 AND X.ADA_VAT10  = 0 AND X.ADA_VAT11 <> 0 THEN 'PPN 11%'
									 WHEN X.VAT_CODE = 'VAT' AND X.ADA_VAT1  = 0 AND X.ADA_VAT10 <> 0 AND X.ADA_VAT11 =  0 THEN 'PPN 10%'
									 WHEN X.VAT_CODE = 'VAT' AND X.ADA_VAT1 <> 0 AND X.ADA_VAT10  = 0 AND X.ADA_VAT11 =  0 THEN 'PPN 1%'
									 ELSE 'NON PPN'
								END
							END
						ELSE
							CASE WHEN SUP.SUPP_JENIS IS NOT NULL OR SUP.SUPP_JENIS <> '' THEN
								CASE WHEN SUP.SUPP_JENIS = 'PIB' THEN
									SUP.SUPP_JENIS
								ELSE
									CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'PPN - TS/MC'
										 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_11 THEN 'PPN 11%'
										 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_10 THEN 'PPN 10%'
										 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_1 THEN 'PPN 1%'
										 ELSE 'PPN'
									END
								END
							ELSE
								CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'PPN - TS/MC'
									 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_11 THEN 'PPN 11%'
									 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_10 THEN 'PPN 10%'
									 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_1 THEN 'PPN 1%'
									 ELSE 'PPN'
								END
							END
						END AS JENIS
					, X.NO_INV
					, X.TGL_INV
					, X.NO_INV_CANCEL
					, X.KODE_SUPP
					, X.NAMA_SUPP
					, X.NO_INV_SUPP
					, X.NO_FP
					, X.AMOUNT_OTHERS
					, X.AMOUNT_BIAYA_FREIGHT
					, X.AMOUNT_PEMBELIAN
					, X.AMOUNT_PEMBELIAN_IC
					, X.AMOUNT_UM
					, X.SUBTOTAL
					, X.AMOUNT_PPN
					, X.TOTAL
					, X.POT_PPH
			FROM	(
						SELECT	'SOLUSI' AS COMPANY
								, HDR.TIPE
								, HDR.REG
								, CASE WHEN HDR.LICENSE > 0 OR HDR.TIER_MAINTENANCE > 0 OR HDR.KODE_SUPP IN ('SUPP289' , 'SUPP424') THEN 'Ya' ELSE 'Tidak' END AS POT_PPH
								, HDR.TIER_MAINTENANCE
								, HDR.ADA_VAT1
								, HDR.ADA_VAT10
								, HDR.ADA_VAT11
								, HDR.VAT_CODE
								, HDR.NO_INV
								, HDR.TGL_INV
								, HDR.NO_INV_CANCEL
								, HDR.KODE_SUPP
								, HDR.NAMA_SUPP
								, HDR.NO_INV_SUPP
								, HDR.NO_FP
								, HDR.AMOUNT_OTHERS
								, (HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST) AS AMOUNT_BIAYA_FREIGHT
								, HDR.AMOUNT_PEMBELIAN
								, HDR.AMOUNT_PEMBELIAN_IC
								, HDR.AMOUNT_UM
								, (HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) AS SUBTOTAL
								, HDR.AMOUNT_PPN
								, (HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM + HDR.AMOUNT_PPN) AS TOTAL
								,ROUND(HDR.AMOUNT_PPN,0) AS AMT_PPN_SUM
								,ROUND(((HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) * 0.11 ),0) AS  AMT_PPN_CAL_11
								,ROUND(((HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) * 0.1 ),0) AS  AMT_PPN_CAL_10
								,ROUND(((HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) * 0.01 ),0) AS  AMT_PPN_CAL_1
						FROM	(
									SELECT	CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' THEN 'INVOICE UM' ELSE 'INVOICE' END AS REG
											, CASE WHEN SUBSTRING(GLE.[Document No_],4,1) = 'T' OR SUBSTRING(GLE.[Document No_],1,2) = 'PP' OR PIL.ITEM > 0 THEN 'TRADE' ELSE 'NON TRADE' END AS TIPE
											, PIL.LICENSE
											, PIL.TIER_MAINTENANCE
											, PIL.ADA_VAT1
											, PIL.ADA_VAT10
											, PIL.ADA_VAT11
											, PIH.[VAT Bus_ Posting Group] AS VAT_CODE
											, GLE.[Document No_] AS NO_INV
											, PIH.[Posting Date] AS TGL_INV
											, '' AS NO_INV_CANCEL
											, PIH.[Pay-to Vendor No_] AS KODE_SUPP
											, PIH.[Pay-to Name] AS NAMA_SUPP
											, PIH.[Vendor Invoice No_] AS NO_INV_SUPP
											, PIH.[Vendor Tax Invoice No_] AS NO_FP
											, SUM(CASE WHEN GLE.[G_L Account No_] NOT IN ('210.111.00', '510.200.00', '116.199.00','510.100.00','118.150.00', '118.290.00', '510.100.01')  THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_OTHERS
											, SUM(CASE WHEN GLE.[G_L Account No_] = '116.199.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_FGOODS
											, SUM(CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_DCOST
											, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_PEMBELIAN
											, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.01' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_PEMBELIAN_IC
											, SUM(CASE WHEN GLE.[G_L Account No_] = '118.150.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_UM
											, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_PPN
									FROM [dbo].[BC_SOLUSI$G_L Entry] GLE
									INNER JOIN [dbo].[BC_SOLUSI$Purch_ Inv_ Header] PIH ON PIH.[No_] = GLE.[Document No_]
									INNER JOIN	(
													SELECT	PLD.[Document No_]
															,SUM(CASE WHEN PLD.[Item Category Code] = 'PS01' THEN 1 ELSE 0 END) AS  LICENSE
															,SUM(CASE WHEN ISNULL(DIMD.DIM_PRODUCT,'') IN ('S401','S404','S405') THEN 1 ELSE 0 END) AS  TIER_MAINTENANCE
															,SUM(CASE WHEN [Type] = 2 THEN 1 ELSE 0 END) AS ITEM
															,SUM(CASE WHEN [Type] = 4 THEN 1 ELSE 0 END) AS FA
															,SUM(CASE WHEN [Type] = 1 THEN 1 ELSE 0 END) AS GL_ACCOUNT
															,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT1' THEN 1 ELSE 0 END) AS ADA_VAT1
															,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT10' THEN 1 ELSE 0 END) AS ADA_VAT10
															,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT11' THEN 1 ELSE 0 END) AS ADA_VAT11
													FROM [dbo].[BC_SOLUSI$Purch_ Inv_ Line] PLD
													LEFT JOIN	(
																	SELECT X.[Dimension Set ID]
																		  ,MAX(CASE WHEN X.[Dimension Code] = 'PRODUCT' THEN X.[Dimension Value Code] ELSE NULL END) AS DIM_PRODUCT
																	FROM [dbo].[BC_SOLUSI$Dimension Set Entry] X
																	GROUP BY [Dimension Set ID]
																) DIMD ON DIMD.[Dimension Set ID] = PLD.[Dimension Set ID]
													GROUP BY [Document No_]
												) PIL ON PIL.[Document No_] = PIH.[No_]
									WHERE GLE.[G_L Account No_] NOT IN ('210.110.00', '220.130.00', '210.113.00')
									GROUP BY GLE.[Document No_], PIH.[Pay-to Vendor No_], PIH.[Pay-to Name] , PIH.[Vendor Invoice No_],
											 PIH.[Vendor Tax Invoice No_], PIH.[Posting Date], PIL.ITEM, PIL.LICENSE, PIL.TIER_MAINTENANCE,
											 PIH.[VAT Bus_ Posting Group], PIL.ADA_VAT1, PIL.ADA_VAT10, PIL.ADA_VAT11
									UNION ALL
									SELECT	CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' THEN 'CREDIT MEMO UM' ELSE 'CREDIT MEMO' END AS REG
											, CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' OR CIL.ITEM > 0 THEN 'TRADE' ELSE 'NON TRADE' END AS TIPE
											, CIL.LICENSE
											, CIL.TIER_MAINTENANCE
											, CIL.ADA_VAT1
											, CIL.ADA_VAT10
											, CIL.ADA_VAT11
											, CIH.[VAT Bus_ Posting Group] AS VAT_CODE
											, GLE.[Document No_] AS NO_INV
											, CIH.[Posting Date] AS TGL_INV
											, CIH.[Applies-to Doc_ No_]  AS NO_INV_CANCEL
											, CIH.[Pay-to Vendor No_] AS KODE_SUPP
											, CIH.[Pay-to Name] AS NAMA_SUPP
											, '' AS NO_INV_SUPP
											, CIH.[Vendor Tax Invoice No_] AS NO_FP
											, SUM(CASE WHEN GLE.[G_L Account No_] NOT IN ('210.111.00', '510.200.00', '116.199.00','510.100.00','118.150.00', '118.290.00', '510.100.01')  THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_OTHERS
											, SUM(CASE WHEN GLE.[G_L Account No_] = '116.199.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_FGOODS
											, SUM(CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_DCOST
											, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_PEMBELIAN
											, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.01' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_PEMBELIAN_IC
											, SUM(CASE WHEN GLE.[G_L Account No_] = '118.150.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_UM
											, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
												GLE.[Amount] ELSE 0
											  END ) AS AMOUNT_PPN
									FROM [dbo].[BC_SOLUSI$G_L Entry] GLE
									INNER JOIN [dbo].[BC_SOLUSI$Purch_ Cr_ Memo Hdr_] CIH ON CIH.[No_] = GLE.[Document No_]
									INNER JOIN	(
													SELECT	PLD.[Document No_]
															,SUM(CASE WHEN PLD.[Item Category Code] = 'PS01' THEN 1 ELSE 0 END) AS  LICENSE
															,SUM(CASE WHEN ISNULL(DIMD.DIM_PRODUCT,'') IN ('S401','S404','S405') THEN 1 ELSE 0 END) AS  TIER_MAINTENANCE
															,SUM(CASE WHEN [Type] = 2 THEN 1 ELSE 0 END) AS ITEM
															,SUM(CASE WHEN [Type] = 4 THEN 1 ELSE 0 END) AS FA
															,SUM(CASE WHEN [Type] = 1 THEN 1 ELSE 0 END) AS GL_ACCOUNT
															,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT1' THEN 1 ELSE 0 END) AS ADA_VAT1
															,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT10' THEN 1 ELSE 0 END) AS ADA_VAT10
															,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT11' THEN 1 ELSE 0 END) AS ADA_VAT11
													FROM [dbo].[BC_SOLUSI$Purch_ Cr_ Memo Line] PLD
													LEFT JOIN	(
																	SELECT X.[Dimension Set ID]
																		  ,MAX(CASE WHEN X.[Dimension Code] = 'PRODUCT' THEN X.[Dimension Value Code] ELSE NULL END) AS DIM_PRODUCT
																	FROM [dbo].[BC_SOLUSI$Dimension Set Entry] X
																	GROUP BY [Dimension Set ID]
																) DIMD ON DIMD.[Dimension Set ID] = PLD.[Dimension Set ID]
													GROUP BY [Document No_]
												) CIL ON CIL.[Document No_] = CIH.[No_]
									WHERE GLE.[G_L Account No_] NOT IN ( '210.110.00', '220.130.00', '210.113.00')
									GROUP BY GLE.[Document No_],  CIH.[Pay-to Vendor No_], CIH.[Pay-to Name] , CIH.[Vendor Tax Invoice No_],
											 CIH.[Applies-to Doc_ No_] , CIH.[Posting Date], CIL.ITEM, CIL.LICENSE, CIL.TIER_MAINTENANCE,
											 CIH.[VAT Bus_ Posting Group], CIL.ADA_VAT1, CIL.ADA_VAT10, CIL.ADA_VAT11
								) HDR
					) X
			LEFT JOIN	(
							SELECT [No_] AS KODE_SUPP, [Dimension Value Code] AS SUPP_JENIS
							FROM [dbo].[BC_SOLUSI$Default Dimension]
							WHERE	[Dimension Code] = 'JENIS PEMBELIAN'
									AND [Table ID] = 23
						) SUP ON SUP.KODE_SUPP = X.KODE_SUPP
			WHERE	($P{Company} = 'SOLUSI')
					AND (X.TGL_INV >= $P{TglFrom} AND X.TGL_INV <= $P{TglUpto})
		) XX
		) XXX
WHERE	XXX.JENIS IN ('NON PPN - IMPORT', 'NON PPN - IMPORT - TS/MC')
			) YYY ON YYY.TAHUN = TGL.TAHUN AND YYY.BULAN_SORT = TGL.BULAN_SORT]]>
	</queryString>
