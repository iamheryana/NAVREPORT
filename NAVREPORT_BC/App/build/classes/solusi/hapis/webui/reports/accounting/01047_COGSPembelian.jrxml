SELECT	'SOLUSI' AS COMPANY
		, ROUND(SUM(X.AMT_FGOODS_AWAL_LEDGER),0) AS AMT_FGOODS_AWAL_LEDGER
		, ROUND(SUM(X.AMT_FGOODS_AWAL_BF),0) AS AMT_FGOODS_AWAL_BF
		, (ROUND(SUM(X.AMT_FGOODS_AWAL_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AWAL_BF),0)) AS AMT_FGOODS_AWAL_FINAL
		, ROUND(SUM(X.AMT_PEMBELIAN),0) AS AMT_PEMBELIAN
		, ((ROUND(SUM(X.AMT_FGOODS_AWAL_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AWAL_BF),0)) + ROUND(SUM(X.AMT_PEMBELIAN),0)) AS AMT_AVAILABLE_STOCK
		, ROUND(SUM(X.AMT_FGOODS_AKHIR_LEDGER),0) AS AMT_FGOODS_AKHIR_LEDGER
		, ROUND(SUM(X.AMT_FGOODS_AKHIR_BF),0) AS AMT_FGOODS_AKHIR_BF
		, (ROUND(SUM(X.AMT_FGOODS_AKHIR_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AKHIR_BF),0)) AS AMT_FGOODS_AKHIR_FINAL
		,	(
				((ROUND(SUM(X.AMT_FGOODS_AWAL_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AWAL_BF),0)) + ROUND(SUM(X.AMT_PEMBELIAN),0)) -
				(ROUND(SUM(X.AMT_FGOODS_AKHIR_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AKHIR_BF),0))
			) AS COGS_FORMULA_TAX
		, ROUND(SUM(X.AMT_COGS_PNL),0) AS AMT_COGS_PNL
		, ROUND(SUM(X.AMT_REBATE),0) AS AMT_REBATE
		, ROUND(SUM(X.AMT_WO_MS),0) AS AMT_WO_MS
		, ROUND(SUM(X.AMT_COGS_CORRECTION),0) AS AMT_COGS_CORRECTION
		, (ROUND(SUM(X.AMT_COGS_PNL),0) + ROUND(SUM(X.AMT_REBATE),0) + ROUND(SUM(X.AMT_WO_MS),0) + ROUND(SUM(X.AMT_COGS_CORRECTION),0)) AS COGS_REAL_FINAL
		,	(
				(
					((ROUND(SUM(X.AMT_FGOODS_AWAL_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AWAL_BF),0)) + ROUND(SUM(X.AMT_PEMBELIAN),0)) -
					(ROUND(SUM(X.AMT_FGOODS_AKHIR_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AKHIR_BF),0))
				) -
				(ROUND(SUM(X.AMT_COGS_PNL),0) + ROUND(SUM(X.AMT_REBATE),0) + ROUND(SUM(X.AMT_WO_MS),0) + ROUND(SUM(X.AMT_COGS_CORRECTION),0))
			) AS SELISIH
