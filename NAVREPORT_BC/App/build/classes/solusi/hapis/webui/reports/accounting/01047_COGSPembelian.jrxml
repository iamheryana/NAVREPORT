<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="01047_COGSPembelian" language="groovy" pageWidth="595" pageHeight="854" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="c3c56047-de54-4477-a40e-d4b81af326aa">
	<property name="ireport.zoom" value="1.3310000000000388"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="33"/>
	<parameter name="Company" class="java.lang.String"/>
	<parameter name="TglFrom" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="TglUpto" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT'AUTOJAYA' AS COMPANY
		, XX.AMT_FGOODS_AWAL_LEDGER
		, XX.AMT_FGOODS_AWAL_BF
		, XX.AMT_FGOODS_AWAL_FINAL
		, XX.AMT_PEMBELIAN
		, XX.AMT_AVAILABLE_STOCK
		, XX.AMT_FGOODS_AKHIR_LEDGER
		, XX.AMT_FGOODS_AKHIR_BF
		, XX.AMT_FGOODS_AKHIR_FINAL
		, XX.COGS_FORMULA_TAX
		, XX.AMT_COGS_PNL
		, XX.AMT_REBATE
		, XX.AMT_WO_MS
		, XX.AMT_COGS_CORRECTION
		, XX.COGS_REAL_FINAL
		, XX.SELISIH
FROM	(
SELECT	'AUTOJAYA' AS COMPANY
		, ROUND(SUM(X.AMT_FGOODS_AWAL_LEDGER),0) AS AMT_FGOODS_AWAL_LEDGER
		, ROUND(SUM(X.AMT_FGOODS_AWAL_BF),0) AS AMT_FGOODS_AWAL_BF
		, (ROUND(SUM(X.AMT_FGOODS_AWAL_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AWAL_BF),0)) AS AMT_FGOODS_AWAL_FINAL
		, ROUND(SUM(X.AMT_PEMBELIAN),0) AS AMT_PEMBELIAN
		, ((ROUND(SUM(X.AMT_FGOODS_AWAL_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AWAL_BF),0)) + ROUND(SUM(X.AMT_PEMBELIAN),0)) AS AMT_AVAILABLE_STOCK
		, ROUND(SUM(X.AMT_FGOODS_AKHIR_LEDGER),0) AS AMT_FGOODS_AKHIR_LEDGER
		, ROUND(SUM(X.AMT_FGOODS_AKHIR_BF),0) AS AMT_FGOODS_AKHIR_BF
		, (ROUND(SUM(X.AMT_FGOODS_AKHIR_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AKHIR_BF),0)) AS AMT_FGOODS_AKHIR_FINAL
		,	(
				((ROUND(SUM(X.AMT_FGOODS_AWAL_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AWAL_BF),0)) + ROUND(SUM(X.AMT_PEMBELIAN),0)) -
				(ROUND(SUM(X.AMT_FGOODS_AKHIR_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AKHIR_BF),0))
			) AS COGS_FORMULA_TAX
		, ROUND(SUM(X.AMT_COGS_PNL),0) AS AMT_COGS_PNL
		, ROUND(SUM(X.AMT_REBATE),0) AS AMT_REBATE
		, ROUND(SUM(X.AMT_WO_MS),0) AS AMT_WO_MS
		, ROUND(SUM(X.AMT_COGS_CORRECTION),0) AS AMT_COGS_CORRECTION
		, (ROUND(SUM(X.AMT_COGS_PNL),0) + ROUND(SUM(X.AMT_REBATE),0) + ROUND(SUM(X.AMT_WO_MS),0) + ROUND(SUM(X.AMT_COGS_CORRECTION),0)) AS COGS_REAL_FINAL
		,	(
				(
					((ROUND(SUM(X.AMT_FGOODS_AWAL_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AWAL_BF),0)) + ROUND(SUM(X.AMT_PEMBELIAN),0)) -
					(ROUND(SUM(X.AMT_FGOODS_AKHIR_LEDGER),0) - ROUND(SUM(X.AMT_FGOODS_AKHIR_BF),0))
				) -
				(ROUND(SUM(X.AMT_COGS_PNL),0) + ROUND(SUM(X.AMT_REBATE),0) + ROUND(SUM(X.AMT_WO_MS),0) + ROUND(SUM(X.AMT_COGS_CORRECTION),0))
			) AS SELISIH
FROM	(
SELECT	SUM(GE.[Amount]) AS AMT_FGOODS_AWAL_LEDGER
		, 0 AS AMT_FGOODS_AWAL_BF
		, 0 AS AMT_PEMBELIAN
		, 0 AS AMT_FGOODS_AKHIR_LEDGER
		, 0 AS AMT_FGOODS_AKHIR_BF
		, 0 AS AMT_COGS_PNL
		, 0 AS AMT_REBATE
		, 0 AS AMT_WO_MS
		, 0 AS AMT_COGS_CORRECTION
FROM [dbo].[BC_AUTOJAYA$G_L Entry] GE
WHERE	GE.[G_L Account No_] IN ('116.199.00' , '116.199.01')
		AND GE.[Posting Date] < $P{TglFrom}
UNION ALL
SELECT	  0 AS AMT_FGOODS_AWAL_LEDGER
		, SUM(XXX.AMOUNT) AS AMT_FGOODS_AWAL_BF
		, 0 AS AMT_PEMBELIAN
		, 0 AS AMT_FGOODS_AKHIR_LEDGER
		, 0 AS AMT_FGOODS_AKHIR_BF
		, 0 AS AMT_COGS_PNL
		, 0 AS AMT_REBATE
		, 0 AS AMT_WO_MS
		, 0 AS AMT_COGS_CORRECTION
FROM	(
			SELECT	'AUTOJAYA' AS COMPANY
					,CASE	WHEN OGR.NO_GR =  'ADJ-SEPT 5' THEN 'Others'
							WHEN OGR.NO_GR =  'GRAT160626' THEN 'GR'
							ELSE 'GR'
					 END AS JNS_DOK
					,POH.[No_] AS NO_PO
					,POH.[Order Date] AS TGL_PO
					,POH.[Pay-to Vendor No_] AS VENDOR_NO
					,POH.[Pay-to Name] AS VENDOR_NAME
					,OGR.NO_GR
					,CASE	WHEN OGR.NO_GR =  'ADJ-SEPT 5' THEN CAST('2015-09-29' AS datetime)
							WHEN OGR.NO_GR =  'GRAT160626' THEN CAST('2016-10-31' AS datetime)
							ELSE PRH.[Posting Date]
					 END AS TGL_RCV
					,ROUND(OGR.AMOUNT,0) AS AMOUNT
			FROM	(
						SELECT	X.NO_GR
								,ROUND(SUM(X.AMOUNT),0) AS AMOUNT
						FROM	(
									SELECT	[Document No_] AS NO_GR
											,SUM([Amount]) * -1 AS AMOUNT
									FROM [dbo].[BC_AUTOJAYA$G_L Entry]
									WHERE [G_L Account No_] = '210.111.00'
											AND [Source Code] = 'INVTPCOST'
											AND [Document No_] LIKE 'G%'
									GROUP BY [Document No_]
									UNION ALL
									SELECT 'ADJ-SEPT 5' AS NO_GR, -303670172 AS AMOUNT_GR
									UNION ALL
									SELECT 'GRAT160626' AS NO_GR,  -16920000 AS AMOUNT_GR
									UNION ALL
									SELECT	X.NO_GR
											, SUM(X.AMOUNT_GR) * -1 AS AMOUNT_GR
									FROM	(
												SELECT	PIL.[Document No_] AS NO_INV
														, PIL.[Receipt No_] AS NO_GR
														, SUM(PIL.[Quantity] * ISNULL(POL.HARGA_SAT,0)) AS AMOUNT_GR
												FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PIL
												LEFT JOIN	(
																SELECT	PRL.[Document No_] AS GR_NO
																		, PRL.[Line No_] AS GR_LINE
																		, PRL.[Unit Cost (LCY)] AS HARGA_SAT
																FROM [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Line] PRL
																WHERE PRL.[Unit Cost (LCY)] <> 0 AND PRL.[Type] = 2
															) POL ON POL.GR_NO = PIL.[Receipt No_] AND POL.GR_LINE = PIL.[Receipt Line No_]
												WHERE	PIL.[Receipt No_] <> ''
														AND PIL.[Document No_] IN	(
																						SELECT DISTINCT [Document No_]
																						FROM [dbo].[BC_AUTOJAYA$G_L Entry]
																						WHERE [G_L Account No_] = '210.111.00'
																								AND [Source Code] = 'INVTPCOST'
																								AND [Document No_] LIKE 'P%'
																								AND [Posting Date] < $P{TglFrom}
																					)
												GROUP BY PIL.[Document No_], PIL.[Receipt No_]
											) X
									GROUP BY X.NO_GR
								) X
						GROUP BY X.NO_GR
					) OGR
			LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Header] PRH ON PRH.[No_] = OGR.NO_GR
			LEFT JOIN [dbo].[BC_AUTOJAYA$Purchase Header] POH ON POH.[No_] = PRH.[Order No_]  AND POH.[Document Type] = 1
			WHERE OGR.AMOUNT <> 0
			UNION ALL
			SELECT	'AUTOJAYA' AS COMPANY
					,'Invoice' AS JNS_DOK
					,POH.[No_] AS NO_PO
					,POH.[Order Date] AS TGL_PO
					,POH.[Pay-to Vendor No_] AS VENDOR_NO
					,POH.[Pay-to Name] AS VENDOR_NAME
					,OGR.NO_INV AS NO_GR
					,PIH.[Posting Date] AS TGL_RCV
					,ROUND(OGR.AMOUNT,0) AS AMOUNT
			FROM	(
						SELECT	X.NO_GR
								, X.NO_INV
								, SUM(X.AMOUNT) * -1 AS AMOUNT
						FROM	(
									SELECT	PIL.[Document No_] AS NO_INV
											, PIL.[Receipt No_] AS NO_GR
											, SUM(PIL.[Quantity] * ISNULL(POL.HARGA_SAT,0)) AS AMOUNT
									FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PIL
									LEFT JOIN	(
													SELECT	PRL.[Document No_] AS GR_NO
															, PRL.[Line No_] AS GR_LINE
															, PRL.[Unit Cost (LCY)] AS HARGA_SAT
													FROM [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Line] PRL
													WHERE PRL.[Unit Cost (LCY)] <> 0 AND PRL.[Type] = 2
												) POL ON POL.GR_NO = PIL.[Receipt No_] AND POL.GR_LINE = PIL.[Receipt Line No_]
									WHERE	PIL.[Receipt No_] <> ''
											AND PIL.[Document No_] IN	(
																			SELECT DISTINCT [Document No_]
																			FROM [dbo].[BC_AUTOJAYA$G_L Entry]
																			WHERE [G_L Account No_] = '210.111.00'
																					AND [Source Code] = 'INVTPCOST'
																					AND [Document No_] LIKE 'P%'
																					AND [Posting Date] < $P{TglFrom}
																		)
									GROUP BY PIL.[Document No_], PIL.[Receipt No_]
								) X
						GROUP BY X.NO_GR, X.NO_INV
					) OGR
			LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Header] PRH ON PRH.[No_] = OGR.NO_GR
			LEFT JOIN [dbo].[BC_AUTOJAYA$Purchase Header] POH ON POH.[No_] = PRH.[Order No_]  AND POH.[Document Type] = 1
			LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH ON PIH.[No_] = OGR.NO_INV
			WHERE PRH.[Posting Date] >= $P{TglFrom}
		) XXX
WHERE XXX.TGL_RCV < $P{TglFrom}
UNION ALL
SELECT	0 AS AMT_FGOODS_AWAL_LEDGER
		, 0 AS AMT_FGOODS_AWAL_BF
		, SUM(DTL.AMOUNT_FGOODS) + SUM(DTL.AMOUNT_DCOST) + SUM(DTL.AMOUNT_PEMBELIAN) + SUM(DTL.AMOUNT_PEMBELIAN_IC) AS AMT_PEMBELIAN
		, 0 AS AMT_FGOODS_AKHIR_LEDGER
		, 0 AS AMT_FGOODS_AKHIR_BF
		, 0 AS AMT_COGS_PNL
		, 0 AS AMT_REBATE
		, 0 AS AMT_WO_MS
		, 0 AS AMT_COGS_CORRECTION
FROM	(
			SELECT	CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' THEN 'INVOICE UM' ELSE 'INVOICE' END AS REG
					, MAX(GLE.[Gen_ Bus_ Posting Group]) AS TIPE
					, GLE.[Document No_] AS NO_INV
					, PIH.[Posting Date] AS TGL_INV
					, '' AS NO_INV_CANCEL
					, PIH.[Pay-to Vendor No_] AS KODE_SUPP
					, PIH.[Pay-to Name] AS NAMA_SUPP
					, PIH.[Vendor Invoice No_] AS NO_INV_SUPP
					, PIH.[Vendor Tax Invoice No_] AS NO_FP
					, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
							GLE.[Amount] ELSE 0
						  END ) AS AMT_PPN
			FROM [dbo].[BC_AUTOJAYA$G_L Entry] GLE
			INNER JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH ON PIH.[No_] = GLE.[Document No_]
			WHERE GLE.[G_L Account No_] NOT IN ('210.110.00', '220.130.00')
			GROUP BY GLE.[Document No_], PIH.[Pay-to Vendor No_], PIH.[Pay-to Name] , PIH.[Vendor Invoice No_],
					 PIH.[Vendor Tax Invoice No_], PIH.[Posting Date]
			UNION ALL
			SELECT	CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' THEN 'CREDIT MEMO UM' ELSE 'CREDIT MEMO' END AS REG
					, MAX(GLE.[Gen_ Bus_ Posting Group]) AS TIPE
					, GLE.[Document No_] AS NO_INV
					, CIH.[Posting Date] AS TGL_INV
					, CIH.[Applies-to Doc_ No_]  AS NO_INV_CANCEL
					, CIH.[Pay-to Vendor No_] AS KODE_SUPP
					, CIH.[Pay-to Name] AS NAMA_SUPP
					, '' AS NO_INV_SUPP
					, CIH.[Vendor Tax Invoice No_] AS NO_FP
					, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
							GLE.[Amount] ELSE 0
						  END ) AS AMT_PPN
			FROM [dbo].[BC_AUTOJAYA$G_L Entry] GLE
			INNER JOIN [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Hdr_] CIH ON CIH.[No_] = GLE.[Document No_]
			WHERE GLE.[G_L Account No_] NOT IN ( '210.110.00', '220.130.00')
			GROUP BY GLE.[Document No_],  CIH.[Pay-to Vendor No_], CIH.[Pay-to Name] , CIH.[Vendor Tax Invoice No_],
					 CIH.[Applies-to Doc_ No_] , CIH.[Posting Date]
		) HDR
INNER JOIN	(

				SELECT	X.NO_INV
						, X.ACC_NO
						, GL.[Name] AS ACC_NAME
						, X.ITEM_CAT
						, IC.[Description] AS ITEM_CAT_DESC
						, SUM(X.AMOUNT_OTHERS) AS AMOUNT_OTHERS
						, SUM(X.AMOUNT_FGOODS) AS AMOUNT_FGOODS
						, SUM(X.AMOUNT_DCOST) AS AMOUNT_DCOST
						, SUM(X.AMOUNT_PEMBELIAN) AS AMOUNT_PEMBELIAN
						, SUM(X.AMOUNT_PEMBELIAN_IC) AS AMOUNT_PEMBELIAN_IC
						, SUM(X.AMOUNT_UM) AS AMOUNT_UM
						, (SUM(X.AMOUNT_OTHERS) + SUM(X.AMOUNT_FGOODS) + SUM(X.AMOUNT_DCOST) + SUM(X.AMOUNT_PEMBELIAN) + SUM(X.AMOUNT_PEMBELIAN_IC) + SUM(X.AMOUNT_UM)) AS DPP
	 					, SUM(X.AMOUNT_PPN) AS AMOUNT_PPN
	 					, (SUM(X.AMOUNT_OTHERS) + SUM(X.AMOUNT_FGOODS) + SUM(X.AMOUNT_DCOST) + SUM(X.AMOUNT_PEMBELIAN) + SUM(X.AMOUNT_PEMBELIAN_IC) + SUM(X.AMOUNT_UM) + SUM(X.AMOUNT_PPN)) AS TOTAL
				FROM	(
							SELECT	GLE.[Document No_] AS NO_INV
									, CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN '510.200.00' ELSE GLE.[G_L Account No_] END AS ACC_NO
									, CASE WHEN GLE.[G_L Account No_] IN ('116.199.00') THEN
											GLE.[Global Dimension 2 Code] ELSE NULL
									  END AS ITEM_CAT
									, SUM(CASE WHEN GLE.[G_L Account No_] NOT IN ('210.111.00', '510.200.00', '116.199.00','510.100.00','118.150.00', '118.290.00', '510.100.01')  THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_OTHERS
									, SUM(CASE WHEN GLE.[G_L Account No_] = '116.199.00' THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_FGOODS
	 								, SUM(CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_DCOST
									, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.00' THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_PEMBELIAN
	 								, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.01' THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_PEMBELIAN_IC
									, SUM(CASE WHEN GLE.[G_L Account No_] = '118.150.00' THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_UM
	 								, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_PPN
							FROM [dbo].[BC_AUTOJAYA$G_L Entry] GLE
							INNER JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH ON PIH.[No_] = GLE.[Document No_]
							WHERE GLE.[G_L Account No_] NOT IN ('210.110.00', '220.130.00')
							GROUP BY GLE.[Document No_], GLE.[Global Dimension 2 Code], GLE.[G_L Account No_]
							UNION ALL
							SELECT	GLE.[Document No_] AS NO_INV
									, CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN '510.200.00' ELSE GLE.[G_L Account No_] END AS ACC_NO
									, CASE WHEN GLE.[G_L Account No_] IN ('116.199.00') THEN
											GLE.[Global Dimension 2 Code] ELSE NULL
									  END AS ITEM_CAT
									, SUM(CASE WHEN GLE.[G_L Account No_] NOT IN ('210.111.00', '510.200.00', '116.199.00','510.100.00','118.150.00', '118.290.00', '510.100.01')  THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_OTHERS
									, SUM(CASE WHEN GLE.[G_L Account No_] = '116.199.00' THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_FGOODS
	 								, SUM(CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_DCOST
									, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.00' THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_PEMBELIAN
	 								, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.01' THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_PEMBELIAN_IC
									, SUM(CASE WHEN GLE.[G_L Account No_] = '118.150.00' THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_UM
	 								, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
	 									GLE.[Amount] ELSE 0
	 								  END ) AS AMOUNT_PPN
							FROM [dbo].[BC_AUTOJAYA$G_L Entry] GLE
							INNER JOIN [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Hdr_] CIH ON CIH.[No_] = GLE.[Document No_]
							WHERE GLE.[G_L Account No_] NOT IN ( '210.110.00', '220.130.00')
							GROUP BY GLE.[Document No_], GLE.[Global Dimension 2 Code], GLE.[G_L Account No_]
						) X
				LEFT JOIN [dbo].[BC_AUTOJAYA$G_L Account] GL ON GL.[No_] = X.ACC_NO
				LEFT JOIN [dbo].[BC_AUTOJAYA$Item Category] IC ON IC.[Code] = X.ITEM_CAT
				GROUP BY X.NO_INV, X.ACC_NO, GL.[Name], X.ITEM_CAT, IC.[Description]
			) DTL ON DTL.NO_INV = HDR.NO_INV
WHERE	DTL.TOTAL <> 0 AND
		(HDR.TGL_INV >= $P{TglFrom} AND HDR.TGL_INV <= $P{TglUpto})
UNION ALL
SELECT	  0 AS AMT_FGOODS_AWAL_LEDGER
		, 0 AS AMT_FGOODS_AWAL_BF
		, 0 AS AMT_PEMBELIAN
		, SUM(GE.[Amount]) AS AMT_FGOODS_AKHIR_LEDGER
		, 0 AS AMT_FGOODS_AKHIR_BF
		, 0 AS AMT_COGS_PNL
		, 0 AS AMT_REBATE
		, 0 AS AMT_WO_MS
		, 0 AS AMT_COGS_CORRECTION
FROM [dbo].[BC_AUTOJAYA$G_L Entry] GE
WHERE	GE.[G_L Account No_] IN ('116.199.00' , '116.199.01')
		AND GE.[Posting Date] <= $P{TglUpto}
UNION ALL
SELECT	  0 AS AMT_FGOODS_AWAL_LEDGER
		, 0 AS AMT_FGOODS_AWAL_BF
		, 0 AS AMT_PEMBELIAN
		, 0 AS AMT_FGOODS_AKHIR_LEDGER
		, SUM(XXX.AMOUNT) AS AMT_FGOODS_AKHIR_BF
		, 0 AS AMT_COGS_PNL
		, 0 AS AMT_REBATE
		, 0 AS AMT_WO_MS
		, 0 AS AMT_COGS_CORRECTION
FROM	(
			SELECT	'AUTOJAYA' AS COMPANY
					,CASE	WHEN OGR.NO_GR =  'ADJ-SEPT 5' THEN 'Others'
							WHEN OGR.NO_GR =  'GRAT160626' THEN 'GR'
							ELSE 'GR'
					 END AS JNS_DOK
					,POH.[No_] AS NO_PO
					,POH.[Order Date] AS TGL_PO
					,POH.[Pay-to Vendor No_] AS VENDOR_NO
					,POH.[Pay-to Name] AS VENDOR_NAME
					,OGR.NO_GR
					,CASE	WHEN OGR.NO_GR =  'ADJ-SEPT 5' THEN CAST('2015-09-29' AS datetime)
							WHEN OGR.NO_GR =  'GRAT160626' THEN CAST('2016-10-31' AS datetime)
							ELSE PRH.[Posting Date]
					 END AS TGL_RCV
					,ROUND(OGR.AMOUNT,0) AS AMOUNT
			FROM	(
						SELECT	X.NO_GR
								,ROUND(SUM(X.AMOUNT),0) AS AMOUNT
						FROM	(
									SELECT	[Document No_] AS NO_GR
											,SUM([Amount]) * -1 AS AMOUNT
									FROM [dbo].[BC_AUTOJAYA$G_L Entry]
									WHERE [G_L Account No_] = '210.111.00'
											AND [Source Code] = 'INVTPCOST'
											AND [Document No_] LIKE 'G%'
									GROUP BY [Document No_]
									UNION ALL
									SELECT 'ADJ-SEPT 5' AS NO_GR, -303670172 AS AMOUNT_GR
									UNION ALL
									SELECT 'GRAT160626' AS NO_GR,  -16920000 AS AMOUNT_GR
									UNION ALL
									SELECT	X.NO_GR
											, SUM(X.AMOUNT_GR) * -1 AS AMOUNT_GR
									FROM	(
												SELECT	PIL.[Document No_] AS NO_INV
														, PIL.[Receipt No_] AS NO_GR
														, SUM(PIL.[Quantity] * ISNULL(POL.HARGA_SAT,0)) AS AMOUNT_GR
												FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PIL
												LEFT JOIN	(
																SELECT	PRL.[Document No_] AS GR_NO
																		, PRL.[Line No_] AS GR_LINE
																		, PRL.[Unit Cost (LCY)] AS HARGA_SAT
																FROM [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Line] PRL
																WHERE PRL.[Unit Cost (LCY)] <> 0 AND PRL.[Type] = 2
															) POL ON POL.GR_NO = PIL.[Receipt No_] AND POL.GR_LINE = PIL.[Receipt Line No_]
												WHERE	PIL.[Receipt No_] <> ''
														AND PIL.[Document No_] IN	(
																						SELECT DISTINCT [Document No_]
																						FROM [dbo].[BC_AUTOJAYA$G_L Entry]
																						WHERE [G_L Account No_] = '210.111.00'
																								AND [Source Code] = 'INVTPCOST'
																								AND [Document No_] LIKE 'P%'
																								AND [Posting Date] <= $P{TglUpto}
																					)
												GROUP BY PIL.[Document No_], PIL.[Receipt No_]
											) X
									GROUP BY X.NO_GR
								) X
						GROUP BY X.NO_GR
					) OGR
			LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Header] PRH ON PRH.[No_] = OGR.NO_GR
			LEFT JOIN [dbo].[BC_AUTOJAYA$Purchase Header] POH ON POH.[No_] = PRH.[Order No_]  AND POH.[Document Type] = 1
			WHERE OGR.AMOUNT <> 0
			UNION ALL
			SELECT	'AUTOJAYA' AS COMPANY
					,'Invoice' AS JNS_DOK
					,POH.[No_] AS NO_PO
					,POH.[Order Date] AS TGL_PO
					,POH.[Pay-to Vendor No_] AS VENDOR_NO
					,POH.[Pay-to Name] AS VENDOR_NAME
					,OGR.NO_INV AS NO_GR
					,PIH.[Posting Date] AS TGL_RCV
					,ROUND(OGR.AMOUNT,0) AS AMOUNT
			FROM	(
						SELECT	X.NO_GR
								, X.NO_INV
								, SUM(X.AMOUNT) * -1 AS AMOUNT
						FROM	(
									SELECT	PIL.[Document No_] AS NO_INV
											, PIL.[Receipt No_] AS NO_GR
											, SUM(PIL.[Quantity] * ISNULL(POL.HARGA_SAT,0)) AS AMOUNT
									FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PIL
									LEFT JOIN	(
													SELECT	PRL.[Document No_] AS GR_NO
															, PRL.[Line No_] AS GR_LINE
															, PRL.[Unit Cost (LCY)] AS HARGA_SAT
													FROM [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Line] PRL
													WHERE PRL.[Unit Cost (LCY)] <> 0 AND PRL.[Type] = 2
												) POL ON POL.GR_NO = PIL.[Receipt No_] AND POL.GR_LINE = PIL.[Receipt Line No_]
									WHERE	PIL.[Receipt No_] <> ''
											AND PIL.[Document No_] IN	(
																			SELECT DISTINCT [Document No_]
																			FROM [dbo].[BC_AUTOJAYA$G_L Entry]
																			WHERE [G_L Account No_] = '210.111.00'
																					AND [Source Code] = 'INVTPCOST'
																					AND [Document No_] LIKE 'P%'
																					AND [Posting Date] <= $P{TglUpto}
																		)
									GROUP BY PIL.[Document No_], PIL.[Receipt No_]
								) X
						GROUP BY X.NO_GR, X.NO_INV
					) OGR
			LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Header] PRH ON PRH.[No_] = OGR.NO_GR
			LEFT JOIN [dbo].[BC_AUTOJAYA$Purchase Header] POH ON POH.[No_] = PRH.[Order No_]  AND POH.[Document Type] = 1
			LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH ON PIH.[No_] = OGR.NO_INV
			WHERE PRH.[Posting Date] > $P{TglUpto}
		) XXX
WHERE XXX.TGL_RCV <= $P{TglUpto}
UNION ALL
