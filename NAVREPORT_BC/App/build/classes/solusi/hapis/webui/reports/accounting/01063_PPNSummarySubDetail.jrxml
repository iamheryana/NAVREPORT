		, XX.ACC_NAME
		, XX.GRP
		, XX.SALDO_AWAL
		, XX.AMT_JAN
		, XX.SALDO_AWAL + XX.AMT_JAN AS UPTO_JAN
		, XX.AMT_FEB
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB AS UPTO_FEB
		, XX.AMT_MAR
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR AS UPTO_MAR
		, XX.AMT_APR
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR AS UPTO_APR
		, XX.AMT_MAY
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY AS UPTO_MAY
		, XX.AMT_JUN
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN AS UPTO_JUN
		, XX.AMT_JUL
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN +
		  XX.AMT_JUL AS UPTO_JUL
		, XX.AMT_AUG
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN +
		  XX.AMT_JUL + XX.AMT_AUG AS UPTO_AUG
		, XX.AMT_SEP
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN +
		  XX.AMT_JUL + XX.AMT_AUG + XX.AMT_SEP AS UPTO_SEP
		, XX.AMT_OCT
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN +
		  XX.AMT_JUL + XX.AMT_AUG + XX.AMT_SEP + XX.AMT_OCT AS UPTO_OCT
		, XX.AMT_NOV
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN +
		  XX.AMT_JUL + XX.AMT_AUG + XX.AMT_SEP + XX.AMT_OCT + XX.AMT_NOV AS UPTO_NOV
		, XX.AMT_DEC
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN +
		  XX.AMT_JUL + XX.AMT_AUG + XX.AMT_SEP + XX.AMT_OCT + XX.AMT_NOV + XX.AMT_DEC AS UPTO_DEC
FROM	(
SELECT	X.NO_URUT
		, X.ACC_NO
		, X.ACC_NAME
		, X.GRP
		, SUM(CASE WHEN X.TAHUN < $P{Tahun} THEN X.AMOUNT ELSE 0 END) AS SALDO_AWAL
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  1 THEN X.AMOUNT ELSE 0 END) AS AMT_JAN
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  2 THEN X.AMOUNT ELSE 0 END) AS AMT_FEB
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  3 THEN X.AMOUNT ELSE 0 END) AS AMT_MAR
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  4 THEN X.AMOUNT ELSE 0 END) AS AMT_APR
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  5 THEN X.AMOUNT ELSE 0 END) AS AMT_MAY
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  6 THEN X.AMOUNT ELSE 0 END) AS AMT_JUN
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  7 THEN X.AMOUNT ELSE 0 END) AS AMT_JUL
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  8 THEN X.AMOUNT ELSE 0 END) AS AMT_AUG
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  9 THEN X.AMOUNT ELSE 0 END) AS AMT_SEP
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  10 THEN X.AMOUNT ELSE 0 END) AS AMT_OCT
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  11 THEN X.AMOUNT ELSE 0 END) AS AMT_NOV
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  12 THEN X.AMOUNT ELSE 0 END) AS AMT_DEC
FROM	(
			SELECT	CASE	WHEN GLE.[G_L Account No_] = '230.190.00' THEN 1
							WHEN GLE.[G_L Account No_] = '118.290.00' THEN 2
							WHEN GLE.[G_L Account No_] = '118.295.00' THEN 3
							ELSE 0 END AS NO_URUT
					, GLE.[G_L Account No_] AS ACC_NO
					, GLA.[Name] AS ACC_NAME
					, GLE.[Posting Date] AS TGL_POSTING
					, DATEPART(YEAR,GLE.[Posting Date]) AS TAHUN
					, DATEPART(MONTH,GLE.[Posting Date]) AS BULAN
					, GLE.[Document No_] AS NO_DOKUMEN
					, CASE WHEN GLE.[Source Code] IN ('PURCHASES', 'SALES') THEN '' ELSE GLE.[Description] END AS POSTING_DESC
					, CASE WHEN  GLE.[Source Code] IN ('PURCHASES', 'SALES') THEN '01-'+GLE.[Source Code] ELSE '02-JOURNAL' END AS GRP
					, GLE.[Amount] AS AMOUNT
			FROM [dbo].[BC_AUTOJAYA$G_L Entry] GLE
			LEFT JOIN [dbo].[BC_AUTOJAYA$G_L Account] GLA ON GLA.[No_] = GLE.[G_L Account No_]
			WHERE GLE.[G_L Account No_] IN ('118.295.00','230.190.00','118.290.00')
		) X
WHERE X.TAHUN <= $P{Tahun}
GROUP BY X.NO_URUT
		, X.ACC_NO
		, X.ACC_NAME
		, X.GRP
		) XX
WHERE $P{Company} = 'AUTOJAYA'
UNION ALL
SELECT	XX.NO_URUT
		, XX.ACC_NO
		, XX.ACC_NAME
		, XX.GRP
		, XX.SALDO_AWAL
		, XX.AMT_JAN
		, XX.SALDO_AWAL + XX.AMT_JAN AS UPTO_JAN
		, XX.AMT_FEB
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB AS UPTO_FEB
		, XX.AMT_MAR
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR AS UPTO_MAR
		, XX.AMT_APR
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR AS UPTO_APR
		, XX.AMT_MAY
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY AS UPTO_MAY
		, XX.AMT_JUN
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN AS UPTO_JUN
		, XX.AMT_JUL
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN +
		  XX.AMT_JUL AS UPTO_JUL
		, XX.AMT_AUG
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN +
		  XX.AMT_JUL + XX.AMT_AUG AS UPTO_AUG
		, XX.AMT_SEP
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN +
		  XX.AMT_JUL + XX.AMT_AUG + XX.AMT_SEP AS UPTO_SEP
		, XX.AMT_OCT
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN +
		  XX.AMT_JUL + XX.AMT_AUG + XX.AMT_SEP + XX.AMT_OCT AS UPTO_OCT
		, XX.AMT_NOV
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN +
		  XX.AMT_JUL + XX.AMT_AUG + XX.AMT_SEP + XX.AMT_OCT + XX.AMT_NOV AS UPTO_NOV
		, XX.AMT_DEC
		, XX.SALDO_AWAL + XX.AMT_JAN + XX.AMT_FEB + XX.AMT_MAR + XX.AMT_APR + XX.AMT_MAY + XX.AMT_JUN +
		  XX.AMT_JUL + XX.AMT_AUG + XX.AMT_SEP + XX.AMT_OCT + XX.AMT_NOV + XX.AMT_DEC AS UPTO_DEC
FROM	(
SELECT	X.NO_URUT
		, X.ACC_NO
		, X.ACC_NAME
		, X.GRP
		, SUM(CASE WHEN X.TAHUN < $P{Tahun} THEN X.AMOUNT ELSE 0 END) AS SALDO_AWAL
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  1 THEN X.AMOUNT ELSE 0 END) AS AMT_JAN
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  2 THEN X.AMOUNT ELSE 0 END) AS AMT_FEB
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  3 THEN X.AMOUNT ELSE 0 END) AS AMT_MAR
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  4 THEN X.AMOUNT ELSE 0 END) AS AMT_APR
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  5 THEN X.AMOUNT ELSE 0 END) AS AMT_MAY
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  6 THEN X.AMOUNT ELSE 0 END) AS AMT_JUN
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  7 THEN X.AMOUNT ELSE 0 END) AS AMT_JUL
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  8 THEN X.AMOUNT ELSE 0 END) AS AMT_AUG
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  9 THEN X.AMOUNT ELSE 0 END) AS AMT_SEP
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  10 THEN X.AMOUNT ELSE 0 END) AS AMT_OCT
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  11 THEN X.AMOUNT ELSE 0 END) AS AMT_NOV
		, SUM(CASE WHEN X.TAHUN = $P{Tahun} AND X.BULAN =  12 THEN X.AMOUNT ELSE 0 END) AS AMT_DEC
FROM	(
			SELECT	CASE	WHEN GLE.[G_L Account No_] = '230.190.00' THEN 1
							WHEN GLE.[G_L Account No_] = '118.290.00' THEN 2
							WHEN GLE.[G_L Account No_] = '118.295.00' THEN 3
							ELSE 0 END AS NO_URUT
					, GLE.[G_L Account No_] AS ACC_NO
					, GLA.[Name] AS ACC_NAME
					, GLE.[Posting Date] AS TGL_POSTING
					, DATEPART(YEAR,GLE.[Posting Date]) AS TAHUN
					, DATEPART(MONTH,GLE.[Posting Date]) AS BULAN
					, GLE.[Document No_] AS NO_DOKUMEN
					, CASE WHEN GLE.[Source Code] IN ('PURCHASES', 'SALES') THEN '' ELSE GLE.[Description] END AS POSTING_DESC
					, CASE WHEN  GLE.[Source Code] IN ('PURCHASES', 'SALES') THEN '01-'+GLE.[Source Code] ELSE '02-JOURNAL' END AS GRP
					, GLE.[Amount] AS AMOUNT
			FROM [dbo].[BC_SOLUSI$G_L Entry] GLE
			LEFT JOIN [dbo].[BC_SOLUSI$G_L Account] GLA ON GLA.[No_] = GLE.[G_L Account No_]
			WHERE GLE.[G_L Account No_] IN ('118.295.00','230.190.00','118.290.00')
		) X
WHERE X.TAHUN <= $P{Tahun}
GROUP BY X.NO_URUT
		, X.ACC_NO
		, X.ACC_NAME
