<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="010902_RekapPembelianBulanan" language="groovy" pageWidth="1500" pageHeight="595" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="1460" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="d67d58b6-db98-43fc-b2fe-6c1d43bc5f9a">
	<property name="ireport.zoom" value="1.6105100000000236"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="PeriodeFrom" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="PeriodeUpto" class="java.util.Date"/>
	<parameter name="Company" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT	UPPER(XIX.BULAN) AS BULAN
		, XIX.BULAN_NO
		, XIX.TAHUN
		, DATEPART(YEAR, $P{PeriodeUpto}) - DATEPART(YEAR, $P{PeriodeFrom}) AS JML_TAHUN
		, SUM(XIX.AMT_AJ_TTL) AS AMT_AJ_TTL
		, SUM(XIX.AMT_SP_TTL) AS AMT_SP_TTL
FROM	(
			SELECT	DATEPART(YEAR,XXX.TGL_INV) AS TAHUN
					, DATEPART(MM,XXX.TGL_INV) AS BULAN_NO
					, DATENAME(MONTH,XXX.TGL_INV) AS BULAN
					, CASE WHEN XXX.COMPANY = 'AUTOJAYA' THEN  XXX.SUBTOTAL_PEMBELIAN ELSE 0 END AS AMT_AJ_TTL
					, CASE WHEN XXX.COMPANY = 'SOLUSI' THEN  XXX.SUBTOTAL_PEMBELIAN ELSE 0 END AS AMT_SP_TTL
			FROM (
						SELECT	XX.COMPANY
								, XX.TIPE
								, XX.REG
								, CASE WHEN XX.JENIS = 'PIB' AND XX.JENIS_PEMBELIAN = 'PEMBELIAN NON INVENTORY' THEN 'PEMBELIAN' ELSE XX.JENIS_PEMBELIAN END AS JENIS_PEMBELIAN
								, CASE	WHEN XX.JENIS = 'PPN 10%' THEN 1
										WHEN XX.JENIS = 'PPN 1%' THEN 2
										WHEN XX.JENIS = 'PPN' THEN 3
										WHEN XX.JENIS = 'PPN - TS/MC' THEN 4
										WHEN XX.JENIS = 'PIB' THEN 5
										WHEN XX.JENIS = 'NON PKP' THEN 6
										WHEN XX.JENIS = 'NON PPN' THEN 7
										WHEN XX.JENIS = 'NON PPN - IMPORT' THEN 8
										WHEN XX.JENIS = 'NON PPN - TS/MC' THEN 9
										WHEN XX.JENIS = 'NON PPN - IMPORT - TS/MC' THEN 10
										ELSE 11 END AS JENIS_SORT
								, XX.JENIS
								, XX.NO_INV
								, XX.TGL_INV
								, XX.NO_INV_CANCEL
								, XX.KODE_SUPP
								, XX.NAMA_SUPP
								, XX.NO_INV_SUPP
								, XX.NO_FP
								, XX.AMOUNT_OTHERS
								, XX.AMOUNT_BIAYA_FREIGHT
								, XX.AMOUNT_PEMBELIAN
								, XX.AMOUNT_PEMBELIAN_IC
								, XX.AMOUNT_UM
								, XX.SUBTOTAL
								, (XX.AMOUNT_BIAYA_FREIGHT + XX.AMOUNT_PEMBELIAN + XX.AMOUNT_PEMBELIAN_IC) AS SUBTOTAL_PEMBELIAN
								, XX.AMOUNT_PPN
								, XX.TOTAL
						FROM	(
									SELECT	X.COMPANY
											, X.TIPE
											, X.REG
											, CASE WHEN X.AMOUNT_BIAYA_FREIGHT = 0 AND X.AMOUNT_PEMBELIAN = 0 AND X.AMOUNT_PEMBELIAN_IC = 0 AND X.AMOUNT_UM = 0 THEN
												'PEMBELIAN NON INVENTORY'
											  ELSE
												'PEMBELIAN'
											  END AS JENIS_PEMBELIAN
											,
												CASE WHEN X.AMT_PPN_SUM = 0 THEN
													CASE WHEN SUP.SUPP_JENIS IS NOT NULL OR SUP.SUPP_JENIS <> '' THEN
														CASE WHEN SUP.SUPP_JENIS IN ('NON PPN - IMPORT', 'PIB') THEN
															CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'NON PPN - IMPORT - TS/MC' ELSE SUP.SUPP_JENIS END
														ELSE
															CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'NON PPN - TS/MC' ELSE SUP.SUPP_JENIS END
														END
													ELSE
														CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'NON PPN - TS/MC'
															 WHEN X.VAT_CODE = 'VAT' AND X.ADA_VAT1  = 0 AND X.ADA_VAT10  <> 0 THEN 'PPN 10%'
															 WHEN X.VAT_CODE = 'VAT' AND X.ADA_VAT1 <> 0 AND X.ADA_VAT10  = 0 THEN 'PPN 1%'
															 ELSE 'NON PPN'
														END
													END
												ELSE
													CASE WHEN SUP.SUPP_JENIS IS NOT NULL OR SUP.SUPP_JENIS <> '' THEN
														CASE WHEN SUP.SUPP_JENIS = 'PIB' THEN
															SUP.SUPP_JENIS
														ELSE
															CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'PPN - TS/MC'
																 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_10 THEN 'PPN 10%'
																 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_1 THEN 'PPN 1%'
																 ELSE 'PPN'
															END
														END
													ELSE
														CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'PPN - TS/MC'
															 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_10 THEN 'PPN 10%'
															 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_1 THEN 'PPN 1%'
															 ELSE 'PPN'
														END
													END
												END AS JENIS
											, X.NO_INV
											, X.TGL_INV
											, X.NO_INV_CANCEL
											, X.KODE_SUPP
											, X.NAMA_SUPP
											, X.NO_INV_SUPP
											, X.NO_FP
											, X.AMOUNT_OTHERS
											, X.AMOUNT_BIAYA_FREIGHT
											, X.AMOUNT_PEMBELIAN
											, X.AMOUNT_PEMBELIAN_IC
											, X.AMOUNT_UM
											, X.SUBTOTAL
											, X.AMOUNT_PPN
											, X.TOTAL
									FROM	(
												SELECT	'AUTOJAYA' AS COMPANY
														, HDR.TIPE
														, HDR.REG
														, HDR.TIER_MAINTENANCE
														, HDR.ADA_VAT1
														, HDR.ADA_VAT10
														, HDR.VAT_CODE
														, HDR.NO_INV
														, HDR.TGL_INV
														, HDR.NO_INV_CANCEL
														, HDR.KODE_SUPP
														, HDR.NAMA_SUPP
														, HDR.NO_INV_SUPP
														, HDR.NO_FP
														, HDR.AMOUNT_OTHERS
														, (HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST) AS AMOUNT_BIAYA_FREIGHT
														, HDR.AMOUNT_PEMBELIAN
														, HDR.AMOUNT_PEMBELIAN_IC
														, HDR.AMOUNT_UM
														, (HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) AS SUBTOTAL
														, HDR.AMOUNT_PPN
														, (HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM + HDR.AMOUNT_PPN) AS TOTAL
														,ROUND(HDR.AMOUNT_PPN,0) AS AMT_PPN_SUM
														,ROUND(((HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) * 0.1 ),0) AS  AMT_PPN_CAL_10
														,ROUND(((HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) * 0.01 ),0) AS  AMT_PPN_CAL_1
												FROM	(
															SELECT	CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' THEN 'INVOICE UM' ELSE 'INVOICE' END AS REG
																	, CASE WHEN SUBSTRING(GLE.[Document No_],4,1) = 'T' OR SUBSTRING(GLE.[Document No_],1,2) = 'PP' OR PIL.ITEM > 0 THEN 'TRADE' ELSE 'NON TRADE' END AS TIPE
																	, PIL.TIER_MAINTENANCE
																	, PIL.ADA_VAT1
																	, PIL.ADA_VAT10
																	, PIH.[VAT Bus_ Posting Group] AS VAT_CODE
																	, GLE.[Document No_] AS NO_INV
																	, PIH.[Posting Date] AS TGL_INV
																	, '' AS NO_INV_CANCEL
																	, PIH.[Pay-to Vendor No_] AS KODE_SUPP
																	, PIH.[Pay-to Name] AS NAMA_SUPP
																	, PIH.[Vendor Invoice No_] AS NO_INV_SUPP
																	, PIH.[Vendor Tax Invoice No_] AS NO_FP
																	, SUM(CASE WHEN GLE.[G_L Account No_] NOT IN ('210.111.00', '510.200.00', '116.199.00','510.100.00','118.150.00', '118.290.00', '510.100.01')  THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_OTHERS
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '116.199.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_FGOODS
																	, SUM(CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_DCOST
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_PEMBELIAN
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.01' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_PEMBELIAN_IC
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '118.150.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_UM
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_PPN
															FROM [dbo].[BC_AUTOJAYA$G_L Entry] GLE
															INNER JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH ON PIH.[No_] = GLE.[Document No_]
															INNER JOIN	(
																			SELECT	PLD.[Document No_]
																					,SUM(CASE WHEN PLD.[Item Category Code] = 'HW01' OR ISNULL(DIMD.DIM_PRODUCT,'') = 'S402' THEN 1 ELSE 0 END) AS  HW_SP
																					,SUM(CASE WHEN ISNULL(DIMD.DIM_PRODUCT,'') IN ('S401','S404','S405') THEN 1 ELSE 0 END) AS  TIER_MAINTENANCE
																					,SUM(CASE WHEN PLD.[Type] = 2 THEN 1 ELSE 0 END) AS ITEM
																					,SUM(CASE WHEN PLD.[Type] = 4 THEN 1 ELSE 0 END) AS FA
																					,SUM(CASE WHEN PLD.[Type] = 1 THEN 1 ELSE 0 END) AS GL_ACCOUNT
																					,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT1' THEN 1 ELSE 0 END) AS ADA_VAT1
																					,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT10' THEN 1 ELSE 0 END) AS ADA_VAT10
																			FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PLD
																			LEFT JOIN	(
																							SELECT X.[Dimension Set ID]
																								  ,MAX(CASE WHEN X.[Dimension Code] = 'PRODUCT' THEN X.[Dimension Value Code] ELSE NULL END) AS DIM_PRODUCT
																							FROM [dbo].[BC_AUTOJAYA$Dimension Set Entry] X
																							GROUP BY [Dimension Set ID]
																						) DIMD ON DIMD.[Dimension Set ID] = PLD.[Dimension Set ID]
																			GROUP BY [Document No_]
																		) PIL ON PIL.[Document No_] = PIH.[No_]
															WHERE GLE.[G_L Account No_] NOT IN ('210.110.00', '220.130.00')
															GROUP BY GLE.[Document No_], PIH.[Pay-to Vendor No_], PIH.[Pay-to Name] , PIH.[Vendor Invoice No_],
																	 PIH.[Vendor Tax Invoice No_], PIH.[Posting Date], PIL.ITEM, PIL.TIER_MAINTENANCE,
																	 PIH.[VAT Bus_ Posting Group], PIL.ADA_VAT1, PIL.ADA_VAT10
															UNION ALL
															SELECT	CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' THEN 'CREDIT MEMO UM' ELSE 'CREDIT MEMO' END AS REG
																	, CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' OR CIL.ITEM > 0 THEN 'TRADE' ELSE 'NON TRADE' END AS TIPE
																	, CIL.TIER_MAINTENANCE
																	, CIL.ADA_VAT1
																	, CIL.ADA_VAT10
																	, CIH.[VAT Bus_ Posting Group] AS VAT_CODE
																	, GLE.[Document No_] AS NO_INV
																	, CIH.[Posting Date] AS TGL_INV
																	, CIH.[Applies-to Doc_ No_]  AS NO_INV_CANCEL
																	, CIH.[Pay-to Vendor No_] AS KODE_SUPP
																	, CIH.[Pay-to Name] AS NAMA_SUPP
																	, '' AS NO_INV_SUPP
																	, CIH.[Vendor Tax Invoice No_] AS NO_FP
																	, SUM(CASE WHEN GLE.[G_L Account No_] NOT IN ('210.111.00', '510.200.00', '116.199.00','510.100.00','118.150.00', '118.290.00', '510.100.01')  THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_OTHERS
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '116.199.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_FGOODS
																	, SUM(CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_DCOST
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_PEMBELIAN
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.01' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_PEMBELIAN_IC
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '118.150.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_UM
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_PPN
															FROM [dbo].[BC_AUTOJAYA$G_L Entry] GLE
															INNER JOIN [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Hdr_] CIH ON CIH.[No_] = GLE.[Document No_]
															INNER JOIN	(
																			SELECT	PLD.[Document No_]
																					,SUM(CASE WHEN PLD.[Item Category Code] = 'HW01' OR ISNULL(DIMD.DIM_PRODUCT,'') = 'S402' THEN 1 ELSE 0 END) AS  HW_SP
																					,SUM(CASE WHEN ISNULL(DIMD.DIM_PRODUCT,'') IN ('S401','S404','S405') THEN 1 ELSE 0 END) AS  TIER_MAINTENANCE
																					,SUM(CASE WHEN PLD.[Type] = 2 THEN 1 ELSE 0 END) AS ITEM
																					,SUM(CASE WHEN PLD.[Type] = 4 THEN 1 ELSE 0 END) AS FA
																					,SUM(CASE WHEN PLD.[Type] = 1 THEN 1 ELSE 0 END) AS GL_ACCOUNT
																					,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT1' THEN 1 ELSE 0 END) AS ADA_VAT1
																					,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT10' THEN 1 ELSE 0 END) AS ADA_VAT10
																			FROM [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Line] PLD
																			LEFT JOIN	(
																							SELECT X.[Dimension Set ID]
																								  ,MAX(CASE WHEN X.[Dimension Code] = 'PRODUCT' THEN X.[Dimension Value Code] ELSE NULL END) AS DIM_PRODUCT
																							FROM [dbo].[BC_AUTOJAYA$Dimension Set Entry] X
																							GROUP BY [Dimension Set ID]
																						) DIMD ON DIMD.[Dimension Set ID] = PLD.[Dimension Set ID]
																			GROUP BY [Document No_]
																		) CIL ON CIL.[Document No_] = CIH.[No_]
															WHERE GLE.[G_L Account No_] NOT IN ( '210.110.00', '220.130.00')
															GROUP BY GLE.[Document No_],  CIH.[Pay-to Vendor No_], CIH.[Pay-to Name] , CIH.[Vendor Tax Invoice No_],
																	 CIH.[Applies-to Doc_ No_] , CIH.[Posting Date], CIL.ITEM, CIL.TIER_MAINTENANCE,
																	 CIH.[VAT Bus_ Posting Group], CIL.ADA_VAT1, CIL.ADA_VAT10
														) HDR
											) X
									LEFT JOIN	(
													SELECT [No_] AS KODE_SUPP, [Dimension Value Code] AS SUPP_JENIS
													FROM [dbo].[BC_AUTOJAYA$Default Dimension]
													WHERE	[Dimension Code] = 'JENIS PEMBELIAN'
															AND [Table ID] = 23
												) SUP ON SUP.KODE_SUPP = X.KODE_SUPP
									WHERE	(X.TGL_INV >= $P{PeriodeFrom} AND X.TGL_INV <= $P{PeriodeUpto})
									UNION ALL
									SELECT	X.COMPANY
											, X.TIPE
											, X.REG
											, CASE WHEN X.AMOUNT_BIAYA_FREIGHT = 0 AND X.AMOUNT_PEMBELIAN = 0 AND X.AMOUNT_PEMBELIAN_IC = 0 THEN
												'PEMBELIAN NON INVENTORY'
											  ELSE
												'PEMBELIAN'
											  END AS JENIS_PEMBELIAN
											,
												CASE WHEN X.AMT_PPN_SUM = 0 THEN
													CASE WHEN SUP.SUPP_JENIS IS NOT NULL OR SUP.SUPP_JENIS <> '' THEN
														CASE WHEN SUP.SUPP_JENIS IN ('NON PPN - IMPORT', 'PIB') THEN
															CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'NON PPN - IMPORT - TS/MC' ELSE SUP.SUPP_JENIS END
														ELSE
															CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'NON PPN - TS/MC' ELSE SUP.SUPP_JENIS END
														END
													ELSE
														CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'NON PPN - TS/MC'
															 WHEN X.VAT_CODE = 'VAT' AND X.ADA_VAT1  = 0 AND X.ADA_VAT10  <> 0 THEN 'PPN 10%'
															 WHEN X.VAT_CODE = 'VAT' AND X.ADA_VAT1 <> 0 AND X.ADA_VAT10  = 0 THEN 'PPN 1%'
															 ELSE 'NON PPN'
														END
													END
												ELSE
													CASE WHEN SUP.SUPP_JENIS IS NOT NULL OR SUP.SUPP_JENIS <> '' THEN
														CASE WHEN SUP.SUPP_JENIS = 'PIB' THEN
															SUP.SUPP_JENIS
														ELSE
															CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'PPN - TS/MC'
																 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_10 THEN 'PPN 10%'
																 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_1 THEN 'PPN 1%'
																 ELSE 'PPN'
															END
														END
													ELSE
														CASE WHEN X.TIER_MAINTENANCE > 0 THEN 'PPN - TS/MC'
															 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_10 THEN 'PPN 10%'
															 WHEN X.AMT_PPN_SUM = X.AMT_PPN_CAL_1 THEN 'PPN 1%'
															 ELSE 'PPN'
														END
													END
												END AS JENIS
											, X.NO_INV
											, X.TGL_INV
											, X.NO_INV_CANCEL
											, X.KODE_SUPP
											, X.NAMA_SUPP
											, X.NO_INV_SUPP
											, X.NO_FP
											, X.AMOUNT_OTHERS
											, X.AMOUNT_BIAYA_FREIGHT
											, X.AMOUNT_PEMBELIAN
											, X.AMOUNT_PEMBELIAN_IC
											, X.AMOUNT_UM
											, X.SUBTOTAL
											, X.AMOUNT_PPN
											, X.TOTAL
									FROM	(
												SELECT	'SOLUSI' AS COMPANY
														, HDR.TIPE
														, HDR.REG
														, HDR.HW_SP
														, HDR.TIER_MAINTENANCE
														, HDR.ADA_VAT1
														, HDR.ADA_VAT10
														, HDR.VAT_CODE
														, HDR.NO_INV
														, HDR.TGL_INV
														, HDR.NO_INV_CANCEL
														, HDR.KODE_SUPP
														, HDR.NAMA_SUPP
														, HDR.NO_INV_SUPP
														, HDR.NO_FP
														, HDR.AMOUNT_OTHERS
														, (HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST) AS AMOUNT_BIAYA_FREIGHT
														, HDR.AMOUNT_PEMBELIAN
														, HDR.AMOUNT_PEMBELIAN_IC
														, HDR.AMOUNT_UM
														, (HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) AS SUBTOTAL
														, HDR.AMOUNT_PPN
														, (HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM + HDR.AMOUNT_PPN) AS TOTAL
														,ROUND(HDR.AMOUNT_PPN,0) AS AMT_PPN_SUM
														,ROUND(((HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) * 0.1 ),0) AS  AMT_PPN_CAL_10
														,ROUND(((HDR.AMOUNT_OTHERS + HDR.AMOUNT_FGOODS + HDR.AMOUNT_DCOST + HDR.AMOUNT_PEMBELIAN + HDR.AMOUNT_PEMBELIAN_IC + HDR.AMOUNT_UM ) * 0.01 ),0) AS  AMT_PPN_CAL_1
												FROM	(
															SELECT	CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' THEN 'INVOICE UM' ELSE 'INVOICE' END AS REG
																	, CASE WHEN SUBSTRING(GLE.[Document No_],4,1) = 'T' OR SUBSTRING(GLE.[Document No_],1,2) = 'PP' OR PIL.ITEM > 0 THEN 'TRADE' ELSE 'NON TRADE' END AS TIPE
																	, PIL.HW_SP
																	, PIL.TIER_MAINTENANCE
																	, PIL.ADA_VAT1
																	, PIL.ADA_VAT10
																	, PIH.[VAT Bus_ Posting Group] AS VAT_CODE
																	, GLE.[Document No_] AS NO_INV
																	, PIH.[Posting Date] AS TGL_INV
																	, '' AS NO_INV_CANCEL
																	, PIH.[Pay-to Vendor No_] AS KODE_SUPP
																	, PIH.[Pay-to Name] AS NAMA_SUPP
																	, PIH.[Vendor Invoice No_] AS NO_INV_SUPP
																	, PIH.[Vendor Tax Invoice No_] AS NO_FP
																	, SUM(CASE WHEN GLE.[G_L Account No_] NOT IN ('210.111.00', '510.200.00', '116.199.00','510.100.00','118.150.00', '118.290.00', '510.100.01')  THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_OTHERS
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '116.199.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_FGOODS
																	, SUM(CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_DCOST
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_PEMBELIAN
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.01' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_PEMBELIAN_IC
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '118.150.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_UM
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_PPN
															FROM [dbo].[BC_SOLUSI$G_L Entry] GLE
															INNER JOIN [dbo].[BC_SOLUSI$Purch_ Inv_ Header] PIH ON PIH.[No_] = GLE.[Document No_]
															INNER JOIN	(
																			SELECT	PLD.[Document No_]
																					,SUM(CASE WHEN PLD.[Item Category Code] = 'HW01' OR ISNULL(DIMD.DIM_PRODUCT,'') = 'S402' THEN 1 ELSE 0 END) AS  HW_SP
																					,SUM(CASE WHEN ISNULL(DIMD.DIM_PRODUCT,'') IN ('S401','S404','S405') THEN 1 ELSE 0 END) AS  TIER_MAINTENANCE
																					,SUM(CASE WHEN [Type] = 2 THEN 1 ELSE 0 END) AS ITEM
																					,SUM(CASE WHEN [Type] = 4 THEN 1 ELSE 0 END) AS FA
																					,SUM(CASE WHEN [Type] = 1 THEN 1 ELSE 0 END) AS GL_ACCOUNT
																					,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT1' THEN 1 ELSE 0 END) AS ADA_VAT1
																					,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT10' THEN 1 ELSE 0 END) AS ADA_VAT10
																			FROM [dbo].[BC_SOLUSI$Purch_ Inv_ Line] PLD
																			LEFT JOIN	(
																							SELECT X.[Dimension Set ID]
																								  ,MAX(CASE WHEN X.[Dimension Code] = 'PRODUCT' THEN X.[Dimension Value Code] ELSE NULL END) AS DIM_PRODUCT
																							FROM [dbo].[BC_SOLUSI$Dimension Set Entry] X
																							GROUP BY [Dimension Set ID]
																						) DIMD ON DIMD.[Dimension Set ID] = PLD.[Dimension Set ID]
																			GROUP BY [Document No_]
																		) PIL ON PIL.[Document No_] = PIH.[No_]
															WHERE GLE.[G_L Account No_] NOT IN ('210.110.00', '220.130.00', '210.113.00')
															GROUP BY GLE.[Document No_], PIH.[Pay-to Vendor No_], PIH.[Pay-to Name] , PIH.[Vendor Invoice No_],
																	 PIH.[Vendor Tax Invoice No_], PIH.[Posting Date], PIL.ITEM, PIL.HW_SP, PIL.TIER_MAINTENANCE,
																	 PIH.[VAT Bus_ Posting Group], PIL.ADA_VAT1, PIL.ADA_VAT10
															UNION ALL
															SELECT	CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' THEN 'CREDIT MEMO UM' ELSE 'CREDIT MEMO' END AS REG
																	, CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' OR CIL.ITEM > 0 THEN 'TRADE' ELSE 'NON TRADE' END AS TIPE
																	, CIL.HW_SP
																	, CIL.TIER_MAINTENANCE
																	, CIL.ADA_VAT1
																	, CIL.ADA_VAT10
																	, CIH.[VAT Bus_ Posting Group] AS VAT_CODE
																	, GLE.[Document No_] AS NO_INV
																	, CIH.[Posting Date] AS TGL_INV
																	, CIH.[Applies-to Doc_ No_]  AS NO_INV_CANCEL
																	, CIH.[Pay-to Vendor No_] AS KODE_SUPP
																	, CIH.[Pay-to Name] AS NAMA_SUPP
																	, '' AS NO_INV_SUPP
																	, CIH.[Vendor Tax Invoice No_] AS NO_FP
																	, SUM(CASE WHEN GLE.[G_L Account No_] NOT IN ('210.111.00', '510.200.00', '116.199.00','510.100.00','118.150.00', '118.290.00', '510.100.01')  THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_OTHERS
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '116.199.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_FGOODS
																	, SUM(CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_DCOST
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_PEMBELIAN
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.01' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_PEMBELIAN_IC
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '118.150.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_UM
																	, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
																		GLE.[Amount] ELSE 0
																	  END ) AS AMOUNT_PPN
															FROM [dbo].[BC_SOLUSI$G_L Entry] GLE
															INNER JOIN [dbo].[BC_SOLUSI$Purch_ Cr_ Memo Hdr_] CIH ON CIH.[No_] = GLE.[Document No_]
															INNER JOIN	(
																			SELECT	PLD.[Document No_]
																					,SUM(CASE WHEN PLD.[Item Category Code] = 'HW01' OR ISNULL(DIMD.DIM_PRODUCT,'') = 'S402' THEN 1 ELSE 0 END) AS  HW_SP
																					,SUM(CASE WHEN ISNULL(DIMD.DIM_PRODUCT,'') IN ('S401','S404','S405') THEN 1 ELSE 0 END) AS  TIER_MAINTENANCE
																					,SUM(CASE WHEN [Type] = 2 THEN 1 ELSE 0 END) AS ITEM
																					,SUM(CASE WHEN [Type] = 4 THEN 1 ELSE 0 END) AS FA
																					,SUM(CASE WHEN [Type] = 1 THEN 1 ELSE 0 END) AS GL_ACCOUNT
																					,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT1' THEN 1 ELSE 0 END) AS ADA_VAT1
																					,SUM(CASE WHEN PLD.[VAT Prod_ Posting Group] = 'VAT10' THEN 1 ELSE 0 END) AS ADA_VAT10
																			FROM [dbo].[BC_SOLUSI$Purch_ Cr_ Memo Line] PLD
																			LEFT JOIN	(
																							SELECT X.[Dimension Set ID]
																								  ,MAX(CASE WHEN X.[Dimension Code] = 'PRODUCT' THEN X.[Dimension Value Code] ELSE NULL END) AS DIM_PRODUCT
																							FROM [dbo].[BC_SOLUSI$Dimension Set Entry] X
																							GROUP BY [Dimension Set ID]
																						) DIMD ON DIMD.[Dimension Set ID] = PLD.[Dimension Set ID]
																			GROUP BY [Document No_]
																		) CIL ON CIL.[Document No_] = CIH.[No_]
															WHERE GLE.[G_L Account No_] NOT IN ( '210.110.00', '220.130.00', '210.113.00')
															GROUP BY GLE.[Document No_],  CIH.[Pay-to Vendor No_], CIH.[Pay-to Name] , CIH.[Vendor Tax Invoice No_],
																	 CIH.[Applies-to Doc_ No_] , CIH.[Posting Date], CIL.ITEM, CIL.HW_SP, CIL.TIER_MAINTENANCE,
																	 CIH.[VAT Bus_ Posting Group], CIL.ADA_VAT1, CIL.ADA_VAT10
														) HDR
											) X
									LEFT JOIN	(
													SELECT [No_] AS KODE_SUPP, [Dimension Value Code] AS SUPP_JENIS
													FROM [dbo].[BC_SOLUSI$Default Dimension]
													WHERE	[Dimension Code] = 'JENIS PEMBELIAN'
															AND [Table ID] = 23
												) SUP ON SUP.KODE_SUPP = X.KODE_SUPP
									WHERE	(X.TGL_INV >= $P{PeriodeFrom} AND X.TGL_INV <= $P{PeriodeUpto})
								) XX
					) XXX
			WHERE	(XXX.JENIS_PEMBELIAN = 'PEMBELIAN')
		) XIX
GROUP BY XIX.BULAN
		, XIX.BULAN_NO
		, XIX.TAHUN]]>
	</queryString>
