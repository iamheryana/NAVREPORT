<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="03003_PrincipalInfoSum_v2" language="groovy" pageWidth="875" pageHeight="595" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="835" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="c3c56047-de54-4477-a40e-d4b81af326aa">
	<property name="ireport.zoom" value="1.6105100000000447"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="Principal" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Pembagi" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Charge" class="java.math.BigDecimal"/>
	<parameter name="TglFrom" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="TglUpto" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT X.TAHUN_PO
		,X.DIM_PRINCIPAL
		,X.PRINCIPAL
		,X.DISTI
		,X.CURR_CODE
		,X.AMOUNT_Q1
		,X.AMOUNT_Q2
		,X.AMOUNT_Q3
		,X.AMOUNT_Q4
		,X.TOTAL
FROM	(
SELECT	POP.TAHUN_PO
		,POP.DIM_PRINCIPAL
		,PRIN.[Name] AS PRINCIPAL
		,VDR.[Name] AS DISTI
		,POP.CURR_CODE
		,CASE WHEN POP.CURR_CODE = 'IDR' THEN SUM(POP.AMOUNT_Q1)/$P{Pembagi} ELSE SUM(POP.AMOUNT_Q1) END AS AMOUNT_Q1
		,CASE WHEN POP.CURR_CODE = 'IDR' THEN SUM(POP.AMOUNT_Q2)/$P{Pembagi} ELSE SUM(POP.AMOUNT_Q2) END AS AMOUNT_Q2
		,CASE WHEN POP.CURR_CODE = 'IDR' THEN SUM(POP.AMOUNT_Q3)/$P{Pembagi} ELSE SUM(POP.AMOUNT_Q3) END AS AMOUNT_Q3
		,CASE WHEN POP.CURR_CODE = 'IDR' THEN SUM(POP.AMOUNT_Q4)/$P{Pembagi} ELSE SUM(POP.AMOUNT_Q4) END AS AMOUNT_Q4
		,  (CASE WHEN POP.CURR_CODE = 'IDR' THEN SUM(POP.AMOUNT_Q1)/$P{Pembagi} ELSE SUM(POP.AMOUNT_Q1) END +
			CASE WHEN POP.CURR_CODE = 'IDR' THEN SUM(POP.AMOUNT_Q2)/$P{Pembagi} ELSE SUM(POP.AMOUNT_Q2) END +
			CASE WHEN POP.CURR_CODE = 'IDR' THEN SUM(POP.AMOUNT_Q3)/$P{Pembagi} ELSE SUM(POP.AMOUNT_Q3) END +
			CASE WHEN POP.CURR_CODE = 'IDR' THEN SUM(POP.AMOUNT_Q4)/$P{Pembagi} ELSE SUM(POP.AMOUNT_Q4) END) AS TOTAL
FROM	(
			SELECT	POP2.COMPANY
					,POP2.NO_PO
					,POP2.VENDOR_NO
					,POP2.TGL_PO
					,POP2.TAHUN_PO
					,POP2.DIM_PRINCIPAL
					,POP2.PRODUCT
					,POP2.PRODUCT_URUT
					,POP2.CURR_CODE
					,CASE WHEN POP2.BULAN_PO IN ('01', '02', '03') THEN POP2.AMOUNT ELSE 0 END AS AMOUNT_Q1
					,CASE WHEN POP2.BULAN_PO IN ('04', '05', '06') THEN POP2.AMOUNT ELSE 0 END AS AMOUNT_Q2
					,CASE WHEN POP2.BULAN_PO IN ('07', '08', '09') THEN POP2.AMOUNT ELSE 0 END AS AMOUNT_Q3
					,CASE WHEN POP2.BULAN_PO IN ('10', '11', '12') THEN POP2.AMOUNT ELSE 0 END AS AMOUNT_Q4
			FROM	(
						SELECT	POP1.COMPANY
								,POP1.NO_PO
								,CASE WHEN POP1.VENDOR_NO = 'SUPP050' THEN 'SUPP072' ELSE POP1.VENDOR_NO END AS VENDOR_NO
								,POP1.TGL_PO
								,POP1.TAHUN_PO
								,POP1.BULAN_PO
								,CASE WHEN POP1.VENDOR_NO IN ('SUPP050', 'SUPP021') AND POP1.CURR_CODE = 'USD' THEN 'IDR' ELSE POP1.CURR_CODE END AS CURR_CODE
								,CASE WHEN POP1.VENDOR_NO IN ('SUPP050', 'SUPP021') AND POP1.CURR_CODE = 'USD' THEN ROUND((POP1.AMOUNT * ISNULL(CER.[Relational Exch_ Rate Amount], 1)),0)
									ELSE POP1.AMOUNT
								 END AS AMOUNT
								,POP1.DIM_PRINCIPAL
								,
									CASE WHEN POP1.DIM_ITEM_CATEGORY = 'PS04' THEN
										CASE WHEN POP1.DIM_PRODUCT = 'S402' THEN 'SPAREPART' ELSE 'CONTRACT MAINTENANCE' END
									ELSE
										CASE WHEN POP1.DIM_ITEM_CATEGORY = 'HW01' AND POP1.DIM_PRODUCT_MAX = 'ZZZZ' THEN
											'PRINTER'
										ELSE
											'HARDWARE'
										END
									END AS PRODUCT
							   ,
									CASE WHEN POP1.DIM_ITEM_CATEGORY = 'PS04' THEN
										CASE WHEN POP1.DIM_PRODUCT = 'S402' THEN 4 ELSE 3 END
									ELSE
										CASE WHEN POP1.DIM_ITEM_CATEGORY = 'HW01' AND POP1.DIM_PRODUCT_MAX = 'ZZZZ' THEN
											1
										ELSE
											2
										END
									END AS PRODUCT_URUT
						FROM	(
									SELECT 'AUTOJAYA' AS COMPANY
											,POH.[No_] AS NO_PO
											,POH.[Pay-to Vendor No_] AS VENDOR_NO
											,POH.[Pay-to Name] AS VENDOR_NAME
											,POH.[Order Date] AS TGL_PO
											,SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) AS TAHUN_PO
											,SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) AS BULAN_PO
											,CASE WHEN POH.[Currency Code] = '' THEN 'IDR' ELSE POH.[Currency Code] END AS CURR_CODE
											, (	SELECT  MAX([Starting Date]) AS EXC_DATE
												FROM [dbo].[BC_AUTOJAYA$Currency Exchange Rate]
												WHERE [Currency Code] = POH.[Currency Code]
														AND [Starting Date] <= POH.[Order Date]
											 ) AS EXC_DATE
											,POD.DIM_PRODUCT
											,MAX(POD.DIM_PRODUCT)OVER(PARTITION BY POH.[No_], POH.[Document Type] ORDER BY POH.[No_], POH.[Document Type]) AS DIM_PRODUCT_MAX
											,POD.DIM_PRINCIPAL
											,POD.DIM_ITEM_CATEGORY
											,POD.AMOUNT
											,(SELECT [dbo].[F_INCLUCE_ON_PI](POH.[Order Date], POH.[Pay-to Vendor No_], POD.DIM_PRINCIPAL, POD.DIM_ITEM_CATEGORY, POD.DIM_PRODUCT))  AS MASUK
									FROM [dbo].[BC_AUTOJAYA$Purchase Header] POH
									LEFT JOIN (
												SELECT PCL.[Document No_]
														,PCL.[Document Type]
														,CASE WHEN DIM.DIM_PRODUCT = 'H003' THEN 'ZZZZ' ELSE DIM.DIM_PRODUCT END AS DIM_PRODUCT
														,DIM.DIM_PRINCIPAL
														,DIM.DIM_ITEM_CATEGORY
														,PCL.AMOUNT
												FROM	(
															SELECT PL.[Document Type]
																  ,PL.[Document No_]
																  ,PL.[Dimension Set ID]
																  ,(ISNULL(PIL.[Quantity],0) * ISNULL(PIL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_AUTOJAYA$Purchase Line] PL
															INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Header] PH ON PH.[Document Type] = PL.[Document Type] AND PH.[No_] = PL.[Document No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Line] PRL ON PRL.[Order No_] = PL.[Document No_] AND PRL.[Order Line No_] = PL.[Line No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PIL ON PIL.[Receipt No_] = PRL.[Document No_] AND PIL.[Receipt Line No_] = PRL.[Line No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH ON PIH.[No_] = PIL.[Document No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
															WHERE	CMH.[No_] IS NULL AND
																	PL.[Type] = 2 AND
																	PH.[Pay-to Vendor No_] = 'SUPP072' AND
																	PL.[Document Type] = 1 AND SUBSTRING(PL.[Document No_],5,1) <> 'P'
																	AND PH.[Order Date]  < CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT PL.[Document Type]
																  ,PL.[Document No_]
																  ,PL.[Dimension Set ID]
																  ,((ISNULL(PL.[Quantity],0) - ISNULL(PL.[Quantity Invoiced],0)) * ISNULL(PL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_AUTOJAYA$Purchase Line] PL
															INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Header] PH ON PH.[Document Type] = PL.[Document Type] AND PH.[No_] = PL.[Document No_]
															WHERE	PL.[Type] = 2 AND
																	PH.[Pay-to Vendor No_] = 'SUPP072' AND
																	PL.[Document Type] = 1 AND SUBSTRING(PL.[Document No_],5,1) <> 'P'
																	AND PH.[Order Date]  < CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT BPL.[Document Type]
																  ,BPL.[Document No_]
																  ,BPL.[Dimension Set ID]
																  ,(ISNULL(PIL.[Quantity],0) * ISNULL(PIL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_AUTOJAYA$Purchase Line] BPL
															INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Header] BPH ON BPH.[Document Type] = BPL.[Document Type] AND BPH.[No_] = BPL.[Document No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purchase Line] PL ON PL.[Blanket Order No_] = BPL.[Document No_] AND PL.[Blanket Order Line No_] = BPL.[Line No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Line] PRL ON PRL.[Order No_] = PL.[Document No_] AND PRL.[Order Line No_] = PL.[Line No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PIL ON PIL.[Receipt No_] = PRL.[Document No_] AND PIL.[Receipt Line No_] = PRL.[Line No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH ON PIH.[No_] = PIL.[Document No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
															WHERE	CMH.[No_] IS NULL AND
																	BPL.[Type] = 2 AND
																	BPH.[Pay-to Vendor No_] = 'SUPP072' AND
																	BPL.[Document Type] = 4 AND SUBSTRING(BPL.[Document No_],6,1) = 'P'
																	AND BPH.[Order Date]  < CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT BPL.[Document Type]
																  ,BPL.[Document No_]
																  ,BPL.[Dimension Set ID]
																  ,((ISNULL(BPL.[Quantity],0) - ISNULL(BPL.[Quantity Invoiced],0)) * ISNULL(BPL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_AUTOJAYA$Purchase Line] BPL
															INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Header] BPH ON BPH.[Document Type] = BPL.[Document Type] AND BPH.[No_] = BPL.[Document No_]
															WHERE	BPL.[Type] = 2 AND
																	BPH.[Pay-to Vendor No_] = 'SUPP072' AND
																	BPL.[Document Type] = 4 AND SUBSTRING(BPL.[Document No_],6,1) = 'P'
																	AND BPH.[Order Date]  < CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT PL.[Document Type]
																  ,PL.[Document No_]
																  ,PL.[Dimension Set ID]
																  ,PL.[Line Amount] AS AMOUNT
															FROM [dbo].[BC_AUTOJAYA$Purchase Line] PL
															INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Header] PH ON PH.[Document Type] = PL.[Document Type] AND PH.[No_] = PL.[Document No_]
															WHERE	PL.[Type] = 2 AND
																	PH.[Pay-to Vendor No_] <> 'SUPP072'
																	AND PH.[Order Date]  < CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT PL.[Document Type]
																  ,PL.[Document No_]
																  ,PL.[Dimension Set ID]
																  ,(ISNULL(PIL.[Quantity],0) * ISNULL(PIL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_AUTOJAYA$Purchase Line] PL
															INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Header] PH ON PH.[Document Type] = PL.[Document Type] AND PH.[No_] = PL.[Document No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Line] PRL ON PRL.[Order No_] = PL.[Document No_] AND PRL.[Order Line No_] = PL.[Line No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PIL ON PIL.[Receipt No_] = PRL.[Document No_] AND PIL.[Receipt Line No_] = PRL.[Line No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH ON PIH.[No_] = PIL.[Document No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
															WHERE	CMH.[No_] IS NULL AND
																	PL.[Type] = 2 AND
																	PL.[Document Type] = 1 AND SUBSTRING(PL.[Document No_],5,1) <> 'P'
																	AND PH.[Order Date]  >= CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT PL.[Document Type]
																  ,PL.[Document No_]
																  ,PL.[Dimension Set ID]
																  ,((ISNULL(PL.[Quantity],0) - ISNULL(PL.[Quantity Invoiced],0)) * ISNULL(PL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_AUTOJAYA$Purchase Line] PL
															INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Header] PH ON PH.[Document Type] = PL.[Document Type] AND PH.[No_] = PL.[Document No_]
															WHERE	PL.[Type] = 2 AND
																	PL.[Document Type] = 1 AND SUBSTRING(PL.[Document No_],5,1) <> 'P'
																	AND PH.[Order Date]  >= CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT BPL.[Document Type]
																  ,BPL.[Document No_]
																  ,BPL.[Dimension Set ID]
																  ,(ISNULL(PIL.[Quantity],0) * ISNULL(PIL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_AUTOJAYA$Purchase Line] BPL
															INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Header] BPH ON BPH.[Document Type] = BPL.[Document Type] AND BPH.[No_] = BPL.[Document No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purchase Line] PL ON PL.[Blanket Order No_] = BPL.[Document No_] AND PL.[Blanket Order Line No_] = BPL.[Line No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Line] PRL ON PRL.[Order No_] = PL.[Document No_] AND PRL.[Order Line No_] = PL.[Line No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PIL ON PIL.[Receipt No_] = PRL.[Document No_] AND PIL.[Receipt Line No_] = PRL.[Line No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH ON PIH.[No_] = PIL.[Document No_]
															LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
															WHERE	CMH.[No_] IS NULL AND
																	BPL.[Type] = 2 AND
																	BPL.[Document Type] = 4 AND SUBSTRING(BPL.[Document No_],6,1) = 'P'
																	AND BPH.[Order Date]  >= CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT BPL.[Document Type]
																  ,BPL.[Document No_]
																  ,BPL.[Dimension Set ID]
																  ,((ISNULL(BPL.[Quantity],0) - ISNULL(BPL.[Quantity Invoiced],0)) * ISNULL(BPL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_AUTOJAYA$Purchase Line] BPL
															INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Header] BPH ON BPH.[Document Type] = BPL.[Document Type] AND BPH.[No_] = BPL.[Document No_]
															WHERE	BPL.[Type] = 2 AND
																	BPL.[Document Type] = 4 AND SUBSTRING(BPL.[Document No_],6,1) = 'P'
																	AND BPH.[Order Date]  >= CAST('11/23/2018' AS DATETIME)
														)PCL
												LEFT JOIN	(
																SELECT [Dimension Set ID]
																	  ,MAX(CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END) AS DIM_ITEM_CATEGORY
																	  ,MAX(CASE WHEN [Dimension Code] = 'PRINCIPAL' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRINCIPAL
																	  ,MAX(CASE WHEN [Dimension Code] = 'PRODUCT' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRODUCT
																FROM [dbo].[BC_AUTOJAYA$Dimension Set Entry]
																GROUP BY [Dimension Set ID]
															) DIM ON DIM.[Dimension Set ID] = PCL.[Dimension Set ID]
											  ) POD ON POD.[Document No_] = POH.[No_] AND POD.[Document Type] = POH.[Document Type]
									WHERE	POD.DIM_PRINCIPAL IN	(
																		SELECT DISTINCT [PRINCIPAL_CODE]
																		FROM [dbo].[T19_PI_ITEM]
																		WHERE [BERLAKU]=  'Y'
																	) AND
											(
												(POH.[Document Type] = 1 AND SUBSTRING(POH.[No_],5,1) <> 'P') OR
												(POH.[Document Type] = 4 AND SUBSTRING(POH.[No_],6,1) = 'P')
											)
								) POP1
						LEFT JOIN [dbo].[BC_AUTOJAYA$Currency Exchange Rate] CER ON CER.[Currency Code] = 'USD' AND  CER.[Starting Date] = POP1.EXC_DATE
						WHERE POP1.MASUK = 'Y'
					) POP2
			UNION ALL
			SELECT	POP2.COMPANY
					,POP2.NO_PO
					,POP2.VENDOR_NO
					,POP2.TGL_PO
					,POP2.TAHUN_PO
					,POP2.DIM_PRINCIPAL
					,POP2.PRODUCT
					,POP2.PRODUCT_URUT
					,POP2.CURR_CODE
					,CASE WHEN POP2.BULAN_PO IN ('01', '02', '03') THEN POP2.AMOUNT ELSE 0 END AS AMOUNT_Q1
					,CASE WHEN POP2.BULAN_PO IN ('04', '05', '06') THEN POP2.AMOUNT ELSE 0 END AS AMOUNT_Q2
					,CASE WHEN POP2.BULAN_PO IN ('07', '08', '09') THEN POP2.AMOUNT ELSE 0 END AS AMOUNT_Q3
					,CASE WHEN POP2.BULAN_PO IN ('10', '11', '12') THEN POP2.AMOUNT ELSE 0 END AS AMOUNT_Q4
			FROM	(
						SELECT	POP1.COMPANY
								,POP1.NO_PO
								,CASE WHEN POP1.VENDOR_NO = 'SUPP050' THEN 'SUPP072' ELSE POP1.VENDOR_NO END AS VENDOR_NO
								,POP1.TGL_PO
								,POP1.TAHUN_PO
								,POP1.BULAN_PO
								,CASE WHEN POP1.VENDOR_NO IN ('SUPP050', 'SUPP021') AND POP1.CURR_CODE = 'USD' THEN 'IDR' ELSE POP1.CURR_CODE END AS CURR_CODE
								,CASE WHEN POP1.VENDOR_NO IN ('SUPP050', 'SUPP021') AND POP1.CURR_CODE = 'USD' THEN ROUND((POP1.AMOUNT * ISNULL(CER.[Relational Exch_ Rate Amount], 1)),0)
									ELSE POP1.AMOUNT
								 END AS AMOUNT
								,POP1.DIM_PRINCIPAL
								,
									CASE WHEN POP1.DIM_ITEM_CATEGORY = 'PS04' THEN
										CASE WHEN POP1.DIM_PRODUCT = 'S402' THEN 'SPAREPART' ELSE 'CONTRACT MAINTENANCE' END
									ELSE
										CASE WHEN POP1.DIM_ITEM_CATEGORY = 'HW01' AND POP1.DIM_PRODUCT_MAX = 'ZZZZ' THEN
											'PRINTER'
										ELSE
											'HARDWARE'
										END
									END AS PRODUCT
							   ,
									CASE WHEN POP1.DIM_ITEM_CATEGORY = 'PS04' THEN
										CASE WHEN POP1.DIM_PRODUCT = 'S402' THEN 4 ELSE 3 END
									ELSE
										CASE WHEN POP1.DIM_ITEM_CATEGORY = 'HW01' AND POP1.DIM_PRODUCT_MAX = 'ZZZZ' THEN
											1
										ELSE
											2
										END
									END AS PRODUCT_URUT
						FROM	(
									SELECT 'SOLUSI' AS COMPANY
											,POH.[No_] AS NO_PO
											,POH.[Pay-to Vendor No_] AS VENDOR_NO
											,POH.[Pay-to Name] AS VENDOR_NAME
											,POH.[Order Date] AS TGL_PO
											,SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) AS TAHUN_PO
											,SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) AS BULAN_PO
											,CASE WHEN POH.[Currency Code] = '' THEN 'IDR' ELSE POH.[Currency Code] END AS CURR_CODE
											, (	SELECT  MAX([Starting Date]) AS EXC_DATE
												FROM [dbo].[BC_AUTOJAYA$Currency Exchange Rate]
												WHERE [Currency Code] = POH.[Currency Code]
														AND [Starting Date] <= POH.[Order Date]
											 ) AS EXC_DATE
											,POD.DIM_PRODUCT
											,MAX(POD.DIM_PRODUCT)OVER(PARTITION BY POH.[No_], POH.[Document Type] ORDER BY POH.[No_], POH.[Document Type]) AS DIM_PRODUCT_MAX
											,POD.DIM_ITEM_CATEGORY
											,POD.DIM_PRINCIPAL
											,POD.AMOUNT
											,(SELECT [dbo].[F_INCLUCE_ON_PI](POH.[Order Date], POH.[Pay-to Vendor No_], POD.DIM_PRINCIPAL, POD.DIM_ITEM_CATEGORY, POD.DIM_PRODUCT))  AS MASUK
									FROM [dbo].[BC_SOLUSI$Purchase Header] POH
									LEFT JOIN (
												SELECT PCL.[Document No_]
														,PCL.[Document Type]
														,CASE WHEN DIM.DIM_PRODUCT = 'H003' THEN 'ZZZZ' ELSE DIM.DIM_PRODUCT END AS DIM_PRODUCT
														,DIM.DIM_PRINCIPAL
														,DIM.DIM_ITEM_CATEGORY
														,PCL.AMOUNT
												FROM	(
															SELECT PL.[Document Type]
																  ,PL.[Document No_]
																  ,PL.[Dimension Set ID]
																  ,(ISNULL(PIL.[Quantity],0) * ISNULL(PIL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_SOLUSI$Purchase Line] PL
															INNER JOIN [dbo].[BC_SOLUSI$Purchase Header] PH ON PH.[Document Type] = PL.[Document Type] AND PH.[No_] = PL.[Document No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Rcpt_ Line] PRL ON PRL.[Order No_] = PL.[Document No_] AND PRL.[Order Line No_] = PL.[Line No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Inv_ Line] PIL ON PIL.[Receipt No_] = PRL.[Document No_] AND PIL.[Receipt Line No_] = PRL.[Line No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Inv_ Header] PIH ON PIH.[No_] = PIL.[Document No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
															WHERE	CMH.[No_] IS NULL AND
																	PL.[Type] = 2 AND
																	PH.[Pay-to Vendor No_] = 'SUPP072' AND
																	PL.[Document Type] = 1 AND SUBSTRING(PL.[Document No_],5,1) <> 'P'
																	AND PH.[Order Date]  < CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT PL.[Document Type]
																  ,PL.[Document No_]
																  ,PL.[Dimension Set ID]
																  ,((ISNULL(PL.[Quantity],0) - ISNULL(PL.[Quantity Invoiced],0)) * ISNULL(PL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_SOLUSI$Purchase Line] PL
															INNER JOIN [dbo].[BC_SOLUSI$Purchase Header] PH ON PH.[Document Type] = PL.[Document Type] AND PH.[No_] = PL.[Document No_]
															WHERE	PL.[Type] = 2 AND
																	PH.[Pay-to Vendor No_] = 'SUPP072' AND
																	PL.[Document Type] = 1 AND SUBSTRING(PL.[Document No_],5,1) <> 'P'
																	AND PH.[Order Date]  < CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT BPL.[Document Type]
																  ,BPL.[Document No_]
																  ,BPL.[Dimension Set ID]
																  ,(ISNULL(PIL.[Quantity],0) * ISNULL(PIL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_SOLUSI$Purchase Line] BPL
															INNER JOIN [dbo].[BC_SOLUSI$Purchase Header] BPH ON BPH.[Document Type] = BPL.[Document Type] AND BPH.[No_] = BPL.[Document No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purchase Line] PL ON PL.[Blanket Order No_] = BPL.[Document No_] AND PL.[Blanket Order Line No_] = BPL.[Line No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Rcpt_ Line] PRL ON PRL.[Order No_] = PL.[Document No_] AND PRL.[Order Line No_] = PL.[Line No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Inv_ Line] PIL ON PIL.[Receipt No_] = PRL.[Document No_] AND PIL.[Receipt Line No_] = PRL.[Line No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Inv_ Header] PIH ON PIH.[No_] = PIL.[Document No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
															WHERE	CMH.[No_] IS NULL AND
																	BPL.[Type] = 2 AND
																	BPH.[Pay-to Vendor No_] = 'SUPP072' AND
																	BPL.[Document Type] = 4 AND SUBSTRING(BPL.[Document No_],6,1) = 'P'
																	AND BPH.[Order Date]  < CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT BPL.[Document Type]
																  ,BPL.[Document No_]
																  ,BPL.[Dimension Set ID]
																  ,((ISNULL(BPL.[Quantity],0) - ISNULL(BPL.[Quantity Invoiced],0)) * ISNULL(BPL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_SOLUSI$Purchase Line] BPL
															INNER JOIN [dbo].[BC_SOLUSI$Purchase Header] BPH ON BPH.[Document Type] = BPL.[Document Type] AND BPH.[No_] = BPL.[Document No_]
															WHERE	BPL.[Type] = 2 AND
																	BPH.[Pay-to Vendor No_] = 'SUPP072' AND
																	BPL.[Document Type] = 4 AND SUBSTRING(BPL.[Document No_],6,1) = 'P'
																	AND BPH.[Order Date]  < CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT PL.[Document Type]
																  ,PL.[Document No_]
																  ,PL.[Dimension Set ID]
																  ,PL.[Line Amount] AS AMOUNT
															FROM [dbo].[BC_SOLUSI$Purchase Line] PL
															INNER JOIN [dbo].[BC_SOLUSI$Purchase Header] PH ON PH.[Document Type] = PL.[Document Type] AND PH.[No_] = PL.[Document No_]
															WHERE	PL.[Type] = 2 AND
																	PH.[Pay-to Vendor No_] <> 'SUPP072'
																	AND PH.[Order Date]  < CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT PL.[Document Type]
																  ,PL.[Document No_]
																  ,PL.[Dimension Set ID]
																  ,(ISNULL(PIL.[Quantity],0) * ISNULL(PIL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_SOLUSI$Purchase Line] PL
															INNER JOIN [dbo].[BC_SOLUSI$Purchase Header] PH ON PH.[Document Type] = PL.[Document Type] AND PH.[No_] = PL.[Document No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Rcpt_ Line] PRL ON PRL.[Order No_] = PL.[Document No_] AND PRL.[Order Line No_] = PL.[Line No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Inv_ Line] PIL ON PIL.[Receipt No_] = PRL.[Document No_] AND PIL.[Receipt Line No_] = PRL.[Line No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Inv_ Header] PIH ON PIH.[No_] = PIL.[Document No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
															WHERE	CMH.[No_] IS NULL AND
																	PL.[Type] = 2 AND
																	PL.[Document Type] = 1 AND SUBSTRING(PL.[Document No_],5,1) <> 'P'
																	AND PH.[Order Date]  >= CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT PL.[Document Type]
																  ,PL.[Document No_]
																  ,PL.[Dimension Set ID]
																  ,((ISNULL(PL.[Quantity],0) - ISNULL(PL.[Quantity Invoiced],0)) * ISNULL(PL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_SOLUSI$Purchase Line] PL
															INNER JOIN [dbo].[BC_SOLUSI$Purchase Header] PH ON PH.[Document Type] = PL.[Document Type] AND PH.[No_] = PL.[Document No_]
															WHERE	PL.[Type] = 2 AND
																	PL.[Document Type] = 1 AND SUBSTRING(PL.[Document No_],5,1) <> 'P'
																	AND PH.[Order Date]  >= CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT BPL.[Document Type]
																  ,BPL.[Document No_]
																  ,BPL.[Dimension Set ID]
																  ,(ISNULL(PIL.[Quantity],0) * ISNULL(PIL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_SOLUSI$Purchase Line] BPL
															INNER JOIN [dbo].[BC_SOLUSI$Purchase Header] BPH ON BPH.[Document Type] = BPL.[Document Type] AND BPH.[No_] = BPL.[Document No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purchase Line] PL ON PL.[Blanket Order No_] = BPL.[Document No_] AND PL.[Blanket Order Line No_] = BPL.[Line No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Rcpt_ Line] PRL ON PRL.[Order No_] = PL.[Document No_] AND PRL.[Order Line No_] = PL.[Line No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Inv_ Line] PIL ON PIL.[Receipt No_] = PRL.[Document No_] AND PIL.[Receipt Line No_] = PRL.[Line No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Inv_ Header] PIH ON PIH.[No_] = PIL.[Document No_]
															LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
															WHERE	CMH.[No_] IS NULL AND
																	BPL.[Type] = 2 AND
																	BPL.[Document Type] = 4 AND SUBSTRING(BPL.[Document No_],6,1) = 'P'
																	AND BPH.[Order Date]  >= CAST('11/23/2018' AS DATETIME)
															UNION ALL
															SELECT BPL.[Document Type]
																  ,BPL.[Document No_]
																  ,BPL.[Dimension Set ID]
																  ,((ISNULL(BPL.[Quantity],0) - ISNULL(BPL.[Quantity Invoiced],0)) * ISNULL(BPL.[Direct Unit Cost],0) ) AS AMOUNT
															FROM [dbo].[BC_SOLUSI$Purchase Line] BPL
															INNER JOIN [dbo].[BC_SOLUSI$Purchase Header] BPH ON BPH.[Document Type] = BPL.[Document Type] AND BPH.[No_] = BPL.[Document No_]
															WHERE	BPL.[Type] = 2 AND
																	BPL.[Document Type] = 4 AND SUBSTRING(BPL.[Document No_],6,1) = 'P'
																	AND BPH.[Order Date]  >= CAST('11/23/2018' AS DATETIME)
														) PCL
												LEFT JOIN	(
																SELECT [Dimension Set ID]
																	  ,MAX(CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END) AS DIM_ITEM_CATEGORY
																	  ,MAX(CASE WHEN [Dimension Code] = 'PRINCIPAL' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRINCIPAL
																	  ,MAX(CASE WHEN [Dimension Code] = 'PRODUCT' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRODUCT
																FROM [dbo].[BC_SOLUSI$Dimension Set Entry]
																GROUP BY [Dimension Set ID]
															) DIM ON DIM.[Dimension Set ID] = PCL.[Dimension Set ID]
											  ) POD ON POD.[Document No_] = POH.[No_] AND POD.[Document Type] = POH.[Document Type]
									WHERE	POD.DIM_PRINCIPAL IN	(
																		SELECT DISTINCT [PRINCIPAL_CODE]
																		FROM [dbo].[T19_PI_ITEM]
																		WHERE [BERLAKU]=  'Y'
																	) AND
											(
												(POH.[Document Type] = 1 AND SUBSTRING(POH.[No_],5,1) <> 'P') OR
												(POH.[Document Type] = 4 AND SUBSTRING(POH.[No_],6,1) = 'P')
											)
								) POP1
						LEFT JOIN [dbo].[BC_AUTOJAYA$Currency Exchange Rate] CER ON CER.[Currency Code] = 'USD' AND  CER.[Starting Date] = POP1.EXC_DATE
						WHERE POP1.MASUK = 'Y'
					) POP2
			UNION ALL
			SELECT	POP2.COMPANY
					,POP2.NO_PO
					,POP2.VENDOR_NO
					,POP2.TGL_PO
					,POP2.TAHUN_PO
					,POP2.DIM_PRINCIPAL
					,POP2.PRODUCT
					,POP2.PRODUCT_URUT
					,POP2.CURR_CODE
					,CASE WHEN POP2.BULAN_PO IN ('01', '02', '03') THEN POP2.AMOUNT ELSE 0 END AS AMOUNT_Q1
					,CASE WHEN POP2.BULAN_PO IN ('04', '05', '06') THEN POP2.AMOUNT ELSE 0 END AS AMOUNT_Q2
					,CASE WHEN POP2.BULAN_PO IN ('07', '08', '09') THEN POP2.AMOUNT ELSE 0 END AS AMOUNT_Q3
					,CASE WHEN POP2.BULAN_PO IN ('10', '11', '12') THEN POP2.AMOUNT ELSE 0 END AS AMOUNT_Q4
			FROM	(
						SELECT	POP1.COMPANY
								,POP1.NO_PO
								,CASE WHEN POP1.VENDOR_NO = 'SUPP050' THEN 'SUPP072' ELSE POP1.VENDOR_NO END AS VENDOR_NO
								,POP1.TGL_PO
								,POP1.TAHUN_PO
								,POP1.BULAN_PO
								,CASE WHEN POP1.VENDOR_NO IN ('SUPP050', 'SUPP021') AND POP1.CURR_CODE = 'USD' THEN 'IDR' ELSE POP1.CURR_CODE END AS CURR_CODE
								,CASE WHEN POP1.VENDOR_NO IN ('SUPP050', 'SUPP021') AND POP1.CURR_CODE = 'USD' THEN ROUND((POP1.AMOUNT * ISNULL(CER.[Relational Exch_ Rate Amount], 1)),0)
									ELSE POP1.AMOUNT
								 END AS AMOUNT
								,POP1.DIM_PRINCIPAL
								,
									CASE WHEN POP1.DIM_ITEM_CATEGORY = 'PS04' THEN
										CASE WHEN POP1.DIM_PRODUCT = 'S402' THEN 'SPAREPART' ELSE 'CONTRACT MAINTENANCE' END
									ELSE
										CASE WHEN POP1.DIM_ITEM_CATEGORY = 'HW01' AND POP1.DIM_PRODUCT_MAX = 'ZZZZ' THEN
											'PRINTER'
										ELSE
											'HARDWARE'
										END
									END AS PRODUCT
							   ,
									CASE WHEN POP1.DIM_ITEM_CATEGORY = 'PS04' THEN
										CASE WHEN POP1.DIM_PRODUCT = 'S402' THEN 4 ELSE 3 END
									ELSE
										CASE WHEN POP1.DIM_ITEM_CATEGORY = 'HW01' AND POP1.DIM_PRODUCT_MAX = 'ZZZZ' THEN
											1
										ELSE
											2
										END
									END AS PRODUCT_URUT
						FROM	(
									SELECT 'ADJUSTMENT' AS COMPANY
											,POH.[No_] AS NO_PO
											,POH.[Pay-to Vendor No_] AS VENDOR_NO
											,POH.[Pay-to Name] AS VENDOR_NAME
											,POH.[Order Date] AS TGL_PO
											,SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) AS TAHUN_PO
											,SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) AS BULAN_PO
											,CASE WHEN POH.[Currency Code] = '' THEN 'IDR' ELSE POH.[Currency Code] END AS CURR_CODE
											, (	SELECT  MAX([Starting Date]) AS EXC_DATE
												FROM [dbo].[BC_AUTOJAYA$Currency Exchange Rate]
												WHERE [Currency Code] = POH.[Currency Code]
														AND [Starting Date] <= POH.[Order Date]
											 ) AS EXC_DATE
											,POD.DIM_PRODUCT
											,MAX(POD.DIM_PRODUCT)OVER(PARTITION BY POH.[No_], POH.[Document Type] ORDER BY POH.[No_], POH.[Document Type]) AS DIM_PRODUCT_MAX
											,POD.DIM_ITEM_CATEGORY
											,POD.DIM_PRINCIPAL
											,POD.AMOUNT
											,(SELECT [dbo].[F_INCLUCE_ON_PI](POH.[Order Date], POH.[Pay-to Vendor No_], POD.DIM_PRINCIPAL, POD.DIM_ITEM_CATEGORY, POD.DIM_PRODUCT))  AS MASUK
									FROM [dbo].[BC_PI-ADJ$Purchase Header] POH
									LEFT JOIN (
												SELECT PCL.[Document No_]
														,PCL.[Document Type]
														,CASE WHEN DIM.DIM_PRODUCT = 'H003' THEN 'ZZZZ' ELSE DIM.DIM_PRODUCT END AS DIM_PRODUCT
														,DIM.DIM_PRINCIPAL
														,DIM.DIM_ITEM_CATEGORY
														,PCL.[Line Amount] AS AMOUNT
												FROM [dbo].[BC_PI-ADJ$Purchase Line] PCL
												LEFT JOIN	(
																SELECT [Dimension Set ID]
																	  ,MAX(CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END) AS DIM_ITEM_CATEGORY
																	  ,MAX(CASE WHEN [Dimension Code] = 'PRINCIPAL' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRINCIPAL
																	  ,MAX(CASE WHEN [Dimension Code] = 'PRODUCT' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRODUCT
																FROM [dbo].[BC_PI-ADJ$Dimension Set Entry]
																GROUP BY [Dimension Set ID]
															) DIM ON DIM.[Dimension Set ID] = PCL.[Dimension Set ID]
												WHERE PCL.[Type] = 2
											  ) POD ON POD.[Document No_] = POH.[No_] AND POD.[Document Type] = POH.[Document Type]
									WHERE	POD.DIM_PRINCIPAL IN	(
																		SELECT DISTINCT [PRINCIPAL_CODE]
																		FROM [dbo].[T19_PI_ITEM]
																		WHERE [BERLAKU]=  'Y'
																	) AND
											(
												(POH.[Document Type] = 1 AND SUBSTRING(POH.[No_],5,1) <> 'P') OR
												(POH.[Document Type] = 4 AND SUBSTRING(POH.[No_],6,1) = 'P')
											)
								) POP1
						LEFT JOIN [dbo].[BC_AUTOJAYA$Currency Exchange Rate] CER ON CER.[Currency Code] = 'USD' AND  CER.[Starting Date] = POP1.EXC_DATE
						WHERE POP1.MASUK = 'Y'
					) POP2
) POP
LEFT JOIN	(
				SELECT	VEN.[No_], MAX([Name]) AS [Name]
						FROM	(
									SELECT [No_], [Name]
									FROM [dbo].[BC_AUTOJAYA$Vendor]
									UNION ALL
									SELECT [No_], [Name]
									FROM [dbo].[BC_SOLUSI$Vendor]
								) VEN
						GROUP BY VEN.[No_]
			) VDR ON VDR.[No_] = POP.VENDOR_NO
LEFT JOIN	(
				SELECT	PRI.[Code], MAX(PRI.[Name]) AS [Name]
				FROM	(
							SELECT [Code]
								  ,[Name]
							FROM [dbo].[BC_AUTOJAYA$Dimension Value]
							WHERE [Dimension Code] = 'PRINCIPAL'
							UNION ALL
							SELECT [Code]
								  ,[Name]
							FROM [dbo].[BC_SOLUSI$Dimension Value]
							WHERE [Dimension Code] = 'PRINCIPAL'
						) PRI
				GROUP BY PRI.[Code]
			) PRIN ON PRIN.[Code] = POP.DIM_PRINCIPAL
WHERE (POP.DIM_PRINCIPAL = $P{Principal} OR $P{Principal} = 'ALL') AND
	  POP.DIM_PRINCIPAL <> '' AND
	  (POP.TGL_PO >= $P{TglFrom} AND POP.TGL_PO <= $P{TglUpto}) AND
	  PRIN.[Name] NOT LIKE '%UNUSED%' AND
	  POP.VENDOR_NO NOT IN ('SUPP005', 'SUPP032')
GROUP BY POP.TAHUN_PO, POP.DIM_PRINCIPAL, PRIN.[Name], VDR.[Name], POP.CURR_CODE
) X
WHERE X.TOTAL <> 0]]>
	</queryString>
