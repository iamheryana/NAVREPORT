<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="05009_SalesTargetAchievement_02" language="groovy" pageWidth="1000" pageHeight="595" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="960" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="b406b291-7ccc-4e85-8d33-6ed4d9da73cd">
	<property name="ireport.zoom" value="1.3310000000001427"/>
	<property name="ireport.x" value="76"/>
	<property name="ireport.y" value="0"/>
	<parameter name="Tahun" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="AmtBig" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="SemAkhir" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="TglUpto" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="weightH" class="java.math.BigDecimal"/>
	<parameter name="weightM" class="java.math.BigDecimal"/>
	<parameter name="weightL" class="java.math.BigDecimal"/>
	<parameter name="ProsesId" class="java.lang.String"/>
	<parameter name="TargetPS" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT	XOX.BRANCHES
		,CAB.[Name] AS BRANCHES_NAME
		, XOX.AMOUNT_SR_TTL
		, XOX.AMOUNT_SR_PS
		, XOX.AMOUNT_UM_TTL
		, XOX.AMOUNT_UM_PS
		, XOX.AMOUNT_SO_TTL
		, XOX.AMOUNT_SO_PS
		, XOX.AMOUNT_TTL_BFC
		, XOX.AMOUNT_TTL_PS_BFC
		, XOX.AMOUNT_FC_TTL
		, XOX.AMOUNT_FC_PS
		, XOX.AMOUNT_TTL_AFC
		, XOX.AMOUNT_TTL_PS_AFC
		, TRG.TARGET_SLS
		, TRG.TARGET_PS
		, TRG.TARGET_SLS - XOX.AMOUNT_TTL_AFC AS PLUS_MINUS_TARGET_TTL
		, TRG.TARGET_PS -  XOX.AMOUNT_TTL_PS_AFC AS PLUS_MINUS_TARGET_PS
		, CASE WHEN XOX.AMOUNT_SR_TTL =  0 THEN 0 ELSE (XOX.AMOUNT_SR_PS / XOX.AMOUNT_SR_TTL) * 100 END AS PCN_PS_SR
		, CASE WHEN XOX.AMOUNT_TTL_BFC =  0 THEN 0 ELSE (XOX.AMOUNT_TTL_PS_BFC / XOX.AMOUNT_TTL_BFC) * 100 END AS PCN_PS_TTL_BFC
		, CASE WHEN XOX.AMOUNT_TTL_AFC =  0 THEN 0 ELSE (XOX.AMOUNT_TTL_PS_AFC / XOX.AMOUNT_TTL_AFC) * 100 END AS PCN_PS_TTL_AFC
		,	CASE WHEN SUM( XOX.AMOUNT_SR_TTL ) OVER (PARTITION BY 1) =  0 THEN 0 ELSE
				(
					SUM( XOX.AMOUNT_SR_PS ) OVER (PARTITION BY 1) /
					SUM( XOX.AMOUNT_SR_TTL ) OVER (PARTITION BY 1)
				) * 100
			END AS PCN_PS_SR_NAS
		,	CASE WHEN SUM( XOX.AMOUNT_TTL_BFC ) OVER (PARTITION BY 1) =  0 THEN 0 ELSE
				(
					SUM( XOX.AMOUNT_TTL_PS_BFC ) OVER (PARTITION BY 1) /
					SUM( XOX.AMOUNT_TTL_BFC ) OVER (PARTITION BY 1)
				) * 100
			END AS PCN_PS_TTL_BFC_NAS
		,	CASE WHEN SUM( XOX.AMOUNT_TTL_AFC ) OVER (PARTITION BY 1) =  0 THEN 0 ELSE
				(
					SUM( XOX.AMOUNT_TTL_PS_AFC ) OVER (PARTITION BY 1) /
					SUM( XOX.AMOUNT_TTL_AFC ) OVER (PARTITION BY 1)
				) * 100
			END AS PCN_PS_TTL_AFC_NAS
FROM	(
SELECT	XXX.BRANCHES
		, SUM(XXX.AMOUNT_SR_TTL) AS AMOUNT_SR_TTL
		, SUM(XXX.AMOUNT_SR_PS) AS AMOUNT_SR_PS
		, SUM(XXX.AMOUNT_UM_TTL) AS AMOUNT_UM_TTL
		, SUM(XXX.AMOUNT_UM_PS) AS AMOUNT_UM_PS
		, SUM(XXX.AMOUNT_SO_TTL) AS AMOUNT_SO_TTL
		, SUM(XXX.AMOUNT_SO_PS) AS AMOUNT_SO_PS
		, (SUM(XXX.AMOUNT_SR_TTL) + SUM(XXX.AMOUNT_UM_TTL) + SUM(XXX.AMOUNT_SO_TTL)) AS AMOUNT_TTL_BFC
		, (SUM(XXX.AMOUNT_SR_PS) + SUM(XXX.AMOUNT_UM_PS) + SUM(XXX.AMOUNT_SO_PS)) AS AMOUNT_TTL_PS_BFC
		, SUM(XXX.AMOUNT_FC_TTL) AS AMOUNT_FC_TTL
		, SUM(XXX.AMOUNT_FC_PS) AS AMOUNT_FC_PS
		, (SUM(XXX.AMOUNT_SR_TTL) + SUM(XXX.AMOUNT_UM_TTL) + SUM(XXX.AMOUNT_SO_TTL) + SUM(XXX.AMOUNT_FC_TTL)) AS AMOUNT_TTL_AFC
		, (SUM(XXX.AMOUNT_SR_PS) + SUM(XXX.AMOUNT_UM_PS) + SUM(XXX.AMOUNT_SO_PS) + SUM(XXX.AMOUNT_FC_PS)) AS AMOUNT_TTL_PS_AFC

FROM	(
SELECT	1 AS SORT_TITLE
		, 'Sales Revenue' AS TITLE
		, BRANCHES
		, SUM(AMOUNT) AS AMOUNT_SR_TTL
		, SUM(CASE WHEN ITEM_CAT IN ('PS01', 'PS02', 'PS03', 'PS04', 'PS05') THEN AMOUNT ELSE 0 END) AS AMOUNT_SR_PS
		, 0 AS AMOUNT_UM_TTL
		, 0 AS AMOUNT_UM_PS
		, 0 AS AMOUNT_SO_TTL
		, 0 AS AMOUNT_SO_PS
		, 0 AS AMOUNT_FC_TTL
		, 0 AS AMOUNT_FC_PS
FROM [dbo].[TEMP08_SALES_REVENUE]
WHERE	PROSES_ID = $P{ProsesId} AND
		GROUP_NO = 1
GROUP BY BRANCHES
UNION ALL
SELECT
		2 AS SORT_TITLE
		, 'Uang Muka' AS TITLE
		, BRANCHES
		, 0 AS AMOUNT_SR_TTL
		, 0 AS AMOUNT_SR_PS
		, SUM(AMOUNT) AS AMOUNT_UM_TTL
		, 0 AS AMOUNT_UM_PS
		, 0 AS AMOUNT_SO_TTL
		, 0 AS AMOUNT_SO_PS
		, 0 AS AMOUNT_FC_TTL
		, 0 AS AMOUNT_FC_PS
FROM [dbo].[TEMP08_SALES_REVENUE]
WHERE	PROSES_ID = $P{ProsesId} AND
		GROUP_NO = 2 AND
		YEAR_OUT_SO = $P{Tahun}
GROUP BY BRANCHES
UNION ALL
SELECT
		3 AS SORT_TITLE
		, 'Oustanding SO' AS TITLE
		, CASE	WHEN BRANCHES = 'JAKARTA' THEN '10'
				WHEN BRANCHES = 'SURABAYA' THEN '15'
				WHEN BRANCHES = 'CIKARANG' THEN '16'
				WHEN BRANCHES = 'SEMARANG' THEN '17'
				WHEN BRANCHES = 'BALI' THEN '19'
				ELSE 'NA' END AS BRANCHES
		, 0 AS AMOUNT_SR_TTL
		, 0 AS AMOUNT_SR_PS
		, 0 AS AMOUNT_UM_TTL
		, 0 AS AMOUNT_UM_PS
		, SUM(AMOUNT) AS AMOUNT_SO_TTL
		, SUM(CASE WHEN ITEM_CAT IN (
										'PS01 - 3rd Party Software'
										,'PS02 - Own Software'
										,'PS03 - Technology Service'
										,'PS04 - Hardware Service'
										,'PS05 - Manage Service'
									)THEN AMOUNT ELSE 0 END) AS AMOUNT_SO_PS
		, 0 AS AMOUNT_FC_TTL
		, 0 AS AMOUNT_FC_PS
FROM [dbo].[TEMP08_SALES_REVENUE]
WHERE	PROSES_ID = $P{ProsesId} AND
		GROUP_NO = 3 AND
		YEAR_OUT_SO = $P{Tahun}
GROUP BY BRANCHES
UNION ALL
SELECT	4 AS SORT_TITLE
		, 'Sales Forecast' AS TITLE
		, XXI.BRANCHES AS BRANCHES
		, 0 AS AMOUNT_SR_TTL
		, 0 AS AMOUNT_SR_PS
		, 0 AS AMOUNT_UM_TTL
		, 0 AS AMOUNT_UM_PS
		, 0 AS AMOUNT_SO_TTL
		, 0 AS AMOUNT_SO_PS
		, SUM(XXI.AMOUNT)  AS AMOUNT_FC_TTL
		, SUM(CASE WHEN XXI.ITEM_CAT = 'PS' THEN XXI.AMOUNT ELSE 0 END) AS AMOUNT_FC_PS
FROM	(
			SELECT	XI.BRANCHES
					,XI.SALES_CODE AS SALESMAN
					,XI.ITEM_CAT_PIPELINE AS ITEM_CAT
					,XI.SUBGROUP_NO
					, CASE	WHEN XI.SUBGROUP_NO = 1 THEN SUM(XI.AMOUNT)
							WHEN XI.SUBGROUP_NO = 2 THEN SUM(XI.AMOUNT) * ($P{weightH}/100)
							WHEN XI.SUBGROUP_NO = 3 THEN SUM(XI.AMOUNT) * ($P{weightM}/100)
							WHEN XI.SUBGROUP_NO = 4 THEN SUM(XI.AMOUNT) * ($P{weightL}/100)
							ELSE 0 END  AS AMOUNT
			FROM	(
						SELECT	2 AS MAIN_GRP
								,1 AS GROUP_NO
								,	CASE	WHEN MIN(X1.GROUP_NO) =  1 THEN	'(A)'
											WHEN MIN(X1.GROUP_NO) =  2 THEN	'(H)'
											WHEN MIN(X1.GROUP_NO) =  3 THEN	'(M)'
											WHEN MIN(X1.GROUP_NO) =  4 THEN	'(L)'
											ELSE '(R)'
									END AS NO_BARIS
								,MIN(X1.GROUP_NO) AS SUBGROUP_NO
								,X1.GROUP_PIPELINE
								,	CASE	WHEN MIN(X1.GROUP_NO) =  1 THEN	'AWARDED PROJECTS'
											WHEN MIN(X1.GROUP_NO) =  2 THEN	'HIGH POTENTIAL CLOSE PROJECTS'
											WHEN MIN(X1.GROUP_NO) =  3 THEN	'MEDIUM POTENTIAL BIG/STRATEGIC PROJECTS'
											WHEN MIN(X1.GROUP_NO) =  4 THEN	'LOW POTENTIAL BIG/STRATEGIC PROJECTS'
											ELSE 'OTHERS PIPELINE'
									END AS GROUP_DESC
								,	CASE WHEN MIN(X1.GROUP_NO) = 1 THEN
										'AWARDED (A)'
									ELSE
										CASE WHEN MIN(X1.GROUP_NO) <> 5 THEN X1.CUSTOMER ELSE X1.BRANCHES_NAME END
									END AS SUBGROUP_DESC
								,	CASE WHEN MIN(X1.GROUP_NO) <> 5 THEN X1.SPV_BY ELSE '' END AS SPV_BY
								,	CASE WHEN MIN(X1.GROUP_NO) <> 5 THEN X1.SALES_CODE ELSE '' END AS SALES_CODE
								, X1.BRANCHES
								, SUM(X1.AMOUNT) AS AMOUNT
								, X1.ITEM_CAT_PIPELINE
						FROM	(
									SELECT	SLS.[Global Dimension 1 Code] AS BRANCHES
											, CAB.[Name] AS BRANCHES_NAME
											, X.SALES_CODE
											, X.CUSTOMER
											, X.CUSTOMER_REAL
											, MAX(MAX(X.SPV_BY)) OVER (PARTITION BY X.CUSTOMER_REAL, CAB.[Name]) AS SPV_BY
											,	CASE WHEN X.SIZE_PROJECT = 'A' THEN
													1
												ELSE
													CASE WHEN X.SIZE_PROJECT = 'H' THEN
														2
													ELSE
														CASE WHEN X.SIZE_PROJECT = 'M' THEN
															3
														ELSE
															CASE WHEN X.SIZE_PROJECT = 'L' THEN
																4
															ELSE
																CASE WHEN ($P{AmtBig}*1000000) <= 0 THEN
																	CASE WHEN SUM(ISNULL(CUST.ADA,0)) > 0 THEN 4 ELSE 5 END
																ELSE
																	CASE WHEN SUM(X.AMOUNT) >= ($P{AmtBig}*1000000) THEN 4
																	ELSE
																		CASE WHEN SUM(ISNULL(CUST.ADA,0)) > 0 THEN 4 ELSE 5 END
																	END
																END
															END
														END
													END
												END AS GROUP_NO
											,	CASE	WHEN X.SIZE_PROJECT = 'A' THEN 'P1'
														WHEN X.SIZE_PROJECT = 'H' THEN 'P2'
														WHEN X.SIZE_PROJECT = 'M' THEN 'P3'
														ELSE 'P4'
												END AS GROUP_PIPELINE
											,SUM(X.AMOUNT) AS AMOUNT
											,X.ITEM_CAT_PIPELINE
									FROM	(
												SELECT	MAD.COMPANY
														,MAD.SALES_CODE
														,MAD.CABANG
														,MAD.CUSTOMER
														,MAD.CUSTOMER_REAL
														,UPPER(MAD.SIZE_PROJECT) AS SIZE_PROJECT
														,MAD.POTENSIAL_PROJECT
														,MAD.SPV_BY
														,MAD.ITEM
														,MAD.ITEM_CAT
														,MAD.HW_PS
														,
															CASE WHEN MAD.JML_TAHUN <> 0 THEN
																CASE WHEN  MAD.EST_Q = 1234 THEN
																	((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN)
																ELSE
																	CASE	WHEN MAD.EST_Q IN (123, 234) THEN (((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4) * 3
																			WHEN MAD.EST_Q IN (12, 34) THEN ((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 2
																			WHEN MAD.EST_Q IN (1,2,3,4) THEN (((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																			ELSE 0
																	END
																END
															ELSE
																(MAD.UNIT_PRICE * MAD.QTY_OUT)
															END AS AMOUNT
														,MAD.DIM_PRINCIPAL
														,MAD.EST_YEAR
														,MAD.ITEM_CAT_PIPELINE
												FROM	(
															SELECT	CASE WHEN SOH.[No_ Series] = 'SBOM' THEN 'AJ' ELSE 'SP' END AS COMPANY
																	,SOH.[Salesperson Code] AS SALES_CODE
																	,SOH.[Responsibility Center] AS CABANG
																	,	CASE WHEN SOH.[Payment Discount _] <> 0 THEN
																			'*) '+SOH.[Sell-to Customer Name]
																		ELSE
																			SOH.[Sell-to Customer Name]
																		END AS CUSTOMER
																	,	SOH.[Sell-to Customer Name] AS CUSTOMER_REAL
																	, 	CASE WHEN SOL.[Warranty Code] = '' THEN
																			SUBSTRING(SOH.[Posting Description], 1,1)
																		ELSE
																			CASE WHEN SOL.[Warranty Code] = 'CLOSED' THEN 'H' ELSE SUBSTRING(SOH.[Posting Description], 1,1) END
																		END AS SIZE_PROJECT
																	, CASE WHEN SUBSTRING(SOH.[Posting Description], 2,1) NOT IN ('1','0') THEN '0' ELSE SUBSTRING(SOH.[Posting Description], 2,1) END AS POTENSIAL_PROJECT
																	,SOH.[Sell-to County] AS SPV_BY
																	,SOL.[No_] AS ITEM
																	,SOL.[Description] AS ITEM_DESC
																	,SOL.[Shortcut Dimension 2 Code] AS ITEM_CAT
																	,SUBSTRING(SOL.[Shortcut Dimension 2 Code] , 1,2) AS HW_PS
																	,CASE WHEN SUBSTRING(SOL.[No_]  , 1,2)  = 'PS' THEN 'PS' ELSE 'HW' END AS ITEM_CAT_PIPELINE
																	,SOL.[Quantity] AS QTY_OUT
																	,SOL.[Unit Price] AS UNIT_PRICE
																	,DIMD.DIM_PRINCIPAL
																	,SOH.[Payment Discount _] AS JML_TAHUN
																	,	CASE WHEN SOH.[Payment Discount _] <> 0 THEN
																			CASE WHEN DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date]) <=  GETDATE() THEN
																				CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) > $P{Tahun} THEN
																					CASE WHEN DATEPART(year, GETDATE()) <= $P{Tahun} THEN
																						$P{Tahun}
																					ELSE
																						0
																					END
																				ELSE
																					CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) = $P{Tahun} THEN
																						CASE WHEN DATEPART(quarter, GETDATE()) IN (1,2) THEN 0 ELSE $P{Tahun} END
																					ELSE
																						0
																					END
																				END
																			ELSE
																				CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) - 1)) > $P{Tahun} THEN
																					CASE WHEN DATEPART(year, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) <= $P{Tahun} THEN
																						$P{Tahun}
																					ELSE
																						0
																					END
																				ELSE
																					CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) - 1)) = $P{Tahun} THEN
																						CASE WHEN DATEPART(quarter, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) IN (1,2) THEN 0 ELSE $P{Tahun} END
																					ELSE
																						0
																					END
																				END
																			END
																		ELSE
																			CASE WHEN DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date]) <=  GETDATE() THEN
																				DATEPART(year, GETDATE()) ELSE DATEPART(year, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date]))
																			END
																		END AS EST_YEAR
																	,	CASE WHEN SOH.[Payment Discount _] <> 0 THEN
																			CASE WHEN DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date]) <=  GETDATE() THEN
																				CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) > $P{Tahun} THEN
																					CASE WHEN DATEPART(year, GETDATE()) < $P{Tahun} THEN
																						1234
																					ELSE
																						CASE WHEN DATEPART(year, GETDATE()) = $P{Tahun} THEN
																							CASE	WHEN DATEPART(quarter, GETDATE()) = 1 THEN 1234
																									WHEN DATEPART(quarter, GETDATE()) = 2 THEN 234
																									WHEN DATEPART(quarter, GETDATE()) = 3 THEN 34
																									ELSE 4
																							END
																						ELSE
																							0
																						END
																					END
																				ELSE
																					CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) = $P{Tahun} THEN
																						CASE WHEN DATEPART(quarter, GETDATE()) = 1 THEN 0
																								WHEN DATEPART(quarter, GETDATE()) = 2 THEN 1
																								WHEN DATEPART(quarter, GETDATE()) = 3 THEN 12
																								ELSE 123
																						END
																					ELSE
																						0
																					END
																				END
																			ELSE
																				CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) - 1)) > $P{Tahun} THEN
																					CASE WHEN DATEPART(year, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) < $P{Tahun} THEN
																						1234
																					ELSE
																						CASE WHEN DATEPART(year, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) = $P{Tahun} THEN
																							CASE	WHEN DATEPART(quarter, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) = 1 THEN 1234
																									WHEN DATEPART(quarter, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) = 2 THEN 234
																									WHEN DATEPART(quarter, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) = 3 THEN 34
																									ELSE 4
																							END
																						ELSE
																							0
																						END
																					END
																				ELSE
																					CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) - 1)) = $P{Tahun} THEN
																						CASE WHEN DATEPART(quarter, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) = 1 THEN 0
																								WHEN DATEPART(quarter, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) = 2 THEN 1
																								WHEN DATEPART(quarter, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) = 3 THEN 12
																								ELSE 123
																						END
																					ELSE
																						0
																					END
																				END
																			END
																		ELSE
																			CASE WHEN DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date]) <=  GETDATE() THEN
																				DATEPART(quarter, GETDATE())
																			ELSE
																				DATEPART(quarter, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date]))
																			END
																		END AS EST_Q
															FROM [dbo].[BC_AUTOJAYA$Sales Header] SOH
															INNER JOIN [dbo].[BC_AUTOJAYA$Sales Line] SOL ON SOL.[Document Type] = SOH.[Document Type] AND SOL.[Document No_] = SOH.[No_]
															LEFT JOIN	(
																			SELECT X.[Dimension Set ID]
																					,MAX(CASE WHEN X.[Dimension Code] = 'PRINCIPAL' THEN X.[Dimension Value Code] ELSE NULL END) AS DIM_PRINCIPAL
																			FROM [dbo].[BC_AUTOJAYA$Dimension Set Entry] X
																			GROUP BY [Dimension Set ID]
																		) DIMD ON DIMD.[Dimension Set ID] = SOL.[Dimension Set ID]
															WHERE SOH.[No_ Series] IN ('SBOM', 'SBOMS')
														) MAD
											) X
									LEFT JOIN [dbo].[BC_AUTOJAYA$Salesperson_Purchaser] SLS ON SLS.[Code] = X.SALES_CODE
									LEFT JOIN [dbo].[BC_AUTOJAYA$Responsibility Center] CAB ON CAB.[Code] = SLS.[Global Dimension 1 Code]
									LEFT JOIN	(
													SELECT	 DISTINCT  X.CUSTOMER_REAL, 1 AS ADA
													FROM	(
																SELECT	MAD.CUSTOMER_REAL
																		,UPPER(MAD.SIZE_PROJECT) AS SIZE_PROJECT
																		,MAD.EST_YEAR
																FROM	(
																			SELECT		SOH.[Sell-to Customer Name] AS CUSTOMER_REAL
																					, 	CASE WHEN SOL.[Warranty Code] = '' THEN
																							SUBSTRING(SOH.[Posting Description], 1,1)
																						ELSE
																							CASE WHEN SOL.[Warranty Code] = 'CLOSED' THEN 'H' ELSE SUBSTRING(SOH.[Posting Description], 1,1) END
																						END AS SIZE_PROJECT
																					,	CASE WHEN SOH.[Payment Discount _] <> 0 THEN
																							CASE WHEN DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date]) <=  GETDATE() THEN
																								CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) > $P{Tahun} THEN
																									CASE WHEN DATEPART(year, GETDATE()) <= $P{Tahun} THEN
																										$P{Tahun}
																									ELSE
																										0
																									END
																								ELSE
																									CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) = $P{Tahun} THEN
																										CASE WHEN DATEPART(quarter, GETDATE()) IN (1,2) THEN 0 ELSE $P{Tahun} END
																									ELSE
																										0
																									END
																								END
																							ELSE
																								CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) - 1)) > $P{Tahun} THEN
																									CASE WHEN DATEPART(year, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) <= $P{Tahun} THEN
																										$P{Tahun}
																									ELSE
																										0
																									END
																								ELSE
																									CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) - 1)) = $P{Tahun} THEN
																										CASE WHEN DATEPART(quarter, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date])) IN (1,2) THEN 0 ELSE $P{Tahun} END
																									ELSE
																										0
																									END
																								END
																							END
																						ELSE
																							CASE WHEN DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date]) <=  GETDATE() THEN
																								DATEPART(year, GETDATE()) ELSE DATEPART(year, DATEADD(MONTH, (6-($P{SemAkhir}-6)) , SOL.[Shipment Date]))
																							END
																						END AS EST_YEAR
																			FROM [dbo].[BC_AUTOJAYA$Sales Header] SOH
																			INNER JOIN [dbo].[BC_AUTOJAYA$Sales Line] SOL ON SOL.[Document Type] = SOH.[Document Type] AND SOL.[Document No_] = SOH.[No_]
																			WHERE SOH.[No_ Series] IN ('SBOM', 'SBOMS')
																		) MAD
															) X
													WHERE	X.EST_YEAR = $P{Tahun}
															AND X.SIZE_PROJECT IN ('M', 'H', 'A')
												) CUST ON CUST.CUSTOMER_REAL = X.CUSTOMER_REAL
									WHERE	X.EST_YEAR = $P{Tahun}
											AND SLS.[Global Dimension 1 Code] IS NOT NULL
									GROUP BY CAB.[Name]
											,X.SALES_CODE
											,X.CUSTOMER
											,X.CUSTOMER_REAL
											,X.SIZE_PROJECT
											,SLS.[Global Dimension 1 Code]
											,X.ITEM_CAT_PIPELINE
								) X1
						GROUP BY X1.CUSTOMER
								,X1.BRANCHES_NAME
								,X1.SPV_BY
								,X1.SALES_CODE
								,X1.BRANCHES
								,X1.GROUP_PIPELINE
								,X1.ITEM_CAT_PIPELINE
					) XI
			WHERE	XI.SUBGROUP_NO <> 5
			GROUP BY XI.BRANCHES
					,XI.SALES_CODE
					,XI.ITEM_CAT_PIPELINE
					,XI.SUBGROUP_NO
		) XXI
GROUP BY XXI.BRANCHES
		) XXX
GROUP BY XXX.BRANCHES
		) XOX
LEFT JOIN	(
				SELECT	SLS_OR_CAB
						, SUM(TARGET) AS TARGET_SLS
						, SUM(TARGET_PS) AS TARGET_PS
				FROM [dbo].[M06_TARGET_PIPELINE]
				WHERE JENIS <> 'SALES' AND TAHUN = $P{Tahun}
				GROUP BY SLS_OR_CAB
			) TRG ON TRG.SLS_OR_CAB = XOX.BRANCHES
LEFT JOIN  [dbo].[BC_AUTOJAYA$Responsibility Center] CAB ON CAB.[Code] = XOX.BRANCHES]]>
	</queryString>
