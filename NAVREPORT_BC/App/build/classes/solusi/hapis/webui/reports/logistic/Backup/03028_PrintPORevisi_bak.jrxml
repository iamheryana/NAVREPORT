	<queryString>
		<![CDATA[SELECT	CPI.[Name] AS NAMA_COMPANY
		,CPI.[Home Page] AS WEB
		,CPI.[Address] AS ADD_COMP_LINE_1
		,CPI.[Address 2]+' '+CPI.[City]+' '+CPI.[County]+' '+CPI.[Post Code] AS ADD_COMP_LINE_2
		,'Phone : '+CPI.[Phone No_]+' '+CPI.[Phone No_ 2]+', Fax No. :'+CPI.[Fax No_] AS PHONE_NO
		,'Email : '+CPI.[E-Mail] AS EMAIL
		,XX.NO_PO
		,PHR.[Buy-from Vendor Name] AS VENDOR_TO
		,PHR.[Buy-from Address] + ' ' + PHR.[Buy-from Address 2] AS ADD_LINE_1
 		,PHR.[Buy-from City]   + ' ' + PHR.[Pay-to Post Code] AS ADD_LINE_2
 		,PHR.[Buy-from Contact] AS CONTACT_NAME
		,VEN.[Phone No_] + ' / ' + VEN.[Fax No_] AS CONTACT_NO
		,VEN.[E-Mail] AS EMAIL_VENDOR
		,PHR.[Posting Date] AS PO_DATE
		,PHR.[Ship-to Address] + ' ' + PHR.[Ship-to Address 2] AS DEL_ADD_LINE_1
		,PHR.[Ship-to City]   + ' ' + PHR.[Ship-to Contact] AS DEL_ADD_LINE_2
		,PHR.[Your Reference] AS CUSTOMER_ORDER_NO
		,PHR.[Vendor Order No_] AS PE_NO
		,$P{Revisi} AS PO_VERSION
		,XX.NO_INV
		,XX.CURR_CODE
		,(ROW_NUMBER() OVER (PARTITION BY XX.NO_PO ORDER BY XX.NO_PO, XX.LINE_NO, XX.NO_ITEM)) AS NO_URUT
		,XX.NO_ITEM
		,XX.ITEM_DESC
		,XX.ITEM_DESC2
		,XX.QTY_INV
		,XX.UOM
		,XX.UNIT_PRICE_INV
		,XX.UNIT_PRICE_PO
		,(XX.QTY_INV * XX.UNIT_PRICE_INV) AS SUBTOTAL
		,FLOOR(SUM((XX.QTY_INV * XX.UNIT_PRICE_INV) * 0.1) OVER (PARTITION BY XX.NO_PO ORDER BY XX.NO_PO)) AS PPN_TOTAL
		,(XX.QTY_INV * XX.UNIT_PRICE_INV) * 0.1 AS PPN
		,(XX.QTY_INV * XX.UNIT_PRICE_INV) + ((XX.QTY_INV * XX.UNIT_PRICE_INV) * 0.1) AS TOTAL
		, $P{Comment} AS COMMENT
		, $P{EndUser} AS END_USER
		, $P{RMA} AS RMA
		,PYT.[Description] AS PAY_TERM
		,PHR.[Expected Receipt Date] AS EXPECTED_DATE
		,PHR.[Shipment Method Code] AS SHIPPING_TERM
		,$P{TTD} AS TTD
FROM	(
			SELECT	X.NO_INV
					,X.CURR_CODE
					,MIN(X.LINE_NO) AS LINE_NO
					,X.NO_ITEM
					,X.ITEM_DESC
					,X.ITEM_DESC2
					,SUM(X.QTY_INV) AS QTY_INV
					,X.UOM
					,X.UNIT_PRICE_INV
					,X.NO_PO
					,X.NO_PO_TYPE
					,X.UNIT_PRICE_PO
			FROM	(
						SELECT
								PIH.[No_] AS NO_INV
								,CASE WHEN PIH.[Currency Code] = ''  THEN 'IDR' ELSE PIH.[Currency Code] END AS CURR_CODE
								,PIL.[Line No_] AS LINE_NO
								,PIL.[No_] AS NO_ITEM
								,PIL.[Description] AS ITEM_DESC
								,PIL.[Description 2] AS ITEM_DESC2
								,PIL.[Quantity] AS QTY_INV
								,PIL.[Unit of Measure] AS UOM
								,PIL.[Direct Unit Cost] AS UNIT_PRICE_INV
								, CASE WHEN PL.[Document No_] IS NOT NULL AND SUBSTRING(PL.[Document No_],5,1) = 'P' THEN PLO.[Document No_] ELSE PL.[Document No_] END AS NO_PO
								, CASE WHEN PL.[Document No_] IS NOT NULL AND SUBSTRING(PL.[Document No_],5,1) = 'P' THEN 4 ELSE 1 END AS NO_PO_TYPE
								, CASE WHEN PL.[Document No_] IS NOT NULL AND SUBSTRING(PL.[Document No_],5,1) = 'P' THEN ISNULL(PL.[Direct Unit Cost],0)  ELSE ISNULL(PLO.[Direct Unit Cost],0) END AS UNIT_PRICE_PO
						FROM [dbo].[AUTOJAYA$Purch_ Inv_ Header] PIH
						LEFT JOIN [dbo].[AUTOJAYA$Purch_ Inv_ Line] PIL ON PIL.[Document No_] =  PIH.[No_]
						LEFT JOIN [dbo].[AUTOJAYA$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
						LEFT JOIN [dbo].[AUTOJAYA$Purch_ Rcpt_ Line] PRL ON PRL.[Document No_] = PIL.[Receipt No_] AND PRL.[Line No_] = PIL.[Receipt Line No_]
						LEFT JOIN [dbo].[AUTOJAYA$Purchase Line] PL ON PL.[Document No_] = PRL.[Order No_] AND PL.[Line No_] = PRL.[Order Line No_] AND PL.[Document Type] = 1
						LEFT JOIN [dbo].[AUTOJAYA$Purchase Line] PLO ON PLO.[Document No_] = PL.[Blanket Order No_] AND PLO.[Line No_] = PL.[Blanket Order Line No_] AND PLO.[Document Type] = 4
						WHERE	CMH.[No_] IS NULL AND
								PIL.[No_] <> ''
					) X
			GROUP BY X.NO_INV
					,X.CURR_CODE
					,X.NO_ITEM
					,X.ITEM_DESC
					,X.ITEM_DESC2
					,X.UOM
					,X.UNIT_PRICE_INV
					,X.NO_PO
					,X.NO_PO_TYPE
					,X.UNIT_PRICE_PO
		) XX
LEFT JOIN [dbo].[AUTOJAYA$Purchase Header] PHR ON PHR.[No_] = XX.NO_PO AND PHR.[Document Type] = XX.NO_PO_TYPE
LEFT JOIN	(
				SELECT	CMN.[Document Type]
						, CMN.[No_]
						, MAX(CMN.COMMENT) AS COMMENT
				FROM	(
							SELECT	PCL.[Document Type]
									,PCL.[No_]
									,(
										SELECT CL.[Comment] + ' ' as 'data()'
										FROM [dbo].[AUTOJAYA$Purch_ Comment Line] CL
										WHERE	CL.[Document Type] = PCL.[Document Type] AND
												CL.[No_] = PCL.[No_]
										for xml path('')
									 ) AS COMMENT
							FROM [dbo].[AUTOJAYA$Purch_ Comment Line] PCL
						) CMN
				GROUP BY CMN.[Document Type], CMN.[No_]
			) CMM ON CMM.[Document Type] = PHR.[Document Type] AND CMM.[No_] = PHR.[No_]
LEFT JOIN [dbo].[AUTOJAYA$Vendor] VEN ON VEN.[No_] = PHR.[Buy-from Vendor No_]
LEFT JOIN [dbo].[AUTOJAYA$Payment Terms] PYT ON PYT.[Code] = PHR.[Payment Terms Code]
LEFT JOIN [dbo].[AUTOJAYA$Company Information] CPI ON 1 = 1
WHERE	XX.NO_PO IS NOT NULL AND XX.UNIT_PRICE_INV <> XX.UNIT_PRICE_PO AND
		XX.NO_INV = $P{NoInvoice} AND (XX.NO_PO = $P{NoPO} OR $P{NoPO} = 'ALL')
UNION ALL
SELECT	CPI.[Name] AS NAMA_COMPANY
		,CPI.[Home Page] AS WEB
		,CPI.[Address] AS ADD_COMP_LINE_1
		,CPI.[Address 2]+' '+CPI.[City]+' '+CPI.[County]+' '+CPI.[Post Code] AS ADD_COMP_LINE_2
		,'Phone : '+CPI.[Phone No_]+' '+CPI.[Phone No_ 2]+', Fax No. :'+CPI.[Fax No_] AS PHONE_NO
		,'Email : '+CPI.[E-Mail] AS EMAIL
		,XX.NO_PO
		,PHR.[Buy-from Vendor Name] AS VENDOR_TO
		,PHR.[Buy-from Address] + ' ' + PHR.[Buy-from Address 2] AS ADD_LINE_1
 		,PHR.[Buy-from City]   + ' ' + PHR.[Pay-to Post Code] AS ADD_LINE_2
 		,PHR.[Buy-from Contact] AS CONTACT_NAME
		,VEN.[Phone No_] + ' / ' + VEN.[Fax No_] AS CONTACT_NO
		,VEN.[E-Mail] AS EMAIL_VENDOR
		,PHR.[Posting Date] AS PO_DATE
		,PHR.[Ship-to Address] + ' ' + PHR.[Ship-to Address 2] AS DEL_ADD_LINE_1
		,PHR.[Ship-to City]   + ' ' + PHR.[Ship-to Contact] AS DEL_ADD_LINE_2
		,PHR.[Your Reference] AS CUSTOMER_ORDER_NO
		,PHR.[Vendor Order No_] AS PE_NO
		,$P{Revisi} AS PO_VERSION
		,XX.NO_INV
		,XX.CURR_CODE
		,(ROW_NUMBER() OVER (PARTITION BY XX.NO_PO ORDER BY XX.NO_PO, XX.LINE_NO, XX.NO_ITEM)) AS NO_URUT
		,XX.NO_ITEM
		,XX.ITEM_DESC
		,XX.ITEM_DESC2
		,XX.QTY_INV
		,XX.UOM
		,XX.UNIT_PRICE_INV
		,XX.UNIT_PRICE_PO
		,(XX.QTY_INV * XX.UNIT_PRICE_INV) AS SUBTOTAL
		,FLOOR(SUM((XX.QTY_INV * XX.UNIT_PRICE_INV) * 0.1) OVER (PARTITION BY XX.NO_PO ORDER BY XX.NO_PO)) AS PPN_TOTAL
		,(XX.QTY_INV * XX.UNIT_PRICE_INV) * 0.1 AS PPN
		,(XX.QTY_INV * XX.UNIT_PRICE_INV) + ((XX.QTY_INV * XX.UNIT_PRICE_INV) * 0.1) AS TOTAL
		, $P{Comment} AS COMMENT
		, $P{EndUser} AS END_USER
		, $P{RMA} AS RMA
		,PYT.[Description] AS PAY_TERM
		,PHR.[Expected Receipt Date] AS EXPECTED_DATE
		,PHR.[Shipment Method Code] AS SHIPPING_TERM
		,$P{TTD} AS TTD
FROM	(
			SELECT	X.NO_INV
					,X.CURR_CODE
					,MIN(X.LINE_NO) AS LINE_NO
					,X.NO_ITEM
					,X.ITEM_DESC
					,X.ITEM_DESC2
					,SUM(X.QTY_INV) AS QTY_INV
					,X.UOM
					,X.UNIT_PRICE_INV
					,X.NO_PO
					,X.NO_PO_TYPE
					,X.UNIT_PRICE_PO
			FROM	(
						SELECT
								PIH.[No_] AS NO_INV
								,CASE WHEN PIH.[Currency Code] = ''  THEN 'IDR' ELSE PIH.[Currency Code] END AS CURR_CODE
								,PIL.[Line No_] AS LINE_NO
								,PIL.[No_] AS NO_ITEM
								,PIL.[Description] AS ITEM_DESC
								,PIL.[Description 2] AS ITEM_DESC2
								,PIL.[Quantity] AS QTY_INV
								,PIL.[Unit of Measure] AS UOM
								,PIL.[Direct Unit Cost] AS UNIT_PRICE_INV
								, CASE WHEN PL.[Document No_] IS NOT NULL AND SUBSTRING(PL.[Document No_],5,1) = 'P' THEN PLO.[Document No_] ELSE PL.[Document No_] END AS NO_PO
								, CASE WHEN PL.[Document No_] IS NOT NULL AND SUBSTRING(PL.[Document No_],5,1) = 'P' THEN 4 ELSE 1 END AS NO_PO_TYPE
								, CASE WHEN PL.[Document No_] IS NOT NULL AND SUBSTRING(PL.[Document No_],5,1) = 'P' THEN ISNULL(PL.[Direct Unit Cost],0)  ELSE ISNULL(PLO.[Direct Unit Cost],0) END AS UNIT_PRICE_PO
						FROM [dbo].[SOLUSI$Purch_ Inv_ Header] PIH
						LEFT JOIN [dbo].[SOLUSI$Purch_ Inv_ Line] PIL ON PIL.[Document No_] =  PIH.[No_]
						LEFT JOIN [dbo].[SOLUSI$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
						LEFT JOIN [dbo].[SOLUSI$Purch_ Rcpt_ Line] PRL ON PRL.[Document No_] = PIL.[Receipt No_] AND PRL.[Line No_] = PIL.[Receipt Line No_]
						LEFT JOIN [dbo].[SOLUSI$Purchase Line] PL ON PL.[Document No_] = PRL.[Order No_] AND PL.[Line No_] = PRL.[Order Line No_] AND PL.[Document Type] = 1
						LEFT JOIN [dbo].[SOLUSI$Purchase Line] PLO ON PLO.[Document No_] = PL.[Blanket Order No_] AND PLO.[Line No_] = PL.[Blanket Order Line No_] AND PLO.[Document Type] = 4
						WHERE	CMH.[No_] IS NULL AND
								PIL.[No_] <> ''
					) X
			GROUP BY X.NO_INV
					,X.CURR_CODE
					,X.NO_ITEM
					,X.ITEM_DESC
					,X.ITEM_DESC2
					,X.UOM
					,X.UNIT_PRICE_INV
					,X.NO_PO
					,X.NO_PO_TYPE
					,X.UNIT_PRICE_PO
		) XX
LEFT JOIN [dbo].[SOLUSI$Purchase Header] PHR ON PHR.[No_] = XX.NO_PO AND PHR.[Document Type] = XX.NO_PO_TYPE
LEFT JOIN	(
				SELECT	CMN.[Document Type]
						, CMN.[No_]
						, MAX(CMN.COMMENT) AS COMMENT
				FROM	(
							SELECT	PCL.[Document Type]
									,PCL.[No_]
									,(
										SELECT CL.[Comment] + ' ' as 'data()'
										FROM [dbo].[SOLUSI$Purch_ Comment Line] CL
										WHERE	CL.[Document Type] = PCL.[Document Type] AND
												CL.[No_] = PCL.[No_]
										for xml path('')
									 ) AS COMMENT
							FROM [dbo].[SOLUSI$Purch_ Comment Line] PCL
						) CMN
				GROUP BY CMN.[Document Type], CMN.[No_]
			) CMM ON CMM.[Document Type] = PHR.[Document Type] AND CMM.[No_] = PHR.[No_]
LEFT JOIN [dbo].[SOLUSI$Vendor] VEN ON VEN.[No_] = PHR.[Buy-from Vendor No_]
LEFT JOIN [dbo].[SOLUSI$Payment Terms] PYT ON PYT.[Code] = PHR.[Payment Terms Code]
LEFT JOIN [dbo].[SOLUSI$Company Information] CPI ON 1 = 1
WHERE	XX.NO_PO IS NOT NULL AND XX.UNIT_PRICE_INV <> XX.UNIT_PRICE_PO AND
		XX.NO_INV = $P{NoInvoice} AND (XX.NO_PO = $P{NoPO} OR $P{NoPO} = 'ALL')]]>
	</queryString>
