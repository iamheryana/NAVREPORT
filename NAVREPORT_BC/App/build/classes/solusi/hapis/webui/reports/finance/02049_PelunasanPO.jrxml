	<parameter name="KodeProject" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT	XXX.NO_PO
		,XXX.TGL_PO
		,XXX.NO_INVOICE
		,XXX.NO_INVIOCE_VENDOR
		,XXX.TGL_INVOICE
		,XXX.VENDOR_NO
		,XXX.DISTI
		,XXX.CURR_CODE
		, CASE WHEN XXX.BARIS = 1 THEN XXX.AMOUNT_PO ELSE 0 END AS AMOUNT_PO
		, CASE WHEN XXX.BARIS = 1 THEN XXX.AMT_BELUM_INV ELSE 0 END AS AMT_BELUM_INV
		, CASE WHEN XXX.BARIS = 1 THEN XXX.AMT_SUDAH_INV ELSE 0 END AS AMT_SUDAH_INV
		, CASE WHEN XXX.BARIS = 1 THEN XXX.AMT_SUDAH_INV_LUNAS ELSE 0 END AS AMT_SUDAH_INV_LUNAS
		, CASE WHEN XXX.BARIS = 1 THEN XXX.AMT_SUDAH_INV_BELUM_LUNAS ELSE 0 END AS AMT_SUDAH_INV_BELUM_LUNAS
		,XXX.AMOUNT
		,XXX.AMT_BELUM_LUNAS
		,XXX.AMT_LUNAS
		,XXX.TGL_LUNAS
		,XXX.NO_PELUNASAN
		,XXX.DIM_PROJECT
FROM	(
SELECT
		PIN.NO_PO
		,PIN.TGL_PO
		,PIN.NO_INVOICE
		,PIN.NO_INVIOCE_VENDOR
		,PIN.TGL_INVOICE
		,PIN.VENDOR_NO
		,VDR.[Name] AS DISTI
		,PIN.CURR_CODE
		,PIN.AMOUNT_PO
		,(PIN.AMOUNT_PO - SUM(PIN.AMOUNT) OVER(PARTITION BY PIN.NO_PO ORDER BY PIN.NO_PO)) AS AMT_BELUM_INV
		, SUM(PIN.AMOUNT) OVER(PARTITION BY PIN.NO_PO ORDER BY PIN.NO_PO) AS AMT_SUDAH_INV
		, SUM(PIN.AMT_LUNAS) OVER(PARTITION BY PIN.NO_PO ORDER BY PIN.NO_PO) AS AMT_SUDAH_INV_LUNAS
		, SUM(PIN.AMT_BELUM_LUNAS) OVER(PARTITION BY PIN.NO_PO ORDER BY PIN.NO_PO) AS AMT_SUDAH_INV_BELUM_LUNAS
		, ROW_NUMBER()OVER(PARTITION BY PIN.NO_PO ORDER BY PIN.NO_PO) AS BARIS
		,PIN.AMOUNT
		,PIN.AMT_BELUM_LUNAS
		,PIN.AMT_LUNAS
		,PIN.TGL_LUNAS
		,PIN.NO_PELUNASAN
		,PIN.DIM_PROJECT
FROM	(
			SELECT	PH.[No_] AS NO_PO
					,PH.[Order Date] AS TGL_PO
					,PIH.NO_INVOICE
					,PIH.NO_INVIOCE_VENDOR
					,PIH.TGL_INVOICE
					,PH.[Pay-to Vendor No_] AS VENDOR_NO
					,PL.DIM_PRINCIPAL
					,CASE WHEN PH.[Currency Code] = '' THEN 'IDR' ELSE PH.[Currency Code] END AS CURR_CODE
					,PL.AMOUNT_PO
					,ISNULL(PIH.AMOUNT,0) AS AMOUNT
					,ISNULL(PIH.AMT_BELUM_LUNAS,0) AS AMT_BELUM_LUNAS
					,ISNULL(PIH.AMT_LUNAS,0) AS AMT_LUNAS
					,PIH.TGL_LUNAS
					,PIH.NO_PELUNASAN
					,DIM.DIM_PROJECT
			FROM [dbo].[BC_AUTOJAYA$Purchase Header] PH
			LEFT JOIN	(
							SELECT [Dimension Set ID]
									,MAX(CASE WHEN [Dimension Code] = 'PROJECT' THEN [Dimension Value Code] ELSE NULL END) AS DIM_PROJECT
							FROM [dbo].[BC_AUTOJAYA$Dimension Set Entry]
							GROUP BY [Dimension Set ID]
						) DIM ON DIM.[Dimension Set ID] = PH.[Dimension Set ID]
			LEFT JOIN	(
							SELECT [Document Type]
								  ,[Document No_]
								  ,MAX(DIM.DIM_PRINCIPAL) AS DIM_PRINCIPAL
								  ,SUM([Amount Including VAT]) AS AMOUNT_PO
							FROM [dbo].[BC_AUTOJAYA$Purchase Line] PCL
							LEFT JOIN	(
											SELECT [Dimension Set ID]
												  ,MAX(CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END) AS DIM_ITEM_CATEGORY
												  ,MAX(CASE WHEN [Dimension Code] = 'PRINCIPAL' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRINCIPAL
												  ,MAX(CASE WHEN [Dimension Code] = 'PRODUCT' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRODUCT
											FROM [dbo].[BC_AUTOJAYA$Dimension Set Entry]
											GROUP BY [Dimension Set ID]
										) DIM ON DIM.[Dimension Set ID] = PCL.[Dimension Set ID]
							GROUP BY [Document Type],[Document No_]
						)	PL ON PL.[Document Type] = PH.[Document Type] AND PL.[Document No_] = PH.[No_]
			LEFT JOIN	(
							SELECT XX.NO_INVOICE
									,XX.NO_INVIOCE_VENDOR
									,XX.TGL_INVOICE
									,XX.NO_PO
									,XX.PO_TYPE
									,XX.CURR_CODE
									,XX.AMOUNT
									,PLN.TGL_LUNAS
									,PLN.NO_PELUNASAN
									,(ISNULL(PLN.RMN_AMOUNT,0) * -1) AS AMT_BELUM_LUNAS
									,(XX.AMOUNT - (ISNULL(PLN.RMN_AMOUNT,0) * -1))  AS AMT_LUNAS
							FROM	(
										SELECT	X.NO_INVOICE
												,X.NO_INVIOCE_VENDOR
												,X.TGL_INVOICE
												,X.NO_PO
												,CASE WHEN SUBSTRING(X.NO_PO,1,3) = 'BPO' THEN 4 ELSE 1 END AS PO_TYPE
												,X.CURR_CODE
												,SUM(X.AMOUNT) AS AMOUNT
										FROM	(
													SELECT 	PIH.[No_] AS NO_INVOICE
															,PIH.[Vendor Invoice No_] AS NO_INVIOCE_VENDOR
															,PIH.[Posting Date] AS TGL_INVOICE
															,	CASE WHEN ISNULL(BPO.BPO,'') <> '' THEN
																	CASE WHEN SUBSTRING(BPO.BPO,6,1) = 'P' THEN BPO.BPO ELSE PRH.[Order No_] END
																	ELSE PRH.[Order No_]
																END AS NO_PO
															,CASE WHEN PIH.[Currency Code] = '' THEN 'IDR' ELSE PIH.[Currency Code] END AS CURR_CODE
															,PIL.AMOUNT
													FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH
													LEFT JOIN	(
																	SELECT PCL.[Document No_]
																			,PCL.[Receipt No_]
																			,SUM(PCL.[Amount Including VAT]) AS AMOUNT
																	FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PCL
																	WHERE [Amount] <> 0
																	GROUP BY [Document No_], [Receipt No_]
																) PIL ON PIL.[Document No_] =  PIH.[No_]
													LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
													LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Header] PRH ON PRH.[No_] = PIL.[Receipt No_]
													LEFT JOIN	(
																	SELECT PH.[No_], MAX(PD.[Blanket Order No_]) AS BPO
																	FROM [dbo].[BC_AUTOJAYA$Purchase Header] PH
																	INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Line] PD ON PD.[Document No_] = PH.[No_]
																	WHERE PH.[Document Type] = 1
																	GROUP BY PH.[No_]
																) BPO ON BPO.[No_] = PRH.[Order No_]
													WHERE CMH.[No_] IS NULL
												) X
										GROUP BY X.NO_INVOICE
												,X.NO_INVIOCE_VENDOR
												,X.TGL_INVOICE
												,X.NO_PO
												,X.CURR_CODE
									) XX
							LEFT JOIN	(
											SELECT
													   VLE.[Document No_] AS NO_INV
													  ,VLE.[External Document No_] AS NO_PO_CUST
													  ,VLD.RMN_AMOUNT
													  ,VLD2.TGL_LUNAS
													  ,VLD2.NO_PELUNASAN
											FROM [dbo].[BC_AUTOJAYA$Vendor Ledger Entry] VLE
											LEFT JOIN (
														SELECT	VLED.[Vendor Ledger Entry No_]
																,SUM(VLED.[Amount]) AS RMN_AMOUNT
														FROM [dbo].[BC_AUTOJAYA$Detailed Vendor Ledg_ Entry] VLED
														GROUP BY VLED.[Vendor Ledger Entry No_]
													  )VLD ON VLD.[Vendor Ledger Entry No_] = VLE.[Entry No_]
											LEFT JOIN (
														SELECT	VLED2.[Vendor Ledger Entry No_]
																,MAX(VLED2.[Posting Date]) AS TGL_LUNAS
																,MAX(VLED2.[Document No_]) AS NO_PELUNASAN
														FROM [dbo].[BC_AUTOJAYA$Detailed Vendor Ledg_ Entry] VLED2
														WHERE VLED2.[Document Type] <> 2
														GROUP BY VLED2.[Vendor Ledger Entry No_]
													  ) VLD2 ON VLD2.[Vendor Ledger Entry No_] = VLE.[Entry No_]
										) PLN ON PLN.NO_INV = XX.NO_INVOICE
						) PIH ON PIH.NO_PO = PH.[No_] AND PIH.PO_TYPE = PH.[Document Type]
			WHERE	((PH.[Document Type] = 1 AND SUBSTRING(PH.[No_],5,1) <> 'P')
					OR
					(PH.[Document Type] = 4 AND SUBSTRING(PH.[No_],6,1) = 'P'))
			UNION ALL
			SELECT	PH.[No_] AS NO_PO
					,PH.[Order Date] AS TGL_PO
					,PIH.NO_INVOICE
					,PIH.NO_INVIOCE_VENDOR
					,PIH.TGL_INVOICE
					,PH.[Pay-to Vendor No_] AS VENDOR_NO
					,PL.DIM_PRINCIPAL
					,CASE WHEN PH.[Currency Code] = '' THEN 'IDR' ELSE PH.[Currency Code] END AS CURR_CODE
					,PL.AMOUNT_PO
					,ISNULL(PIH.AMOUNT,0) AS AMOUNT
					,ISNULL(PIH.AMT_BELUM_LUNAS,0) AS AMT_BELUM_LUNAS
					,ISNULL(PIH.AMT_LUNAS,0) AS AMT_LUNAS
					,PIH.TGL_LUNAS
					,PIH.NO_PELUNASAN
					,DIM.DIM_PROJECT
			FROM [dbo].[BC_SOLUSI$Purchase Header] PH
			LEFT JOIN	(
							SELECT [Dimension Set ID]
									,MAX(CASE WHEN [Dimension Code] = 'PROJECT' THEN [Dimension Value Code] ELSE NULL END) AS DIM_PROJECT
							FROM [dbo].[BC_SOLUSI$Dimension Set Entry]
							GROUP BY [Dimension Set ID]
						) DIM ON DIM.[Dimension Set ID] = PH.[Dimension Set ID]
			LEFT JOIN	(
							SELECT [Document Type]
								  ,[Document No_]
								  ,MAX(DIM.DIM_PRINCIPAL) AS DIM_PRINCIPAL
								  ,SUM([Amount Including VAT]) AS AMOUNT_PO
							FROM [dbo].[BC_SOLUSI$Purchase Line] PCL
							LEFT JOIN	(
											SELECT [Dimension Set ID]
												  ,MAX(CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END) AS DIM_ITEM_CATEGORY
												  ,MAX(CASE WHEN [Dimension Code] = 'PRINCIPAL' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRINCIPAL
												  ,MAX(CASE WHEN [Dimension Code] = 'PRODUCT' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRODUCT
											FROM [dbo].[BC_SOLUSI$Dimension Set Entry]
											GROUP BY [Dimension Set ID]
										) DIM ON DIM.[Dimension Set ID] = PCL.[Dimension Set ID]
							GROUP BY [Document Type],[Document No_]
						)	PL ON PL.[Document Type] = PH.[Document Type] AND PL.[Document No_] = PH.[No_]
			LEFT JOIN	(
							SELECT XX.NO_INVOICE
									,XX.NO_INVIOCE_VENDOR
									,XX.TGL_INVOICE
									,XX.NO_PO
									,XX.PO_TYPE
									,XX.CURR_CODE
									,XX.AMOUNT
									,PLN.TGL_LUNAS
									,PLN.NO_PELUNASAN
									,(ISNULL(PLN.RMN_AMOUNT,0) * -1) AS AMT_BELUM_LUNAS
									,(XX.AMOUNT - (ISNULL(PLN.RMN_AMOUNT,0) * -1))  AS AMT_LUNAS
							FROM	(
										SELECT	X.NO_INVOICE
												,X.NO_INVIOCE_VENDOR
												,X.TGL_INVOICE
												,X.NO_PO
												,CASE WHEN SUBSTRING(X.NO_PO,1,3) = 'BPO' THEN 4 ELSE 1 END AS PO_TYPE
												,X.CURR_CODE
												,SUM(X.AMOUNT) AS AMOUNT
										FROM	(
													SELECT 	PIH.[No_] AS NO_INVOICE
															,PIH.[Vendor Invoice No_] AS NO_INVIOCE_VENDOR
															,PIH.[Posting Date] AS TGL_INVOICE
															,	CASE WHEN ISNULL(BPO.BPO,'') <> '' THEN
																	CASE WHEN SUBSTRING(BPO.BPO,6,1) = 'P' THEN BPO.BPO ELSE PRH.[Order No_] END
																	ELSE PRH.[Order No_]
																END AS NO_PO
															,CASE WHEN PIH.[Currency Code] = '' THEN 'IDR' ELSE PIH.[Currency Code] END AS CURR_CODE
															,PIL.AMOUNT
													FROM [dbo].[BC_SOLUSI$Purch_ Inv_ Header] PIH
													LEFT JOIN	(
																	SELECT PCL.[Document No_]
																			,PCL.[Receipt No_]
																			,SUM(PCL.[Amount]) AS AMOUNT
																	FROM [dbo].[BC_SOLUSI$Purch_ Inv_ Line] PCL
																	WHERE [Amount] <> 0
																	GROUP BY [Document No_], [Receipt No_]
																) PIL ON PIL.[Document No_] =  PIH.[No_]
													LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
													LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Rcpt_ Header] PRH ON PRH.[No_] = PIL.[Receipt No_]
													LEFT JOIN	(
																	SELECT PH.[No_], MAX(PD.[Blanket Order No_]) AS BPO
																	FROM [dbo].[BC_SOLUSI$Purchase Header] PH
																	INNER JOIN [dbo].[BC_SOLUSI$Purchase Line] PD ON PD.[Document No_] = PH.[No_]
																	WHERE PH.[Document Type] = 1
																	GROUP BY PH.[No_]
																) BPO ON BPO.[No_] = PRH.[Order No_]
													WHERE CMH.[No_] IS NULL
												) X
										GROUP BY X.NO_INVOICE
												,X.NO_INVIOCE_VENDOR
												,X.TGL_INVOICE
												,X.NO_PO
												,X.CURR_CODE
									) XX
							LEFT JOIN	(
											SELECT
													   VLE.[Document No_] AS NO_INV
													  ,VLE.[External Document No_] AS NO_PO_CUST
													  ,VLD.RMN_AMOUNT
													  ,VLD2.TGL_LUNAS
													  ,VLD2.NO_PELUNASAN
											FROM [dbo].[BC_SOLUSI$Vendor Ledger Entry] VLE
											LEFT JOIN (
														SELECT	VLED.[Vendor Ledger Entry No_]
																,SUM(VLED.[Amount]) AS RMN_AMOUNT
														FROM [dbo].[BC_SOLUSI$Detailed Vendor Ledg_ Entry] VLED
														GROUP BY VLED.[Vendor Ledger Entry No_]
													  )VLD ON VLD.[Vendor Ledger Entry No_] = VLE.[Entry No_]
											LEFT JOIN (
														SELECT	VLED2.[Vendor Ledger Entry No_]
																,MAX(VLED2.[Posting Date]) AS TGL_LUNAS
																,MAX(VLED2.[Document No_]) AS NO_PELUNASAN
														FROM [dbo].[BC_SOLUSI$Detailed Vendor Ledg_ Entry] VLED2
														WHERE VLED2.[Document Type] <> 2
														GROUP BY VLED2.[Vendor Ledger Entry No_]
													  ) VLD2 ON VLD2.[Vendor Ledger Entry No_] = VLE.[Entry No_]
										) PLN ON PLN.NO_INV = XX.NO_INVOICE
						) PIH ON PIH.NO_PO = PH.[No_] AND PIH.PO_TYPE = PH.[Document Type]
			WHERE	((PH.[Document Type] = 1 AND SUBSTRING(PH.[No_],5,1) <> 'P')
					OR
					(PH.[Document Type] = 4 AND SUBSTRING(PH.[No_],6,1) = 'P'))
		) PIN
LEFT JOIN	(
				SELECT	VEN.[No_], MAX([Name]) AS [Name]
						FROM	(
									SELECT [No_], [Name]
									FROM [dbo].[BC_AUTOJAYA$Vendor]
									UNION ALL
									SELECT [No_], [Name]
									FROM [dbo].[BC_SOLUSI$Vendor]
								) VEN
						GROUP BY VEN.[No_]
			) VDR ON VDR.[No_] = PIN.VENDOR_NO
		) XXX
WHERE	(XXX.DIM_PROJECT = $P{KodeProject} OR $P{KodeProject} = 'ALL') AND
		(XXX.VENDOR_NO = $P{Vendor} OR $P{Vendor}='ALL') AND
		(XXX.TGL_PO >= $P{TglFrom} AND XXX.TGL_PO <= $P{TglUpto})]]>
	</queryString>
