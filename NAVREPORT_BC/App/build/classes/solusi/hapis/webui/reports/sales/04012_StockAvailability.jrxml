<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="04012_StockAvailability" language="groovy" pageWidth="1250" pageHeight="842" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="1210" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="2b0922b7-0bc8-4065-b3b5-01d4f7c69ab7">
	<property name="ireport.zoom" value="2.1435888100000673"/>
	<property name="ireport.x" value="849"/>
	<property name="ireport.y" value="144"/>
	<parameter name="Item" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ItemCategory" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ProdGroup" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="SUBREPORT_DIR" class="java.lang.String">
		<defaultValueExpression><![CDATA["\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="Company" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="JenisRpt" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT	STA.COMPANY
		, STA.ITEM_NO
		, STA.DESCRIPTIONS
		, STA.UOM
		, STA.ITEM_CAT
		, STA.ITEM_CAT_DESC
		, STA.PROD_GROUP
		, STA.LOC_CODE
		, SUM(SUM(STA.TOTAL)) OVER (PARTITION BY STA.COMPANY, STA.ITEM_NO) AS TOTAL_STOCK_ITEM
		, SUM(STA.STOCK_OH) AS STOCK_OH
		, SUM(STA.ON_ORDER) AS ON_ORDER
		, SUM(STA.ON_ORDER_OTP) AS ON_ORDER_OTP
		, SUM(STA.OTP_READY) AS OTP_READY
		, SUM(STA.TOTAL) AS TOTAL
		, SUM(STA.RSRV_OH) AS RSRV_OH
		, SUM(STA.RSRV_ON_ORDER) AS RSRV_ON_ORDER
		, SUM(STA.RSRV_ON_ORDER_OTP) AS RSRV_ON_ORDER_OTP
		, SUM(STA.RSRV_OTP_READY) AS RSRV_OTP_READY
		, SUM(STA.AVAILABLE_OH) AS AVAILABLE_OH
		, SUM(STA.STOCK_AVAILABLE) AS STOCK_AVAILABLE
		, MIN(STA.ITEM_SUB) AS ITEM_SUB
		, MAX(STA.BLOCKED) AS BLOCKED
FROM	(
SELECT	'AUTOJAYA' AS COMPANY
		, STKG.ITEM_NO
		, ITM.[Description] AS DESCRIPTIONS
		, ITM.[Base Unit of Measure] AS UOM
		, ITM.[Item Category Code] AS ITEM_CAT
		, ITC.[Description] AS ITEM_CAT_DESC
		, ITM.[Product Group Code] AS PROD_GROUP
		, CASE WHEN (STKG.STOCK_OH+STKG.ON_ORDER+STKG.ON_ORDER_OTP+STKG.OTP_READY) = 0 THEN '' ELSE STKG.LOC_CODE END AS LOC_CODE
		, STKG.STOCK_OH
		, STKG.ON_ORDER
		, STKG.ON_ORDER_OTP
		, STKG.OTP_READY
		, (STKG.STOCK_OH+STKG.ON_ORDER+STKG.ON_ORDER_OTP+STKG.OTP_READY) AS TOTAL
		, STKG.RSRV_OH
		, STKG.RSRV_ON_ORDER
		, STKG.RSRV_ON_ORDER_OTP
		, STKG.RSRV_OTP_READY
		, CASE WHEN STKG.LOC_CODE IN  ('HO-ALK01','HO-ALK02','CKR-SC','CKR-TD','DPS-SC','DPS-TD','HO-SC','HO-TD','SBY-SC','SBY-TD','SMR-SC','SMR-TD','') THEN
			(STKG.STOCK_OH - STKG.RSRV_OH)
		  ELSE
			0
		  END AS AVAILABLE_OH
		, CASE WHEN STKG.LOC_CODE IN  ('HO-ALK01','HO-ALK02','CKR-SC','CKR-TD','DPS-SC','DPS-TD','HO-SC','HO-TD','SBY-SC','SBY-TD','SMR-SC','SMR-TD','') THEN
			((STKG.STOCK_OH+STKG.ON_ORDER+STKG.ON_ORDER_OTP+STKG.OTP_READY)-(STKG.RSRV_OH+STKG.RSRV_ON_ORDER+STKG.RSRV_ON_ORDER_OTP+STKG.RSRV_OTP_READY))
		  ELSE 0
		  END AS STOCK_AVAILABLE
		, ISNULL(SUB.ITEM_SUB, 'N') AS ITEM_SUB
		, ITM.[Blocked] AS BLOCKED
FROM	(
			SELECT	ITE.[No_] AS ITEM_NO
					,'HO-TD' AS LOC_CODE
					, 0 AS STOCK_OH
					, 0 AS ON_ORDER
					, 0 AS ON_ORDER_OTP
					, 0 AS OTP_READY
					, 0 AS RSRV_OH
					, 0 AS RSRV_ON_ORDER
					, 0 AS RSRV_ON_ORDER_OTP
					, 0 AS RSRV_OTP_READY
			FROM [dbo].[BC_AUTOJAYA$Item] ITE
			WHERE	ITE.[Inventory Value Zero] = 0
					AND (ITE.[No_] like '%'+$P{Item}+'%' OR $P{Item} = 'ALL')
			UNION ALL
			SELECT STK.ITEM_NO
					, STK.LOC_CODE
					, SUM(STK.STOCK_OH) AS STOCK_OH
					, SUM(STK.ON_ORDER) AS ON_ORDER
					, SUM(STK.ON_ORDER_OTP) AS ON_ORDER_OTP
					, SUM(STK.OTP_READY) AS OTP_READY
					, SUM(STK.RSRV_OH) AS RSRV_OH
					, SUM(STK.RSRV_ON_ORDER) AS RSRV_ON_ORDER
					, SUM(STK.RSRV_ON_ORDER_OTP) AS RSRV_ON_ORDER_OTP
					, SUM(STK.RSRV_OTP_READY) AS RSRV_OTP_READY
			FROM (
					SELECT
							[Item No_] AS ITEM_NO
							,[Location Code] AS LOC_CODE
							,SUM([Quantity]) AS STOCK_OH
							, 0 AS ON_ORDER
							, 0 AS ON_ORDER_OTP
							, 0 AS OTP_READY
							, 0 AS RSRV_OH
							, 0 AS RSRV_ON_ORDER
							, 0 AS RSRV_ON_ORDER_OTP
							, 0 AS RSRV_OTP_READY
					FROM [dbo].[BC_AUTOJAYA$Item Ledger Entry]
					GROUP BY [Item No_],[Location Code]
					UNION ALL
					SELECT POD.[No_] AS ITEM_NO
							,POD.[Location Code] AS LOC_CODE
							, 0 AS STOCK_OH
							,SUM(POD.[Outstanding Qty_ (Base)]) AS ON_ORDER
							, 0 AS ON_ORDER_OTP
							, 0 AS OTP_READY
							, 0 AS RSRV_OH
							, 0 AS RSRV_ON_ORDER
							, 0 AS RSRV_ON_ORDER_OTP
							, 0 AS RSRV_OTP_READY
					FROM [dbo].[BC_AUTOJAYA$Purchase Header] POH
					INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Line] POD ON POD.[Document No_] = POH.[No_]
					WHERE POH.[Document Type] = 1 AND
						SUBSTRING( POH.[No_], 5, 1) <> 'P' AND
						POD.[Type] = 2
					GROUP BY POD.[No_], POD.[Location Code]
					UNION ALL
					SELECT	BPO.ITEM_NO
							,BPO.LOC_CODE
							, 0 AS STOCK_OH
							, 0 AS ON_ORDER
							, SUM(BPO.ON_ORDER) AS ON_ORDER_OTP
							, 0 AS OTP_READY
							, 0 AS RSRV_OH
							, 0 AS RSRV_ON_ORDER
							, 0 AS RSRV_ON_ORDER_OTP
							, 0 AS RSRV_OTP_READY
					FROM	(
								SELECT	POD.[No_] AS ITEM_NO
										,POD.[Location Code] AS LOC_CODE
										,POD.[Quantity (Base)] - ISNULL(POO.QTY_PO,0) AS ON_ORDER
								FROM [dbo].[BC_AUTOJAYA$Purchase Header] POH
								INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Line] POD ON POD.[Document No_] = POH.[No_]
								LEFT JOIN	(
												SELECT	PD.[Blanket Order No_] AS NO_BPO
													,PD.[Blanket Order Line No_] AS NO_BPO_LINE
													,SUM(PD.[Quantity (Base)]) AS QTY_PO
												FROM [dbo].[BC_AUTOJAYA$Purchase Header] PH
												INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Line] PD ON PD.[Document No_] = PH.[No_]
												WHERE PH.[Document Type] = 1 AND
													  PD.[Blanket Order No_] <> ''
												GROUP BY PD.[Blanket Order No_], PD.[Blanket Order Line No_]
											) POO ON POO.NO_BPO = POD.[Document No_] AND POO.NO_BPO_LINE = POD.[Line No_]
								WHERE POH.[Document Type] = 4 AND
											SUBSTRING( POH.[No_], 6, 1) = 'P' AND
											POD.[Type] = 2
							) BPO
					GROUP BY BPO.ITEM_NO, BPO.LOC_CODE
					UNION ALL
					SELECT	OTP.ITEM_NO
							,OTP.LOC_CODE
							, 0 AS STOCK_OH
							, 0 AS ON_ORDER
							, SUM(OTP.ON_ORDER_OTP) AS ON_ORDER_OTP
							, SUM(OTP.OTP_READY) AS OTP_READY
							, 0 AS RSRV_OH
							, 0 AS RSRV_ON_ORDER
							, 0 AS RSRV_ON_ORDER_OTP
							, 0 AS RSRV_OTP_READY
					FROM	(
								SELECT POD.[No_] AS ITEM_NO
										,POD.[Location Code] AS LOC_CODE
										, CASE WHEN ISNULL(POD.[Expected Receipt Date], GETDATE()) > GETDATE() THEN POD.[Outstanding Qty_ (Base)] ELSE 0 END AS ON_ORDER_OTP
										, CASE WHEN ISNULL(POD.[Expected Receipt Date], GETDATE()) <= GETDATE() THEN POD.[Outstanding Qty_ (Base)] ELSE 0 END AS OTP_READY
								FROM [dbo].[BC_AUTOJAYA$Purchase Header] POH
								INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Line] POD ON POD.[Document No_] = POH.[No_]
								WHERE POH.[Document Type] = 1 AND
									SUBSTRING( POH.[No_], 5, 1) = 'P' AND
									POD.[Type] = 2
							) OTP
					GROUP BY OTP.ITEM_NO, OTP.LOC_CODE
					UNION ALL
					SELECT	RSV.ITEM_NO
							, RSV.LOC_CODE
							, 0 AS STOCK_OH
							, 0 AS ON_ORDER
							, 0 AS ON_ORDER_OTP
							, 0 AS OTP_READY
							, SUM(RSV.RSRV_OH) AS RSRV_OH
							, SUM(RSV.RSRV_ON_ORDER) AS RSRV_ON_ORDER
							, SUM(RSV.RSRV_ON_ORDER_OTP) AS RSRV_ON_ORDER_OTP
							, SUM(RSV.RSRV_OTP_READY) AS RSRV_OTP_READY
					FROM	(
								SELECT  RES.[Item No_] AS ITEM_NO
										, RES.[Location Code] AS LOC_CODE
										, CASE WHEN [Source ID] = '' THEN RES.[Quantity (Base)] ELSE 0 END AS RSRV_OH
										, CASE WHEN SUBSTRING(RES.[Source ID], 5, 1) <> 'P' AND RES.[Source ID] <> '' THEN RES.[Quantity (Base)] ELSE 0 END AS RSRV_ON_ORDER
										, CASE WHEN SUBSTRING(RES.[Source ID], 5, 1) = 'P' AND ISNULL(PUR.EXP_RCP_DATE,GETDATE()) <= GETDATE() THEN  RES.[Quantity (Base)] ELSE 0 END AS RSRV_OTP_READY
										, CASE WHEN SUBSTRING(RES.[Source ID], 5, 1) = 'P' AND ISNULL(PUR.EXP_RCP_DATE,GETDATE()) > GETDATE() THEN  RES.[Quantity (Base)] ELSE 0 END AS RSRV_ON_ORDER_OTP
								FROM [dbo].[BC_AUTOJAYA$Reservation Entry] RES
								LEFT JOIN	(
												SELECT MAX([Expected Receipt Date]) AS EXP_RCP_DATE,  [Document No_], [Line No_]
												FROM [dbo].[BC_AUTOJAYA$Purchase Line]
												GROUP BY [Document No_], [Line No_]
											) PUR ON PUR.[Document No_] = RES.[Source ID] AND PUR.[Line No_] = RES.[Source Ref_ No_]
								WHERE RES.[Reservation Status] = 0 AND
									  RES.[Positive] = 1 AND
									  SUBSTRING(RES.[Source ID], 1, 2) <> 'TO'
							) RSV
					GROUP BY RSV.ITEM_NO, RSV.LOC_CODE
					UNION ALL
					SELECT	QTO.ITEM_NO
							,QTO.LOC_CODE
							, 0 AS STOCK_OH
							, 0 AS ON_ORDER
							, 0 AS ON_ORDER_OTP
							, 0 AS OTP_READY
							, 0 AS RSRV_OH
							, 0 AS RSRV_ON_ORDER
							--Reservasi terhadap TO dianggap belum reservasi
							--, CASE WHEN QTO2.ITEM_NO IS NULL THEN QTO.QTY_TO ELSE 0 END AS RSRV_ON_ORDER
							, 0 AS RSRV_ON_ORDER_OTP
							, 0 RSRV_OTP_READY
					FROM	(
								SELECT  RE.[Item No_] AS ITEM_NO
										, TH.[Transfer-from Code] AS LOC_CODE
										, RE.[Source ID] AS NO_TO
										, SUM(RE.[Quantity (Base)]) QTY_TO
								FROM [dbo].[BC_AUTOJAYA$Reservation Entry] RE
								LEFT JOIN [dbo].[BC_AUTOJAYA$Transfer Header] TH ON TH.[No_] = RE.[Source ID]
								WHERE RE.[Reservation Status] = 0 AND
									  RE.[Positive] = 1 AND
									  SUBSTRING(RE.[Source ID], 1, 2) = 'TO'
								GROUP BY RE.[Item No_], TH.[Transfer-from Code], RE.[Source ID]
							) QTO
					LEFT JOIN (
									SELECT	TRO.[Item No_] AS ITEM_NO
											,TRO.[Location Code] AS LOC_CODE
											,TRO2.[Source ID] AS NO_TO
											,SUM(TRO.[Quantity (Base)]) AS QTY_TO
									FROM [dbo].[BC_AUTOJAYA$Reservation Entry] TRO
									LEFT JOIN [dbo].[BC_AUTOJAYA$Reservation Entry] TRO2 ON TRO2.[Entry No_] = TRO.[Entry No_] AND TRO2.[Positive] = 0
									WHERE TRO.[Reservation Status] = 0 AND
										  TRO.[Positive] = 1 AND
										  SUBSTRING(TRO2.[Source ID], 1, 2) = 'TO'
									GROUP BY TRO.[Item No_], TRO.[Location Code], TRO2.[Source ID]
								) QTO2 ON	QTO2.ITEM_NO = QTO.ITEM_NO AND
											QTO2.LOC_CODE = QTO.LOC_CODE AND
											QTO2.NO_TO = QTO.NO_TO AND
											QTO2.QTY_TO = QTO.QTY_TO
				 ) STK
			GROUP BY STK.ITEM_NO, STK.LOC_CODE
		)STKG
LEFT JOIN [dbo].[BC_AUTOJAYA$Item] ITM ON ITM.[No_] = STKG.ITEM_NO
LEFT JOIN [dbo].[BC_AUTOJAYA$Item Category] ITC ON ITC.[Code] = ITM.[Item Category Code]
LEFT JOIN	(
				SELECT	IST.[No_] AS ITEM_NO
						, CASE WHEN SUM(CASE WHEN ISNULL(STO.STOCK_OH, 0) > 0 THEN 1 ELSE 0 END) > 0 THEN 'Y' ELSE 'N' END AS ITEM_SUB
				FROM [dbo].[BC_AUTOJAYA$Item Substitution] IST
				LEFT JOIN	(
								SELECT STK.ITEM_NO
										, SUM(STK.STOCK_OH) AS STOCK_OH
										, SUM(STK.RSRV_OH) AS RSRV_OH
								FROM (
										SELECT
												[Item No_] AS ITEM_NO
												,[Location Code] AS LOC_CODE
												,SUM([Quantity]) AS STOCK_OH
												, 0 AS RSRV_OH
										FROM [dbo].[BC_AUTOJAYA$Item Ledger Entry]
										GROUP BY [Item No_],[Location Code]
										UNION ALL
										SELECT	RSV.ITEM_NO
												, RSV.LOC_CODE
												, 0 AS STOCK_OH
												, SUM(RSV.RSRV_OH) AS RSRV_OH
										FROM	(
													SELECT  RES.[Item No_] AS ITEM_NO
															, RES.[Location Code] AS LOC_CODE
															, CASE WHEN [Source ID] = '' THEN RES.[Quantity (Base)] ELSE 0 END AS RSRV_OH
													FROM [dbo].[BC_AUTOJAYA$Reservation Entry] RES
													WHERE RES.[Reservation Status] = 0 AND
															RES.[Positive] = 1 AND
															SUBSTRING(RES.[Source ID], 1, 2) <> 'TO'
												) RSV
										GROUP BY RSV.ITEM_NO, RSV.LOC_CODE
										) STK
								GROUP BY STK.ITEM_NO
							) STO ON STO.ITEM_NO = IST.[Substitute No_]
				GROUP BY IST.[No_]
			) SUB ON SUB.ITEM_NO = STKG.ITEM_NO
WHERE ITM.[Inventory Value Zero] = 0
	AND (STKG.ITEM_NO like '%'+$P{Item}+'%' OR $P{Item} = 'ALL')
	AND (ITM.[Item Category Code] = $P{ItemCategory} OR $P{ItemCategory} = 'ALL')
	AND (ITM.[Product Group Code] = $P{ProdGroup} OR $P{ProdGroup} = 'ALL')
	AND ($P{Company} = 'AUTOJAYA' OR $P{Company} = 'ALL')
UNION ALL
SELECT	'SOLUSI' AS COMPANY
		, STKG.ITEM_NO
		, ITM.[Description] AS DESCRIPTIONS
		, ITM.[Base Unit of Measure] AS UOM
		, ITM.[Item Category Code] AS ITEM_CAT
		, ITC.[Description] AS ITEM_CAT_DESC
		, ITM.[Product Group Code] AS PROD_GROUP
		, CASE WHEN (STKG.STOCK_OH+STKG.ON_ORDER+STKG.ON_ORDER_OTP+STKG.OTP_READY) = 0 THEN '' ELSE STKG.LOC_CODE END AS LOC_CODE
		, STKG.STOCK_OH
		, STKG.ON_ORDER
		, STKG.ON_ORDER_OTP
		, STKG.OTP_READY
		, (STKG.STOCK_OH+STKG.ON_ORDER+STKG.ON_ORDER_OTP+STKG.OTP_READY) AS TOTAL
		, STKG.RSRV_OH
		, STKG.RSRV_ON_ORDER
		, STKG.RSRV_ON_ORDER_OTP
		, STKG.RSRV_OTP_READY
		, CASE WHEN STKG.LOC_CODE IN  ('HO-ALK01','HO-ALK02','CKR-SC','CKR-TD','DPS-SC','DPS-TD','HO-SC','HO-TD','SBY-SC','SBY-TD','SMR-SC','SMR-TD','') THEN
			(STKG.STOCK_OH - STKG.RSRV_OH)
		  ELSE
			0
		  END AS AVAILABLE_OH
		, CASE WHEN STKG.LOC_CODE IN  ('HO-ALK01','HO-ALK02','CKR-SC','CKR-TD','DPS-SC','DPS-TD','HO-SC','HO-TD','SBY-SC','SBY-TD','SMR-SC','SMR-TD','') THEN
			((STKG.STOCK_OH+STKG.ON_ORDER+STKG.ON_ORDER_OTP+STKG.OTP_READY)-(STKG.RSRV_OH+STKG.RSRV_ON_ORDER+STKG.RSRV_ON_ORDER_OTP+STKG.RSRV_OTP_READY))
		  ELSE 0
		  END AS STOCK_AVAILABLE
		, ISNULL(SUB.ITEM_SUB, 'N') AS ITEM_SUB
		, ITM.[Blocked] AS BLOCKED
FROM	(
			SELECT STK.ITEM_NO
					, STK.LOC_CODE
					, SUM(STK.STOCK_OH) AS STOCK_OH
					, SUM(STK.ON_ORDER) AS ON_ORDER
					, SUM(STK.ON_ORDER_OTP) AS ON_ORDER_OTP
					, SUM(STK.OTP_READY) AS OTP_READY
					, SUM(STK.RSRV_OH) AS RSRV_OH
					, SUM(STK.RSRV_ON_ORDER) AS RSRV_ON_ORDER
					, SUM(STK.RSRV_ON_ORDER_OTP) AS RSRV_ON_ORDER_OTP
					, SUM(STK.RSRV_OTP_READY) AS RSRV_OTP_READY
			FROM (
					SELECT	ITE.[No_] AS ITEM_NO
							,'HO-TD' AS LOC_CODE
							, 0 AS STOCK_OH
							, 0 AS ON_ORDER
							, 0 AS ON_ORDER_OTP
							, 0 AS OTP_READY
							, 0 AS RSRV_OH
							, 0 AS RSRV_ON_ORDER
							, 0 AS RSRV_ON_ORDER_OTP
							, 0 AS RSRV_OTP_READY
					FROM [dbo].[BC_SOLUSI$Item] ITE
					WHERE	ITE.[Inventory Value Zero] = 0
							AND (ITE.[No_] like '%'+$P{Item}+'%' OR $P{Item} = 'ALL')
					UNION ALL
					SELECT
							[Item No_] AS ITEM_NO
							,[Location Code] AS LOC_CODE
							,SUM([Quantity]) AS STOCK_OH
							, 0 AS ON_ORDER
							, 0 AS ON_ORDER_OTP
							, 0 AS OTP_READY
							, 0 AS RSRV_OH
							, 0 AS RSRV_ON_ORDER
							, 0 AS RSRV_ON_ORDER_OTP
							, 0 AS RSRV_OTP_READY
					FROM [dbo].[BC_SOLUSI$Item Ledger Entry]
					GROUP BY [Item No_],[Location Code]
					UNION ALL
					SELECT POD.[No_] AS ITEM_NO
							,POD.[Location Code] AS LOC_CODE
							, 0 AS STOCK_OH
							,SUM(POD.[Outstanding Qty_ (Base)]) AS ON_ORDER
							, 0 AS ON_ORDER_OTP
							, 0 AS OTP_READY
							, 0 AS RSRV_OH
							, 0 AS RSRV_ON_ORDER
							, 0 AS RSRV_ON_ORDER_OTP
							, 0 AS RSRV_OTP_READY
					FROM [dbo].[BC_SOLUSI$Purchase Header] POH
					INNER JOIN [dbo].[BC_SOLUSI$Purchase Line] POD ON POD.[Document No_] = POH.[No_]
					WHERE POH.[Document Type] = 1 AND
						SUBSTRING( POH.[No_], 5, 1) <> 'P' AND
						POD.[Type] = 2
					GROUP BY POD.[No_], POD.[Location Code]
					UNION ALL
					SELECT	BPO.ITEM_NO
							,BPO.LOC_CODE
							, 0 AS STOCK_OH
							, 0 AS ON_ORDER
							, SUM(BPO.ON_ORDER) AS ON_ORDER_OTP
							, 0 AS OTP_READY
							, 0 AS RSRV_OH
							, 0 AS RSRV_ON_ORDER
							, 0 AS RSRV_ON_ORDER_OTP
							, 0 AS RSRV_OTP_READY
					FROM	(
								SELECT	POD.[No_] AS ITEM_NO
										,POD.[Location Code] AS LOC_CODE
										,POD.[Quantity (Base)] - ISNULL(POO.QTY_PO,0) AS ON_ORDER
								FROM [dbo].[BC_SOLUSI$Purchase Header] POH
								INNER JOIN [dbo].[BC_SOLUSI$Purchase Line] POD ON POD.[Document No_] = POH.[No_]
								LEFT JOIN	(
												SELECT	PD.[Blanket Order No_] AS NO_BPO
													,PD.[Blanket Order Line No_] AS NO_BPO_LINE
													,SUM(PD.[Quantity (Base)]) AS QTY_PO
												FROM [dbo].[BC_SOLUSI$Purchase Header] PH
												INNER JOIN [dbo].[BC_SOLUSI$Purchase Line] PD ON PD.[Document No_] = PH.[No_]
												WHERE PH.[Document Type] = 1 AND
													  PD.[Blanket Order No_] <> ''
												GROUP BY PD.[Blanket Order No_], PD.[Blanket Order Line No_]
											) POO ON POO.NO_BPO = POD.[Document No_] AND POO.NO_BPO_LINE = POD.[Line No_]
								WHERE POH.[Document Type] = 4 AND
											SUBSTRING( POH.[No_], 6, 1) = 'P' AND
											POD.[Type] = 2
							) BPO
					GROUP BY BPO.ITEM_NO, BPO.LOC_CODE
					UNION ALL
					SELECT	OTP.ITEM_NO
							,OTP.LOC_CODE
							, 0 AS STOCK_OH
							, 0 AS ON_ORDER
							, SUM(OTP.ON_ORDER_OTP) AS ON_ORDER_OTP
							, SUM(OTP.OTP_READY) AS OTP_READY
							, 0 AS RSRV_OH
							, 0 AS RSRV_ON_ORDER
							, 0 AS RSRV_ON_ORDER_OTP
							, 0 AS RSRV_OTP_READY
					FROM	(
								SELECT POD.[No_] AS ITEM_NO
										,POD.[Location Code] AS LOC_CODE
										, CASE WHEN ISNULL(POD.[Expected Receipt Date], GETDATE()) > GETDATE() THEN POD.[Outstanding Qty_ (Base)] ELSE 0 END AS ON_ORDER_OTP
										, CASE WHEN ISNULL(POD.[Expected Receipt Date], GETDATE()) <= GETDATE() THEN POD.[Outstanding Qty_ (Base)] ELSE 0 END AS OTP_READY
								FROM [dbo].[BC_SOLUSI$Purchase Header] POH
								INNER JOIN [dbo].[BC_SOLUSI$Purchase Line] POD ON POD.[Document No_] = POH.[No_]
								WHERE POH.[Document Type] = 1 AND
									SUBSTRING( POH.[No_], 5, 1) = 'P' AND
									POD.[Type] = 2
							) OTP
					GROUP BY OTP.ITEM_NO, OTP.LOC_CODE
					UNION ALL
					SELECT	RSV.ITEM_NO
							, RSV.LOC_CODE
							, 0 AS STOCK_OH
							, 0 AS ON_ORDER
							, 0 AS ON_ORDER_OTP
							, 0 AS OTP_READY
							, SUM(RSV.RSRV_OH) AS RSRV_OH
							, SUM(RSV.RSRV_ON_ORDER) AS RSRV_ON_ORDER
							, SUM(RSV.RSRV_ON_ORDER_OTP) AS RSRV_ON_ORDER_OTP
							, SUM(RSV.RSRV_OTP_READY) AS RSRV_OTP_READY
					FROM	(
								SELECT  RES.[Item No_] AS ITEM_NO
										, RES.[Location Code] AS LOC_CODE
										, CASE WHEN [Source ID] = '' THEN RES.[Quantity (Base)] ELSE 0 END AS RSRV_OH
										, CASE WHEN SUBSTRING(RES.[Source ID], 5, 1) <> 'P' AND RES.[Source ID] <> '' THEN RES.[Quantity (Base)] ELSE 0 END AS RSRV_ON_ORDER
										, CASE WHEN SUBSTRING(RES.[Source ID], 5, 1) = 'P' AND ISNULL(PUR.EXP_RCP_DATE,GETDATE()) <= GETDATE() THEN  RES.[Quantity (Base)] ELSE 0 END AS RSRV_OTP_READY
										, CASE WHEN SUBSTRING(RES.[Source ID], 5, 1) = 'P' AND ISNULL(PUR.EXP_RCP_DATE,GETDATE()) > GETDATE() THEN  RES.[Quantity (Base)] ELSE 0 END AS RSRV_ON_ORDER_OTP
								FROM [dbo].[BC_SOLUSI$Reservation Entry] RES
								LEFT JOIN	(
												SELECT MAX([Expected Receipt Date]) AS EXP_RCP_DATE,  [Document No_], [Line No_]
												FROM [dbo].[BC_SOLUSI$Purchase Line]
												GROUP BY [Document No_], [Line No_]
											) PUR ON PUR.[Document No_] = RES.[Source ID] AND PUR.[Line No_] = RES.[Source Ref_ No_]
								WHERE RES.[Reservation Status] = 0 AND
									  RES.[Positive] = 1 AND
									  SUBSTRING(RES.[Source ID], 1, 2) <> 'TO'
							) RSV
					GROUP BY RSV.ITEM_NO, RSV.LOC_CODE
					UNION ALL
					SELECT	QTO.ITEM_NO
							,QTO.LOC_CODE
							, 0 AS STOCK_OH
							, 0 AS ON_ORDER
							, 0 AS ON_ORDER_OTP
							, 0 AS OTP_READY
							, 0 AS RSRV_OH
							, 0 AS RSRV_ON_ORDER
							--Reservasi terhadap TO dianggap belum reservasi
							--, CASE WHEN QTO2.ITEM_NO IS NULL THEN QTO.QTY_TO ELSE 0 END AS RSRV_ON_ORDER
							, 0 AS RSRV_ON_ORDER_OTP
							, 0 RSRV_OTP_READY
					FROM	(
								SELECT  RE.[Item No_] AS ITEM_NO
										, TH.[Transfer-from Code] AS LOC_CODE
										, RE.[Source ID] AS NO_TO
										, SUM(RE.[Quantity (Base)]) QTY_TO
								FROM [dbo].[BC_SOLUSI$Reservation Entry] RE
								LEFT JOIN [dbo].[BC_SOLUSI$Transfer Header] TH ON TH.[No_] = RE.[Source ID]
								WHERE RE.[Reservation Status] = 0 AND
									  RE.[Positive] = 1 AND
									  SUBSTRING(RE.[Source ID], 1, 2) = 'TO'
								GROUP BY RE.[Item No_], TH.[Transfer-from Code], RE.[Source ID]
							) QTO
					LEFT JOIN (
									SELECT	TRO.[Item No_] AS ITEM_NO
											,TRO.[Location Code] AS LOC_CODE
											,TRO2.[Source ID] AS NO_TO
											,SUM(TRO.[Quantity (Base)]) AS QTY_TO
									FROM [dbo].[BC_SOLUSI$Reservation Entry] TRO
									LEFT JOIN [dbo].[BC_SOLUSI$Reservation Entry] TRO2 ON TRO2.[Entry No_] = TRO.[Entry No_] AND TRO2.[Positive] = 0
									WHERE TRO.[Reservation Status] = 0 AND
										  TRO.[Positive] = 1 AND
										  SUBSTRING(TRO2.[Source ID], 1, 2) = 'TO'
									GROUP BY TRO.[Item No_], TRO.[Location Code], TRO2.[Source ID]
								) QTO2 ON	QTO2.ITEM_NO = QTO.ITEM_NO AND
											QTO2.LOC_CODE = QTO.LOC_CODE AND
											QTO2.NO_TO = QTO.NO_TO AND
											QTO2.QTY_TO = QTO.QTY_TO
				 ) STK
			GROUP BY STK.ITEM_NO, STK.LOC_CODE
		)STKG
LEFT JOIN [dbo].[BC_SOLUSI$Item] ITM ON ITM.[No_] = STKG.ITEM_NO
LEFT JOIN [dbo].[BC_AUTOJAYA$Item Category] ITC ON ITC.[Code] = ITM.[Item Category Code]
LEFT JOIN	(
				SELECT	IST.[No_] AS ITEM_NO
						, CASE WHEN SUM(CASE WHEN ISNULL(STO.STOCK_OH,0) > 0 THEN 1 ELSE 0 END) > 0 THEN 'Y' ELSE 'N' END AS ITEM_SUB
				FROM [dbo].[BC_SOLUSI$Item Substitution] IST
				LEFT JOIN	(
								SELECT STK.ITEM_NO
										, SUM(STK.STOCK_OH) AS STOCK_OH
										, SUM(STK.RSRV_OH) AS RSRV_OH
								FROM (
										SELECT
												[Item No_] AS ITEM_NO
												,[Location Code] AS LOC_CODE
												,SUM([Quantity]) AS STOCK_OH
												, 0 AS RSRV_OH
										FROM [dbo].[BC_SOLUSI$Item Ledger Entry]
										GROUP BY [Item No_],[Location Code]
										UNION ALL
										SELECT	RSV.ITEM_NO
												, RSV.LOC_CODE
												, 0 AS STOCK_OH
												, SUM(RSV.RSRV_OH) AS RSRV_OH
										FROM	(
													SELECT  RES.[Item No_] AS ITEM_NO
															, RES.[Location Code] AS LOC_CODE
															, CASE WHEN [Source ID] = '' THEN RES.[Quantity (Base)] ELSE 0 END AS RSRV_OH
													FROM [dbo].[BC_SOLUSI$Reservation Entry] RES
													WHERE RES.[Reservation Status] = 0 AND
															RES.[Positive] = 1 AND
															SUBSTRING(RES.[Source ID], 1, 2) <> 'TO'
												) RSV
										GROUP BY RSV.ITEM_NO, RSV.LOC_CODE
										) STK
								GROUP BY STK.ITEM_NO
							) STO ON STO.ITEM_NO = IST.[Substitute No_]
				GROUP BY IST.[No_]
			) SUB ON SUB.ITEM_NO = STKG.ITEM_NO
WHERE
	ITM.[Inventory Value Zero] = 0
	AND (STKG.ITEM_NO like '%'+$P{Item}+'%' OR $P{Item} = 'ALL')
	AND (ITM.[Item Category Code] = $P{ItemCategory} OR $P{ItemCategory} = 'ALL')
	AND (ITM.[Product Group Code] = $P{ProdGroup} OR $P{ProdGroup} = 'ALL')
	AND ($P{Company} = 'SOLUSI' OR $P{Company} = 'ALL')
		) STA
GROUP BY	STA.COMPANY
			, STA.ITEM_NO
			, STA.DESCRIPTIONS
			, STA.UOM
			, STA.ITEM_CAT
			, STA.ITEM_CAT_DESC
			, STA.PROD_GROUP
			, STA.LOC_CODE]]>
	</queryString>
