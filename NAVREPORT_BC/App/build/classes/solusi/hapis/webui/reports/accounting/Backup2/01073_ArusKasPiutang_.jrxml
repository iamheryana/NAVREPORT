	<parameter name="TglFrom" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="TglUpto" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Company" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT	XXX.GROUP_MAIN
		, XXX.GROUPS
		,	CASE	WHEN XXX.GROUPS = 'AA' THEN 'Piutang Sesuai Rumus'
					WHEN XXX.GROUPS = 'BB' THEN 'Penerimaan  Kas & Bank'
					WHEN XXX.GROUPS = 'CC' THEN 'Offset Kelebihan Unpplied Payment'
					WHEN XXX.GROUPS = 'DD' THEN 'Selisih Biaya Bank / Lainnya'
					WHEN XXX.GROUPS = 'EE' THEN 'Intercoy'
					WHEN XXX.GROUPS = 'FF' THEN 'PPh Dipotong Pihak Lain / PPn tidak dipungut'
					ELSE ''
			END AS GROUPS_DESC
		, XXX.SUB_GROUP
		, XXX.SUB_GROUP_DESC
		, XXX.SUB_DTL
		, XXX.AMOUNT_TOTAL
		, XXX.AMT_USAHA
		, XXX.AMT_USAHA_HIT
		, XXX.AMT_USAHA_AR
		, XXX.AMT_USAHA_UP
		, XXX.AMT_NON_USAHA
FROM	(
SELECT	'X1' AS GROUP_MAIN
		,'AA' AS GROUPS
		, 1 AS SUB_GROUP
		,'Saldo Awal - Piutang Dagang' AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, SUM([Amount]) AS AMT_USAHA
		, SUM([Amount]) AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM [dbo].[AUTOJAYA$G_L Entry]
WHERE	[G_L Account No_] IN ('114.110.00', '114.140.00')
		AND [Posting Date] < $P{TglFrom}
UNION ALL
SELECT	'X1' AS GROUP_MAIN
		,'AA' AS GROUPS
		, 2 AS SUB_GROUP
		,'Sales' AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, SUM([Amount]) * -1  AS AMT_USAHA
		, SUM([Amount]) * -1  AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM [dbo].[AUTOJAYA$G_L Entry]
WHERE	([G_L Account No_] BETWEEN '400.110.00' AND'420.110.01')
		AND ([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
UNION ALL
SELECT	'X1' AS GROUP_MAIN
		,'AA' AS GROUPS
		, 3 AS SUB_GROUP
		,'PPN Keluaran' AS SUB_GROUP_DESC
		,CASE WHEN X.SUB_DTL = 'A' THEN 'A. PPN Keluaran 10 %' ELSE 'B. PPN Keluaran WAPU / KWS. BERIKAT / BENDAHARA' END AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, FLOOR(CASE WHEN X.SUB_DTL = 'A' THEN SUM(SUM(X.AMOUNT)) OVER (PARTITION BY 1) * -1 ELSE SUM(X.AMOUNT) END) AS AMT_USAHA
		, FLOOR(CASE WHEN X.SUB_DTL = 'A' THEN SUM(SUM(X.AMOUNT)) OVER (PARTITION BY 1) * -1 ELSE SUM(X.AMOUNT) END) AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM	(
			SELECT 'A' AS SUB_DTL, 0 AS AMOUNT
			UNION ALL
			SELECT 'B' AS SUB_DTL, 0 AS AMOUNT
			UNION ALL
			SELECT	CASE WHEN PPN.PPN_ID = 'EFID0003' THEN 'A' ELSE 'B' END AS SUB_DTL
					, GLE.[Amount] AS AMOUNT
			FROM [dbo].[AUTOJAYA$G_L Entry] GLE
			LEFT JOIN	(
							SELECT [No_]
									, [E-Faktur Register Mapping ID] AS PPN_ID
							FROM [dbo].[AUTOJAYA$E-Faktur Register Header - n]
							WHERE [Document Type] = 5
							UNION ALL
							SELECT	SCM.[No_]
									, EFK.[E-Faktur Register Mapping ID] AS PPN_ID
							FROM [dbo].[AUTOJAYA$Sales Cr_Memo Header] SCM
							LEFT JOIN  [dbo].[AUTOJAYA$E-Faktur Register Header - n] EFK ON EFK.[No_] = SCM.[Applies-to Doc_ No_] AND  EFK.[Document Type] = 5
						) PPN ON PPN.[No_] = GLE.[Document No_]
			WHERE (GLE.[G_L Account No_]='230.190.00')
					AND (GLE.[Posting Date] >= $P{TglFrom} AND GLE.[Posting Date] <= $P{TglUpto})
					AND GLE.[Source Code] = 'SALES'
		) X
GROUP BY X.SUB_DTL
UNION ALL
SELECT	'X1' AS GROUP_MAIN
		,'AA' AS GROUPS
		, 4 AS SUB_GROUP
		,'Saldo Akhir - Piutang Dagang' AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, SUM([Amount]) * -1 AS AMT_USAHA
		, SUM([Amount]) * -1 AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM [dbo].[AUTOJAYA$G_L Entry]
WHERE [G_L Account No_] IN ('114.110.00', '114.140.00')
		AND [Posting Date] <= $P{TglUpto}
UNION ALL
SELECT	'X2' AS GROUP_MAIN
		,'BB' AS GROUPS
		, HDR.NO_URUT AS SUB_GROUP
		, HDR.KETERANGAN AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, HDR.AMOUNT_TOTAL AS AMOUNT_TOTAL
		, (DTL.AMT_USAHA_AR + DTL.AMT_USAHA_UP) AS AMT_USAHA
		, (DTL.AMT_USAHA_AR + DTL.AMT_USAHA_UP) * -1 AS AMT_USAHA_HIT
		, DTL.AMT_USAHA_AR AS AMT_USAHA_AR
		, DTL.AMT_USAHA_UP AS AMT_USAHA_UP
		, DTL.AMT_NON_USAHA
FROM	(
			SELECT	ROW_NUMBER() OVER (PARTITION BY 1 ORDER BY  GLE.[G_L Account No_], BAC.[No_]) AS NO_URUT
					,BAC.[No_] AS KODE_BANK
					,BAC.[Name] AS KETERANGAN
					,SUM(GLE.[Debit Amount]) AS AMOUNT_TOTAL
			FROM [dbo].[AUTOJAYA$G_L Entry] GLE
			LEFT JOIN [dbo].[AUTOJAYA$Bank Account] BAC ON BAC.[No_] = GLE.[Source No_]
			WHERE	(
						(GLE.[G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
						(GLE.[G_L Account No_] BETWEEN '112.110.00' AND '112.162.00')
					)
					AND (GLE.[Posting Date] >= $P{TglFrom} AND GLE.[Posting Date] <= $P{TglUpto})
					AND GLE.[Debit Amount] <> 0
			GROUP BY BAC.[Name], BAC.[No_], GLE.[G_L Account No_]
		) HDR
LEFT JOIN	(
				SELECT	XX.[Source No_]
						, SUM(CASE WHEN (XX.ADA_PIUTANG_DAGANG + XX.ADA_PIUTANG_DAGANG_INTERCOY) > 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_USAHA_AR
						, SUM(CASE WHEN (XX.ADA_UNAPPLIED_PAYMENT) > 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_USAHA_UP
						, SUM(CASE WHEN (XX.ADA_PIUTANG_DAGANG + XX.ADA_PIUTANG_DAGANG_INTERCOY + XX.ADA_UNAPPLIED_PAYMENT) = 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_NON_USAHA
				FROM	(
							SELECT	X.[Posting Date]
									, X.[Document No_]
									, X.[Transaction No_]
									, X.[G_L Account No_]
									, X.[Source No_]
									, X.JENIS_ACC
									, SUM(X.ADA_PIUTANG_DAGANG) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_PIUTANG_DAGANG
									, SUM(X.ADA_PIUTANG_DAGANG_INTERCOY) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_PIUTANG_DAGANG_INTERCOY
									, SUM(X.ADA_UNAPPLIED_PAYMENT) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_UNAPPLIED_PAYMENT
									, X.[Debit Amount]
							FROM	(
										SELECT	[Posting Date]
												, [Document No_]
												, [Transaction No_]
												, [G_L Account No_]
												, [Source No_]
												,	CASE WHEN	([G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
																([G_L Account No_] BETWEEN '112.110.00' AND '112.162.00') THEN
														'BANK'
													ELSE
														'NON' END AS JENIS_ACC
												, CASE WHEN [G_L Account No_] = '114.110.00' THEN 1 ELSE 0 END AS ADA_PIUTANG_DAGANG
												, CASE WHEN [G_L Account No_] = '114.140.00'  THEN 1 ELSE 0 END AS ADA_PIUTANG_DAGANG_INTERCOY
												, CASE WHEN [G_L Account No_] = '220.185.00' THEN 1 ELSE 0 END AS ADA_UNAPPLIED_PAYMENT
												, SUM([Debit Amount]) AS [Debit Amount]
												, SUM([Credit Amount]) AS [Credit Amount]
										FROM [dbo].[AUTOJAYA$G_L Entry]
										WHERE	([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
										GROUP BY [Posting Date]
												, [Document No_]
												, [Transaction No_]
												, [G_L Account No_]
												, [Source No_]
									) X
						) XX
				WHERE XX.JENIS_ACC =  'BANK'
				GROUP BY XX.[Source No_]
			) DTL ON DTL.[Source No_] = HDR.KODE_BANK
UNION ALL
SELECT	'X2' AS GROUP_MAIN
		,'CC' AS GROUPS
		, 1 AS SUB_GROUP
		,'Offset Kelebihan Unpplied Payment' AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, 0 AS AMT_USAHA
		, 0 * -1 AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
UNION ALL
SELECT	'X2' AS GROUP_MAIN
		,'DD' AS GROUPS
		, 1 AS SUB_GROUP
		,'Selisih Biaya Bank / Lainnya' AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, SUM(XXX.SELISIH) AS AMT_USAHA
		, SUM(XXX.SELISIH) * -1 AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM	(
			SELECT	XX.*
					,	SUM(XX.[Debit Amount]) OVER  (PARTITION BY XX.[Posting Date], XX.[Document No_] , XX.[Transaction No_], XX.GL_ACC)
						-
						SUM(XX.[Credit Amount]) OVER  (PARTITION BY XX.[Posting Date], XX.[Document No_] , XX.[Transaction No_], XX.GL_ACC)
						AS SELISIH
			FROM	(
						SELECT	X.[Posting Date]
								, X.[Document No_]
								, X.[Transaction No_]
								, X.[G_L Account No_]
								, X.GL_ACC
								, SUM(X.ADA_KASBANK) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_KASBANK
								, SUM(X.ADA_AR) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_AR
								, SUM(X.ADA_AR_IC) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_AR_IC
								, SUM(X.ADA_UN_PAY) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_UN_PAY
								, X.[Debit Amount]
								, X.[Credit Amount]
						FROM	(
									SELECT	[Posting Date]
											, [Document No_]
											, [Transaction No_]
											, [G_L Account No_]
											, CASE WHEN	(	([G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
															([G_L Account No_] BETWEEN '112.110.00' AND '112.162.00') OR
															[G_L Account No_] = '114.110.00' OR
															[G_L Account No_] = '114.140.00' OR
															[G_L Account No_] = '220.185.00' OR
															[G_L Account No_] = '118.215.00') THEN 'ACC' ELSE 'NON' END AS GL_ACC
											, CASE WHEN	(	([G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
															([G_L Account No_] BETWEEN '112.110.00' AND '112.162.00')
														) THEN 1 ELSE 0 END AS ADA_KASBANK
											, CASE WHEN [G_L Account No_] = '114.110.00' THEN 1 ELSE 0 END AS ADA_AR
											, CASE WHEN [G_L Account No_] = '114.140.00'  THEN 1 ELSE 0 END AS ADA_AR_IC
											, CASE WHEN [G_L Account No_] = '220.185.00' THEN 1 ELSE 0 END AS ADA_UN_PAY
											, SUM([Debit Amount]) AS [Debit Amount]
											, SUM([Credit Amount]) AS [Credit Amount]
									FROM [dbo].[AUTOJAYA$G_L Entry]
									WHERE	([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
											AND [Source Code] NOT IN ( 'SALES' , 'INVTPCOST', 'PURCHASES', 'FAGLJNL', 'PAYMENTJNL')
									GROUP BY [Posting Date]
											, [Document No_]
											, [Transaction No_]
											, [G_L Account No_]
								) X
		) XX
WHERE	(
			(XX.ADA_KASBANK > 0 AND XX.ADA_AR_IC > 0)
			OR
			(XX.ADA_KASBANK > 0 AND XX.ADA_UN_PAY > 0)
			OR
			(XX.ADA_KASBANK > 0 AND XX.ADA_AR > 0)
		)
		) XXX
WHERE	XXX.SELISIH <> 0 AND
		XXX.GL_ACC = 'NON'
UNION ALL
SELECT	'X2' AS GROUP_MAIN
		,'EE' AS GROUPS
		, X.URUTAN AS SUB_GROUP
		, CASE	WHEN X.URUTAN = 1 THEN 'Pelunasan Piutang - Non Bank (Intern)'
				WHEN X.URUTAN = 2 THEN 'Pelunasan Piutang - Non Bank (Antar Company)'
				ELSE '' END AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, SUM(X.AMOUNT) AS AMT_USAHA
		, SUM(X.AMOUNT) * -1 AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM	(
			SELECT 1 AS URUTAN, 0 AS AMOUNT
			UNION ALL
			SELECT 2 AS URUTAN, 0 AS AMOUNT
			UNION ALL
			SELECT	 CASE	WHEN [Source No_] IN ('AUJA001', 'AUJA002') THEN 1
							WHEN [Source No_] = 'SOPE001' THEN 2
							ELSE 3 END AS URUTAN
					, [Amount] * -1 AS AMOUNT
			FROM [dbo].[AUTOJAYA$G_L Entry]
			WHERE	(
						([G_L Account No_] BETWEEN '400.110.00' AND'420.110.01') OR
						[G_L Account No_] ='230.190.00'
					)
					AND ([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
					AND [Source No_] IN ('AUJA001', 'AUJA002','SOPE001')
		) X
GROUP BY X.URUTAN
UNION ALL
SELECT	'X3' AS GROUP_MAIN
		,'FF' AS GROUPS
		, 1 AS SUB_GROUP
		, 'PPh Pasal 22 Bendahara' AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, 0 AS AMT_USAHA
		, 0 AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
UNION ALL
SELECT	'X3' AS GROUP_MAIN
		,'FF' AS GROUPS
		, 2 AS SUB_GROUP
		, 'PPh Pasal 23' AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
			, SUM(CASE WHEN (XX.ADA_PIUTANG_DAGANG + XX.ADA_PIUTANG_DAGANG_INTERCOY + XX.ADA_UNAPPLIED_PAYMENT) > 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_USAHA
		, SUM(CASE WHEN (XX.ADA_PIUTANG_DAGANG + XX.ADA_PIUTANG_DAGANG_INTERCOY + XX.ADA_UNAPPLIED_PAYMENT) > 0 THEN XX.[Debit Amount] ELSE 0 END) * -1 AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM	(
			SELECT	X.[Posting Date]
					, X.[Document No_]
					, X.[Transaction No_]
					, X.[G_L Account No_]
					, X.[Source No_]
					, X.JENIS_ACC
					, SUM(X.ADA_PIUTANG_DAGANG) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_PIUTANG_DAGANG
					, SUM(X.ADA_PIUTANG_DAGANG_INTERCOY) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_PIUTANG_DAGANG_INTERCOY
					, SUM(X.ADA_UNAPPLIED_PAYMENT) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_UNAPPLIED_PAYMENT
					, X.[Debit Amount]
			FROM	(
						SELECT	[Posting Date]
								, [Document No_]
								, [Transaction No_]
								, [G_L Account No_]
								, [Source No_]
								,	CASE WHEN [G_L Account No_] = '118.215.00' THEN
										'PPH23'
									ELSE
										'NON' END AS JENIS_ACC
								, CASE WHEN [G_L Account No_] = '114.110.00' THEN 1 ELSE 0 END AS ADA_PIUTANG_DAGANG
								, CASE WHEN [G_L Account No_] = '114.140.00'  THEN 1 ELSE 0 END AS ADA_PIUTANG_DAGANG_INTERCOY
								, CASE WHEN [G_L Account No_] = '220.185.00' THEN 1 ELSE 0 END AS ADA_UNAPPLIED_PAYMENT
								, SUM([Debit Amount]) AS [Debit Amount]
								, SUM([Credit Amount]) AS [Credit Amount]
						FROM [dbo].[AUTOJAYA$G_L Entry]
						WHERE	([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
						GROUP BY [Posting Date]
								, [Document No_]
								, [Transaction No_]
								, [G_L Account No_]
								, [Source No_]
					) X
		) XX
WHERE XX.JENIS_ACC =  'PPH23'
		) XXX
WHERE $P{Company} = 'AUTOJAYA']]>
	</queryString>
