	<parameter name="PeriodeFrom" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="PeriodeUpto" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT	X.PILAR
		, X.PRODUCT_GRP
		, X.PRODUCT_GRP_SORT
		, ROUND((SUM(X.LAST_YEAR) / 1000000) ,2) AS LAST_YEAR
		, ROUND((SUM(X.CURR_YEAR) / 1000000) ,2) AS CURR_YEAR
FROM	(
			SELECT
					MPP.PILLAR AS PILAR
					,
						CASE	WHEN TSR.ITEM_CAT_CODE = 'HW01' THEN
									CASE
											WHEN TSR.PRODUCT_CODE = 'H002' THEN 'MOBILE COMPUTING'
											WHEN TSR.PRODUCT_CODE = 'H001' THEN 'SCANNER'
											WHEN TSR.PRODUCT_CODE = 'H003' THEN 'PRINTER'
											WHEN TSR.PRODUCT_CODE = 'H013' THEN 'PRINTER'
											WHEN TSR.PRODUCT_CODE = 'H004' THEN 'RFID'
											WHEN TSR.PRODUCT_CODE = 'H026' THEN 'RFID'
											WHEN TSR.PRODUCT_CODE = 'H026' THEN 'OTHERS'
											WHEN TSR.PRODUCT_CODE = 'H005' THEN 'WIRELESS INFRASTUCTURE'
											WHEN TSR.PRODUCT_CODE = 'H019' THEN 'NETWORK SECURITY'
											WHEN TSR.PRODUCT_CODE = 'H020' THEN 'SERVER'
											WHEN TSR.PRODUCT_CODE = 'H021' THEN 'NETWORK INFRASTRUCTURE'
											WHEN TSR.PRODUCT_CODE = 'H027' THEN 'OTHERS'
											WHEN TSR.PRODUCT_CODE = 'H011' THEN 'SECURITY SYSTEM'
											WHEN TSR.PRODUCT_CODE = 'H015' THEN 'GATE SYSTEM'
											WHEN TSR.PRODUCT_CODE = 'H022' THEN 'IP CAMERA'
											WHEN TSR.PRODUCT_CODE = 'H023' THEN 'ACCESS CONTROL'
											WHEN TSR.PRODUCT_CODE = 'H024' THEN 'ALARM SYSTEM'
											WHEN TSR.PRODUCT_CODE = 'H029' THEN 'PUSH TO TALK OVER CELLULAR'
											WHEN TSR.PRODUCT_CODE = 'H028' THEN 'OTHERS'
											ELSE 'OTHERS'
									END
								WHEN TSR.ITEM_CAT_CODE = 'PS01' THEN
									CASE	WHEN TSR.PRODUCT_CODE = 'S101' THEN 'SOFTWARE LICENSE - CLEAR PASS'
											WHEN TSR.PRODUCT_CODE = 'S102' THEN 'SOFTWARE LICENSE - AIR WAVE'
											WHEN TSR.PRODUCT_CODE = 'S103' THEN 'SOFTWARE LICENSE - OVS-AVS'
											WHEN TSR.PRODUCT_CODE = 'S104' THEN 'FORTINET LICENSE'
											WHEN TSR.PRODUCT_CODE = 'S106' THEN 'SOFTWARE LICENSE-EXTREME CONTROLLER'
											WHEN TSR.PRODUCT_CODE = 'S107' THEN 'SOFTWARE LICENSE-ARUBA CONTROLLER'
											WHEN TSR.PRODUCT_CODE = 'S108' THEN 'SOFTWARE LICENSE-VIDEO SURVEILANCE SYSTEM'
											WHEN TSR.PRODUCT_CODE = 'S109' THEN 'SOFTWARE LICENSE - SAMSUNG'
											WHEN TSR.PRODUCT_CODE = 'S110' THEN 'SOFTWARE LICENSE - ACCESS CONTROL'
											WHEN TSR.PRODUCT_CODE = 'S111' THEN 'SOFTWARE LICENSE - PTTOC'
											WHEN TSR.PRODUCT_CODE = 'S112' THEN 'SOFTWARE LICENSE – ALIBABA CLOUD'
											WHEN TSR.PRODUCT_CODE = 'S113' THEN 'SOFTWARE LICENSE – SANGFOR'
											ELSE 'SOFTWARE LICENSE - OTHERS'
									END
								WHEN TSR.ITEM_CAT_CODE = 'PS02' THEN
									CASE	WHEN TSR.PRODUCT_CODE = 'S201' THEN 'PACKAGE OWN SOFTWARE'
											WHEN TSR.PRODUCT_CODE = 'S202' THEN 'CUSTOMIZE OWN SOFTWARE'
											ELSE 'OWN SOFTWARE - OTHERS'
									END
								WHEN TSR.ITEM_CAT_CODE = 'PS03' THEN 'PROFESSIONAL SERVICES'
								WHEN TSR.ITEM_CAT_CODE = 'PS04' THEN
									CASE	WHEN TSR.PRODUCT_CODE = 'S401' THEN 'TIER SERVICE'
											WHEN TSR.PRODUCT_CODE = 'S402' THEN 'SPARE PART / ON CALL REPAIR'
											WHEN TSR.PRODUCT_CODE = 'S403' THEN 'ON CALL VISIT'
											WHEN TSR.PRODUCT_CODE = 'S404' THEN 'ACS CARE'
											WHEN TSR.PRODUCT_CODE = 'S405' THEN 'PRINCIPAL/DISTI CARE'
											ELSE 'HARDWARE SERVICE - OTHERS'
									END
								WHEN TSR.ITEM_CAT_CODE = 'PS05' THEN 'MANAGE SERVICE'
								ELSE
									'OTHERS'
								END
						AS PRODUCT_GRP
					,
						CASE	WHEN TSR.ITEM_CAT_CODE = 'HW01' THEN
									CASE
											WHEN TSR.PRODUCT_CODE = 'H002' THEN '1.01. MOBILE COMPUTING'
											WHEN TSR.PRODUCT_CODE = 'H001' THEN '1.02. SCANNER'
											WHEN TSR.PRODUCT_CODE = 'H003' THEN '1.03. PRINTER'
											WHEN TSR.PRODUCT_CODE = 'H013' THEN '1.03. PRINTER'
											WHEN TSR.PRODUCT_CODE = 'H004' THEN '1.04. RFID'
											WHEN TSR.PRODUCT_CODE = 'H026' THEN '1.04. RFID'
											WHEN TSR.PRODUCT_CODE = 'H026' THEN '1.05. OTHERS'
											WHEN TSR.PRODUCT_CODE = 'H005' THEN '2.01. WIRELESS INFRASTUCTURE'
											WHEN TSR.PRODUCT_CODE = 'H019' THEN '2.02. NETWORK SECURITY'
											WHEN TSR.PRODUCT_CODE = 'H020' THEN '2.03. SERVER'
											WHEN TSR.PRODUCT_CODE = 'H021' THEN '2.04. NETWORK INFRASTRUCTURE'
											WHEN TSR.PRODUCT_CODE = 'H027' THEN '2.05. OTHERS'
											WHEN TSR.PRODUCT_CODE = 'H011' THEN '3.01. SECURITY SYSTEM'
											WHEN TSR.PRODUCT_CODE = 'H015' THEN '3.02. GATE SYSTEM'
											WHEN TSR.PRODUCT_CODE = 'H022' THEN '3.03. IP CAMERA'
											WHEN TSR.PRODUCT_CODE = 'H023' THEN '3.04. ACCESS CONTROL'
											WHEN TSR.PRODUCT_CODE = 'H024' THEN '3.05. ALARM SYSTEM'
											WHEN TSR.PRODUCT_CODE = 'H029' THEN '3.06. PUSH TO TALK OVER CELLULAR'
											WHEN TSR.PRODUCT_CODE = 'H028' THEN '3.07. OTHERS'
											ELSE '1.05. OTHERS'
									END
								WHEN TSR.ITEM_CAT_CODE = 'PS01' THEN
									CASE	WHEN TSR.PRODUCT_CODE = 'S101' THEN '4.01. SOFTWARE LICENSE - CLEAR PASS'
											WHEN TSR.PRODUCT_CODE = 'S102' THEN '4.02. SOFTWARE LICENSE - AIR WAVE'
											WHEN TSR.PRODUCT_CODE = 'S103' THEN '4.03. SOFTWARE LICENSE - OVS-AVS'
											WHEN TSR.PRODUCT_CODE = 'S104' THEN '4.04. UTM BUNDLE - FORTINET'
											WHEN TSR.PRODUCT_CODE = 'S106' THEN '4.05. SOFTWARE LICENSE-EXTREME CONTROLLER'
											WHEN TSR.PRODUCT_CODE = 'S107' THEN '4.06. SOFTWARE LICENSE-ARUBA CONTROLLER'
											WHEN TSR.PRODUCT_CODE = 'S108' THEN '4.07. SOFTWARE LICENSE-VIDEO SURVEILANCE SYSTEM'
											WHEN TSR.PRODUCT_CODE = 'S109' THEN '4.08. SOFTWARE LICENSE - SAMSUNG'
											WHEN TSR.PRODUCT_CODE = 'S110' THEN '4.09. SOFTWARE LICENSE - ACCESS CONTROL'
											WHEN TSR.PRODUCT_CODE = 'S111' THEN '4.10. SOFTWARE LICENSE - PTTOC'
											WHEN TSR.PRODUCT_CODE = 'S112' THEN '4.11. SOFTWARE LICENSE – ALIBABA CLOUD'
											WHEN TSR.PRODUCT_CODE = 'S113' THEN '4.12. SOFTWARE LICENSE – SANGFOR'
											ELSE '4.13. SOFTWARE LICENSE - OTHERS'
									END
								WHEN TSR.ITEM_CAT_CODE = 'PS02' THEN
									CASE	WHEN TSR.PRODUCT_CODE = 'S201' THEN '4.11. PACKAGE OWN SOFTWARE'
											WHEN TSR.PRODUCT_CODE = 'S202' THEN '4.12. CUSTOMIZE OWN SOFTWARE'
											ELSE '4.13. OWN SOFTWARE - OTHERS'
									END
								WHEN TSR.ITEM_CAT_CODE = 'PS03' THEN '4.14. PROFESSIONAL SERVICES'
								WHEN TSR.ITEM_CAT_CODE = 'PS04' THEN
									CASE	WHEN TSR.PRODUCT_CODE = 'S401' THEN '4.15. TIER SERVICE'
											WHEN TSR.PRODUCT_CODE = 'S402' THEN '4.16. SPARE PART / ON CALL REPAIR'
											WHEN TSR.PRODUCT_CODE = 'S403' THEN '4.17. ON CALL VISIT'
											WHEN TSR.PRODUCT_CODE = 'S404' THEN '4.18. ACS CARE'
											WHEN TSR.PRODUCT_CODE = 'S405' THEN '4.19. PRINCIPAL/DISTI CARE'
											ELSE '4.20. HARDWARE SERVICE - OTHERS'
									END
								WHEN TSR.ITEM_CAT_CODE = 'PS05' THEN '4.21. MANAGE SERVICE'
								ELSE
									'1.05. OTHERS'
								END
						AS PRODUCT_GRP_SORT
					, TSR.LAST_YEAR
					, TSR.CURR_YEAR
			FROM	(
						SELECT ITEM_CAT_CODE
								, PRODUCT_CODE
								, 0 AS LAST_YEAR
								, AMOUNT_IDR AS CURR_YEAR
						FROM [dbo].[T_SALES_REV]
						WHERE POSTING_DATE >= $P{PeriodeFrom} AND POSTING_DATE <= $P{PeriodeUpto}
						UNION ALL
						SELECT ITEM_CAT_CODE
								, PRODUCT_CODE
								, AMOUNT_IDR AS LAST_YEAR
								, 0 AS CURR_YEAR
						FROM [dbo].[T_SALES_REV]
						WHERE POSTING_DATE >= DATEADD(YEAR, -1, $P{PeriodeFrom}) AND POSTING_DATE <= DATEADD(YEAR, -1, $P{PeriodeUpto})
					) TSR
			LEFT JOIN [dbo].[M_MAPPING_PILLAR] MMP ON MMP.PRODUCT_CODE = TSR.PRODUCT_CODE
			LEFT JOIN [dbo].[M_PILLAR] MPP ON MPP.PILLAR_NO = ISNULL(MMP.PILLAR_NO, 1)
		) X
GROUP BY X.PILAR, X.PRODUCT_GRP, X.PRODUCT_GRP_SORT]]>
	</queryString>
