	<parameter name="TipeRpt" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT	1 AS GRP
		, XX.COMPANY
		, XX.TGL_INV
		, XX.TGL_DUE_DATE
		, XX.NO_INV
		, XX.BILL_TO
		, XX.BILL_TO_NAME
		, XX.CURR_CODE
		, XX.AMOUNT_INV
		, XX.AMOUNT_INV_IDR
		, CASE WHEN XX.TGL_DUE_DATE <= $P{R1From} THEN $P{R1From} ELSE XX.TGL_DUE_DATE END AS TGL_DUE_DATE_GRP
		, XX.TIPE
		, XX.NO_INV_VENDOR
		, CASE	WHEN XX.CUST_GRP = 'ALFA' AND (XX.BILL_TO = 'ALFA227' OR XX.SELL_TO LIKE '%MIDI%')THEN 'MIDI UTAMA INDONESIA'
				WHEN XX.CUST_GRP = 'ALFA' AND (XX.BILL_TO = 'AL01622' OR XX.SELL_TO LIKE '%LAWSON%')THEN 'LANCAR WIGUNA SEJAHTERA'
				WHEN XX.CUST_GRP = 'ALFA' AND (XX.SELL_TO LIKE 'DC LOMBOK%')THEN 'DC LOMBOK-SAT'
				WHEN XX.CUST_GRP = 'ALFA' AND (XX.SELL_TO LIKE 'DC CILEUNGSI%')THEN 'DC CILEUNGSI - SAT'
				WHEN XX.CUST_GRP = 'INDOMARCO' AND XX.BILL_TO = 'IDMR180' THEN 'IDMR-HO'
				ELSE XX.SELL_TO END AS SELL_TO
		, CASE	WHEN XX.CUST_GRP = 'ALFA' AND (XX.BILL_TO = 'ALFA227' OR XX.SELL_TO LIKE '%MIDI%') THEN 'MIDI UTAMA INDONESIA'
				WHEN XX.CUST_GRP = 'ALFA' AND (XX.BILL_TO = 'AL01622' OR XX.SELL_TO LIKE '%LAWSON%')THEN 'LANCAR WIGUNA SEJAHTERA'
				WHEN XX.CUST_GRP = 'ALFA' AND (XX.SELL_TO LIKE 'DC LOMBOK%')THEN 'DC LOMBOK-SAT'
				WHEN XX.CUST_GRP = 'ALFA' AND (XX.SELL_TO LIKE 'DC CILEUNGSI%')THEN 'DC CILEUNGSI - SAT'
				WHEN XX.CUST_GRP = 'INDOMARCO' AND XX.BILL_TO = 'IDMR180' THEN 'INDOMARCO PRISMATAMA'
				ELSE XX.SELL_TO_NAME END AS SELL_TO_NAME
		, XX.SELL_TO AS SELL_TO_REAL
		, XX.CUST_GRP
		, XX.INV_DESC
		, XX.NO_FP
		, XX.ENTRY_NO
		, XX.AMOUNT_ORI
		, XX.REMAIN_AMOUNT
		, CAB.[Name] CABANG
FROM	(
			SELECT	'AUTOJAYA' AS COMPANY
					, X.TGL_INV
					, X.TGL_DUE_DATE
					, X.NO_INV
					, X.BILL_TO
					, X.BILL_TO_NAME
					, X.CURR_CODE
					, X.AMOUNT_INV
					, (X.AMOUNT_INV_IDR / $P{AmountPembagi}) AS AMOUNT_INV_IDR
					, X.AMOUNT_INV_IN_IDR
					, X.INV_DESC
					, X.NO_FP
					, X.ENTRY_NO
					, X.AMOUNT_ORI
					, X.AMOUNT_INV_IDR AS REMAIN_AMOUNT
					, X.TIPE
					, X.NO_INV_VENDOR
					, X.CABANG
					, X.SELL_TO
					, X.SELL_TO_NAME
					, X.CUST_GRP
			FROM	(
						SELECT CLE.[Posting Date] AS TGL_INV
								--,	CASE WHEN CUS.[Customer Posting Group] ='INDOMARCO' THEN
								--		CASE WHEN DATEDIFF(DAY, CLE.[Posting Date], CLE.[Due Date]) < 60 THEN
								--			DATEADD(DAY,60,CLE.[Posting Date]) ELSE CLE.[Due Date]
								--		END
								--	ELSE
								--		CASE WHEN CUS.[Customer Posting Group] ='ALFA' THEN
								--			CASE WHEN DATEDIFF(DAY, CLE.[Posting Date], CLE.[Due Date]) < 35 THEN
								--				DATEADD(DAY,35,CLE.[Posting Date]) ELSE CLE.[Due Date]
								--			END
								--		ELSE
								--			CLE.[Due Date]
								--		END
								--	END AS TGL_DUE_DATE
								,CLE.[Due Date] AS TGL_DUE_DATE
								,CLE.[Customer No_] AS BILL_TO
								,CUS.[Name] AS BILL_TO_NAME
								,CLE.[Document No_] AS NO_INV
								,CASE WHEN CLE.[Currency Code] <> '' THEN CLE.[Currency Code] ELSE 'IDR' END AS CURR_CODE
								,CASE WHEN CLE.[Document Type] = 2 THEN 'INVOICE'
									WHEN CLE.[Document Type] = 3 THEN 'CREDIT MEMO'
									ELSE 'PAYMENT'
								END AS TIPE
								,CLE.[External Document No_] AS NO_INV_VENDOR
								,	CASE WHEN CLE.[Currency Code] <> '' THEN
										ROUND(
												CASE WHEN CUS.[Tax Area Code] IN ('070.', '020.') THEN
													CASE WHEN CLD.RMN_AMOUNT > CLD.ORI_AMOUNT_DPP THEN CLD.ORI_AMOUNT_DPP ELSE CLD.RMN_AMOUNT END
												ELSE
													CLD.RMN_AMOUNT
												END
											,2)
									ELSE 0
									END AS AMOUNT_INV
								,	CASE WHEN CLE.[Currency Code] <> '' THEN 0
									ELSE
										ROUND(
												CASE WHEN CUS.[Tax Area Code] IN ('070.', '020.') THEN
													CASE WHEN CLD.RMN_AMOUNT_IDR > CLD.ORI_AMOUNT_DPP_IDR THEN CLD.ORI_AMOUNT_DPP_IDR ELSE CLD.RMN_AMOUNT_IDR END
												ELSE
													CLD.RMN_AMOUNT_IDR
												END
											,0)
									END AS AMOUNT_INV_IDR
								,	ROUND(
									CASE WHEN CUS.[Tax Area Code] IN ('070.', '020.') THEN
										CASE WHEN CLD.RMN_AMOUNT_IDR > CLD.ORI_AMOUNT_DPP_IDR THEN CLD.ORI_AMOUNT_DPP_IDR ELSE CLD.RMN_AMOUNT_IDR END
									ELSE
										CLD.RMN_AMOUNT_IDR
									END ,0) AS  AMOUNT_INV_IN_IDR
								, CASE WHEN CLE.[Global Dimension 1 Code] = '' THEN 10 ELSE CLE.[Global Dimension 1 Code] END AS CABANG
								, SIH.[Sell-to Customer No_] AS SELL_TO
								, SIH.[Sell-to Customer Name] AS SELL_TO_NAME
								, SIH.[Posting Description] AS INV_DESC
								, SIH.[Faktur Pajak No_] AS NO_FP
								, CLE.[Entry No_] AS ENTRY_NO
								, CLD.AMOUNT_ORI AS AMOUNT_ORI
								, CUS.[Customer Posting Group] AS CUST_GRP
						FROM [dbo].[BC_AUTOJAYA$Cust_ Ledger Entry] CLE
						LEFT JOIN [dbo].[BC_AUTOJAYA$Sales Invoice Header] SIH ON SIH.[No_] = CLE.[Document No_]
						LEFT JOIN [dbo].[BC_AUTOJAYA$Customer] CUS ON CUS.[No_] = CLE.[Customer No_]
						LEFT JOIN	(
										SELECT	CLED.[Cust_ Ledger Entry No_]
												,SUM(CASE WHEN CLED.[Entry Type] = 1 THEN CLED.[Amount] ELSE 0 END) AS AMOUNT_ORI
												,SUM (
														CASE WHEN CLED.[Entry Type] = 1 THEN
															CASE WHEN CLED.[Posting Date] < CAST('04/01/2022' AS DATETIME) THEN
																CLED.[Amount] / 1.1
															ELSE
																CLED.[Amount] / 1.11
															END
														ELSE 0 END
													 ) AS ORI_AMOUNT_DPP
												,SUM (
														CASE WHEN CLED.[Entry Type] = 1 THEN
															CASE WHEN CLED.[Posting Date] < CAST('04/01/2022' AS DATETIME) THEN
																CLED.[Amount (LCY)] / 1.1
															ELSE
																CLED.[Amount (LCY)] / 1.11
															END
														ELSE 0 END
												     ) AS ORI_AMOUNT_DPP_IDR
												,SUM(CLED.[Amount]) AS RMN_AMOUNT
												,SUM(CLED.[Amount (LCY)]) AS RMN_AMOUNT_IDR
										FROM [dbo].[BC_AUTOJAYA$Detailed Cust_ Ledg_ Entry] CLED
										GROUP BY CLED.[Cust_ Ledger Entry No_]
									) CLD ON CLD.[Cust_ Ledger Entry No_] = CLE.[Entry No_]
						WHERE CLD.RMN_AMOUNT <> 0
					) X
			WHERE X.AMOUNT_INV_IN_IDR < -10000 OR AMOUNT_INV_IN_IDR > 10000
			UNION ALL
			SELECT	'SOLUSI' AS COMPANY
					, X.TGL_INV
					, X.TGL_DUE_DATE
					, X.NO_INV
					, X.BILL_TO
					, X.BILL_TO_NAME
					, X.CURR_CODE
					, X.AMOUNT_INV
					, (X.AMOUNT_INV_IDR / $P{AmountPembagi}) AS AMOUNT_INV_IDR
					, X.AMOUNT_INV_IN_IDR
					, X.INV_DESC
					, X.NO_FP
					, X.ENTRY_NO
					, X.AMOUNT_ORI
					, X.AMOUNT_INV_IDR AS REMAIN_AMOUNT
					, X.TIPE
					, X.NO_INV_VENDOR
					, X.CABANG
					, X.SELL_TO
					, X.SELL_TO_NAME
					, X.CUST_GRP
			FROM	(
						SELECT	CLE.[Posting Date] AS TGL_INV
								--,	CASE WHEN CUS.[Customer Posting Group] ='INDOMARCO' THEN
								--		CASE WHEN DATEDIFF(DAY, CLE.[Posting Date], CLE.[Due Date]) < 60 THEN
								--			DATEADD(DAY,60,CLE.[Posting Date]) ELSE CLE.[Due Date]
								--		END
								--	ELSE
								--		CASE WHEN CUS.[Customer Posting Group] ='ALFA' THEN
								--			CASE WHEN DATEDIFF(DAY, CLE.[Posting Date], CLE.[Due Date]) < 35 THEN
								--				DATEADD(DAY,35,CLE.[Posting Date]) ELSE CLE.[Due Date]
								--			END
								--		ELSE
								--			CLE.[Due Date]
								--		END
								--	END AS TGL_DUE_DATE
								,CLE.[Due Date] AS TGL_DUE_DATE
								,CLE.[Customer No_] AS BILL_TO
								,CUS.[Name] AS BILL_TO_NAME
								,CLE.[Document No_] AS NO_INV
								,CASE WHEN CLE.[Currency Code] <> '' THEN CLE.[Currency Code] ELSE 'IDR' END AS CURR_CODE
								,CASE WHEN CLE.[Document Type] = 2 THEN 'INVOICE'
									WHEN CLE.[Document Type] = 3 THEN 'CREDIT MEMO'
									ELSE 'PAYMENT'
								END AS TIPE
								,CLE.[External Document No_] AS NO_INV_VENDOR
								,	CASE WHEN CLE.[Currency Code] <> '' THEN
										ROUND(
												CASE WHEN CUS.[Tax Area Code] IN ('070.', '020.') THEN
													CASE WHEN CLD.RMN_AMOUNT > CLD.ORI_AMOUNT_DPP THEN CLD.ORI_AMOUNT_DPP ELSE CLD.RMN_AMOUNT END
												ELSE
													CLD.RMN_AMOUNT
												END
											,2)
									ELSE 0
									END AS AMOUNT_INV
								,	CASE WHEN CLE.[Currency Code] <> '' THEN 0
									ELSE
										ROUND(
												CASE WHEN CUS.[Tax Area Code] IN ('070.', '020.') THEN
													CASE WHEN CLD.RMN_AMOUNT_IDR > CLD.ORI_AMOUNT_DPP_IDR THEN CLD.ORI_AMOUNT_DPP_IDR ELSE CLD.RMN_AMOUNT_IDR END
												ELSE
													CLD.RMN_AMOUNT_IDR
												END
											,0)
									END AS AMOUNT_INV_IDR
								,	ROUND(
									CASE WHEN CUS.[Tax Area Code] IN ('070.', '020.') THEN
										CASE WHEN CLD.RMN_AMOUNT_IDR > CLD.ORI_AMOUNT_DPP_IDR THEN CLD.ORI_AMOUNT_DPP_IDR ELSE CLD.RMN_AMOUNT_IDR END
									ELSE
										CLD.RMN_AMOUNT_IDR
									END ,0) AS  AMOUNT_INV_IN_IDR
								, CASE WHEN CLE.[Global Dimension 1 Code] = '' THEN 10 ELSE CLE.[Global Dimension 1 Code] END AS CABANG
								, SIH.[Sell-to Customer No_] AS SELL_TO
								, SIH.[Sell-to Customer Name] AS SELL_TO_NAME
								, SIH.[Posting Description] AS INV_DESC
								, SIH.[Faktur Pajak No_] AS NO_FP
								, CLE.[Entry No_] AS ENTRY_NO
								, CLD.AMOUNT_ORI AS AMOUNT_ORI
								, CUS.[Customer Posting Group] AS CUST_GRP
						FROM [dbo].[BC_SOLUSI$Cust_ Ledger Entry] CLE
						LEFT JOIN [dbo].[BC_SOLUSI$Sales Invoice Header] SIH ON SIH.[No_] = CLE.[Document No_]
						LEFT JOIN [dbo].[BC_AUTOJAYA$Customer] CUS ON CUS.[No_] = CLE.[Customer No_]
						LEFT JOIN	(
										SELECT	CLED.[Cust_ Ledger Entry No_]
												,SUM(CASE WHEN CLED.[Entry Type] = 1 THEN CLED.[Amount] ELSE 0 END) AS AMOUNT_ORI
												,SUM (
														CASE WHEN CLED.[Entry Type] = 1 THEN
															CASE WHEN CLED.[Posting Date] < CAST('04/01/2022' AS DATETIME) THEN
																CLED.[Amount] / 1.1
															ELSE
																CLED.[Amount] / 1.11
															END
														ELSE 0 END
													 ) AS ORI_AMOUNT_DPP
												,SUM (
														CASE WHEN CLED.[Entry Type] = 1 THEN
															CASE WHEN CLED.[Posting Date] < CAST('04/01/2022' AS DATETIME) THEN
																CLED.[Amount (LCY)] / 1.1
															ELSE
																CLED.[Amount (LCY)] / 1.11
															END
														ELSE 0 END
												     ) AS ORI_AMOUNT_DPP_IDR
												,SUM(CLED.[Amount]) AS RMN_AMOUNT
												,SUM(CLED.[Amount (LCY)]) AS RMN_AMOUNT_IDR
										FROM [dbo].[BC_SOLUSI$Detailed Cust_ Ledg_ Entry] CLED
										GROUP BY CLED.[Cust_ Ledger Entry No_]
									) CLD ON CLD.[Cust_ Ledger Entry No_] = CLE.[Entry No_]
						WHERE CLD.RMN_AMOUNT <> 0
					) X
			WHERE X.AMOUNT_INV_IN_IDR < -10000 OR AMOUNT_INV_IN_IDR > 10000
		) XX
LEFT JOIN [dbo].[BC_AUTOJAYA$Responsibility Center] CAB ON CAB.[Code] = XX.CABANG
WHERE	(XX.COMPANY = $P{Company} OR  $P{Company} = 'ALL') AND
		(XX.BILL_TO BETWEEN $P{VendorFrom} AND $P{VendorUpto}) AND
		($P{Currency} = 'ALL' OR XX.CURR_CODE = $P{Currency}) AND
		($P{Cabang} = 'ALL' OR CAB.[Name] = $P{Cabang}) AND
		XX.TGL_DUE_DATE <= DATEADD(DAY, 27, $P{R1From})]]>
	</queryString>
