	<queryString>
		<![CDATA[SELECT	XXX.COMPANY
		, XXX.NO_SO
		, MAX(XXX.NO_BSO) OVER(PARTITION BY XXX.NO_SO ORDER BY XXX.NO_SO, XXX.NO_ITEM) AS NO_BSO
		, XXX.TIPE
		, XXX.CABANG
		, XXX.SALES_CODE
		, XXX.SALES_NAME
		, XXX.DC_NAME
		, XXX.CUST_CODE
		, XXX.CUST_NAME
		, XXX.ORDER_DATE
		, XXX.STATUS_SO
		, XXX.TERM_OF_PAY
		, XXX.NO_PO_CUST
		, XXX.CURR_CODE
		, XXX.CURR_FAKTOR
		, XXX.AMOUNT_IDR
		, XXX.AMOUNT
		, XXX.NO_SO_LINE
		, ROW_NUMBER()OVER(PARTITION BY XXX.NO_SO ORDER BY XXX.NO_SO, XXX.NO_ITEM) AS BARIS
		, XXX.NO_ITEM
		, XXX.ITEM_DESC
		, XXX.QTY
		, XXX.QTY_SHIPPED
		, XXX.QTY_OUT
		, XXX.RSRV_OH
		, XXX.RSRV_ON_ORDER
		, XXX.RSRV_OTP_READY
		, XXX.RSRV_OTP_ON_ORDER
		, CASE WHEN XXX.EXP_DELIVERY <= '1753-01-01' THEN NULL ELSE XXX.EXP_DELIVERY END AS EXP_DELIVERY
		, XXX.STATUS_LINE
FROM	(
SELECT	'AUTOJAYA' AS COMPANY
		, XY.NO_SO
		, XY.TIPE
		, XY.CABANG
		, XY.SALES_CODE
		, XY.SALES_NAME
		, XY.DC_NAME
		, XY.CUST_CODE
		, XY.CUST_NAME
		, XY.ORDER_DATE
		, XY.STATUS_SO
		, XY.TERM_OF_PAY
		, XY.NO_PO_CUST
		, XY.CURR_CODE
		, XY.CURR_FAKTOR
		, XY.AMOUNT_IDR
		, XY.AMOUNT
		, XY.NO_SO_LINE
		, CASE WHEN XY.NO_BSO = '' THEN '.' ELSE XY.NO_BSO END AS NO_BSO
		, XY.NO_ITEM
		, XY.ITEM_DESC
		, XY.QTY
		, XY.QTY_SHIPPED
		, XY.QTY_OUT
		, XY.RSRV_OH
		, XY.RSRV_ON_ORDER
		, XY.RSRV_OTP_READY
		, XY.RSRV_OTP_ON_ORDER
		, XY.EXP_DELIVERY
		, XY.STATUS_LINE
FROM	(
			SELECT SH.[No_] AS NO_SO
					, CASE WHEN SUBSTRING( SH.[No_], 1, 4) = 'SOAS' THEN 'SC' ELSE
						CASE WHEN SUBSTRING( SH.[No_], 1, 2) IN ('8S','9S') THEN 'AI' ELSE 'TD' END
					  END AS TIPE
					, ISNULL(CAB.[Name], 'JAKARTA') AS CABANG
					, CASE WHEN SH.[Salesperson Code] = '' THEN '.' ELSE SH.[Salesperson Code] END AS SALES_CODE
					, SLS.[Name] AS SALES_NAME
					, SH.[Sell-to Customer No_] AS DC_NAME
					, SH.[Bill-to Customer No_] AS CUST_CODE
					, SH.[Bill-to Name] AS CUST_NAME
					, SH.[Order Date] AS ORDER_DATE
					, CASE	WHEN SH.[Status] = 0 THEN 'OPEN'
							WHEN SH.[Status] = 1 THEN 'RELEASED'
							WHEN SH.[Status] = 2 THEN 'PENDING APPROVAL'
							WHEN SH.[Status] = 3 THEN 'RELEASED'
							ELSE '' END AS STATUS_SO
					, SH.[Payment Terms Code] AS TERM_OF_PAY
					, SH.[External Document No_] AS NO_PO_CUST
					, SH.[Currency Code] AS CURR_CODE
					, SH.[Currency Factor] AS CURR_FAKTOR
					, SH.[Requested Delivery Date] AS EXP_DELIVERY
					, ISNULL(CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT / SH.[Currency Factor],0) ELSE SL.AMOUNT END,0) AS AMOUNT_IDR
					, ISNULL(SL.AMOUNT,0) AS AMOUNT
					, SL.NO_SO_LINE
					, SL.NO_ITEM
					, SL.ITEM_DESC
					, SL.QTY
					, SL.QTY_SHIPPED
					, (SL.QTY - SL.QTY_SHIPPED) AS QTY_OUT
					, SL.NO_BSO
					, ISNULL(SLR.RSRV_OH,0) AS RSRV_OH
					, ISNULL(SLR.RSRV_ON_ORDER,0) AS RSRV_ON_ORDER
					, ISNULL(SLR.RSRV_OTP_READY,0) AS RSRV_OTP_READY
					, ISNULL(SLR.RSRV_OTP_ON_ORDER,0) AS RSRV_OTP_ON_ORDER
					,	CASE WHEN ITM.[Inventory Value Zero] = 1 THEN
							'READY'
						ELSE
							CASE WHEN SL.QTY_INVOICED = SL.QTY THEN
								'INVOICED'
							ELSE
								CASE WHEN SL.QTY_SHIPPED > 0 THEN
									CASE WHEN SL.QTY_SHIPPED = SL.QTY THEN 'FULL DELIVERED' ELSE 'PARTIAL DELIVERED' END
								ELSE
									CASE WHEN SL.QTY = ISNULL(SLR.RSRV_OH,0) THEN 'READY'
									ELSE
										CASE WHEN SL.QTY > (ISNULL(SLR.RSRV_OH,0)+ISNULL(SLR.RSRV_ON_ORDER,0)+ISNULL(SLR.RSRV_OTP_READY,0)+ISNULL(SLR.RSRV_OTP_ON_ORDER,0)) THEN 'NEED TO RESERVE' ELSE 'WAITING' END
									END
								END
							END
						END AS STATUS_LINE
			FROM [dbo].[BC_AUTOJAYA$Sales Header] SH
			LEFT JOIN  (
							SELECT [Document No_]
									,[Line No_] AS NO_SO_LINE
									,[No_] AS NO_ITEM
									,[Description] AS ITEM_DESC
									,[Amount] AS AMOUNT
									,[Quantity (Base)] AS QTY
									,[Qty_ Shipped (Base)] AS QTY_SHIPPED
									,[Qty_ Invoiced (Base)] AS QTY_INVOICED
									,[Blanket Order No_] AS NO_BSO
							FROM [dbo].[BC_AUTOJAYA$Sales Line]
							WHERE [Type] <> 0
						)SL ON SL.[Document No_] = SH.[No_]
			LEFT JOIN (
							SELECT	REA.[Item No_]
									, REA.[Source ID] AS NO_SO
									, REA.[Source Ref_ No_] AS NO_SO_LINE
									, SUM(CASE WHEN REB.[Source ID] = '' THEN REB.[Quantity (Base)] ELSE 0 END) AS RSRV_OH
									, SUM(CASE WHEN SUBSTRING(REB.[Source ID], 5, 1) <> 'P' AND REB.[Source ID] <> '' THEN REB.[Quantity (Base)] ELSE 0 END) AS RSRV_ON_ORDER
									, SUM(
											CASE WHEN SUBSTRING(REB.[Source ID], 5, 1) = 'P' AND  ISNULL(PUR.EXP_RCP_DATE,GETDATE()) <= GETDATE() THEN
												REB.[Quantity (Base)] ELSE 0
											END
										  ) AS RSRV_OTP_READY
									, SUM(
											CASE WHEN SUBSTRING(REB.[Source ID], 5, 1) = 'P' AND  ISNULL(PUR.EXP_RCP_DATE,GETDATE()) > GETDATE() THEN
												REB.[Quantity (Base)] ELSE 0
											END
										  ) AS RSRV_OTP_ON_ORDER
							FROM [dbo].[BC_AUTOJAYA$Reservation Entry] REA
							LEFT JOIN [dbo].[BC_AUTOJAYA$Reservation Entry] REB ON REA.[Entry No_] = REB.[Entry No_] AND REB.[Positive] = 1
							LEFT JOIN	(
											SELECT	MAX(PPUR.EXP_RCP_DATE) AS EXP_RCP_DATE
													, PPUR.[Document No_]
													, PPUR.[Line No_]
											FROM	(
														SELECT
																(CASE WHEN POH.[Currency Code] = '' THEN POD.[Expected Receipt Date] ELSE DATEADD(DAY, 7, POD.[Expected Receipt Date]) END) AS EXP_RCP_DATE
																, POD.[Document No_]
																, POD.[Line No_]
														FROM [dbo].[BC_AUTOJAYA$Purchase Header] POH
														INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Line] POD ON POD.[Document No_] = POH.[No_]
													) PPUR
											GROUP BY PPUR.[Document No_], PPUR.[Line No_]
										) PUR ON PUR.[Document No_] = REB.[Source ID] AND PUR.[Line No_] = REB.[Source Ref_ No_]
							WHERE REA.[Positive] = 0
							GROUP BY REA.[Item No_], REA.[Source ID], REA.[Source Ref_ No_]
						) SLR ON SL.[Document No_] = SLR.NO_SO AND SL.NO_SO_LINE = SLR.NO_SO_LINE
			LEFT JOIN [dbo].[BC_AUTOJAYA$Item] ITM ON ITM.[No_] = SL.NO_ITEM
			LEFT JOIN [dbo].[BC_AUTOJAYA$Responsibility Center] CAB ON CAB.[Code] = SH.[Shortcut Dimension 1 Code]
			LEFT JOIN [dbo].[BC_AUTOJAYA$Salesperson_Purchaser] SLS ON SLS.[Code] = SH.[Salesperson Code]
			WHERE SH.[Document Type] = 1 AND (SH.[Sell-to Customer No_] = $P{DCName} OR   $P{DCName} = 'X')
		) XY
WHERE
(XY.CABANG =  $P{Cabang} OR $P{Cabang} = 'ALL') AND
(XY.SALES_CODE = $P{Sales} OR $P{Sales} = 'ALL') AND
(XY.NO_SO >= $P{NoSOFrom} AND XY.NO_SO <= $P{NoSOUpto}) AND
(XY.CUST_CODE = $P{Customer} OR $P{Customer} = 'ALL') AND
($P{SudahInvoice} ='NO' OR $P{SudahInvoice} ='ALL')
UNION ALL
SELECT	'SOLUSI' AS COMPANY
		, XY.NO_SO
		, XY.TIPE
		, XY.CABANG
		, XY.SALES_CODE
		, XY.SALES_NAME
		, XY.DC_NAME
		, XY.CUST_CODE
		, XY.CUST_NAME
		, XY.ORDER_DATE
		, XY.STATUS_SO
		, XY.TERM_OF_PAY
		, XY.NO_PO_CUST
		, XY.CURR_CODE
		, XY.CURR_FAKTOR
		, XY.AMOUNT_IDR
		, XY.AMOUNT
		, XY.NO_SO_LINE
		, CASE WHEN XY.NO_BSO = '' THEN '.' ELSE XY.NO_BSO END AS NO_BSO
		, XY.NO_ITEM
		, XY.ITEM_DESC
		, XY.QTY
		, XY.QTY_SHIPPED
		, XY.QTY_OUT
		, XY.RSRV_OH
		, XY.RSRV_ON_ORDER
		, XY.RSRV_OTP_READY
		, XY.RSRV_OTP_ON_ORDER
		, XY.EXP_DELIVERY
		, XY.STATUS_LINE
FROM	(
			SELECT SH.[No_] AS NO_SO
					, CASE WHEN SUBSTRING( SH.[No_], 1, 4) = 'SOSS' THEN 'SC' ELSE
						CASE WHEN SUBSTRING( SH.[No_], 1, 2) IN ('8S','9S') THEN 'AI' ELSE 'TD' END
					  END AS TIPE
					, ISNULL(CAB.[Name], 'JAKARTA') AS CABANG
					, CASE WHEN SH.[Salesperson Code] = '' THEN '.' ELSE SH.[Salesperson Code] END AS SALES_CODE
					, SLS.[Name] AS SALES_NAME
					, SH.[Sell-to Customer No_] AS DC_NAME
					, SH.[Bill-to Customer No_] AS CUST_CODE
					, SH.[Bill-to Name] AS CUST_NAME
					, SH.[Order Date] AS ORDER_DATE
					, CASE	WHEN SH.[Status] = 0 THEN 'OPEN'
							WHEN SH.[Status] = 1 THEN 'RELEASED'
							WHEN SH.[Status] = 2 THEN 'PENDING APPROVAL'
							WHEN SH.[Status] = 3 THEN 'RELEASED'
							ELSE '' END AS STATUS_SO
					, SH.[Payment Terms Code] AS TERM_OF_PAY
					, SH.[External Document No_] AS NO_PO_CUST
					, SH.[Currency Code] AS CURR_CODE
					, SH.[Currency Factor] AS CURR_FAKTOR
					, SH.[Requested Delivery Date] AS EXP_DELIVERY
					, ISNULL(CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT / SH.[Currency Factor],0) ELSE SL.AMOUNT END,0) AS AMOUNT_IDR
					, ISNULL(SL.AMOUNT,0) AS AMOUNT
					, SL.NO_SO_LINE
					, SL.NO_ITEM
					, SL.ITEM_DESC
					, SL.QTY
					, SL.QTY_SHIPPED
					, (SL.QTY - SL.QTY_SHIPPED) AS QTY_OUT
					, SL.NO_BSO
					, ISNULL(SLR.RSRV_OH,0) AS RSRV_OH
					, ISNULL(SLR.RSRV_ON_ORDER,0) AS RSRV_ON_ORDER
					, ISNULL(SLR.RSRV_OTP_READY,0) AS RSRV_OTP_READY
					, ISNULL(SLR.RSRV_OTP_ON_ORDER,0) AS RSRV_OTP_ON_ORDER
					,	CASE WHEN ITM.[Inventory Value Zero] = 1 THEN
							'READY'
						ELSE
							CASE WHEN SL.QTY_INVOICED = SL.QTY THEN
								'INVOICED'
							ELSE
								CASE WHEN SL.QTY_SHIPPED > 0 THEN
									CASE WHEN SL.QTY_SHIPPED = SL.QTY THEN 'FULL DELIVERED' ELSE 'PARTIAL DELIVERED' END
								ELSE
									CASE WHEN SL.QTY = ISNULL(SLR.RSRV_OH,0) THEN 'READY'
									ELSE
										CASE WHEN SL.QTY > (ISNULL(SLR.RSRV_OH,0)+ISNULL(SLR.RSRV_ON_ORDER,0)+ISNULL(SLR.RSRV_OTP_READY,0)) THEN 'NEED TO RESERVE' ELSE 'WAITING' END
									END
								END
							END
						END AS STATUS_LINE
			FROM [dbo].[BC_SOLUSI$Sales Header] SH
			LEFT JOIN  (
							SELECT [Document No_]
									,[Line No_] AS NO_SO_LINE
									,[No_] AS NO_ITEM
									,[Description] AS ITEM_DESC
									,[Amount] AS AMOUNT
									,[Quantity (Base)] AS QTY
									,[Qty_ Shipped (Base)] AS QTY_SHIPPED
									,[Qty_ Invoiced (Base)] AS QTY_INVOICED
									,[Blanket Order No_] AS NO_BSO
							FROM [dbo].[BC_SOLUSI$Sales Line]
							WHERE [Type] <> 0
						)SL ON SL.[Document No_] = SH.[No_]
			LEFT JOIN (
							SELECT	REA.[Item No_]
									, REA.[Source ID] AS NO_SO
									, REA.[Source Ref_ No_] AS NO_SO_LINE
									, SUM(CASE WHEN REB.[Source ID] = '' THEN REB.[Quantity (Base)] ELSE 0 END) AS RSRV_OH
									, SUM(CASE WHEN SUBSTRING(REB.[Source ID], 5, 1) <> 'P' AND REB.[Source ID] <> '' THEN REB.[Quantity (Base)] ELSE 0 END) AS RSRV_ON_ORDER
									, SUM(
											CASE WHEN SUBSTRING(REB.[Source ID], 5, 1) = 'P' AND  ISNULL(PUR.EXP_RCP_DATE,GETDATE()) <= GETDATE() THEN
												REB.[Quantity (Base)] ELSE 0
											END
										  ) AS RSRV_OTP_READY
									, SUM(
											CASE WHEN SUBSTRING(REB.[Source ID], 5, 1) = 'P' AND  ISNULL(PUR.EXP_RCP_DATE,GETDATE()) > GETDATE() THEN
												REB.[Quantity (Base)] ELSE 0
											END
										  ) AS RSRV_OTP_ON_ORDER
							FROM [dbo].[BC_SOLUSI$Reservation Entry] REA
							LEFT JOIN [dbo].[BC_SOLUSI$Reservation Entry] REB ON REA.[Entry No_] = REB.[Entry No_] AND REB.[Positive] = 1
							LEFT JOIN	(
											SELECT	MAX(PPUR.EXP_RCP_DATE) AS EXP_RCP_DATE
													, PPUR.[Document No_]
													, PPUR.[Line No_]
											FROM	(
														SELECT
																(CASE WHEN POH.[Currency Code] = '' THEN POD.[Expected Receipt Date] ELSE DATEADD(DAY, 7, POD.[Expected Receipt Date]) END) AS EXP_RCP_DATE
																, POD.[Document No_]
																, POD.[Line No_]
														FROM [dbo].[BC_SOLUSI$Purchase Header] POH
														INNER JOIN [dbo].[BC_SOLUSI$Purchase Line] POD ON POD.[Document No_] = POH.[No_]
													) PPUR
											GROUP BY PPUR.[Document No_], PPUR.[Line No_]
										) PUR ON PUR.[Document No_] = REB.[Source ID] AND PUR.[Line No_] = REB.[Source Ref_ No_]
							WHERE REA.[Positive] = 0
							GROUP BY REA.[Item No_], REA.[Source ID], REA.[Source Ref_ No_]
						) SLR ON SL.[Document No_] = SLR.NO_SO AND SL.NO_SO_LINE = SLR.NO_SO_LINE
			LEFT JOIN [dbo].[BC_SOLUSI$Item] ITM ON ITM.[No_] = SL.NO_ITEM
			LEFT JOIN [dbo].[BC_AUTOJAYA$Responsibility Center] CAB ON CAB.[Code] = SH.[Shortcut Dimension 1 Code]
			LEFT JOIN [dbo].[BC_AUTOJAYA$Salesperson_Purchaser] SLS ON SLS.[Code] = SH.[Salesperson Code]
			WHERE SH.[Document Type] = 1 AND (SH.[Sell-to Customer No_] = $P{DCName} OR   $P{DCName} = 'X')
		) XY
WHERE
(XY.CABANG =  $P{Cabang} OR $P{Cabang} = 'ALL') AND
(XY.SALES_CODE = $P{Sales} OR $P{Sales} = 'ALL') AND
(XY.NO_SO >= $P{NoSOFrom} AND XY.NO_SO <= $P{NoSOUpto}) AND
(XY.CUST_CODE = $P{Customer} OR $P{Customer} = 'ALL') AND
($P{SudahInvoice} ='NO' OR $P{SudahInvoice} ='ALL')
UNION ALL
SELECT	XY.COMPANY
		, XY.NO_SO
		, XY.TIPE
		, XY.CABANG
		, XY.SALES_CODE
		, XY.SALES_NAME
		, XY.DC_NAME
		, XY.CUST_CODE
		, XY.CUST_NAME
		, XY.ORDER_DATE
		, XY.STATUS_SO
		, XY.TERM_OF_PAY
		, XY.NO_PO_CUST
		, XY.CURR_CODE
		, XY.CURR_FAKTOR
		, XY.AMOUNT_IDR
		, XY.AMOUNT
		, XY.NO_SO_LINE
		, CASE WHEN XY.NO_BSO = '' THEN '.' ELSE XY.NO_BSO END AS NO_BSO
		, XY.NO_ITEM
		, XY.ITEM_DESC
		, XY.QTY
		, XY.QTY_SHIPPED
		, XY.QTY_OUT
		, XY.RSRV_OH
		, XY.RSRV_ON_ORDER
		, XY.RSRV_OTP_READY
		, XY.RSRV_OTP_ON_ORDER
		, XY.EXP_DELIVERY
		, XY.STATUS_LINE
FROM	(
			SELECT	'AUTOJAYA' AS COMPANY
					, SHA.[No_] AS NO_SO
					, CASE WHEN SUBSTRING( SHA.[No_], 1, 4) = 'SOAS' THEN 'SC' ELSE
						CASE WHEN SUBSTRING( SHA.[No_], 1, 2) IN ('8S','9S') THEN 'AI' ELSE 'TD' END
						END AS TIPE
					, ISNULL(CAB.[Name], 'JAKARTA') AS CABANG
					, CASE WHEN SHA.[Salesperson Code] = '' THEN '.' ELSE SHA.[Salesperson Code] END AS SALES_CODE
					, SLS.[Name] AS SALES_NAME
					, SHA.[Sell-to Customer No_] AS DC_NAME
					, SHA.[Bill-to Customer No_] AS CUST_CODE
					, SHA.[Bill-to Name] AS CUST_NAME
					, SHA.[Order Date] AS ORDER_DATE
					, 'RELEASED' AS STATUS_SO
					, SHA.[Payment Terms Code] AS TERM_OF_PAY
					, SHA.[External Document No_] AS NO_PO_CUST
					, SHA.[Currency Code] AS CURR_CODE
					, SHA.[Currency Factor] AS CURR_FAKTOR
					, ISNULL(CASE WHEN SHA.[Currency Code] <> '' THEN ROUND(SLA.Amount / SHA.[Currency Factor],0) ELSE SLA.Amount END,0) AS AMOUNT_IDR
					, ISNULL(SLA.Amount,0) AS AMOUNT
					, SLA.[Blanket Order No_] AS NO_BSO
					, SLA.[Line No_] AS  NO_SO_LINE
					, SLA.[No_] AS NO_ITEM
					, SLA.[Description] AS ITEM_DESC
					, SLA.[Quantity] AS QTY
					, SLA.[Quantity] AS QTY_SHIPPED
					, 0 AS QTY_OUT
					, 0 AS RSRV_OH
					, 0 AS RSRV_ON_ORDER
					, 0 AS RSRV_OTP_READY
					, 0 AS RSRV_OTP_ON_ORDER
					, SHA.[Requested Delivery Date] AS EXP_DELIVERY
					, 'INVOICED' AS STATUS_LINE
			FROM [dbo].[BC_AUTOJAYA$Sales Header Archive] SHA
			INNER JOIN	(
							SELECT SHA1.[Document Type]
									, SHA1.[No_]
									, MAX([Version No_]) AS [Version No_]
							FROM [dbo].[BC_AUTOJAYA$Sales Header Archive] SHA1
							LEFT JOIN [dbo].[BC_AUTOJAYA$Sales Header] SH1 ON SH1.[Document Type] = SHA1.[Document Type] AND SH1.[No_] = SHA1.[No_]
							WHERE SH1.[No_] IS NULL
							GROUP BY SHA1.[Document Type], SHA1.[No_]
						) SH ON SH.[Document Type] = SHA.[Document Type] AND SH.[No_] = SHA.[No_] AND SH.[Version No_] = SHA.[Version No_]
			INNER JOIN [dbo].[BC_AUTOJAYA$Sales Line Archive] SLA ON SHA.[Document Type] = SLA.[Document Type] AND SHA.[No_] = SLA.[Document No_] AND SHA.[Version No_] = SLA.[Version No_]
			LEFT JOIN [dbo].[BC_AUTOJAYA$Responsibility Center] CAB ON CAB.[Code] = SHA.[Shortcut Dimension 1 Code]
			LEFT JOIN [dbo].[BC_AUTOJAYA$Salesperson_Purchaser] SLS ON SLS.[Code] = SHA.[Salesperson Code]
			LEFT JOIN [dbo].[BC_AUTOJAYA$Item] ITM ON ITM.[No_] = SLA.[No_]
			WHERE	(SHA.[Document Type] = 1 AND SLA.[Type] <> 0)
			UNION ALL
			SELECT	'SOLUSI' AS COMPANY
					, SHA.[No_] AS NO_SO
					, CASE WHEN SUBSTRING( SHA.[No_], 1, 4) = 'SOAS' THEN 'SC' ELSE
						CASE WHEN SUBSTRING( SHA.[No_], 1, 2) IN ('8S','9S') THEN 'AI' ELSE 'TD' END
						END AS TIPE
					, ISNULL(CAB.[Name], 'JAKARTA') AS CABANG
					, CASE WHEN SHA.[Salesperson Code] = '' THEN '.' ELSE SHA.[Salesperson Code] END AS SALES_CODE
					, SLS.[Name] AS SALES_NAME
					, SHA.[Sell-to Customer No_] AS DC_NAME
					, SHA.[Bill-to Customer No_] AS CUST_CODE
					, SHA.[Bill-to Name] AS CUST_NAME
					, SHA.[Order Date] AS ORDER_DATE
					, 'RELEASED' AS STATUS_SO
					, SHA.[Payment Terms Code] AS TERM_OF_PAY
					, SHA.[External Document No_] AS NO_PO_CUST
					, SHA.[Currency Code] AS CURR_CODE
					, SHA.[Currency Factor] AS CURR_FAKTOR
					, ISNULL(CASE WHEN SHA.[Currency Code] <> '' THEN ROUND(SLA.Amount / SHA.[Currency Factor],0) ELSE SLA.Amount END,0) AS AMOUNT_IDR
					, ISNULL(SLA.Amount,0) AS AMOUNT
					, SLA.[Blanket Order No_] AS NO_BSO
					, SLA.[Line No_] AS  NO_SO_LINE
					, SLA.[No_] AS NO_ITEM
					, SLA.[Description] AS ITEM_DESC
					, SLA.[Quantity] AS QTY
					, SLA.[Quantity] AS QTY_SHIPPED
					, 0 AS QTY_OUT
					, 0 AS RSRV_OH
					, 0 AS RSRV_ON_ORDER
					, 0 AS RSRV_OTP_READY
					, 0 AS RSRV_OTP_ON_ORDER
					, SHA.[Requested Delivery Date] AS EXP_DELIVERY
					, 'INVOICED' AS STATUS_LINE
			FROM [dbo].[BC_SOLUSI$Sales Header Archive] SHA
			INNER JOIN	(
							SELECT SHA1.[Document Type]
									, SHA1.[No_]
									, MAX([Version No_]) AS [Version No_]
							FROM [dbo].[BC_SOLUSI$Sales Header Archive] SHA1
							LEFT JOIN [dbo].[BC_SOLUSI$Sales Header] SH1 ON SH1.[Document Type] = SHA1.[Document Type] AND SH1.[No_] = SHA1.[No_]
							WHERE SH1.[No_] IS NULL
							GROUP BY SHA1.[Document Type], SHA1.[No_]
						) SH ON SH.[Document Type] = SHA.[Document Type] AND SH.[No_] = SHA.[No_] AND SH.[Version No_] = SHA.[Version No_]
			INNER JOIN [dbo].[BC_SOLUSI$Sales Line Archive] SLA ON SHA.[Document Type] = SLA.[Document Type] AND SHA.[No_] = SLA.[Document No_] AND SHA.[Version No_] = SLA.[Version No_]
			LEFT JOIN [dbo].[BC_AUTOJAYA$Responsibility Center] CAB ON CAB.[Code] = SHA.[Shortcut Dimension 1 Code]
			LEFT JOIN [dbo].[BC_AUTOJAYA$Salesperson_Purchaser] SLS ON SLS.[Code] = SHA.[Salesperson Code]
			LEFT JOIN [dbo].[BC_SOLUSI$Item] ITM ON ITM.[No_] = SLA.[No_]
			WHERE	(SHA.[Document Type] = 1 AND SLA.[Type] <> 0)
		) XY
WHERE
		(XY.DC_NAME= $P{DCName} OR   $P{DCName} = 'X') AND
		(XY.CABANG =  $P{Cabang} OR $P{Cabang} = 'ALL') AND
		(XY.SALES_CODE = $P{Sales} OR $P{Sales} = 'ALL') AND
		(XY.NO_SO >= $P{NoSOFrom} AND XY.NO_SO <= $P{NoSOUpto}) AND
		(XY.CUST_CODE = $P{Customer} OR $P{Customer} = 'ALL') AND
		($P{SudahInvoice} ='YES' OR $P{SudahInvoice} ='ALL')
		) XXX
WHERE (XXX.COMPANY = $P{Company} OR $P{Company} = 'ALL') AND
	(XXX.NO_BSO >= $P{NoBSOFrom} AND XXX.NO_BSO <= $P{NoBSOUpto}) AND
	(XXX.STATUS_SO = 'RELEASED') AND
	(XXX.ORDER_DATE >= $P{TglOrderFrom} AND XXX.ORDER_DATE <= $P{TglOrderUpto}) AND
	((XXX.NO_PO_CUST LIKE $P{NoPOCust}+'%' AND $P{NoPOCust} <> 'ALL') OR ($P{NoPOCust} = 'ALL'))]]>
	</queryString>
