<?xml version="1.0" encoding="UTF-8" ?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:w="http://www.zkoss.org/2005/zk/client"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul/zul.xsd">

	<window id="windowT21DaftarPenawaranSalesMain" apply="${T21DaftarPenawaranSalesMainCtrl}"	border="none" width="100%">

		<div sclass="z-toolbar" style="padding:0">
			<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"	width="100%">

				<!-- COMMON BUTTONS -->
				<toolbar align="start" style="float:left; border-style: none;">
					<button id="btnSearch" height="24px" />
					<button id="btnClear" height="24px" />
				</toolbar>

				<!-- CRUD BUTTONS -->
				<toolbar align="end"
					style="float:right; border-style: none;">
					<button id="btnNew" height="24px" />
					<button id="btnEdit" height="24px" />
					<button id="btnDelete" height="24px" />
					<button id="btnListing" height="24px" />
					<button id="btnSave" height="24px" />
					<button id="btnOK" height="24px" />
					<button id="btnCancel" height="24px" />
					<button id="btnAwarded" height="24px" />					
					<button id="btnLost" height="24px" />
					<button id="btnPostponed" height="24px" />
					<button id="btnCanceled" height="24px" />
					<button id="btnClosed" height="24px" />
				</toolbar>
			</hbox>
		</div>


		<tabbox id="tabbox_T21DaftarPenawaranSalesMain">
			<tabs>
				<tab id="tabT21DaftarPenawaranSalesList"
					label="${c:l('common.List')}" />
				<tab id="tabT21DaftarPenawaranSalesDetail"
					label="${c:l('common.Details')}" />
				<tab id="tabT21DaftarPenawaranSalesPrint"
					label="${c:l('common.Print')}" />
			</tabs>

			<tabpanels>
				<!-- TabPanel LIST / LISTE -->
				<tabpanel id="tabPanelT21DaftarPenawaranSalesList" height="100%"
					style="border: none; padding: 0px; " />

				<!-- TabPanel DETAILS / DETAILS -->
				<tabpanel id="tabPanelT21DaftarPenawaranSalesDetail" height="100%"
					style="border: none; padding: 0px; " />
					
				<!-- TabPanel LISTING -->
				<tabpanel id="tabPanelT21DaftarPenawaranSalesPrint" height="100%"
					style="border: none; padding: 0px; " />

			</tabpanels>
		</tabbox>


	</window>
</zk>

