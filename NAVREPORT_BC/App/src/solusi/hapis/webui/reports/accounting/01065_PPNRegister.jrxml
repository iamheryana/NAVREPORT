								SUM(CASE WHEN CML.[No_] = '230.190.00' THEN CML.[Amount] ELSE 0 END))/ CMH.[Currency Factor]),2)
							END * -1 AS PPN
						, CASE WHEN SIH.[No_] IS NOT NULL THEN 'NORMAL' ELSE 'CASE' END AS STATUS_DOK
						, SIH.[No_] AS NO_DOK_LAWAN
						, SIH.[Posting Date] AS TGL_DOK_LAWAN
						, EFHL.[Faktur Pajak No_] AS NO_FP_LAWAN
						, EFHL.[Faktur Pajak Date] AS TGL_FP_LAWAN
				FROM [dbo].[BC_AUTOJAYA$Sales Cr_Memo Header] CMH
				INNER JOIN [dbo].[BC_AUTOJAYA$Sales Cr_Memo Line] CML ON CML.[Document No_] = CMH.[No_]
				LEFT JOIN [dbo].[BC_AUTOJAYA$Sales Invoice Header] SIH ON CMH.[Applies-to Doc_ No_] = SIH.[No_]
				LEFT JOIN [dbo].[BC_AUTOJAYA$E-Faktur Register Header - n] EFH ON EFH.[Document Type] = 6 AND EFH.[No_] = CMH.[No_]
				LEFT JOIN [dbo].[BC_AUTOJAYA$E-Faktur Register Header - n] EFHL ON EFHL.[Document Type] = 5 AND EFHL.[No_] = SIH.[No_]
