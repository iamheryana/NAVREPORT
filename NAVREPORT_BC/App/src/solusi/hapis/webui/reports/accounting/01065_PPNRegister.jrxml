		, Y.NO_FP
		, REPLACE(REPLACE(Y.NO_FP, '.', ''), '-','') AS NO_FP_CLEAN
		, CAST(Y.DPP AS BIGINT) AS DPP
		, CAST(X.PPN AS BIGINT) AS PPN
		, X.PPN AS PPN_ON_NAV
		, (X.PPN - CAST(X.PPN AS BIGINT)) AS PPN_DECIMAL
		, Y.STATUS_DOK
		, Y.TGL_DOK_LAWAN
		, Y.NO_DOK_LAWAN
		, Y.TGL_FP_LAWAN
		, Y.NO_FP_LAWAN
FROM	(
			SELECT	GLE.[G_L Account No_] AS ACC_NO
					, GLE.[Posting Date] AS TGL_POSTING
					, GLE.[Document No_] AS NO_DOK
					, SUM(GLE.[Amount]) * -1 AS PPN
			FROM [dbo].[BC_SOLUSI$G_L Entry] GLE
			LEFT JOIN [dbo].[BC_SOLUSI$G_L Account] GLA ON GLA.[No_] = GLE.[G_L Account No_]
			WHERE	GLE.[G_L Account No_] = '230.190.00'
					AND GLE.[Source Code] = 'SALES'
			GROUP BY GLE.[G_L Account No_]
					, GLE.[Posting Date]
					, GLE.[Document No_]
		) X
LEFT JOIN	(
				SELECT	'INVOICE' AS TIPE_DOK
						, SIH.[No_] AS NO_DOK
						, SIH.[Posting Date] AS TGL_DOK
						, SIH.[Bill-to Customer No_] AS CUST_SUPP
						, SIH.[Bill-to Name] AS CUST_SUPP_NAME
						, SIH.[External Document No_] AS NO_PO_CUST
						, EFH.[Faktur Pajak No_] AS NO_FP
						, EFH.[Faktur Pajak Date] AS TGL_FP
						,	CASE WHEN SIH.[Currency Code] = '' THEN
								SUM(CASE WHEN SIL.[No_] = '230.190.00' THEN 0 ELSE SIL.[Amount] END)
							ELSE
								ROUND((SUM(CASE WHEN SIL.[No_] = '230.190.00' THEN 0 ELSE SIL.[Amount] END)/ SIH.[Currency Factor]),2)
							END AS DPP
						,	CASE WHEN SIH.[Currency Code] = '' THEN
								(SUM(SIL.[Amount Including VAT] - SIL.[Amount]) +
								SUM(CASE WHEN SIL.[No_] = '230.190.00' THEN SIL.[Amount] ELSE 0 END))
							ELSE
								ROUND(((SUM(SIL.[Amount Including VAT] - SIL.[Amount]) +
								SUM(CASE WHEN SIL.[No_] = '230.190.00' THEN SIL.[Amount] ELSE 0 END))/ SIH.[Currency Factor]),2)
							END AS PPN
						, CASE WHEN SCM.[No_] IS NOT NULL THEN 'CANCEL' ELSE 'NORMAL' END AS STATUS_DOK
						, SCM.[No_] AS NO_DOK_LAWAN
						, SCM.[Posting Date] AS TGL_DOK_LAWAN
						, EFHL.[Faktur Pajak No_] AS NO_FP_LAWAN
						, EFHL.[Faktur Pajak Date] AS TGL_FP_LAWAN
				FROM [dbo].[BC_SOLUSI$Sales Invoice Header] SIH
				INNER JOIN [dbo].[BC_SOLUSI$Sales Invoice Line] SIL ON SIL.[Document No_] = SIH.[No_]
				LEFT JOIN [dbo].[BC_SOLUSI$Sales Cr_Memo Header] SCM ON SCM.[Applies-to Doc_ No_] = SIH.[No_] AND SCM.[Source Code] <> 'DELETE'
				LEFT JOIN [dbo].[BC_SOLUSI$E-Faktur Register Header - n] EFH ON EFH.[Document Type] = 5 AND EFH.[No_] = SIH.[No_]
				LEFT JOIN [dbo].[BC_SOLUSI$E-Faktur Register Header - n] EFHL ON EFHL.[Document Type] = 6 AND EFHL.[No_] = SCM.[No_]
				GROUP BY SIH.[No_]
						, SIH.[Posting Date]
						, SIH.[Bill-to Customer No_]
						, SIH.[Bill-to Name]
						, SCM.[No_]
						, SCM.[Posting Date]
						, SIH.[External Document No_]
						, SIH.[Currency Code]
						, SIH.[Currency Factor]
						, EFH.[Faktur Pajak No_]
						, EFH.[Faktur Pajak Date]
						, EFHL.[Faktur Pajak No_]
						, EFHL.[Faktur Pajak Date]
				UNION ALL
				SELECT	'CREDIT MEMO' AS TIPE_DOK
						, CMH.[No_] AS NO_DOK
						, CMH.[Posting Date] AS TGL_DOK
						, CMH.[Bill-to Customer No_] AS CUST_SUPP
						, CMH.[Bill-to Name] AS CUST_SUPP_NAME
						, CMH.[External Document No_] AS NO_PO_CUST
						, EFH.[Faktur Pajak No_] AS NO_FP
						, EFH.[Faktur Pajak Date] AS TGL_FP
						,	CASE WHEN CMH.[Currency Code] = '' THEN
								SUM(CASE WHEN CML.[No_] = '230.190.00' THEN 0 ELSE CML.[Amount] END)
							ELSE
								ROUND((SUM(CASE WHEN CML.[No_] = '230.190.00' THEN 0 ELSE CML.[Amount] END)/ CMH.[Currency Factor]),2)
							END * -1 AS DPP
						,	CASE WHEN CMH.[Currency Code] = '' THEN
								(SUM(CML.[Amount Including VAT] - CML.[Amount]) +
								SUM(CASE WHEN CML.[No_] = '230.190.00' THEN CML.[Amount] ELSE 0 END))
							ELSE
								ROUND(((SUM(CML.[Amount Including VAT] - CML.[Amount]) +
								SUM(CASE WHEN CML.[No_] = '230.190.00' THEN CML.[Amount] ELSE 0 END))/ CMH.[Currency Factor]),2)
							END * -1 AS PPN
						, CASE WHEN SIH.[No_] IS NOT NULL THEN 'NORMAL' ELSE 'CASE' END AS STATUS_DOK
						, SIH.[No_] AS NO_DOK_LAWAN
						, SIH.[Posting Date] AS TGL_DOK_LAWAN
						, EFHL.[Faktur Pajak No_] AS NO_FP_LAWAN
						, EFHL.[Faktur Pajak Date] AS TGL_FP_LAWAN
				FROM [dbo].[BC_SOLUSI$Sales Cr_Memo Header] CMH
				INNER JOIN [dbo].[BC_SOLUSI$Sales Cr_Memo Line] CML ON CML.[Document No_] = CMH.[No_]
				LEFT JOIN [dbo].[BC_SOLUSI$Sales Invoice Header] SIH ON CMH.[Applies-to Doc_ No_] = SIH.[No_]
				LEFT JOIN [dbo].[BC_SOLUSI$E-Faktur Register Header - n] EFH ON EFH.[Document Type] = 6 AND EFH.[No_] = CMH.[No_]
				LEFT JOIN [dbo].[BC_SOLUSI$E-Faktur Register Header - n] EFHL ON EFHL.[Document Type] = 5 AND EFHL.[No_] = SIH.[No_]
				WHERE CMH.[Source Code] <> 'DELETE'
				GROUP BY CMH.[No_]
						, CMH.[Posting Date]
						, CMH.[Bill-to Customer No_]
						, CMH.[Bill-to Name]
						, SIH.[No_]
						, SIH.[Posting Date]
						, CMH.[External Document No_]
						, CMH.[Currency Code]
						, CMH.[Currency Factor]
						, EFH.[Faktur Pajak No_]
						, EFH.[Faktur Pajak Date]
						, EFHL.[Faktur Pajak No_]
						, EFHL.[Faktur Pajak Date]
			) Y ON Y.NO_DOK = X.NO_DOK
WHERE	($P{Company} = 'SOLUSI') AND
		($P{Tipe} = 'SALES') AND
		(X.TGL_POSTING >=$P{TglInvFrom} AND X.TGL_POSTING<=$P{TglInvTo})]]>
	</queryString>
