				INNER JOIN [dbo].[BC_AUTOJAYA$Sales Invoice Line] SIL ON SIL.[Document No_] = SIH.[No_]
				LEFT JOIN [dbo].[BC_AUTOJAYA$Sales Cr_Memo Header] SCM ON SCM.[Applies-to Doc_ No_] = SIH.[No_] AND SCM.[Source Code] <> 'DELETE'
				LEFT JOIN [dbo].[BC_AUTOJAYA$E-Faktur Register Header - n] EFH ON EFH.[Document Type] = 5 AND EFH.[No_] = SIH.[No_]
				LEFT JOIN [dbo].[BC_AUTOJAYA$E-Faktur Register Header - n] EFHL ON EFHL.[Document Type] = 6 AND EFHL.[No_] = SCM.[No_]
				GROUP BY SIH.[No_]
						, SIH.[Posting Date]
						, SIH.[Bill-to Customer No_]
						, SIH.[Bill-to Name]
						, SCM.[No_]
						, SCM.[Posting Date]
						, SIH.[External Document No_]
						, SIH.[Currency Code]
						, SIH.[Currency Factor]
						, EFH.[Faktur Pajak No_]
						, EFH.[Faktur Pajak Date]
						, EFHL.[Faktur Pajak No_]
						, EFHL.[Faktur Pajak Date]
