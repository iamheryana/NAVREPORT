	<property name="ireport.y" value="206"/>
	<parameter name="TglFrom" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="TglUpto" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Company" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT	XXX.GROUP_MAIN
		, XXX.GROUPS
		,	CASE	WHEN XXX.GROUPS = '(A.1)' THEN 'Piutang Sesuai Rumus'
					WHEN XXX.GROUPS = '(B.1)' THEN 'Penerimaan  Kas & Bank'
					WHEN XXX.GROUPS = '(B.2)' THEN 'Offsett AR dengan Lain-lain'
					WHEN XXX.GROUPS = '(B.3)' THEN 'Intercoy'
					WHEN XXX.GROUPS = '(B.4)' THEN 'PPh Dipotong Pihak Lain / PPn tidak dipungut'
					ELSE ''
			END AS GROUPS_DESC
		, XXX.SUB_GROUP
		, XXX.SUB_GROUP_DESC
		, XXX.SUB_DTL
		, XXX.AMOUNT_TOTAL
		, XXX.AMT_USAHA
		, XXX.AMT_USAHA_HIT
		, XXX.AMT_USAHA_AR
		, XXX.AMT_USAHA_UP
		, XXX.AMT_NON_USAHA
FROM	(
SELECT	'X1' AS GROUP_MAIN
		,'(A.1)' AS GROUPS
		, 1 AS SUB_GROUP
		, 'Saldo Awal - Piutang' AS SUB_GROUP_DESC
		, X.SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, SUM(X.AMOUNT) AS AMT_USAHA
		,  SUM(X.AMOUNT) AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM	(
			SELECT '1. Piutang Dagang' AS SUB_DTL, 0 AS AMOUNT
			UNION ALL
			SELECT '2. Piutang Dagang Antar Company' AS SUB_DTL, 0 AS AMOUNT
			UNION ALL
			SELECT '3. Piutang (Sponsorship)' AS SUB_DTL, 0 AS AMOUNT
			UNION ALL
			SELECT
					CASE	WHEN [G_L Account No_] = '114.110.00' THEN '1. Piutang Dagang'
							WHEN [G_L Account No_] = '114.140.00' THEN '2. Piutang Dagang Antar Company'
							ELSE '' END	AS SUB_DTL
					, [Amount] AS AMOUNT
			FROM [dbo].[AUTOJAYA$G_L Entry]
			WHERE	[G_L Account No_] IN ('114.110.00', '114.140.00')
					AND [Posting Date] < $P{TglFrom}
			UNION ALL
			SELECT '3. Piutang (Sponsorship)' AS SUB_DTL, SUM(GLE.[Amount]) AS AMOUNT
			FROM [dbo].[AUTOJAYA$G_L Entry] GLE
			WHERE	GLE.[G_L Account No_] IN ('114.155.00')
					AND GLE.[Source Code] = 'SALES'
					AND GLE.[Posting Date] < $P{TglFrom}
			UNION ALL
			SELECT '3. Piutang (Sponsorship)' AS SUB_DTL, SUM(X.APPLIED_AMT) AS AMOUNT
			FROM	(
						SELECT	CID.[Posting Date] AS TGL_INV
								, CID.[Document No_] AS NO_INV
								, DCL.[Cust_ Ledger Entry No_] AS INV_ID
								, DCL.[Posting Date] AS APPLIED_TGL
								, DCL.[Applied Cust_ Ledger Entry No_] AS APPLIED_ID
								, APL.[Document No_] AS APPLIED_DOC
								, DCL.[Amount] AS APPLIED_AMT
						FROM [dbo].[AUTOJAYA$Detailed Cust_ Ledg_ Entry] DCL
						LEFT JOIN [dbo].[AUTOJAYA$Cust_ Ledger Entry] APL ON APL.[Entry No_] = DCL.[Applied Cust_ Ledger Entry No_]
						LEFT JOIN [dbo].[AUTOJAYA$Cust_ Ledger Entry] CID ON CID.[Entry No_] = DCL.[Cust_ Ledger Entry No_]
						LEFT JOIN [dbo].[AUTOJAYA$G_L Entry] GLE ON GLE.[Entry No_] =  DCL.[Cust_ Ledger Entry No_]
						WHERE	DCL.[Document Type] <> 2 AND
								DCL.[Applied Cust_ Ledger Entry No_] <> 0 AND
								GLE.[G_L Account No_] IN ('114.155.00') AND
								GLE.[Source Code] = 'SALES' AND
								GLE.[Entry No_] IS NOT NULL AND
								DCL.[Posting Date] < $P{TglFrom}
					) X
		) X
GROUP BY X.SUB_DTL
UNION ALL
SELECT	'X1' AS GROUP_MAIN
		,'(A.1)' AS GROUPS
		, 2 AS SUB_GROUP
		,'Sales' AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, SUM([Amount]) * -1  AS AMT_USAHA
		, SUM([Amount]) * -1  AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM [dbo].[AUTOJAYA$G_L Entry]
WHERE	([G_L Account No_] BETWEEN '400.110.00' AND'420.110.01')
		AND ([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
UNION ALL
SELECT	'X1' AS GROUP_MAIN
		,'(A.1)' AS GROUPS
		, 3 AS SUB_GROUP
		,'PPN Keluaran' AS SUB_GROUP_DESC
		,CASE WHEN X.SUB_DTL = 'A' THEN 'A. PPN Keluaran 10 %' ELSE 'B. PPN Keluaran WAPU / KWS. BERIKAT / BENDAHARA' END AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, FLOOR(CASE WHEN X.SUB_DTL = 'A' THEN SUM(SUM(X.AMOUNT)) OVER (PARTITION BY 1) * -1 ELSE SUM(X.AMOUNT) END) AS AMT_USAHA
		, FLOOR(CASE WHEN X.SUB_DTL = 'A' THEN SUM(SUM(X.AMOUNT)) OVER (PARTITION BY 1) * -1 ELSE SUM(X.AMOUNT) END) AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM	(
			SELECT 'A' AS SUB_DTL, 0 AS AMOUNT
			UNION ALL
			SELECT 'B' AS SUB_DTL, 0 AS AMOUNT
			UNION ALL
			SELECT	CASE WHEN PPN.PPN_ID = 'EFID0003' THEN 'A' ELSE 'B' END AS SUB_DTL
					, GLE.[Amount] AS AMOUNT
			FROM [dbo].[AUTOJAYA$G_L Entry] GLE
			LEFT JOIN	(
							SELECT [No_]
									, [E-Faktur Register Mapping ID] AS PPN_ID
							FROM [dbo].[AUTOJAYA$E-Faktur Register Header - n]
							WHERE [Document Type] = 5
							UNION ALL
							SELECT	SCM.[No_]
									, EFK.[E-Faktur Register Mapping ID] AS PPN_ID
							FROM [dbo].[AUTOJAYA$Sales Cr_Memo Header] SCM
							LEFT JOIN  [dbo].[AUTOJAYA$E-Faktur Register Header - n] EFK ON EFK.[No_] = SCM.[Applies-to Doc_ No_] AND  EFK.[Document Type] = 5
						) PPN ON PPN.[No_] = GLE.[Document No_]
			WHERE (GLE.[G_L Account No_]='230.190.00')
					AND (GLE.[Posting Date] >= $P{TglFrom} AND GLE.[Posting Date] <= $P{TglUpto})
					AND GLE.[Source Code] = 'SALES'
		) X
GROUP BY X.SUB_DTL
UNION ALL
SELECT	'X1' AS GROUP_MAIN
		,'(A.1)' AS GROUPS
		, 4 AS SUB_GROUP
		,'Saldo Akhir - Piutang' AS SUB_GROUP_DESC
		, X.SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, SUM(X.AMOUNT) * -1 AS AMT_USAHA
		, SUM(X.AMOUNT) * -1 AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM	(
			SELECT '1. Piutang Dagang' AS SUB_DTL, 0 AS AMOUNT
			UNION ALL
			SELECT '2. Piutang Dagang Antar Company' AS SUB_DTL, 0 AS AMOUNT
			UNION ALL
			SELECT '3. Piutang (Sponsorship)' AS SUB_DTL, 0 AS AMOUNT
			UNION ALL
			SELECT
					CASE	WHEN [G_L Account No_] = '114.110.00' THEN '1. Piutang Dagang'
							WHEN [G_L Account No_] = '114.140.00' THEN '2. Piutang Dagang Antar Company'
							ELSE '' END	AS SUB_DTL
					, [Amount] AS AMOUNT
			FROM [dbo].[AUTOJAYA$G_L Entry]
			WHERE	[G_L Account No_] IN ('114.110.00', '114.140.00')
					AND [Posting Date] <= $P{TglUpto}
			UNION ALL
			SELECT '3. Piutang (Sponsorship)' AS SUB_DTL, SUM(GLE.[Amount]) AS AMOUNT
			FROM [dbo].[AUTOJAYA$G_L Entry] GLE
			WHERE	GLE.[G_L Account No_] IN ('114.155.00')
					AND GLE.[Source Code] = 'SALES'
					AND GLE.[Posting Date] <= $P{TglUpto}
			UNION ALL
			SELECT '3. Piutang (Sponsorship)' AS SUB_DTL, SUM(X.APPLIED_AMT) AS AMOUNT
			FROM	(
						SELECT	CID.[Posting Date] AS TGL_INV
								, CID.[Document No_] AS NO_INV
								, DCL.[Cust_ Ledger Entry No_] AS INV_ID
								, DCL.[Posting Date] AS APPLIED_TGL
								, DCL.[Applied Cust_ Ledger Entry No_] AS APPLIED_ID
								, APL.[Document No_] AS APPLIED_DOC
								, DCL.[Amount] AS APPLIED_AMT
						FROM [dbo].[AUTOJAYA$Detailed Cust_ Ledg_ Entry] DCL
						LEFT JOIN [dbo].[AUTOJAYA$Cust_ Ledger Entry] APL ON APL.[Entry No_] = DCL.[Applied Cust_ Ledger Entry No_]
						LEFT JOIN [dbo].[AUTOJAYA$Cust_ Ledger Entry] CID ON CID.[Entry No_] = DCL.[Cust_ Ledger Entry No_]
						LEFT JOIN [dbo].[AUTOJAYA$G_L Entry] GLE ON GLE.[Entry No_] =  DCL.[Cust_ Ledger Entry No_]
						WHERE	DCL.[Document Type] <> 2 AND
								DCL.[Applied Cust_ Ledger Entry No_] <> 0 AND
								GLE.[G_L Account No_] IN ('114.155.00') AND
								GLE.[Source Code] = 'SALES' AND
								GLE.[Entry No_] IS NOT NULL AND
								DCL.[Posting Date] <= $P{TglUpto}
					) X
		) X
GROUP BY X.SUB_DTL
UNION ALL
SELECT	'X2' AS GROUP_MAIN
		,'(B.1)' AS GROUPS
		, HDR.NO_URUT AS SUB_GROUP
		, HDR.KETERANGAN AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, HDR.AMOUNT_TOTAL AS AMOUNT_TOTAL
		, DTL.AMT_USAHA_AR AS AMT_USAHA
		, DTL.AMT_USAHA_AR * -1 AS AMT_USAHA_HIT
		, DTL.AMT_USAHA_AR AS AMT_USAHA_AR
		, DTL.AMT_USAHA_UP AS AMT_USAHA_UP
		, DTL.AMT_NON_USAHA
FROM	(
			SELECT	ROW_NUMBER() OVER (PARTITION BY 1 ORDER BY  GLE.[G_L Account No_], BAC.[No_]) AS NO_URUT
					,BAC.[No_] AS KODE_BANK
					,BAC.[Name] AS KETERANGAN
					,SUM(GLE.[Debit Amount]) AS AMOUNT_TOTAL
			FROM [dbo].[AUTOJAYA$G_L Entry] GLE
			LEFT JOIN [dbo].[AUTOJAYA$Bank Account] BAC ON BAC.[No_] = GLE.[Source No_]
			WHERE	(
						(GLE.[G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
						(GLE.[G_L Account No_] BETWEEN '112.110.00' AND '112.162.00')
					)
					AND (GLE.[Posting Date] >= $P{TglFrom} AND GLE.[Posting Date] <= $P{TglUpto})
					AND GLE.[Debit Amount] <> 0
			GROUP BY BAC.[Name], BAC.[No_], GLE.[G_L Account No_]
		) HDR
LEFT JOIN	(
				SELECT	XX.[Source No_]
						, SUM(CASE WHEN (XX.ADA_AR + XX.ADA_AR_SPONSOR + XX.ADA_AR_IC) > 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_USAHA_AR
						, SUM(CASE WHEN (XX.ADA_UN_PAY) > 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_USAHA_UP
						, SUM(CASE WHEN (XX.ADA_AR + XX.ADA_AR_SPONSOR + XX.ADA_AR_IC + XX.ADA_UN_PAY) = 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_NON_USAHA
				FROM	(
							SELECT	X.[Posting Date]
									, X.[Document No_]
									, X.[Transaction No_]
									, X.[G_L Account No_]
									, X.[Source No_]
									, X.JENIS_ACC
									, SUM(X.ADA_AR) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_AR
									, SUM(X.ADA_AR_SPONSOR) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_AR_SPONSOR
									, SUM(X.ADA_AR_IC) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_AR_IC
									, SUM(X.ADA_UN_PAY) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_UN_PAY
									, X.[Debit Amount]
							FROM	(
										SELECT	[Posting Date]
												, [Document No_]
												, [Transaction No_]
												, [G_L Account No_]
												, [Source No_]
												,	CASE WHEN	([G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
																([G_L Account No_] BETWEEN '112.110.00' AND '112.162.00') THEN
														'BANK'
													ELSE
														'NON' END AS JENIS_ACC
												, CASE WHEN [G_L Account No_] = '114.110.00' THEN 1 ELSE 0 END AS ADA_AR
												, CASE WHEN [G_L Account No_] = '114.155.00'  THEN 1 ELSE 0 END AS ADA_AR_SPONSOR
												, CASE WHEN [G_L Account No_] = '114.140.00'  THEN 1 ELSE 0 END AS ADA_AR_IC
												, CASE WHEN [G_L Account No_] = '220.185.00' THEN 1 ELSE 0 END AS ADA_UN_PAY
												, SUM([Debit Amount]) AS [Debit Amount]
												, SUM([Credit Amount]) AS [Credit Amount]
										FROM [dbo].[AUTOJAYA$G_L Entry]
										WHERE	([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
										GROUP BY [Posting Date]
												, [Document No_]
												, [Transaction No_]
												, [G_L Account No_]
												, [Source No_]
									) X
						) XX
				WHERE XX.JENIS_ACC =  'BANK'
				GROUP BY XX.[Source No_]
			) DTL ON DTL.[Source No_] = HDR.KODE_BANK
UNION ALL
SELECT	'X2' AS GROUP_MAIN
		, CASE WHEN X.[G_L Account No_] = '118.215.00' THEN '(B.4)' ELSE '(B.2)' END AS GROUPS
		, CASE WHEN X.[G_L Account No_] = '118.215.00' THEN 2 ELSE ROW_NUMBER() OVER (PARTITION BY 1 ORDER BY  X.[G_L Account No_]) END AS SUB_GROUP
		, CASE WHEN X.[G_L Account No_] = '118.215.00' THEN 'PPh Pasal 23' ELSE GLA.[Name] END AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, SUM(X.[Debit Amount] - X.[Credit Amount]) AS AMT_USAHA
		, SUM(X.[Debit Amount] - X.[Credit Amount]) * -1 AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM	(
			SELECT
					GLE.[Posting Date]
					, GLE.[Document No_]
					, GLE.[Transaction No_]
					, GLE.[G_L Account No_]
					, CASE WHEN	(	(GLE.[G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
									(GLE.[G_L Account No_] BETWEEN '112.110.00' AND '112.162.00') OR
									GLE.[G_L Account No_] = '114.110.00'OR
									GLE.[G_L Account No_] = '114.155.00') THEN 'ACC' ELSE 'NON' END AS GL_ACC
					, GLE.[Source No_]
					, GLE.[Source Code]
					, CASE WHEN	(	(GLE.[G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
									(GLE.[G_L Account No_] BETWEEN '112.110.00' AND '112.162.00')
								) THEN 1 ELSE 0 END AS ADA_KASBANK
					, CASE WHEN GLE.[G_L Account No_] = '220.185.00' THEN 1 ELSE 0 END AS ADA_UN_PAY
					, SUM(GLE.[Debit Amount]) AS [Debit Amount]
					, SUM(GLE.[Credit Amount]) AS [Credit Amount]
			FROM [dbo].[AUTOJAYA$G_L Entry] GLE
			WHERE GLE.[Transaction No_] IN	(
												SELECT	[Transaction No_]
												FROM [dbo].[AUTOJAYA$G_L Entry]
												WHERE	[G_L Account No_] IN ('114.110.00', '114.155.00') AND
														[Source Code] <> 'SALES' AND
														([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
											)
			GROUP BY GLE.[Posting Date]
					, GLE.[Document No_]
					, GLE.[Transaction No_]
					, GLE.[G_L Account No_]
					, GLE.[Source No_]
					, GLE.[Source Code]
		) X
LEFT JOIN [dbo].[AUTOJAYA$G_L Account] GLA ON GLA.[No_] = X.[G_L Account No_]
WHERE X.GL_ACC = 'NON'
GROUP BY X.[G_L Account No_], GLA.[Name]
UNION ALL
SELECT	'X2' AS GROUP_MAIN
		,'(B.3)' AS GROUPS
		, X.URUTAN AS SUB_GROUP
		, CASE	WHEN X.URUTAN = 1 THEN 'Pelunasan Piutang - Non Bank (Intern)'
				WHEN X.URUTAN = 2 THEN 'Pelunasan Piutang - Non Bank (Antar Company)'
				ELSE '' END AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, SUM(X.AMOUNT) AS AMT_USAHA
		, SUM(X.AMOUNT) * -1 AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
FROM	(
			SELECT 1 AS URUTAN, 0 AS AMOUNT
			UNION ALL
			SELECT 2 AS URUTAN, 0 AS AMOUNT
			UNION ALL
			SELECT	 CASE	WHEN [Source No_] IN ('AUJA001', 'AUJA002') THEN 1
							WHEN [Source No_] = 'SOPE001' THEN 2
							ELSE 3 END AS URUTAN
					, [Amount] * -1 AS AMOUNT
			FROM [dbo].[AUTOJAYA$G_L Entry]
			WHERE	(
						([G_L Account No_] BETWEEN '400.110.00' AND'420.110.01') OR
						[G_L Account No_] ='230.190.00'
					)
					AND ([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
					AND [Source No_] IN ('AUJA001', 'AUJA002','SOPE001')
		) X
GROUP BY X.URUTAN
UNION ALL
SELECT	'X2' AS GROUP_MAIN
		,'(B.4)' AS GROUPS
		, 1 AS SUB_GROUP
		, 'PPh Pasal 22 Bendahara' AS SUB_GROUP_DESC
		, '' AS SUB_DTL
		, 0 AS AMOUNT_TOTAL
		, 0 AS AMT_USAHA
		, 0 AS AMT_USAHA_HIT
		, 0 AS AMT_USAHA_AR
		, 0 AS AMT_USAHA_UP
		, 0 AS AMT_NON_USAHA
		) XXX
WHERE $P{Company} = 'AUTOJAYA']]>
	</queryString>
