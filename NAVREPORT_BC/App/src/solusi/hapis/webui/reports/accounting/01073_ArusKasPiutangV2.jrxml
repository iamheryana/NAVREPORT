	<variable name="AMT_USAHA_UP_2" class="java.math.BigDecimal" resetType="Group" resetGroup="Groups" calculation="Sum">
		<variableExpression><![CDATA[$F{AMT_USAHA_UP}]]></variableExpression>
	</variable>
	<variable name="AMT_NON_USAHA_2" class="java.math.BigDecimal" resetType="Group" resetGroup="Groups" calculation="Sum">
		<variableExpression><![CDATA[$F{AMT_NON_USAHA}]]></variableExpression>
	</variable>
	<variable name="AMT_USAHA_4" class="java.math.BigDecimal" resetType="Group" resetGroup="Groups" calculation="Sum">
		<variableExpression><![CDATA[$F{AMT_USAHA}]]></variableExpression>
	</variable>
	<variable name="AMT_USAHA_5" class="java.math.BigDecimal" resetType="Group" resetGroup="GroupMain" calculation="Sum">
		<variableExpression><![CDATA[$F{AMT_USAHA}]]></variableExpression>
