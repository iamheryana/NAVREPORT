	<queryString>
		<![CDATA[SELECT	INV.COMP
		,INV.COMPANY
		,INV.ALAMAT1
		,INV.ALAMAT2
		,INV.PHONE_FAX
		,INV.EMAIL
		,INV.WEBPAGE
		,INV.INV_NO
		,INV.TGL_INV
		,(INV.TGL_INV + 3 + 14) AS TGL_JATUH_TEMPO
		,INV.SLM
		,INV.VALUTA
		,INV.KETERANGAN
		,INV.PEMBELI
		,INV.ALAMAT_PEMBELI
		,INV.PO_CUST
		,INV.SURAT_JALAN
		, ROW_NUMBER() OVER (PARTITION BY INV.INV_NO, INV.NORUT ORDER BY INV.INV_NO,INV.NO_URUT) AS NO_ITEM
		,INV.NO_URUT
		,INV.TIPE
		,INV.NAMA_BARANG
		,INV.QTY
		,INV.HARGA_SATUAN
		,INV.JUMLAH_HARGA
		,INV.POTONGAN
		,INV.TOTAL
		,INV.JUMLAH
		,INV.UANG_MUKA
		,INV.DPP
		,INV.VAT
		,INV.YHDB
		,INV.TERBILANG
		, CASE	WHEN INV.YHDB >= 5000000  THEN 10000 ELSE 0 END AS MATERAI
		,CBA.[Name] AS ACC_DC
		,CBA.[Bank Account No_] AS ACC_NO
		,	CASE WHEN INV.KODE_CUST IN ('ALFA024', 'AL00605')
				THEN 'VA' ELSE 'NORMAL'
			END AS BA_TYPE
		,TTD.TTD
		,TTD.JABATAN
FROM	(
			SELECT	'AUTOJAYA' AS COMP
					,XY.COMPANY
					,XY.ALAMAT1
					,XY.ALAMAT2
					,XY.PHONE_FAX
					,XY.EMAIL
					,XY.WEBPAGE
					,X.INV_NO
					,X.TGL_INV
					,X.SLM
					,X.NAMA_DC
					,X.VALUTA
					,X.KETERANGAN
					,X.PEMBELI
					,X.KODE_CUST
					,X.ALAMAT_PEMBELI
					,X.PO_CUST
					,X.SURAT_JALAN
					,X.NO_URUT
					,X.NAMA_BARANG
					,X.QTY
					,X.HARGA_SATUAN
					,X.JUMLAH_HARGA
					,X.POTONGAN
					,X.TOTAL
					,X.JUMLAH
					,X.UANG_MUKA
					,X.DPP
					,X.VAT
					,X.TIPE
					,X.NORUT
					, (X.DPP+X.VAT) AS YHDB
					, (SELECT [NavBI].[dbo].terbilang((X.DPP+X.VAT))) AS TERBILANG
			FROM	(
						SELECT	SIH.[No_] AS INV_NO
								,SIH.[Posting Date] AS TGL_INV
								,SLS.[Name] AS SLM
								,SIH.[Sell-to Customer No_] AS NAMA_DC
								,CASE WHEN SIH.[Currency Code] = '' THEN 'IDR' ELSE SIH.[Currency Code] END AS VALUTA
								,(SIH.[Sell-to Customer Name] + ' / '+SIH.[Posting Description]) AS KETERANGAN
								,SIH.[Bill-to Name] AS PEMBELI
								,SIH.[Bill-to Customer No_] KODE_CUST
								,(SIH.[Bill-to Address]+' '+SIH.[Bill-to Address 2]) AS ALAMAT_PEMBELI
								,SIH.[External Document No_] AS PO_CUST
								,(

									SELECT '| '+DO.[No_] + ' | ' as 'data()'
									FROM	(
												SELECT DOH.[No_]
													  ,DOH.[Order No_]
												FROM [dbo].[BC_AUTOJAYA$Sales Shipment Header] DOH
												INNER  JOIN	(
																SELECT X.NO_DO
																FROM	(
																			SELECT [Document No_] AS NO_DO
																				  ,SUM([Quantity]) AS JML
																			FROM [dbo].[BC_AUTOJAYA$Sales Shipment Line]
																			GROUP BY [Document No_]
																		) X
																WHERE X.JML <> 0
															) DOD ON DOD.NO_DO = DOH.[No_]
											) DO
									WHERE	DO.[Order No_] = SIH.[Order No_]
									for xml path('')
									) AS SURAT_JALAN
								,SIL.[Line No_] AS NO_URUT
								,SIL.[Description] AS NAMA_BARANG
								,SIL.[Quantity] AS QTY
								,SIL.[Unit Price] AS HARGA_SATUAN
								,(SIL.[Quantity] * SIL.[Unit Price]) AS JUMLAH_HARGA
								,SIL.[Line Discount Amount] AS POTONGAN
								,SIL.[Amount] AS TOTAL
								,SIL.[Type] AS TIPE
								,CASE WHEN SIL.[Type] = 0 THEN 'NO' ELSE 'YES' END AS NORUT
								, SUM(CASE WHEN SIL.[No_] NOT IN ('400.110.03', '220.190.00') THEN SIL.[Amount] ELSE 0 END) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) AS JUMLAH
								, SUM(CASE WHEN SIL.[No_] IN ('400.110.03', '220.190.00') THEN SIL.[Amount] ELSE 0 END) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) AS UANG_MUKA
								, SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) AS DPP
								,	CASE WHEN SIH.[Posting Date] < CAST('04/01/2022' AS DATETIME) THEN
										CASE WHEN SIH.[Currency Code] <> '' THEN
											ROUND(((SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) * 0.1)),2)
										ELSE
											FLOOR((SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) * 0.1))
										END
									ELSE
										CASE WHEN SIH.[Posting Date] < CAST('01/01/2025' AS DATETIME) THEN
											CASE WHEN SIH.[Currency Code] <> '' THEN
												ROUND(((SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) * 0.11)),2)
											ELSE
												FLOOR((SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) * 0.11))
											END
										ELSE
											CASE WHEN SIH.[Currency Code] <> '' THEN
												ROUND(((SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) * 0.11)),2)
											ELSE
												FLOOR((SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) * 0.11))
											END
										END
									END AS VAT
						FROM [dbo].[BC_AUTOJAYA$Sales Invoice Header] SIH
						INNER JOIN	(
										SELECT
												SVH.[No_]
												,SOL.[Line No_]
												,SOL.[Description]
												,SOL.[Quantity]
												,SOL.[Unit Price]
												,SOL.[Line Discount Amount]
												,SOL.[Amount]
												,SOL.[Type]
										FROM [dbo].[BC_AUTOJAYA$Sales Invoice Header] SVH
										INNER JOIN	(
														SELECT	SLH.[No_]
																,SLL.[Line No_]
																,SLL.[Description]
																,SLL.[Quantity]
																,SLL.[Unit Price]
																,SLL.[Line Discount Amount]
																,SLL.[Amount]
																,SLL.[Type]
														FROM [dbo].[BC_AUTOJAYA$Sales Header] SLH
														INNER JOIN [dbo].[BC_AUTOJAYA$Sales Line] SLL ON SLL.[Document No_] = SLH.[No_] AND SLL.[Document Type] = SLH.[Document Type]
														WHERE SLH.[Document Type] = 1
														UNION ALL
														SELECT	SHA.[No_]
																,SLA.[Line No_]
																,SLA.[Description]
																,SLA.[Quantity]
																,SLA.[Unit Price]
																,SLA.[Line Discount Amount]
																,SLA.[Amount]
																,SLA.[Type]
														FROM	(
																	SELECT  [Document Type]
																			, [No_]
																			, MAX([Version No_]) AS [Version No_]
																	FROM	[dbo].[BC_AUTOJAYA$Sales Header Archive]
																	GROUP BY [Document Type], [No_]
																) SHA
														INNER JOIN [dbo].[BC_AUTOJAYA$Sales Line Archive] SLA ON SLA.[Document No_] = SHA.[No_] AND SLA.[Document Type] = SHA.[Document Type] AND SLA.[Version No_] = SHA.[Version No_]
														LEFT JOIN [dbo].[BC_AUTOJAYA$Sales Header] SLH ON SLH.[No_] = SHA.[No_] AND SLH.[Document Type] = SHA.[Document Type]
														WHERE	SHA.[Document Type] = 1 AND
																SLH.[No_] IS NULL
													) SOL ON SOL.[No_]  = SVH.[Prepayment Order No_]
										WHERE  ($P{PrintItemNol} = 'N' AND SOL.[Unit Price] <> 0) OR $P{PrintItemNol} = 'Y'
									) SIL ON SIL.[No_] = SIH.[No_]
						LEFT JOIN [dbo].[BC_AUTOJAYA$Salesperson_Purchaser] SLS ON SLS.[Code] = SIH.[Salesperson Code]
						WHERE SIH.[No_] >= $P{InvoiceFrom} AND SIH.[No_] <= $P{InvoiceUpto}
					) X
			LEFT JOIN	(
							SELECT [Name] AS COMPANY
									,[Address] AS ALAMAT1
									,[Address 2] +', '+ [City] + ' ' + [County] + ' ' +[Post Code] AS ALAMAT2
									,'Telp.: '+[Phone No_]+ ' ' +[Phone No_ 2]+', Fax. : '+[Fax No_] AS PHONE_FAX
									,'Email : '+[E-Mail] AS EMAIL
									,'Website : '+[Home Page] AS WEBPAGE
							FROM [dbo].[BC_AUTOJAYA$Company Information]
						) XY ON 1=1
			UNION ALL
			SELECT	'SOLUSI' AS COMP
					,XY.COMPANY
					,XY.ALAMAT1
					,XY.ALAMAT2
					,XY.PHONE_FAX
					,XY.EMAIL
					,XY.WEBPAGE
					,X.INV_NO
					,X.TGL_INV
					,X.SLM
					,X.NAMA_DC
					,X.VALUTA
					,X.KETERANGAN
					,X.PEMBELI
					,X.KODE_CUST
					,X.ALAMAT_PEMBELI
					,X.PO_CUST
					,X.SURAT_JALAN
					,X.NO_URUT
					,X.NAMA_BARANG
					,X.QTY
					,X.HARGA_SATUAN
					,X.JUMLAH_HARGA
					,X.POTONGAN
					,X.TOTAL
					,X.JUMLAH
					,X.UANG_MUKA
					,X.DPP
					,X.VAT
					,X.TIPE
					,X.NORUT
					, (X.DPP+X.VAT) AS YHDB
					, (SELECT [NavBI].[dbo].terbilang((X.DPP+X.VAT))) AS TERBILANG
			FROM	(
						SELECT	SIH.[No_] AS INV_NO
								,SIH.[Posting Date] AS TGL_INV
								,SLS.[Name] AS SLM
								,SIH.[Sell-to Customer No_] AS NAMA_DC
								,CASE WHEN SIH.[Currency Code] = '' THEN 'IDR' ELSE SIH.[Currency Code] END AS VALUTA
								,(SIH.[Sell-to Customer Name] + ' / '+SIH.[Posting Description]) AS KETERANGAN
								,SIH.[Bill-to Name] AS PEMBELI
								,SIH.[Bill-to Customer No_] KODE_CUST
								,(SIH.[Bill-to Address]+' '+SIH.[Bill-to Address 2]) AS ALAMAT_PEMBELI
								,SIH.[External Document No_] AS PO_CUST
								,(

									SELECT '| '+DO.[No_] + ' | ' as 'data()'
									FROM	(
												SELECT DOH.[No_]
													  ,DOH.[Order No_]
												FROM [dbo].[BC_SOLUSI$Sales Shipment Header] DOH
												INNER  JOIN	(
																SELECT X.NO_DO
																FROM	(
																			SELECT [Document No_] AS NO_DO
																				  ,SUM([Quantity]) AS JML
																			FROM [dbo].[BC_SOLUSI$Sales Shipment Line]
																			GROUP BY [Document No_]
																		) X
																WHERE X.JML <> 0
															) DOD ON DOD.NO_DO = DOH.[No_]
											) DO
									WHERE	DO.[Order No_] = SIH.[Order No_]
									for xml path('')
									) AS SURAT_JALAN
								,SIL.[Line No_] AS NO_URUT
								,SIL.[Description] AS NAMA_BARANG
								,SIL.[Quantity] AS QTY
								,SIL.[Unit Price] AS HARGA_SATUAN
								,(SIL.[Quantity] * SIL.[Unit Price]) AS JUMLAH_HARGA
								,SIL.[Line Discount Amount] AS POTONGAN
								,SIL.[Amount] AS TOTAL
								,SIL.[Type] AS TIPE
								,CASE WHEN SIL.[Type] = 0 THEN 'NO' ELSE 'YES' END AS NORUT
								, SUM(CASE WHEN SIL.[No_] NOT IN ('400.110.03', '220.190.00') THEN SIL.[Amount] ELSE 0 END) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) AS JUMLAH
								, SUM(CASE WHEN SIL.[No_] IN ('400.110.03', '220.190.00')  THEN SIL.[Amount] ELSE 0 END) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) AS UANG_MUKA
								, SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) AS DPP
								,	CASE WHEN SIH.[Posting Date] < CAST('04/01/2022' AS DATETIME) THEN
										CASE WHEN SIH.[Currency Code] <> '' THEN
											ROUND(((SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) * 0.1)),2)
										ELSE
											FLOOR((SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) * 0.1))
										END
									ELSE
										CASE WHEN SIH.[Posting Date] < CAST('01/01/2025' AS DATETIME) THEN
											CASE WHEN SIH.[Currency Code] <> '' THEN
												ROUND(((SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) * 0.11)),2)
											ELSE
												FLOOR((SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) * 0.11))
											END
										ELSE
											CASE WHEN SIH.[Currency Code] <> '' THEN
												ROUND(((SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) * 0.11)),2)
											ELSE
												FLOOR((SUM(SIL.[Amount]) OVER (PARTITION BY SIH.[No_] ORDER BY SIH.[No_]) * 0.11))
											END
										END
									END AS VAT
						FROM [dbo].[BC_SOLUSI$Sales Invoice Header] SIH
						INNER JOIN	(
										SELECT
												SVH.[No_]
												,SOL.[Line No_]
												,SOL.[Description]
												,SOL.[Quantity]
												,SOL.[Unit Price]
												,SOL.[Line Discount Amount]
												,SOL.[Amount]
												,SOL.[Type]
										FROM [dbo].[BC_SOLUSI$Sales Invoice Header] SVH
										INNER JOIN	(
														SELECT	SLH.[No_]
																,SLL.[Line No_]
																,SLL.[Description]
																,SLL.[Quantity]
																,SLL.[Unit Price]
																,SLL.[Line Discount Amount]
																,SLL.[Amount]
																,SLL.[Type]
														FROM [dbo].[BC_SOLUSI$Sales Header] SLH
														INNER JOIN [dbo].[BC_SOLUSI$Sales Line] SLL ON SLL.[Document No_] = SLH.[No_] AND SLL.[Document Type] = SLH.[Document Type]
														WHERE SLH.[Document Type] = 1
														UNION ALL
														SELECT	SHA.[No_]
																,SLA.[Line No_]
																,SLA.[Description]
																,SLA.[Quantity]
																,SLA.[Unit Price]
																,SLA.[Line Discount Amount]
																,SLA.[Amount]
																,SLA.[Type]
														FROM	(
																	SELECT  [Document Type]
																			, [No_]
																			, MAX([Version No_]) AS [Version No_]
																	FROM	[dbo].[BC_SOLUSI$Sales Header Archive]
																	GROUP BY [Document Type], [No_]
																) SHA
														INNER JOIN [dbo].[BC_SOLUSI$Sales Line Archive] SLA ON SLA.[Document No_] = SHA.[No_] AND SLA.[Document Type] = SHA.[Document Type] AND SLA.[Version No_] = SHA.[Version No_]
														LEFT JOIN [dbo].[BC_SOLUSI$Sales Header] SLH ON SLH.[No_] = SHA.[No_] AND SLH.[Document Type] = SHA.[Document Type]
														WHERE	SHA.[Document Type] = 1 AND
																SLH.[No_] IS NULL
													) SOL ON SOL.[No_]  = SVH.[Prepayment Order No_]
										WHERE  ($P{PrintItemNol} = 'N' AND SOL.[Unit Price] <> 0) OR $P{PrintItemNol} = 'Y'
									) SIL ON SIL.[No_] = SIH.[No_]
						LEFT JOIN [dbo].[BC_AUTOJAYA$Salesperson_Purchaser] SLS ON SLS.[Code] = SIH.[Salesperson Code]
						WHERE SIH.[No_] >= $P{InvoiceFrom} AND SIH.[No_] <= $P{InvoiceUpto}
					) X
			LEFT JOIN	(
							SELECT [Name] AS COMPANY
									,[Address] AS ALAMAT1
									,[Address 2] +', '+ [City] + ' ' + [County] + ' ' +[Post Code] AS ALAMAT2
									,'Telp.: '+[Phone No_]+ ' ' +[Phone No_ 2]+', Fax. : '+[Fax No_] AS PHONE_FAX
									,'Email : '+[E-Mail] AS EMAIL
									,'Website : '+[Home Page] AS WEBPAGE
							FROM [dbo].[BC_SOLUSI$Company Information]
						) XY ON 1=1
		) INV
LEFT JOIN [dbo].[BC_AUTOJAYA$Customer Bank Account] CBA ON  CBA.[Customer No_] = INV.NAMA_DC AND CBA.[Code] = 'VA01'
LEFT JOIN	(
				SELECT	'AUTOJAYA' AS COMPANY
						,[Sales Invoice - Name] AS TTD
						,[Sales Invoice - Job Title] AS JABATAN
				FROM [dbo].[BC_AUTOJAYA$General Ledger Setup]
				UNION ALL
				SELECT	'SOLUSI' AS COMPANY
						,[Sales Invoice - Name] AS TTD
						,[Sales Invoice - Job Title] AS JABATAN
				FROM [dbo].[BC_SOLUSI$General Ledger Setup]
			) TTD ON TTD.COMPANY = INV.COMP]]>
	</queryString>
