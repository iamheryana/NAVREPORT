<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="01074_ArusKasPiutangDetail" language="groovy" pageWidth="1300" pageHeight="595" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="1300" leftMargin="0" rightMargin="0" topMargin="20" bottomMargin="20" uuid="e4a9c5ca-8c3f-4842-92fe-fcbf6b01d302">
	<property name="ireport.zoom" value="1.4641000000000042"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="TglFrom" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="TglUpto" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Company" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="JenisDetail" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="KodeSubGroup" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="KodeGroup" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT	XXX.COMPANY
		, XXX.KODE_GRP
		, BAC.[Name] AS GRP_NAME
		, XXX.KODE_SUBGRP
		, XXX.TGL_POSTING
		, XXX.NO_VOUCHER
		, XXX.KETERANGAN
		, XXX.AMOUNT
		, XXX.AMT_USAHA_AR
		, XXX.AMT_USAHA_UP
		, XXX.AMT_NON_USAHA
FROM	(
			SELECT	'AUTOJAYA' AS COMPANY
					, XX.[Source No_] AS KODE_GRP
					, CASE	WHEN SUM(XX.ADA_AR + XX.ADA_AR_SPONSOR + XX.ADA_AR_IC) > 0 THEN 'AR'
							WHEN SUM(XX.ADA_UN_PAY) > 0 THEN 'UNAPPLIED'
							ELSE 'NON USAHA' END AS KODE_SUBGRP
					, XX.[Posting Date] AS TGL_POSTING
					, XX.[Document No_] AS NO_VOUCHER
					, XX.[Description] AS KETERANGAN
					, SUM(XX.[Debit Amount]) AS AMOUNT
					, SUM(CASE	WHEN (XX.ADA_AR + XX.ADA_AR_SPONSOR + XX.ADA_AR_IC) > 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_USAHA_AR
					, SUM(CASE	WHEN (XX.ADA_UN_PAY) > 0 AND (XX.ADA_AR + XX.ADA_AR_SPONSOR + XX.ADA_AR_IC) = 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_USAHA_UP
					, SUM(CASE WHEN (XX.ADA_AR + XX.ADA_AR_SPONSOR + XX.ADA_AR_IC + XX.ADA_UN_PAY) = 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_NON_USAHA
			FROM	(
						SELECT	X.[Posting Date]
								, X.[Document No_]
								, X.[Transaction No_]
								, X.[G_L Account No_]
								, X.[Source No_]
								, X.JENIS_ACC
								, X.[Description]
								, SUM(X.ADA_AR) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_AR
								, SUM(X.ADA_AR_SPONSOR) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_AR_SPONSOR
								, SUM(X.ADA_AR_IC) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_AR_IC
								, SUM(X.ADA_UN_PAY) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_UN_PAY
								, X.[Debit Amount]
						FROM	(
									SELECT	GLE.[Posting Date]
											, GLE.[Document No_]
											, GLE.[Transaction No_]
											, GLE.[G_L Account No_]
											, GLE.[Source No_]
											, MAX(GLE.[Description]) AS [Description]
											,	CASE WHEN	(GLE.[G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
															(GLE.[G_L Account No_] BETWEEN '112.110.00' AND '112.162.00') THEN
													'BANK'
												ELSE
													'NON' END AS JENIS_ACC
											, CASE WHEN GLE.[G_L Account No_] = '114.110.00' THEN 1 ELSE 0 END AS ADA_AR
											, CASE WHEN GLE.[G_L Account No_] = '114.155.00' AND PLD.[Document No_] IS NOT NULL THEN 1 ELSE 0 END AS ADA_AR_SPONSOR
											, CASE WHEN GLE.[G_L Account No_] = '114.140.00' THEN 1 ELSE 0 END AS ADA_AR_IC
											, CASE WHEN GLE.[G_L Account No_] = '220.185.00' THEN 1 ELSE 0 END AS ADA_UN_PAY
											, SUM(GLE.[Debit Amount]) AS [Debit Amount]
											, SUM(GLE.[Credit Amount]) AS [Credit Amount]
									FROM [dbo].[BC_AUTOJAYA$G_L Entry] GLE
									LEFT JOIN	(
													SELECT DISTINCT [Document No_]
													FROM	[dbo].[BC_AUTOJAYA$Detailed Cust_ Ledg_ Entry]
													WHERE	[Document Type] <> 2
												) PLD ON PLD.[Document No_] = GLE.[Document No_]
									WHERE	(GLE.[Posting Date] >= $P{TglFrom} AND GLE.[Posting Date] <= $P{TglUpto})
									GROUP BY GLE.[Posting Date]
											, GLE.[Document No_]
											, GLE.[Transaction No_]
											, GLE.[G_L Account No_]
											, GLE.[Source No_]
											, PLD.[Document No_]
								) X
					) XX
			WHERE XX.JENIS_ACC =  'BANK' AND $P{Company} = 'AUTOJAYA'
			GROUP BY XX.[Source No_]
					, XX.[Posting Date]
					, XX.[Document No_]
					, XX.[Description]
			UNION ALL
			SELECT	'SOLUSI' AS COMPANY
					, XX.[Source No_] AS KODE_GRP
					, CASE	WHEN SUM(XX.ADA_AR + XX.ADA_AR_SPONSOR + XX.ADA_AR_IC) > 0 THEN 'AR'
							WHEN SUM(XX.ADA_UN_PAY) > 0 THEN 'UNAPPLIED'
							ELSE 'NON USAHA' END AS KODE_SUBGRP
					, XX.[Posting Date] AS TGL_POSTING
					, XX.[Document No_] AS NO_VOUCHER
					, XX.[Description] AS KETERANGAN
					, SUM(XX.[Debit Amount]) AS AMOUNT
					, SUM(CASE	WHEN (XX.ADA_AR + XX.ADA_AR_SPONSOR + XX.ADA_AR_IC) > 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_USAHA_AR
					, SUM(CASE	WHEN (XX.ADA_UN_PAY) > 0 AND (XX.ADA_AR + XX.ADA_AR_SPONSOR + XX.ADA_AR_IC) = 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_USAHA_UP
					, SUM(CASE WHEN (XX.ADA_AR + XX.ADA_AR_SPONSOR + XX.ADA_AR_IC + XX.ADA_UN_PAY) = 0 THEN XX.[Debit Amount] ELSE 0 END) AS AMT_NON_USAHA
			FROM	(
						SELECT	X.[Posting Date]
								, X.[Document No_]
								, X.[Transaction No_]
								, X.[G_L Account No_]
								, X.[Source No_]
								, X.JENIS_ACC
								, X.[Description]
								, SUM(X.ADA_AR) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_AR
								, SUM(X.ADA_AR_SPONSOR) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_AR_SPONSOR
								, SUM(X.ADA_AR_IC) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_AR_IC
								, SUM(X.ADA_UN_PAY) OVER (PARTITION BY X.[Posting Date], X.[Document No_] , X.[Transaction No_]) AS ADA_UN_PAY
								, X.[Debit Amount]
						FROM	(
									SELECT	GLE.[Posting Date]
											, GLE.[Document No_]
											, GLE.[Transaction No_]
											, GLE.[G_L Account No_]
											, GLE.[Source No_]
											, MAX(GLE.[Description]) AS [Description]
											,	CASE WHEN	(GLE.[G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
															(GLE.[G_L Account No_] BETWEEN '112.110.00' AND '112.162.00') THEN
													'BANK'
												ELSE
													'NON' END AS JENIS_ACC
											, CASE WHEN GLE.[G_L Account No_] = '114.110.00' THEN 1 ELSE 0 END AS ADA_AR
											, CASE WHEN GLE.[G_L Account No_] = '114.155.00' AND PLD.[Document No_] IS NOT NULL THEN 1 ELSE 0 END AS ADA_AR_SPONSOR
											, CASE WHEN GLE.[G_L Account No_] = '114.140.00' THEN 1 ELSE 0 END AS ADA_AR_IC
											, CASE WHEN GLE.[G_L Account No_] = '220.185.00' THEN 1 ELSE 0 END AS ADA_UN_PAY
											, SUM(GLE.[Debit Amount]) AS [Debit Amount]
											, SUM(GLE.[Credit Amount]) AS [Credit Amount]
									FROM [dbo].[BC_SOLUSI$G_L Entry] GLE
									LEFT JOIN	(
													SELECT DISTINCT [Document No_]
													FROM	[dbo].[BC_SOLUSI$Detailed Cust_ Ledg_ Entry]
													WHERE	[Document Type] <> 2
												) PLD ON PLD.[Document No_] = GLE.[Document No_]
									WHERE	(GLE.[Posting Date] >= $P{TglFrom} AND GLE.[Posting Date] <= $P{TglUpto})
									GROUP BY GLE.[Posting Date]
											, GLE.[Document No_]
											, GLE.[Transaction No_]
											, GLE.[G_L Account No_]
											, GLE.[Source No_]
											, PLD.[Document No_]
								) X
					) XX
			WHERE XX.JENIS_ACC =  'BANK' AND $P{Company} = 'SOLUSI'
			GROUP BY XX.[Source No_]
					, XX.[Posting Date]
					, XX.[Document No_]
					, XX.[Description]
		) XXX
LEFT JOIN [dbo].[BC_AUTOJAYA$Bank Account] BAC ON BAC.[No_] = XXX.KODE_GRP
WHERE	XXX.AMOUNT <> 0 AND
		$P{JenisDetail} = 'B1' AND
		(XXX.KODE_GRP = $P{KodeGroup} OR $P{KodeGroup} = 'ALL') AND
		(XXX.KODE_SUBGRP = $P{KodeSubGroup} OR $P{KodeSubGroup} = 'ALL')
UNION ALL
SELECT	XX.COMPANY
		, XX.KODE_GRP
		, XX.GRP_NAME
		, XX.KODE_SUBGRP
		, XX.TGL_POSTING
		, XX.NO_VOUCHER
		, XX.KETERANGAN
		, XX.AMOUNT
		, XX.AMT_USAHA_AR
		, XX.AMT_USAHA_UP
		, XX.AMT_NON_USAHA
FROM	(
			SELECT
					'AUTOJAYA' AS COMPANY
					, CASE	WHEN X.[G_L Account No_] = '118.215.00' THEN 'B41'
							WHEN X.[G_L Account No_] = '118.216.00' THEN 'B42'
							WHEN X.[G_L Account No_] = '118.217.00' THEN 'B43'
							WHEN X.[G_L Account No_] = '230.190.00' THEN 'B44'
							ELSE 'B2' END AS JENIS_DETAIL
					, CASE	WHEN X.[G_L Account No_] = '118.215.00' THEN '-'
							WHEN X.[G_L Account No_] = '118.216.00' THEN '-'
							WHEN X.[G_L Account No_] = '118.217.00' THEN '-'
							WHEN X.[G_L Account No_] = '230.190.00' THEN '-'
							ELSE ISNULL(X.[G_L Account No_],'N/A') END AS KODE_GRP
					, CASE	WHEN X.[G_L Account No_] = '118.215.00' THEN 'PPh Pasal 23'
							WHEN X.[G_L Account No_] = '118.216.00' THEN 'PPh Pasal 22'
							WHEN X.[G_L Account No_] = '118.217.00' THEN 'PPn WAPU / PPn Ditangguhkan'
							WHEN X.[G_L Account No_] = '230.190.00' THEN 'PPn Kws. Berikat'
							ELSE ISNULL(GLA.[Name],'N/A') END AS GRP_NAME
					, 'NA' AS KODE_SUBGRP
					, X.TGL_POSTING
					, X.NO_VOUCHER
					, X.KETERANGAN
					, SUM(X.[Debit Amount] - X.[Credit Amount]) AS AMOUNT
					, 0 AS AMT_USAHA_AR
					, 0 AS AMT_USAHA_UP
					, 0 AS AMT_NON_USAHA
			FROM	(
						SELECT	CPPN.[G_L Account No_]
								,'NON' AS GL_ACC
								, CPPN.[Posting Date] AS TGL_POSTING
								, CPPN.[Document No_] AS NO_VOUCHER
								, CPPN.[Description] AS KETERANGAN
								, CPPN.[Debit Amount] * -1 AS [Debit Amount]
								, CPPN.[Credit Amount] * -1 AS [Credit Amount]
						FROM	(
									SELECT	[Transaction No_]
											, [G_L Account No_]
											, [Posting Date]
											, [Document No_]
											, [Description]
											, SUM(CASE WHEN [G_L Account No_] = '400.110.03' THEN 1 ELSE 0 END) OVER (PARTITION BY [Transaction No_]) AS ADA_SALES
											, [Debit Amount]
											, [Credit Amount]
									FROM [dbo].[BC_AUTOJAYA$G_L Entry]
									WHERE	[G_L Account No_] IN ('230.190.00', '400.110.03') AND
											[Source Code] = 'GENJNL' AND
											([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
								) CPPN
						WHERE CPPN.ADA_SALES = 1 AND [G_L Account No_] = '230.190.00'
						UNION ALL
						SELECT XO.[G_L Account No_]
								, XO.GL_ACC
								, XO.[Posting Date] AS TGL_POSTING
								, XO.[Document No_] AS NO_VOUCHER
								, XO.[Description] AS KETERANGAN
								, XO.[Debit Amount]
								, XO.[Credit Amount]
						FROM	(
									SELECT
											GLE.[Posting Date]
											, GLE.[Document No_]
											, GLE.[Transaction No_]
											, GLE.[G_L Account No_]
											, CASE	WHEN	(	(GLE.[G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
																(GLE.[G_L Account No_] BETWEEN '112.110.00' AND '112.162.00') )
															THEN 'BANK'
													WHEN	(	GLE.[G_L Account No_] = '114.110.00' OR
																GLE.[G_L Account No_] = '114.155.00' OR
																GLE.[G_L Account No_] = '220.130.00' OR
																GLE.[G_L Account No_] = '114.140.00' OR
																GLE.[G_L Account No_] = '400.110.03'
															) THEN 'ACC'
													ELSE 'NON' END AS GL_ACC
											, GLE.[Source No_]
											, GLE.[Source Code]
											, MAX(GLE.[Description]) AS [Description]
											, CASE WHEN	(	(GLE.[G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
															(GLE.[G_L Account No_] BETWEEN '112.110.00' AND '112.162.00')
														) THEN 1 ELSE 0 END AS ADA_KASBANK
											, CASE WHEN GLE.[G_L Account No_] = '220.185.00' THEN 1 ELSE 0 END AS ADA_UN_PAY
											, SUM(GLE.[Debit Amount]) AS [Debit Amount]
											, SUM(GLE.[Credit Amount]) AS [Credit Amount]
									FROM [dbo].[BC_AUTOJAYA$G_L Entry] GLE
									WHERE GLE.[Transaction No_] IN	(
																		SELECT	PLL.[Transaction No_]
																		FROM [dbo].[BC_AUTOJAYA$G_L Entry] PLL
																		LEFT JOIN	(
																						SELECT DISTINCT [Document No_]
																						FROM	[dbo].[BC_AUTOJAYA$Detailed Cust_ Ledg_ Entry]
																						WHERE	[Document Type] <> 2
																					) PLD ON PLD.[Document No_] = PLL.[Document No_]
																		WHERE	PLL.[G_L Account No_] IN ('114.110.00', '114.155.00') AND
																				PLL.[Source No_] NOT IN ('AUJA001', 'AUJA002','SOPE001') AND
																				PLL.[Source Code] <> 'SALES' AND
																				(
																					PLD.[Document No_] IS NOT NULL OR
																					(PLD.[Document No_] IS NULL AND PLL.[Document No_] IN ('JVAC21080006', 'JV-DV2108/007'))
																				) AND
																				(PLL.[Posting Date] >= $P{TglFrom} AND PLL.[Posting Date] <= $P{TglUpto})
																	)
									GROUP BY GLE.[Posting Date]
											, GLE.[Document No_]
											, GLE.[Transaction No_]
											, GLE.[G_L Account No_]
											, GLE.[Source No_]
											, GLE.[Source Code]
								) XO
					) X
			LEFT JOIN [dbo].[BC_AUTOJAYA$G_L Account] GLA ON GLA.[No_] = X.[G_L Account No_]
			WHERE	(X.GL_ACC = 'NON' OR (X.GL_ACC = 'BANK' AND X.[Credit Amount] <> 0))
					AND $P{Company} = 'AUTOJAYA'
			GROUP BY	X.[G_L Account No_]
						, GLA.[Name]
						, X.TGL_POSTING
						, X.NO_VOUCHER
						, X.KETERANGAN
			UNION ALL
			SELECT
					'SOLUSI' AS COMPANY
					, CASE	WHEN X.[G_L Account No_] = '118.215.00' THEN 'B41'
							WHEN X.[G_L Account No_] = '118.216.00' THEN 'B42'
							WHEN X.[G_L Account No_] = '118.217.00' THEN 'B43'
							WHEN X.[G_L Account No_] = '230.190.00' THEN 'B44'
							ELSE 'B2' END AS JENIS_DETAIL
					, CASE	WHEN X.[G_L Account No_] = '118.215.00' THEN '-'
							WHEN X.[G_L Account No_] = '118.216.00' THEN '-'
							WHEN X.[G_L Account No_] = '118.217.00' THEN '-'
							WHEN X.[G_L Account No_] = '230.190.00' THEN '-'
							ELSE ISNULL(X.[G_L Account No_],'N/A') END AS KODE_GRP
					, CASE	WHEN X.[G_L Account No_] = '118.215.00' THEN 'PPh Pasal 23'
							WHEN X.[G_L Account No_] = '118.216.00' THEN 'PPh Pasal 22'
							WHEN X.[G_L Account No_] = '118.217.00' THEN 'PPn WAPU / PPn Ditangguhkan'
							WHEN X.[G_L Account No_] = '230.190.00' THEN 'PPn Kws. Berikat'
							ELSE ISNULL(GLA.[Name],'N/A') END AS GRP_NAME
					, 'NA' AS KODE_SUBGRP
					, X.TGL_POSTING
					, X.NO_VOUCHER
					, X.KETERANGAN
					, SUM(X.[Debit Amount] - X.[Credit Amount]) AS AMOUNT
					, 0 AS AMT_USAHA_AR
					, 0 AS AMT_USAHA_UP
					, 0 AS AMT_NON_USAHA
			FROM	(
						SELECT	CPPN.[G_L Account No_]
								,'NON' AS GL_ACC
								, CPPN.[Posting Date] AS TGL_POSTING
								, CPPN.[Document No_] AS NO_VOUCHER
								, CPPN.[Description] AS KETERANGAN
								, CPPN.[Debit Amount] * -1 AS [Debit Amount]
								, CPPN.[Credit Amount] * -1 AS [Credit Amount]
						FROM	(
									SELECT	[Transaction No_]
											, [G_L Account No_]
											, [Posting Date]
											, [Document No_]
											, [Description]
											, SUM(CASE WHEN [G_L Account No_] = '400.110.03' THEN 1 ELSE 0 END) OVER (PARTITION BY [Transaction No_]) AS ADA_SALES
											, [Debit Amount]
											, [Credit Amount]
									FROM [dbo].[BC_SOLUSI$G_L Entry]
									WHERE	[G_L Account No_] IN ('230.190.00', '400.110.03') AND
											[Source Code] = 'GENJNL' AND
											([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
								) CPPN
						WHERE CPPN.ADA_SALES = 1 AND [G_L Account No_] = '230.190.00'
						UNION ALL
						SELECT XO.[G_L Account No_]
								, XO.GL_ACC
								, XO.[Posting Date] AS TGL_POSTING
								, XO.[Document No_] AS NO_VOUCHER
								, XO.[Description] AS KETERANGAN
								, XO.[Debit Amount]
								, XO.[Credit Amount]
						FROM	(
									SELECT
											GLE.[Posting Date]
											, GLE.[Document No_]
											, GLE.[Transaction No_]
											, GLE.[G_L Account No_]
											, CASE	WHEN	(	(GLE.[G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
																(GLE.[G_L Account No_] BETWEEN '112.110.00' AND '112.162.00') )
															THEN 'BANK'
													WHEN	(	GLE.[G_L Account No_] = '114.110.00' OR
																(GLE.[G_L Account No_] = '114.155.00' AND GLE.[Document No_] <> 'JVTB1910007') OR
																GLE.[G_L Account No_] = '220.130.00' OR
																GLE.[G_L Account No_] = '114.140.00' OR
																GLE.[G_L Account No_] = '400.110.03'
															) THEN 'ACC'
													WHEN  (GLE.[G_L Account No_] = '114.155.00' AND GLE.[Document No_] = 'JVTB1910007') THEN 'NON'
													ELSE 'NON' END AS GL_ACC
											, GLE.[Source No_]
											, GLE.[Source Code]
											, MAX(GLE.[Description]) AS [Description]
											, CASE WHEN	(	(GLE.[G_L Account No_] BETWEEN '110.110.00' AND '110.150.00') OR
															(GLE.[G_L Account No_] BETWEEN '112.110.00' AND '112.162.00')
														) THEN 1 ELSE 0 END AS ADA_KASBANK
											, CASE WHEN GLE.[G_L Account No_] = '220.185.00' THEN 1 ELSE 0 END AS ADA_UN_PAY
											, SUM(GLE.[Debit Amount]) AS [Debit Amount]
											, SUM(GLE.[Credit Amount]) AS [Credit Amount]
									FROM [dbo].[BC_SOLUSI$G_L Entry] GLE
									WHERE GLE.[Transaction No_] IN	(
																		SELECT	PLL.[Transaction No_]
																		FROM [dbo].[BC_SOLUSI$G_L Entry] PLL
																		LEFT JOIN	(
																						SELECT DISTINCT [Document No_]
																						FROM	[dbo].[BC_SOLUSI$Detailed Cust_ Ledg_ Entry]
																						WHERE	[Document Type] <> 2
																					) PLD ON PLD.[Document No_] = PLL.[Document No_]
																		WHERE	PLL.[G_L Account No_] IN ('114.110.00', '114.155.00') AND
																				PLL.[Source No_] NOT IN ('AUJA001', 'AUJA002','SOPE001') AND
																				PLL.[Source Code] <> 'SALES' AND
																				PLD.[Document No_] IS NOT NULL AND
																				(PLL.[Posting Date] >= $P{TglFrom} AND PLL.[Posting Date] <= $P{TglUpto})
																	)
									GROUP BY GLE.[Posting Date]
											, GLE.[Document No_]
											, GLE.[Transaction No_]
											, GLE.[G_L Account No_]
											, GLE.[Source No_]
											, GLE.[Source Code]
								) XO
					) X
			LEFT JOIN [dbo].[BC_SOLUSI$G_L Account] GLA ON GLA.[No_] = X.[G_L Account No_]
			WHERE	(X.GL_ACC = 'NON' OR (X.GL_ACC = 'BANK' AND X.[Credit Amount] <> 0))
					AND $P{Company} = 'SOLUSI'
			GROUP BY	X.[G_L Account No_]
						, GLA.[Name]
						, X.TGL_POSTING
						, X.NO_VOUCHER
						, X.KETERANGAN
		) XX
WHERE	XX.AMOUNT <> 0 AND
		XX.JENIS_DETAIL = $P{JenisDetail}
UNION ALL
SELECT XX.COMPANY
		, XX.KODE_GRP
		, XX.GRP_NAME
		, XX.KODE_SUBGRP
		, XX.TGL_POSTING
		, XX.NO_VOUCHER
		, XX.KETERANGAN
		, XX.AMOUNT
		, XX.AMT_USAHA_AR
		, XX.AMT_USAHA_UP
		, XX.AMT_NON_USAHA
FROM	(
			SELECT
					'AUTOJAYA' AS COMPANY
					, CASE	WHEN X.URUTAN = 1 THEN 'B31'
							WHEN X.URUTAN = 2 THEN 'B32'
							ELSE '' END AS JENIS_DETAIL
					, 'NA' AS KODE_GRP
					, CASE	WHEN X.URUTAN = 1 THEN 'Pelunasan Piutang - Non Bank (Intern)'
							WHEN X.URUTAN = 2 THEN 'Pelunasan Piutang - Non Bank (Antar Company)'
							ELSE '' END AS GRP_NAME
					, 'NA' AS KODE_SUBGRP
					, X.TGL_POSTING
					, X.NO_VOUCHER
					, X.KETERANGAN
					, X.AMOUNT
					, 0 AS AMT_USAHA_AR
					, 0 AS AMT_USAHA_UP
					, 0 AS AMT_NON_USAHA
			FROM	(
						SELECT	 CASE	WHEN GLE.[Source No_] IN ('AUJA001', 'AUJA002') THEN 1
										WHEN GLE.[Source No_] = 'SOPE001' THEN 2
										ELSE 3 END AS URUTAN
								, DCL.[Posting Date] AS TGL_POSTING
								, GLE2.[Document No_] AS NO_VOUCHER
								, GLE2.[Description] AS KETERANGAN
								, DCL.[Amount (LCY)] * -1 AS AMOUNT
						FROM	(
									SELECT	 [Cust_ Ledger Entry No_]
											, [Posting Date]
											, [Applied Cust_ Ledger Entry No_]
											, SUM([Amount (LCY)]) AS [Amount (LCY)]
									FROM [dbo].[BC_AUTOJAYA$Detailed Cust_ Ledg_ Entry]
									WHERE	[Document Type] <> 2 AND
											[Applied Cust_ Ledger Entry No_] <> 0 AND
											([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
									GROUP BY  [Cust_ Ledger Entry No_]
											, [Posting Date]
											, [Applied Cust_ Ledger Entry No_]
								) DCL
						LEFT JOIN [dbo].[BC_AUTOJAYA$Cust_ Ledger Entry] APL ON APL.[Entry No_] = DCL.[Applied Cust_ Ledger Entry No_]
						LEFT JOIN [dbo].[BC_AUTOJAYA$Cust_ Ledger Entry] CID ON CID.[Entry No_] = DCL.[Cust_ Ledger Entry No_]
						LEFT JOIN [dbo].[BC_AUTOJAYA$G_L Entry] GLE ON GLE.[Entry No_] =  DCL.[Cust_ Ledger Entry No_]
						LEFT JOIN [dbo].[BC_AUTOJAYA$G_L Entry] GLE2 ON GLE2.[Entry No_] =  DCL.[Applied Cust_ Ledger Entry No_]
						WHERE	DCL.[Amount (LCY)] <> 0 AND
								GLE.[G_L Account No_] IN ('114.110.00', '114.140.00') AND
								GLE.[Source No_] IN ('AUJA001', 'AUJA002','SOPE001') AND
								GLE.[Source Code] = 'SALES' AND
								GLE.[Entry No_] IS NOT NULL
					) X
			WHERE $P{Company} = 'AUTOJAYA'
			UNION ALL
			SELECT
					'SOLUSI' AS COMPANY
					, CASE	WHEN X.URUTAN = 1 THEN 'B31'
							WHEN X.URUTAN = 2 THEN 'B32'
							ELSE '' END AS JENIS_DETAIL
					, 'NA' AS KODE_GRP
					, CASE	WHEN X.URUTAN = 1 THEN 'Pelunasan Piutang - Non Bank (Intern)'
							WHEN X.URUTAN = 2 THEN 'Pelunasan Piutang - Non Bank (Antar Company)'
							ELSE '' END AS GRP_NAME
					, 'NA' AS KODE_SUBGRP
					, X.TGL_POSTING
					, X.NO_VOUCHER
					, X.KETERANGAN
					, X.AMOUNT
					, 0 AS AMT_USAHA_AR
					, 0 AS AMT_USAHA_UP
					, 0 AS AMT_NON_USAHA
			FROM	(
						SELECT	 CASE	WHEN GLE.[Source No_] IN ('AUJA001', 'AUJA002') THEN 2
										WHEN GLE.[Source No_] = 'SOPE001' THEN 1
										ELSE 3 END AS URUTAN
								, DCL.[Posting Date] AS TGL_POSTING
								, GLE2.[Document No_] AS NO_VOUCHER
								, GLE2.[Description] AS KETERANGAN
								, DCL.[Amount (LCY)] * -1 AS AMOUNT
						FROM	(
									SELECT	 [Cust_ Ledger Entry No_]
											, [Posting Date]
											, [Applied Cust_ Ledger Entry No_]
											, SUM([Amount (LCY)]) AS [Amount (LCY)]
									FROM [dbo].[BC_SOLUSI$Detailed Cust_ Ledg_ Entry]
									WHERE	[Document Type] <> 2 AND
											[Applied Cust_ Ledger Entry No_] <> 0 AND
											([Posting Date] >= $P{TglFrom} AND [Posting Date] <= $P{TglUpto})
									GROUP BY  [Cust_ Ledger Entry No_]
											, [Posting Date]
											, [Applied Cust_ Ledger Entry No_]
								) DCL
						LEFT JOIN [dbo].[BC_SOLUSI$Cust_ Ledger Entry] APL ON APL.[Entry No_] = DCL.[Applied Cust_ Ledger Entry No_]
						LEFT JOIN [dbo].[BC_SOLUSI$Cust_ Ledger Entry] CID ON CID.[Entry No_] = DCL.[Cust_ Ledger Entry No_]
						LEFT JOIN [dbo].[BC_SOLUSI$G_L Entry] GLE ON GLE.[Entry No_] =  DCL.[Cust_ Ledger Entry No_]
						LEFT JOIN [dbo].[BC_SOLUSI$G_L Entry] GLE2 ON GLE2.[Entry No_] =  DCL.[Applied Cust_ Ledger Entry No_]
						WHERE	DCL.[Amount (LCY)] <> 0 AND
								GLE.[G_L Account No_] IN ('114.110.00', '114.140.00') AND
								GLE.[Source No_] IN ('AUJA001', 'AUJA002','SOPE001') AND
								GLE.[Source Code] = 'SALES' AND
								GLE.[Entry No_] IS NOT NULL
					) X
			 WHERE $P{Company} = 'SOLUSI'
		) XX
WHERE	XX.AMOUNT <> 0 AND
		XX.JENIS_DETAIL = $P{JenisDetail}]]>
	</queryString>
