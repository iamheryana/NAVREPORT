								,CUS.[Customer Posting Group] AS CUST_GRP
								,MAX(CASE WHEN SSIH.[Order No_] <> '' THEN SSIH.[Order No_] ELSE SSIH.[Prepayment Order No_] END) AS NO_SO
						FROM [dbo].[BC_AUTOJAYA$Sales Invoice Header] SSIH
						INNER JOIN [dbo].[BC_AUTOJAYA$Sales Invoice Line] SSID ON SSID.[Document No_] = SSIH.[No_]
						LEFT JOIN  [dbo].[BC_AUTOJAYA$Customer] CUS ON CUS.[No_] = SSIH.[Bill-to Customer No_]
						GROUP BY SSIH.[Posting Date], SSIH.[No_]
								,SSIH.[Sell-to Customer No_]
								,SSIH.[Sell-to Customer Name]
								,SSIH.[Bill-to Customer No_]
								,SSIH.[Bill-to Name]
								,SSIH.[Posting Description]
								,SSIH.[External Document No_]
								,SSIH.[Faktur Pajak No_]
								,SSIH.[Currency Code]
								,SSIH.[Salesperson Code]
								,CUS.[Customer Posting Group]
					) X
			LEFT JOIN [dbo].[BC_AUTOJAYA$Sales Cr_Memo Header] CRM ON CRM.[Applies-to Doc_ No_] = X.NO_INV
			INNER JOIN (
							SELECT
									   CLE.[Document No_] AS NO_INV
									  ,CLE.[External Document No_] AS NO_PO_CUST
									  ,CLD.RMN_AMOUNT
									  ,CLD2.TGL_LUNAS
									  ,CLD2.NO_PELUNASAN
							FROM [dbo].[BC_AUTOJAYA$Cust_ Ledger Entry] CLE
							LEFT JOIN (
										SELECT	CLED.[Cust_ Ledger Entry No_]
												,SUM(CLED.[Amount]) AS RMN_AMOUNT
										FROM [dbo].[BC_AUTOJAYA$Detailed Cust_ Ledg_ Entry] CLED
										GROUP BY CLED.[Cust_ Ledger Entry No_]
									  )CLD ON CLD.[Cust_ Ledger Entry No_] = CLE.[Entry No_]
							LEFT JOIN (
										SELECT	CLED2.[Cust_ Ledger Entry No_]
												,MAX(CLED2.[Posting Date]) AS TGL_LUNAS
												,MAX(CLED2.[Document No_]) AS NO_PELUNASAN
										FROM [dbo].[BC_AUTOJAYA$Detailed Cust_ Ledg_ Entry] CLED2
										WHERE CLED2.[Document Type] <> 2
										GROUP BY CLED2.[Cust_ Ledger Entry No_]
									  ) CLD2 ON CLD2.[Cust_ Ledger Entry No_] = CLE.[Entry No_]
							WHERE CLD.RMN_AMOUNT = 0
					  )	LN ON LN.NO_INV = X.NO_INV
			LEFT JOIN [dbo].[BC_AUTOJAYA$Sales Shipment Header] SJH ON SJH.[Order No_] = X.NO_SO
			WHERE	CRM.[No_] IS NULL AND
					X.NO_SO <> '' AND
					X.NILAI_INVOICE <> 0 AND
					LN.NO_PELUNASAN IS NOT NULL AND
					($P{Company} = 'AUTOJAYA' OR $P{Company} = 'ALL') AND
					(X.CUST_GRP = $P{CustomerGroup} OR $P{CustomerGroup} = 'ALL') AND
					(X.BILL_TO_CODE = $P{CustomerNo} OR $P{CustomerNo} = 'ALL') AND
					(X.TGL_INV >= $P{TglInvFrom} AND X.TGL_INV <= $P{TglInvUpto})
			GROUP BY X.TGL_INV
					,X.NO_INV
					,X.NO_FP
					,X.NO_PO_CUST
					,X.SELL_TO_CODE
					,X.SELL_TO_NAME
					,X.BILL_TO_CODE
					,X.BILL_TO_NAME
					,X.DESCRIPTIONS
					,X.NILAI_INVOICE
					,X.CUST_GRP
					,LN.TGL_LUNAS
					,LN.NO_PELUNASAN
			UNION ALL
			SELECT
					'SOLUSI' AS COMPANY
					,X.TGL_INV
					,X.NO_INV
					,X.NO_FP
					,X.NO_PO_CUST
					,X.SELL_TO_CODE
					,X.SELL_TO_NAME
					,X.BILL_TO_CODE
					,X.BILL_TO_NAME
					,X.DESCRIPTIONS
					,X.NILAI_INVOICE
					,X.CUST_GRP
					,LN.TGL_LUNAS
					,LN.NO_PELUNASAN
					,MAX(SJH.[Posting Date]) AS TGL_SJ
					,MAX(SJH.[No_]) AS NO_SJ
					,DATEDIFF (DAY, MAX(SJH.[Posting Date]), X.TGL_INV) AS LAMA_BTB
					,DATEDIFF (DAY, X.TGL_INV, LN.TGL_LUNAS) AS SELISIH
					,	CASE	WHEN X.CUST_GRP = 'INDOMARCO' THEN
									CASE WHEN X.NO_INV LIKE 'SI%' THEN
										CASE	WHEN X.BILL_TO_CODE = 'IDMR180' THEN 'No Invoice NON Khusus - INDOMARCO'
												WHEN X.BILL_TO_CODE = 'ID00284' THEN 'No Invoice NON Khusus - INDOMARCO'
												WHEN X.BILL_TO_CODE = 'IDMR008' THEN 'No Invoice NON Khusus - ICC'
												WHEN X.BILL_TO_CODE = 'IDMR110' THEN 'No Invoice NON Khusus - ICC'
												WHEN X.BILL_TO_CODE = 'ID01602' THEN 'No Invoice NON Khusus - INTI PAKET'
												WHEN X.BILL_TO_CODE = 'ID01436' THEN 'No Invoice NON Khusus - INTI IDOLA'
												ELSE
													CASE WHEN X.SELL_TO_CODE = 'IDMR008' THEN 'No Invoice NON Khusus - ICC - Franchise'
														 WHEN X.SELL_TO_CODE = 'IDMR110' THEN 'No Invoice NON Khusus - ICC - Franchise'
														 WHEN X.SELL_TO_CODE = 'IDMR180' THEN 'No Invoice NON Khusus - INDOMARCO - Franchise'
														 WHEN X.SELL_TO_CODE = 'IDMR180' THEN 'No Invoice NON Khusus - INDOMARCO - Franchise'
														 WHEN X.SELL_TO_CODE = 'ID01602' THEN 'No Invoice NON Khusus - INTI PAKET - Franchise'
														 WHEN X.SELL_TO_CODE = 'ID01436' THEN 'No Invoice NON Khusus - INTI IDOLA - Franchise'
														 WHEN X.SELL_TO_CODE LIKE 'DC%' THEN
															CASE WHEN X.BILL_TO_CODE NOT LIKE 'DC%' THEN
																'No Invoice NON Khusus - INDOMARCO - Franchise'
															ELSE
																'No Invoice NON Khusus - INDOMARCO LAIN-LAIN'
															END
														 ELSE 'No Invoice NON Khusus - INDOMARCO LAIN-LAIN'
													END
												END
									ELSE
										CASE	WHEN X.BILL_TO_CODE = 'IDMR180' THEN 'No Invoice Khusus - INDOMARCO'
												WHEN X.BILL_TO_CODE = 'ID00284' THEN 'No Invoice Khusus - INDOMARCO'
												WHEN X.BILL_TO_CODE = 'IDMR008' THEN 'No Invoice Khusus - ICC'
												WHEN X.BILL_TO_CODE = 'IDMR110' THEN 'No Invoice Khusus - ICC'
												WHEN X.BILL_TO_CODE = 'ID01602' THEN 'No Invoice Khusus - INTI PAKET'
												WHEN X.BILL_TO_CODE = 'ID01436' THEN 'No Invoice Khusus - INTI IDOLA'
												ELSE
													CASE WHEN X.SELL_TO_CODE = 'IDMR008' THEN 'No Invoice Khusus - ICC - Franchise'
														 WHEN X.SELL_TO_CODE = 'IDMR110' THEN 'No Invoice Khusus - ICC - Franchise'
														 WHEN X.SELL_TO_CODE = 'IDMR180' THEN 'No Invoice Khusus - INDOMARCO - Franchise'
														 WHEN X.SELL_TO_CODE = 'IDMR180' THEN 'No Invoice Khusus - INDOMARCO - Franchise'
														 WHEN X.SELL_TO_CODE = 'ID01602' THEN 'No Invoice Khusus - INTI PAKET - Franchise'
														 WHEN X.SELL_TO_CODE = 'ID01436' THEN 'No Invoice Khusus - INTI IDOLA - Franchise'
														 WHEN X.SELL_TO_CODE LIKE 'DC%' THEN
															CASE WHEN X.BILL_TO_CODE NOT LIKE 'DC%' THEN
																'No Invoice Khusus - INDOMARCO - Franchise'
															ELSE
																'No Invoice Khusus - INDOMARCO LAIN-LAIN'
															END
														 ELSE 'No Invoice Khusus - INDOMARCO LAIN-LAIN'
													END
												END
									END
								WHEN X.CUST_GRP = 'ALFA' THEN
									CASE WHEN X.NO_INV LIKE 'SI%' THEN
										CASE	WHEN X.BILL_TO_CODE = 'ALFA024' THEN 'No Invoice NON Khusus - SAT'
												WHEN X.BILL_TO_CODE = 'AL00605' THEN 'No Invoice NON Khusus - SAT'
												WHEN X.BILL_TO_CODE = 'ALFA227' THEN 'No Invoice NON Khusus - MIDI'
												ELSE
													CASE WHEN SELL_TO_CODE LIKE 'DC%' THEN
															CASE WHEN BILL_TO_CODE NOT LIKE 'DC%' THEN
																'No Invoice NON Khusus - SAT - Franchise'
															ELSE
																'No Invoice NON Khusus - SAT LAIN-LAIN'
															END
													ELSE
														'No Invoice NON Khusus - SAT LAIN-LAIN'
													END
										END
									ELSE
										CASE	WHEN X.BILL_TO_CODE = 'ALFA024' THEN 'No Invoice Khusus - SAT'
												WHEN X.BILL_TO_CODE = 'ALFA227' THEN 'No Invoice Khusus - MIDI'
												ELSE
													CASE WHEN X.SELL_TO_CODE LIKE 'DC%' THEN
															CASE WHEN X.BILL_TO_CODE NOT LIKE 'DC%' THEN
																'No Invoice Khusus - SAT - Franchise'
															ELSE
																'No Invoice Khusus - SAT LAIN-LAIN'
															END
													ELSE
														'No Invoice Khusus - SAT LAIN-LAIN'
													END
										END
									END
								WHEN X.CUST_GRP = 'JNT' THEN
									CASE	WHEN X.BILL_TO_CODE = 'GLJE001' THEN 'JNT - GLOBAL JET EXPRESS'
											WHEN X.BILL_TO_CODE = 'GLJE002' THEN 'JNT - GLOBAL JET ECOMMERCE'
											WHEN X.BILL_TO_CODE = 'GLJE007' THEN 'JNT - SEMUT MERAH SQUAD'
											WHEN X.BILL_TO_CODE = 'GLJE030' THEN 'JNT - LUCKY MOM INDONESIA'
											WHEN X.BILL_TO_CODE = 'GLES001' THEN 'JNT - GLOBAL EXPRESS SEJAHTERA'
											WHEN X.BILL_TO_CODE = 'GLJE032' THEN 'JNT - GLOBAL JET CARGO'
											ELSE
												CASE WHEN X.SELL_TO_CODE = 'GLJE001' THEN
													'JNT - GLOBAL JET - AGENT'
												ELSE
													'JNT - LAIN-LAIN'
												END
									END
								ELSE 'Trade'
						END AS JENIS
			FROM	(
						SELECT
								SSIH.[Posting Date] AS TGL_INV
								,SSIH.[No_] AS NO_INV
								,SSIH.[Salesperson Code] AS SALES
								,SSIH.[Sell-to Customer No_] AS SELL_TO_CODE
								,SSIH.[Sell-to Customer Name] AS SELL_TO_NAME
								,SSIH.[Bill-to Customer No_] AS BILL_TO_CODE
								,SSIH.[Bill-to Name] AS BILL_TO_NAME
								,SSIH.[Posting Description] AS DESCRIPTIONS
								,SSIH.[External Document No_] AS NO_PO_CUST
								,SSIH.[Faktur Pajak No_] AS NO_FP
								,SSIH.[Currency Code] AS VALUTA
								,SUM(SSID.[Amount Including VAT]) AS NILAI_INVOICE
								,CUS.[Customer Posting Group] AS CUST_GRP
								,MAX(CASE WHEN SSIH.[Order No_] <> '' THEN SSIH.[Order No_] ELSE SSIH.[Prepayment Order No_] END) AS NO_SO
						FROM [dbo].[BC_SOLUSI$Sales Invoice Header] SSIH
						INNER JOIN [dbo].[BC_SOLUSI$Sales Invoice Line] SSID ON SSID.[Document No_] = SSIH.[No_]
						LEFT JOIN  [dbo].[BC_AUTOJAYA$Customer] CUS ON CUS.[No_] = SSIH.[Bill-to Customer No_]
						GROUP BY SSIH.[Posting Date], SSIH.[No_]
								,SSIH.[Sell-to Customer No_]
								,SSIH.[Sell-to Customer Name]
								,SSIH.[Bill-to Customer No_]
								,SSIH.[Bill-to Name]
								,SSIH.[Posting Description]
								,SSIH.[External Document No_]
								,SSIH.[Faktur Pajak No_]
								,SSIH.[Currency Code]
								,SSIH.[Salesperson Code]
								,CUS.[Customer Posting Group]
					) X
			LEFT JOIN [dbo].[BC_SOLUSI$Sales Cr_Memo Header] CRM ON CRM.[Applies-to Doc_ No_] = X.NO_INV
			INNER JOIN (
							SELECT
									   CLE.[Document No_] AS NO_INV
									  ,CLE.[External Document No_] AS NO_PO_CUST
									  ,CLD.RMN_AMOUNT
									  ,CLD2.TGL_LUNAS
									  ,CLD2.NO_PELUNASAN
							FROM [dbo].[BC_SOLUSI$Cust_ Ledger Entry] CLE
							LEFT JOIN (
										SELECT	CLED.[Cust_ Ledger Entry No_]
												,SUM(CLED.[Amount]) AS RMN_AMOUNT
										FROM [dbo].[BC_SOLUSI$Detailed Cust_ Ledg_ Entry] CLED
										GROUP BY CLED.[Cust_ Ledger Entry No_]
									  )CLD ON CLD.[Cust_ Ledger Entry No_] = CLE.[Entry No_]
							LEFT JOIN (
										SELECT	CLED2.[Cust_ Ledger Entry No_]
												,MAX(CLED2.[Posting Date]) AS TGL_LUNAS
												,MAX(CLED2.[Document No_]) AS NO_PELUNASAN
										FROM [dbo].[BC_SOLUSI$Detailed Cust_ Ledg_ Entry] CLED2
										WHERE CLED2.[Document Type] <> 2
										GROUP BY CLED2.[Cust_ Ledger Entry No_]
									  ) CLD2 ON CLD2.[Cust_ Ledger Entry No_] = CLE.[Entry No_]
							WHERE CLD.RMN_AMOUNT = 0
					  )	LN ON LN.NO_INV = X.NO_INV
			LEFT JOIN [dbo].[BC_SOLUSI$Sales Shipment Header] SJH ON SJH.[Order No_] = X.NO_SO
			WHERE	CRM.[No_] IS NULL AND
					X.NO_SO <> '' AND
					X.NILAI_INVOICE <> 0 AND
					LN.NO_PELUNASAN IS NOT NULL AND
					($P{Company} = 'SOLUSI' OR $P{Company} = 'ALL') AND
					(X.CUST_GRP = $P{CustomerGroup} OR $P{CustomerGroup} = 'ALL') AND
					(X.BILL_TO_CODE = $P{CustomerNo} OR $P{CustomerNo} = 'ALL') AND
					(X.TGL_INV >= $P{TglInvFrom} AND X.TGL_INV <= $P{TglInvUpto})
			GROUP BY X.TGL_INV
					,X.NO_INV
