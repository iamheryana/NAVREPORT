												WHEN X.BILL_TO_CODE = 'ID01602' THEN 'No Invoice NON Khusus - INTI PAKET'
												WHEN X.BILL_TO_CODE = 'ID01436' THEN 'No Invoice NON Khusus - INTI IDOLA'
												ELSE
													CASE WHEN X.SELL_TO_CODE = 'IDMR008' THEN 'No Invoice NON Khusus - ICC - Franchise'
														 WHEN X.SELL_TO_CODE = 'IDMR110' THEN 'No Invoice NON Khusus - ICC - Franchise'
														 WHEN X.SELL_TO_CODE = 'IDMR180' THEN 'No Invoice NON Khusus - INDOMARCO - Franchise'
														 WHEN X.SELL_TO_CODE = 'IDMR180' THEN 'No Invoice NON Khusus - INDOMARCO - Franchise'
														 WHEN X.SELL_TO_CODE = 'ID01602' THEN 'No Invoice NON Khusus - INTI PAKET - Franchise'
														 WHEN X.SELL_TO_CODE = 'ID01436' THEN 'No Invoice NON Khusus - INTI IDOLA - Franchise'
														 WHEN X.SELL_TO_CODE LIKE 'DC%' THEN
															CASE WHEN X.BILL_TO_CODE NOT LIKE 'DC%' THEN
																'No Invoice NON Khusus - INDOMARCO - Franchise'
															ELSE
																'No Invoice NON Khusus - INDOMARCO LAIN-LAIN'
															END
														 ELSE 'No Invoice NON Khusus - INDOMARCO LAIN-LAIN'
													END
												END
									ELSE
										CASE	WHEN X.BILL_TO_CODE = 'IDMR180' THEN 'No Invoice Khusus - INDOMARCO'
												WHEN X.BILL_TO_CODE = 'ID00284' THEN 'No Invoice Khusus - INDOMARCO'
												WHEN X.BILL_TO_CODE = 'IDMR008' THEN 'No Invoice Khusus - ICC'
												WHEN X.BILL_TO_CODE = 'IDMR110' THEN 'No Invoice Khusus - ICC'
												WHEN X.BILL_TO_CODE = 'ID01602' THEN 'No Invoice Khusus - INTI PAKET'
												WHEN X.BILL_TO_CODE = 'ID01436' THEN 'No Invoice Khusus - INTI IDOLA'
												ELSE
													CASE WHEN X.SELL_TO_CODE = 'IDMR008' THEN 'No Invoice Khusus - ICC - Franchise'
														 WHEN X.SELL_TO_CODE = 'IDMR110' THEN 'No Invoice Khusus - ICC - Franchise'
														 WHEN X.SELL_TO_CODE = 'IDMR180' THEN 'No Invoice Khusus - INDOMARCO - Franchise'
														 WHEN X.SELL_TO_CODE = 'IDMR180' THEN 'No Invoice Khusus - INDOMARCO - Franchise'
														 WHEN X.SELL_TO_CODE = 'ID01602' THEN 'No Invoice Khusus - INTI PAKET - Franchise'
														 WHEN X.SELL_TO_CODE = 'ID01436' THEN 'No Invoice Khusus - INTI IDOLA - Franchise'
														 WHEN X.SELL_TO_CODE LIKE 'DC%' THEN
															CASE WHEN X.BILL_TO_CODE NOT LIKE 'DC%' THEN
																'No Invoice Khusus - INDOMARCO - Franchise'
															ELSE
																'No Invoice Khusus - INDOMARCO LAIN-LAIN'
															END
														 ELSE 'No Invoice Khusus - INDOMARCO LAIN-LAIN'
													END
												END
									END
								WHEN X.CUST_GRP = 'ALFA' THEN
									CASE WHEN X.NO_INV LIKE 'SI%' THEN
										CASE	WHEN X.BILL_TO_CODE = 'ALFA024' THEN 'No Invoice NON Khusus - SAT'
												WHEN X.BILL_TO_CODE = 'AL00605' THEN 'No Invoice NON Khusus - SAT'
												WHEN X.BILL_TO_CODE = 'ALFA227' THEN 'No Invoice NON Khusus - MIDI'
												ELSE
													CASE WHEN SELL_TO_CODE LIKE 'DC%' THEN
															CASE WHEN BILL_TO_CODE NOT LIKE 'DC%' THEN
																'No Invoice NON Khusus - SAT - Franchise'
															ELSE
																'No Invoice NON Khusus - SAT LAIN-LAIN'
															END
													ELSE
														'No Invoice NON Khusus - SAT LAIN-LAIN'
													END
										END
									ELSE
										CASE	WHEN X.BILL_TO_CODE = 'ALFA024' THEN 'No Invoice Khusus - SAT'
												WHEN X.BILL_TO_CODE = 'ALFA227' THEN 'No Invoice Khusus - MIDI'
												ELSE
													CASE WHEN X.SELL_TO_CODE LIKE 'DC%' THEN
															CASE WHEN X.BILL_TO_CODE NOT LIKE 'DC%' THEN
																'No Invoice Khusus - SAT - Franchise'
															ELSE
																'No Invoice Khusus - SAT LAIN-LAIN'
															END
													ELSE
														'No Invoice Khusus - SAT LAIN-LAIN'
													END
										END
									END
								WHEN X.CUST_GRP = 'JNT' THEN
									CASE	WHEN X.BILL_TO_CODE = 'GLJE001' THEN 'JNT - GLOBAL JET EXPRESS'
											WHEN X.BILL_TO_CODE = 'GLJE002' THEN 'JNT - GLOBAL JET ECOMMERCE'
											WHEN X.BILL_TO_CODE = 'GLJE007' THEN 'JNT - SEMUT MERAH SQUAD'
											WHEN X.BILL_TO_CODE = 'GLJE030' THEN 'JNT - LUCKY MOM INDONESIA'
											WHEN X.BILL_TO_CODE = 'GLES001' THEN 'JNT - GLOBAL EXPRESS SEJAHTERA'
											WHEN X.BILL_TO_CODE = 'GLJE032' THEN 'JNT - GLOBAL JET CARGO'
											ELSE
												CASE WHEN X.SELL_TO_CODE = 'GLJE001' THEN
