	<queryString>
		<![CDATA[SELECT	XX.COMPANY
		,XX.TGL_INV
		,XX.NO_INV
		,XX.NO_FP
		,XX.NO_PO_CUST
		,XX.SELL_TO_CODE
		,XX.SELL_TO_NAME
		,XX.BILL_TO_CODE
		,XX.BILL_TO_NAME
		,XX.DESCRIPTIONS
		,XX.NILAI_INVOICE
		,XX.TGL_LUNAS
		,XX.NO_PELUNASAN
		,XX.SELISIH
		,AVG(XX.SELISIH) OVER  (PARTITION BY XX.JENIS ORDER BY XX.JENIS) AS RATA_RATA
		,XX.TGL_SJ
		,XX.NO_SJ
		,XX.LAMA_BTB
		,AVG(XX.LAMA_BTB) OVER  (PARTITION BY XX.JENIS ORDER BY XX.JENIS) AS RATA_RATA_BTB
		,XX.JENIS
