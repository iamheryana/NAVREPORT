																				CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) > ($P{Tahun} + 1) THEN
																					CASE WHEN DATEPART(year, GETDATE()) <= ($P{Tahun} + 1) THEN
																						($P{Tahun} + 1)
