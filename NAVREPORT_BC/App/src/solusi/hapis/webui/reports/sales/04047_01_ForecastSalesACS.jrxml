<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="04047_01_ForecastSalesACS" language="groovy" pageWidth="543" pageHeight="595" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="503" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="b406b291-7ccc-4e85-8d33-6ed4d9da73cd">
	<property name="ireport.zoom" value="1.610510000000168"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="2"/>
	<parameter name="Tahun" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="AmtBig" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="SpvBy" class="java.lang.String"/>
	<parameter name="Sem1Akhir" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="TglUpto" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="weightH" class="java.math.BigDecimal"/>
	<parameter name="weightM" class="java.math.BigDecimal"/>
	<parameter name="weightL" class="java.math.BigDecimal"/>
	<parameter name="ProsesId" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT	POP.MAIN_GRP
		,POP.GROUP_NO
		,POP.GROUP_DESC
		,POP.NO_BARIS
		,POP.SUBGROUP_NO
		,POP.SUBGROUP_DESC
		,POP.AMOUNT
		,POP.AMOUNT_S1
		,POP.AMOUNT_S2
		,POP.AMOUNT_S1_NY
		,POP.AMOUNT_S2_NY
		,POP.AMT_1
		,POP.AMT_2
		,POP.AMT_TTL
		, CASE WHEN POP.GROUP_NO = 2 THEN POP.AMT_1 ELSE POP.AMT_1 * -1 END AS SUM_AMT_1
		, CASE WHEN POP.GROUP_NO = 2 THEN POP.AMT_2 ELSE POP.AMT_2 * -1 END AS SUM_AMT_2
		, CASE WHEN POP.GROUP_NO = 2 THEN POP.AMT_TTL ELSE POP.AMT_TTL * -1 END AS SUM_AMT_TTL
		,POP.TITLE_1
		,POP.TITLE_2
		,POP.TITLE_TTL
		,POP.TOTAL_GROUP
		,POP.TOTAL_MAIN_GROUP
FROM	(
SELECT	PPP.MAIN_GRP
		,PPP.GROUP_NO
		,PPP.GROUP_DESC
		,PPP.NO_BARIS
		,PPP.SUBGROUP_NO
		,PPP.SUBGROUP_DESC
		,PPP.AMOUNT
		,PPP.AMOUNT_S1
		,PPP.AMOUNT_S2
		,PPP.AMOUNT_S1_NY
		,PPP.AMOUNT_S2_NY
		,	CASE WHEN PPP.MODEL_LAP = 1 THEN
				PPP.AMOUNT_S1
			ELSE
				CASE WHEN PPP.MAIN_GRP = 1 THEN
					PPP.AMOUNT ELSE PPP.AMOUNT_S2
				END
			END AS AMT_1
		,	CASE WHEN PPP.MODEL_LAP = 1 THEN
				PPP.AMOUNT_S2 ELSE (PPP.AMOUNT_S1_NY + PPP.AMOUNT_S2_NY)
			END AS AMT_2
		,	CASE WHEN PPP.MODEL_LAP = 1 THEN
				(PPP.AMOUNT_S1 + PPP.AMOUNT_S2) ELSE NULL
			END AS AMT_TTL
		,	CASE WHEN PPP.MODEL_LAP = 1 THEN
				'SEM. I - '+ CAST(($P{Tahun}) as varchar(4)) ELSE CAST(($P{Tahun}) as varchar(4))
			END AS TITLE_1
		,	CASE WHEN PPP.MODEL_LAP = 1 THEN
				'SEM. II - '+ CAST(($P{Tahun}) as varchar(4)) ELSE CAST(($P{Tahun} + 1) as varchar(4))
			END AS TITLE_2
		,	CASE WHEN PPP.MODEL_LAP = 1 THEN
				'TOTAL' ELSE NULL
			END AS TITLE_TTL
		,	CASE WHEN PPP.MAIN_GRP =  1 THEN
				CASE WHEN PPP.GROUP_NO = 1 THEN 'TOTAL CLOSE SALES - ' + CAST(($P{Tahun}) as varchar(4))
						WHEN PPP.GROUP_NO = 2 THEN 'SALES TARGET - ' + CAST(($P{Tahun}) as varchar(4))
						ELSE ''
				END
			ELSE
				'TOTAL EXISTING PIPELINE - '	+ CAST(($P{Tahun}) as varchar(4))
			END AS TOTAL_GROUP
		,	CASE WHEN PPP.MAIN_GRP =  1 THEN
				'SALES - "STILL TO BE ACHIEVE" / ("OVER") - '+ CAST(($P{Tahun}) as varchar(4))
			ELSE
				CASE WHEN PPP.MODEL_LAP = 1 THEN
					'PIPELINE - "STILL NEEDED" / (OVER) - '+ CAST(($P{Tahun}) as varchar(4))
				ELSE
					'PIPELINE - "STILL NEEDED" / ("OVER") - UPTO OCT '+ CAST(($P{Tahun}) as varchar(4))
				END
			END AS TOTAL_MAIN_GROUP
FROM	(
SELECT	PIP.MAIN_GRP
		,PIP.GROUP_NO
		,PIP.GROUP_DESC
		,PIP.NO_BARIS
		,PIP.SUBGROUP_NO
		,PIP.SUBGROUP_DESC
		,PIP.AMOUNT
		,PIP.AMOUNT_S1
		,PIP.AMOUNT_S2
		,PIP.AMOUNT_S1_NY
		,PIP.AMOUNT_S2_NY
		,	CASE WHEN DATEPART(YEAR, $P{TglUpto}) <> $P{Tahun} THEN	1 ELSE
				CASE WHEN  SUBSTRING(CONVERT(varchar, $P{TglUpto}, 105), 7,4) >= SUBSTRING(CONVERT(varchar,  GETDATE(), 105), 7,4) THEN
					CASE WHEN  DATEPART(QUARTER, $P{TglUpto})  IN (3,4) THEN 2 ELSE 1 END
				ELSE
					1
				END
			END AS MODEL_LAP
FROM	(
			SELECT  1 AS MAIN_GRP
					,2 AS GROUP_NO
					,('SALES TARGET ') AS GROUP_DESC
					,'1' AS NO_BARIS
					,1 AS SUBGROUP_NO
					,'Sales Target' AS SUBGROUP_DESC
					,ISNULL((ROUND((SUM(TGT.TGT_S1)/1000000),2) + ROUND((SUM(TGT.TGT_S2)/1000000),2)) , 0)AS AMOUNT
					,ISNULL(ROUND((SUM(TGT.TGT_S1)/1000000),2),0) AS AMOUNT_S1
					,ISNULL(ROUND((SUM(TGT.TGT_S2)/1000000),2),0) AS AMOUNT_S2
					, 0 AS AMOUNT_S1_NY
					, 0 AS AMOUNT_S2_NY
			FROM	(
						SELECT
								CASE WHEN M06.JENIS = 'S1' THEN M06.[TARGET] ELSE 0 END AS TGT_S1
								,CASE WHEN M06.JENIS = 'S2' THEN M06.[TARGET] ELSE 0 END AS TGT_S2
						FROM	[dbo].[M06_TARGET_PIPELINE] M06
						WHERE	M06.TAHUN = $P{Tahun}  AND
								M06.JENIS <> 'SALES'
					) TGT
			UNION ALL
			SELECT	SRX.MAIN_GRP
					,SRX.GROUP_NO
					,SRX.GROUP_DESC
					,SRX.NO_BARIS
					,SRX.SUBGROUP_NO
					,SRX.SUBGROUP_DESC
					,SRX.AMOUNT
					,SRX.AMOUNT_S1
					,SRX.AMOUNT_S2
					,SRX.AMOUNT_S1_NY
					,SRX.AMOUNT_S2_NY
			FROM	(
			SELECT	1 AS MAIN_GRP
					,1 AS GROUP_NO
					,('SALES REVENUE ') AS GROUP_DESC
					,'-' AS NO_BARIS
					,1 AS SUBGROUP_NO
					,'Sales Revenue' AS SUBGROUP_DESC
					,ISNULL((ROUND((SUM(XX.AMOUNT_S1)/1000000),2) + ROUND((SUM(XX.AMOUNT_S2)/1000000),2)) , 0)AS AMOUNT
					,ISNULL(ROUND((SUM(XX.AMOUNT_S1)/1000000),2),0) AS AMOUNT_S1
					,ISNULL(ROUND((SUM(XX.AMOUNT_S2)/1000000),2),0) AS AMOUNT_S2
					, 0 AS AMOUNT_S1_NY
					, 0 AS AMOUNT_S2_NY
			FROM	(
						SELECT	SUM(CASE WHEN Q_OUT_SO IN (1,2) THEN AMOUNT ELSE 0 END) AS AMOUNT_S1
								, SUM(CASE WHEN Q_OUT_SO IN (3,4) THEN AMOUNT ELSE 0 END) AS AMOUNT_S2
						FROM [dbo].[TEMP08_SALES_REVENUE]
						WHERE	GROUP_NO = 1 AND
								PROSES_ID = $P{ProsesId}
					) XX
			UNION ALL
			SELECT	1 AS MAIN_GRP
					,1 AS GROUP_NO
					,('SALES REVENUE ') AS GROUP_DESC
					,'-' AS NO_BARIS
					,2 AS SUBGROUP_NO
					,'Down Payment' AS SUBGROUP_DESC
					,(ROUND((SUM(UMX.AMOUNT_S1)/1000000),2)+ROUND((SUM(UMX.AMOUNT_S2)/1000000),2)) AS AMOUNT
					, ROUND((SUM(UMX.AMOUNT_S1)/1000000),2) AS AMOUNT_S1
					, ROUND((SUM(UMX.AMOUNT_S2)/1000000),2) AS AMOUNT_S2
					, ROUND((SUM(UMX.AMOUNT_S1_NY)/1000000),2) AS AMOUNT_S1_NY
					, ROUND((SUM(UMX.AMOUNT_S2_NY)/1000000),2) AS AMOUNT_S2_NY

			FROM	(
						SELECT	SUM(CASE WHEN YEAR_OUT_SO = $P{Tahun} AND Q_OUT_SO IN (1,2) THEN AMOUNT ELSE 0 END) AS AMOUNT_S1
								, SUM(CASE WHEN YEAR_OUT_SO = $P{Tahun} AND Q_OUT_SO IN (3,4) THEN AMOUNT ELSE 0 END) AS AMOUNT_S2
								, SUM(CASE WHEN YEAR_OUT_SO = $P{Tahun} + 1 AND Q_OUT_SO IN (1,2) THEN AMOUNT ELSE 0 END) AS AMOUNT_S1_NY
								, SUM(CASE WHEN YEAR_OUT_SO = $P{Tahun} + 1 AND Q_OUT_SO IN (3,4) THEN AMOUNT ELSE 0 END) AS AMOUNT_S2_NY
						FROM [dbo].[TEMP08_SALES_REVENUE]
						WHERE	GROUP_NO = 2 AND
								PROSES_ID = $P{ProsesId}
					) UMX
			UNION ALL
			SELECT	1 AS MAIN_GRP
					,1 AS GROUP_NO
					,('SALES REVENUE ') AS GROUP_DESC
					,'-' AS NO_BARIS
					,3 AS SUBGROUP_NO
					,'Outstanding SO' AS SUBGROUP_DESC
					,(ROUND((SUM(OSO.AMOUNT_S1)/1000000),2)+ROUND((SUM(OSO.AMOUNT_S2)/1000000),2)) AS AMOUNT
					, ROUND((SUM(OSO.AMOUNT_S1)/1000000),2) AS AMOUNT_S1
					, ROUND((SUM(OSO.AMOUNT_S2)/1000000),2) AS AMOUNT_S2
					, ROUND((SUM(OSO.AMOUNT_S1_NY)/1000000),2) AS AMOUNT_S1_NY
					, ROUND((SUM(OSO.AMOUNT_S2_NY)/1000000),2) AS AMOUNT_S2_NY
			FROM	(
						SELECT	SUM(CASE WHEN YEAR_OUT_SO = $P{Tahun} AND Q_OUT_SO IN (1,2) THEN AMOUNT ELSE 0 END) AS AMOUNT_S1
								, SUM(CASE WHEN YEAR_OUT_SO = $P{Tahun} AND Q_OUT_SO IN (3,4) THEN AMOUNT ELSE 0 END) AS AMOUNT_S2
								, SUM(CASE WHEN YEAR_OUT_SO = $P{Tahun} + 1 AND Q_OUT_SO IN (1,2) THEN AMOUNT ELSE 0 END) AS AMOUNT_S1_NY
								, SUM(CASE WHEN YEAR_OUT_SO = $P{Tahun} + 1 AND Q_OUT_SO IN (3,4) THEN AMOUNT ELSE 0 END) AS AMOUNT_S2_NY
						FROM [dbo].[TEMP08_SALES_REVENUE]
						WHERE	GROUP_NO = 3 AND
								PROSES_ID = $P{ProsesId}
					) OSO
					) SRX
			WHERE SRX.AMOUNT <> 0
			UNION ALL
			SELECT	XI.MAIN_GRP
					,XI.GROUP_NO
					,'EXISTING PIPELINE' AS GROUP_DESC
					,XI.NO_BARIS
					,XI.SUBGROUP_NO
					,XI.GROUP_DESC AS SUBGROUP_DESC
					, CASE	WHEN XI.SUBGROUP_NO = 1 THEN SUM(XI.AMOUNT)
							WHEN XI.SUBGROUP_NO = 2 THEN SUM(XI.AMOUNT) * ($P{weightH}/100)
							WHEN XI.SUBGROUP_NO = 3 THEN SUM(XI.AMOUNT) * ($P{weightM}/100)
							WHEN XI.SUBGROUP_NO = 4 THEN SUM(XI.AMOUNT) * ($P{weightL}/100)
							ELSE 0 END  AS AMOUNT
					, CASE	WHEN XI.SUBGROUP_NO = 1 THEN SUM(XI.AMOUNT_S1)
							WHEN XI.SUBGROUP_NO = 2 THEN SUM(XI.AMOUNT_S1) * ($P{weightH}/100)
							WHEN XI.SUBGROUP_NO = 3 THEN SUM(XI.AMOUNT_S1) * ($P{weightM}/100)
							WHEN XI.SUBGROUP_NO = 4 THEN SUM(XI.AMOUNT_S1) * ($P{weightL}/100)
							ELSE 0 END  AS AMOUNT_S1
					, CASE	WHEN XI.SUBGROUP_NO = 1 THEN SUM(XI.AMOUNT_S2)
							WHEN XI.SUBGROUP_NO = 2 THEN SUM(XI.AMOUNT_S2) * ($P{weightH}/100)
							WHEN XI.SUBGROUP_NO = 3 THEN SUM(XI.AMOUNT_S2) * ($P{weightM}/100)
							WHEN XI.SUBGROUP_NO = 4 THEN SUM(XI.AMOUNT_S2) * ($P{weightL}/100)
							ELSE 0 END  AS AMOUNT_S2
					, CASE	WHEN XI.SUBGROUP_NO = 1 THEN SUM(XI.AMOUNT_S1_NY)
							WHEN XI.SUBGROUP_NO = 2 THEN SUM(XI.AMOUNT_S1_NY) * ($P{weightH}/100)
							WHEN XI.SUBGROUP_NO = 3 THEN SUM(XI.AMOUNT_S1_NY) * ($P{weightM}/100)
							WHEN XI.SUBGROUP_NO = 4 THEN SUM(XI.AMOUNT_S1_NY) * ($P{weightL}/100)
							ELSE 0 END  AS AMOUNT_S1_NY
					, CASE	WHEN XI.SUBGROUP_NO = 1 THEN SUM(XI.AMOUNT_S2_NY)
							WHEN XI.SUBGROUP_NO = 2 THEN SUM(XI.AMOUNT_S2_NY) * ($P{weightH}/100)
							WHEN XI.SUBGROUP_NO = 3 THEN SUM(XI.AMOUNT_S2_NY) * ($P{weightM}/100)
							WHEN XI.SUBGROUP_NO = 4 THEN SUM(XI.AMOUNT_S2_NY) * ($P{weightL}/100)
							ELSE 0 END  AS AMOUNT_S2_NY
			FROM	(
						SELECT	2 AS MAIN_GRP
								,1 AS GROUP_NO
								,	CASE	WHEN MIN(X1.GROUP_NO) =  1 THEN	'(A)'
											WHEN MIN(X1.GROUP_NO) =  2 THEN	'(H)'
											WHEN MIN(X1.GROUP_NO) =  3 THEN	'(M)'
											WHEN MIN(X1.GROUP_NO) =  4 THEN	'(L)'
											ELSE '(L)'
									END AS NO_BARIS
								,MIN(X1.GROUP_NO) AS SUBGROUP_NO
								,X1.GROUP_PIPELINE
								,	CASE	WHEN MIN(X1.GROUP_NO) =  1 THEN	'AWARDED PROJECTS'
											WHEN MIN(X1.GROUP_NO) =  2 THEN	'HIGH POTENTIAL CLOSE PROJECTS'
											WHEN MIN(X1.GROUP_NO) =  3 THEN	'MEDIUM POTENTIAL BIG/STRATEGIC PROJECTS'
											WHEN MIN(X1.GROUP_NO) =  4 THEN	'LOW POTENTIAL BIG/STRATEGIC PROJECTS'
											ELSE 'LOW POTENTIAL BIG/STRATEGIC PROJECTS'
									END AS GROUP_DESC
								,	CASE WHEN MIN(X1.GROUP_NO) = 1 THEN
										'AWARDED (A)'
									ELSE
										CASE WHEN MIN(X1.GROUP_NO) <> 5 THEN X1.CUSTOMER ELSE X1.BRANCHES_NAME END
									END AS SUBGROUP_DESC
								,	CASE WHEN MIN(X1.GROUP_NO) <> 5 THEN X1.SPV_BY ELSE '' END AS SPV_BY
								,	CASE WHEN MIN(X1.GROUP_NO) <> 5 THEN X1.SALES_CODE ELSE '' END AS SALES_CODE
								, X1.BRANCHES
								, SUM(X1.AMOUNT) AS AMOUNT
								, SUM(X1.AMOUNT_S1) AS AMOUNT_S1
								, SUM(X1.AMOUNT_S2) AS AMOUNT_S2
								, 0 AS AMOUNT_S1_NY
								, 0 AS AMOUNT_S2_NY
						FROM	(
									SELECT	SLS.[Global Dimension 1 Code] AS BRANCHES
											, CAB.[Name] AS BRANCHES_NAME
											, X.SALES_CODE
											, X.CUSTOMER
											, X.CUSTOMER_REAL
											, MAX(MAX(X.SPV_BY)) OVER (PARTITION BY X.CUSTOMER_REAL, CAB.[Name]) AS SPV_BY
											,	CASE WHEN X.SIZE_PROJECT = 'A' THEN
													1
												ELSE
													CASE WHEN X.SIZE_PROJECT = 'H' THEN
														2
													ELSE
														CASE WHEN X.SIZE_PROJECT = 'M' THEN
															3
														ELSE
															CASE WHEN X.SIZE_PROJECT = 'L' THEN
																4
															ELSE
																CASE WHEN ($P{AmtBig}*1) <= 0 THEN
																	CASE WHEN SUM(ISNULL(CUST.ADA,0)) > 0 THEN 4 ELSE 4 END
																ELSE
																	CASE WHEN SUM(X.AMOUNT) >= ($P{AmtBig}*1) THEN 4
																	ELSE
																		CASE WHEN SUM(ISNULL(CUST.ADA,0)) > 0 THEN 4 ELSE 4 END
																	END
																END
															END
														END
													END
												END AS GROUP_NO
											,	CASE	WHEN X.SIZE_PROJECT = 'A' THEN 'P1'
														WHEN X.SIZE_PROJECT = 'H' THEN 'P2'
														WHEN X.SIZE_PROJECT = 'M' THEN 'P3'
														ELSE 'P4'
												END AS GROUP_PIPELINE
											,SUM(X.AMOUNT) AS AMOUNT
											,SUM(X.AMOUNT_S1) AS AMOUNT_S1
											,SUM(X.AMOUNT_S2) AS AMOUNT_S2
									FROM	(
												SELECT	MAD.COMPANY
														,MAD.SALES_CODE
														,MAD.CABANG
														,MAD.CUSTOMER
														,MAD.CUSTOMER_REAL
														,UPPER(MAD.SIZE_PROJECT) AS SIZE_PROJECT
														,MAD.POTENSIAL_PROJECT
														,MAD.SPV_BY
														,MAD.ITEM
														,MAD.ITEM_CAT
														,MAD.HW_PS
														,ROUND((
															CASE WHEN MAD.JML_TAHUN <> 0 THEN
																CASE WHEN  MAD.EST_Q = 1234 THEN
																	((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN)
																ELSE
																	CASE	WHEN MAD.EST_Q IN (123, 234) THEN (((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4) * 3
																			WHEN MAD.EST_Q IN (12, 34) THEN ((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 2
																			WHEN MAD.EST_Q IN (1,2,3,4) THEN (((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																			ELSE 0
																	END
																END
															ELSE
																(MAD.UNIT_PRICE * MAD.QTY_OUT)
															END
															/ 1000000 ),2) AS AMOUNT
														,	ROUND(((
																		CASE WHEN MAD.JML_TAHUN <> 0 THEN
																			CASE WHEN MAD.EST_Q IN (1, 1234, 123, 12) THEN
																				(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																			ELSE
																				0
																			END
																		ELSE
																			CASE WHEN MAD.EST_Q IN (1, 1234, 123, 12) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																		END
																		+
																		CASE WHEN MAD.JML_TAHUN <> 0 THEN
																			CASE WHEN MAD.EST_Q IN (2, 1234, 123, 12, 234) THEN
																				(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																			ELSE
																				0
																			END
																		ELSE
																			CASE WHEN MAD.EST_Q IN (2, 1234, 123, 12, 234) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																		END
															) / 1000000) ,2) AS AMOUNT_S1
														,	ROUND(((
																		CASE WHEN MAD.JML_TAHUN <> 0 THEN
																			CASE WHEN MAD.EST_Q IN (3, 1234, 123, 234, 34) THEN
																				(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																			ELSE
																				0
																			END
																		ELSE
																			CASE WHEN MAD.EST_Q IN (3, 1234, 123, 234, 34) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																		END
																		+
																		CASE WHEN MAD.JML_TAHUN <> 0 THEN
																			CASE WHEN MAD.EST_Q IN (4, 1234, 234, 34) THEN
																				(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																			ELSE
																				0
																			END
																		ELSE
																			CASE WHEN MAD.EST_Q IN (4, 1234, 234, 34) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																		END
															) / 1000000) ,2) AS AMOUNT_S2
														,MAD.DIM_PRINCIPAL
														,MAD.EST_YEAR
												FROM	(
															SELECT	CASE WHEN SOH.[No_ Series] = 'SBOM' THEN 'AJ' ELSE 'SP' END AS COMPANY
																	,SOH.[Salesperson Code] AS SALES_CODE
																	,SOH.[Responsibility Center] AS CABANG
																	,	CASE WHEN SOH.[Payment Discount _] <> 0 THEN
																			'*) '+SOH.[Sell-to Customer Name]
																		ELSE
																			SOH.[Sell-to Customer Name]
																		END AS CUSTOMER
																	,	SOH.[Sell-to Customer Name] AS CUSTOMER_REAL
																	, 	CASE WHEN SOL.[Warranty Code] = '' THEN
																			SUBSTRING(SOH.[Posting Description], 1,1)
																		ELSE
																			CASE WHEN SOL.[Warranty Code] = 'CLOSED' THEN 'H' ELSE SUBSTRING(SOH.[Posting Description], 1,1) END
																		END AS SIZE_PROJECT
																	, CASE WHEN SUBSTRING(SOH.[Posting Description], 2,1) NOT IN ('1','0') THEN '0' ELSE SUBSTRING(SOH.[Posting Description], 2,1) END AS POTENSIAL_PROJECT
																	,SOH.[Sell-to County] AS SPV_BY
																	,SOL.[No_] AS ITEM
																	,SOL.[Description] AS ITEM_DESC
																	,SOL.[Shortcut Dimension 2 Code] AS ITEM_CAT
																	,SUBSTRING(SOL.[Shortcut Dimension 2 Code] , 1,2) AS HW_PS
																	,SOL.[Quantity] AS QTY_OUT
																	,SOL.[Unit Price] AS UNIT_PRICE
																	,DIMD.DIM_PRINCIPAL
																	,SOH.[Payment Discount _] AS JML_TAHUN
																	,	CASE WHEN SOH.[Payment Discount _] <> 0 THEN
																			CASE WHEN DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date]) <=  GETDATE() THEN
																				CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) > $P{Tahun} THEN
																					CASE WHEN DATEPART(year, GETDATE()) <= $P{Tahun} THEN
																						$P{Tahun}
																					ELSE
																						0
																					END
																				ELSE
																					CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) = $P{Tahun} THEN
																						CASE WHEN DATEPART(quarter, GETDATE()) IN (1,2) THEN 0 ELSE $P{Tahun} END
																					ELSE
																						0
																					END
																				END
																			ELSE
																				CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) - 1)) > $P{Tahun} THEN
																					CASE WHEN DATEPART(year, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) <= $P{Tahun} THEN
																						$P{Tahun}
																					ELSE
																						0
																					END
																				ELSE
