										GROUP BY  XXX.NO_SJ, XXX.TGL_INV
									) DOI ON DOI.NO_SJ = DOO.NO_SJ
						LEFT JOIN [dbo].[AUTOJAYA$Sales Cr_Memo Header] CRM ON CRM.[Applies-to Doc_ No_] = DOI.NO_INVOICE
