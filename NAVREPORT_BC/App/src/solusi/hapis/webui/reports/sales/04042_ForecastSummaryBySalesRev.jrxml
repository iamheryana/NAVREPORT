<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="04042_ForecastSummaryBySalesRev" language="groovy" pageWidth="1600" pageHeight="595" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="1560" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="b406b291-7ccc-4e85-8d33-6ed4d9da73cd">
	<property name="ireport.zoom" value="1.3310000000001325"/>
	<property name="ireport.x" value="1050"/>
	<property name="ireport.y" value="96"/>
	<parameter name="Tahun" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Potensi" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Size" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Cabang" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Sem1Akhir" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ProsesId" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT	1 AS GRP
		, XI.CABANG
		, XI.CABANG_NAMA
		, XI.SALES_CODE
		, XI.SALES_NAME
		, XI.AMOUNT_POTENSI_Q1_0
		, XI.AMOUNT_POTENSI_Q1_1
		, XI.AMOUNT_POTENSI_Q2_0
		, XI.AMOUNT_POTENSI_Q2_1
		, XI.AMOUNT_POTENSI_S1_0
		, XI.AMOUNT_POTENSI_S1_1
		, XI.AMOUNT_REALISASI_S1
		, XI.AMOUNT_POTENSI_Q3_0
		, XI.AMOUNT_POTENSI_Q3_1
		, XI.AMOUNT_POTENSI_Q4_0
		, XI.AMOUNT_POTENSI_Q4_1
		, XI.AMOUNT_POTENSI_S2_0
		, XI.AMOUNT_POTENSI_S2_1
		, XI.AMOUNT_REALISASI_S2
		, XI.AMOUNT_REALISASI
		, XI.AMOUNT_POTENSI
		, ISNULL(TRG.AMOUNT_TARGET, 0) AS AMOUNT_TARGET
		, CASE WHEN $P{Tahun} <> DATEPART(year, GETDATE()) THEN 1 ELSE DATEPART(quarter, GETDATE()) END AS CURR_Q
FROM	(
			SELECT	FBR.BRANCHES AS CABANG
					,CAB.[Name] AS CABANG_NAMA
					, FBR.SALES_CODE
					, FBR.SALES_NAME
					, SUM(FBR.AMOUNT_POTENSI_Q1_0) AS AMOUNT_POTENSI_Q1_0
					, SUM(FBR.AMOUNT_POTENSI_Q1_1) AS AMOUNT_POTENSI_Q1_1
					, SUM(FBR.AMOUNT_POTENSI_Q2_0) AS AMOUNT_POTENSI_Q2_0
					, SUM(FBR.AMOUNT_POTENSI_Q2_1) AS AMOUNT_POTENSI_Q2_1
					, (	SUM(FBR.AMOUNT_POTENSI_Q1_0) + SUM(FBR.AMOUNT_POTENSI_Q2_0) ) AS AMOUNT_POTENSI_S1_0
					, (	SUM(FBR.AMOUNT_POTENSI_Q1_1) + SUM(FBR.AMOUNT_POTENSI_Q2_1) ) AS AMOUNT_POTENSI_S1_1
					, SUM(FBR.AMOUNT_REALISASI_S1) AS AMOUNT_REALISASI_S1
					, SUM(FBR.AMOUNT_POTENSI_Q3_0) AS AMOUNT_POTENSI_Q3_0
					, SUM(FBR.AMOUNT_POTENSI_Q3_1) AS AMOUNT_POTENSI_Q3_1
					, SUM(FBR.AMOUNT_POTENSI_Q4_0) AS AMOUNT_POTENSI_Q4_0
					, SUM(FBR.AMOUNT_POTENSI_Q4_1) AS AMOUNT_POTENSI_Q4_1
					, (	SUM(FBR.AMOUNT_POTENSI_Q3_0) + SUM(FBR.AMOUNT_POTENSI_Q4_0) ) AS AMOUNT_POTENSI_S2_0
					, (	SUM(FBR.AMOUNT_POTENSI_Q3_1) + SUM(FBR.AMOUNT_POTENSI_Q4_1) ) AS AMOUNT_POTENSI_S2_1
					, SUM(FBR.AMOUNT_REALISASI_S2) AS AMOUNT_REALISASI_S2
					, (SUM(FBR.AMOUNT_REALISASI_S1) + SUM(FBR.AMOUNT_REALISASI_S2)) AS AMOUNT_REALISASI
					, SUM(FBR.AMOUNT_POTENSI) AS AMOUNT_POTENSI
			FROM	(
						SELECT U.BRANCHES
								, U.SALES_CODE
								, U.SALES_NAME
								, U.AMOUNT_REALISASI_S1
								, U.AMOUNT_REALISASI_S2
								, U.AMOUNT_POTENSI
								, U.AMOUNT_POTENSI_Q1_0
								, U.AMOUNT_POTENSI_Q1_1
								, U.AMOUNT_POTENSI_Q2_0
								, U.AMOUNT_POTENSI_Q2_1
								, U.AMOUNT_POTENSI_Q3_0
								, U.AMOUNT_POTENSI_Q3_1
								, U.AMOUNT_POTENSI_Q4_0
								, U.AMOUNT_POTENSI_Q4_1
						FROM	(
									SELECT	CASE WHEN XX.SALES_CODE = 'XXX' THEN '10' ELSE SLS.[Global Dimension 1 Code] END AS BRANCHES
											, XX.SALES_CODE
											, CASE WHEN XX.SALES_CODE = 'XXX' THEN 'OTHERS' ELSE  SLS.[Name] END AS SALES_NAME
											, 0 AS AMOUNT_REALISASI_S1
											, 0 AS AMOUNT_REALISASI_S2
											, SUM(XX.AMOUNT_POTENSI) AS AMOUNT_POTENSI
											, SUM(XX.AMOUNT_POTENSI_Q1_0) AS AMOUNT_POTENSI_Q1_0
											, SUM(XX.AMOUNT_POTENSI_Q1_1) AS AMOUNT_POTENSI_Q1_1
											, SUM(XX.AMOUNT_POTENSI_Q2_0) AS AMOUNT_POTENSI_Q2_0
											, SUM(XX.AMOUNT_POTENSI_Q2_1) AS AMOUNT_POTENSI_Q2_1
											, SUM(XX.AMOUNT_POTENSI_Q3_0) AS AMOUNT_POTENSI_Q3_0
											, SUM(XX.AMOUNT_POTENSI_Q3_1) AS AMOUNT_POTENSI_Q3_1
											, SUM(XX.AMOUNT_POTENSI_Q4_0) AS AMOUNT_POTENSI_Q4_0
											, SUM(XX.AMOUNT_POTENSI_Q4_1) AS AMOUNT_POTENSI_Q4_1
									FROM	(
												SELECT	M06.SLS_OR_CAB  AS SALES_CODE
														, M06.[TAHUN] AS YEAR_SUM
														, 0 AS AMOUNT_POTENSI
														, 0 AS AMOUNT_POTENSI_Q1_0
														, 0 AS AMOUNT_POTENSI_Q1_1
														, 0 AS AMOUNT_POTENSI_Q2_0
														, 0 AS AMOUNT_POTENSI_Q2_1
														, 0 AS AMOUNT_POTENSI_Q3_0
														, 0 AS AMOUNT_POTENSI_Q3_1
														, 0 AS AMOUNT_POTENSI_Q4_0
														, 0 AS AMOUNT_POTENSI_Q4_1
												FROM	[dbo].[M06_TARGET_PIPELINE] M06
												WHERE	M06.TAHUN = $P{Tahun} AND
														M06.JENIS = 'SALES' AND
														M06.[STATUS] = 'ACTIVE'
												UNION ALL
												SELECT	X.SALES_CODE
														,X.EST_YEAR AS YEAR_SUM
														,SUM(X.AMOUNT) AS AMOUNT_POTENSI
														, SUM(X.AMOUNT_Q1_0) AS AMOUNT_POTENSI_Q1_0
														, SUM(X.AMOUNT_Q1_1) AS AMOUNT_POTENSI_Q1_1
														, SUM(X.AMOUNT_Q2_0) AS AMOUNT_POTENSI_Q2_0
														, SUM(X.AMOUNT_Q2_1) AS AMOUNT_POTENSI_Q2_1
														, SUM(X.AMOUNT_Q3_0) AS AMOUNT_POTENSI_Q3_0
														, SUM(X.AMOUNT_Q3_1) AS AMOUNT_POTENSI_Q3_1
														, SUM(X.AMOUNT_Q4_0) AS AMOUNT_POTENSI_Q4_0
														, SUM(X.AMOUNT_Q4_1) AS AMOUNT_POTENSI_Q4_1
												FROM	(
															SELECT	MAD.COMPANY
																	,MAD.SALES_CODE
																	,MAD.CABANG
																	,MAD.SIZE_PROJECT
																	,MAD.POTENSIAL_PROJECT
																	,MAD.ITEM
																	,MAD.ITEM_CAT
																	,MAD.HW_PS
																	,	CASE WHEN MAD.JML_TAHUN <> 0 THEN
																			CASE WHEN  MAD.EST_Q = 1234 THEN
																				((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN)
																			ELSE
																				CASE	WHEN MAD.EST_Q IN (123, 234) THEN (((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4) * 3
																						WHEN MAD.EST_Q IN (12, 34) THEN ((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 2
																						WHEN MAD.EST_Q IN (1,2,3,4) THEN (((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																						ELSE 0
																				END
																			END
																		ELSE
																			(MAD.UNIT_PRICE * MAD.QTY_OUT)
																		END AS AMOUNT
																	,	CASE WHEN MAD.POTENSIAL_PROJECT = '0' THEN
																			CASE WHEN MAD.JML_TAHUN <> 0 THEN
																				CASE WHEN MAD.EST_Q IN (1, 1234, 123, 12) THEN
																					(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																				ELSE
																					0
																				END
																			ELSE
																				CASE WHEN MAD.EST_Q IN (1, 1234, 123, 12) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																			END
																		ELSE 0 END AS AMOUNT_Q1_0
																	,	CASE WHEN MAD.POTENSIAL_PROJECT = '1' THEN
																			CASE WHEN MAD.JML_TAHUN <> 0 THEN
																				CASE WHEN MAD.EST_Q IN (1, 1234, 123, 12) THEN
																					(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																				ELSE
																					0
																				END
																			ELSE
																				CASE WHEN MAD.EST_Q IN (1, 1234, 123, 12) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																			END
																		ELSE 0 END AS AMOUNT_Q1_1
																	,	CASE WHEN MAD.POTENSIAL_PROJECT = '0' THEN
																			CASE WHEN MAD.JML_TAHUN <> 0 THEN
																				CASE WHEN MAD.EST_Q IN (2, 1234, 123, 12, 234) THEN
																					(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																				ELSE
																					0
																				END
																			ELSE
																				CASE WHEN MAD.EST_Q IN (2, 1234, 123, 12, 234) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																			END
																		END AS AMOUNT_Q2_0
																	,	CASE WHEN MAD.POTENSIAL_PROJECT = '1' THEN
																			CASE WHEN MAD.JML_TAHUN <> 0 THEN
																				CASE WHEN MAD.EST_Q IN (2, 1234, 123, 12, 234) THEN
																					(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																				ELSE
																					0
																				END
																			ELSE
																				CASE WHEN MAD.EST_Q IN (2, 1234, 123, 12, 234) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																			END
																		END AS AMOUNT_Q2_1
																	,	CASE WHEN MAD.POTENSIAL_PROJECT = '0' THEN
																			CASE WHEN MAD.JML_TAHUN <> 0 THEN
																				CASE WHEN MAD.EST_Q IN (3, 1234, 123, 234, 34) THEN
																					(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																				ELSE
																					0
																				END
																			ELSE
																				CASE WHEN MAD.EST_Q IN (3, 1234, 123, 234, 34) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																			END
																		END AS AMOUNT_Q3_0
																	,	CASE WHEN MAD.POTENSIAL_PROJECT = '1' THEN
																			CASE WHEN MAD.JML_TAHUN <> 0 THEN
																				CASE WHEN MAD.EST_Q IN (3, 1234, 123, 234, 34) THEN
																					(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																				ELSE
																					0
																				END
																			ELSE
																				CASE WHEN MAD.EST_Q IN (3, 1234, 123, 234, 34) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																			END
																		END AS AMOUNT_Q3_1
																	,	CASE WHEN MAD.POTENSIAL_PROJECT = '0' THEN
																			CASE WHEN MAD.JML_TAHUN <> 0 THEN
																				CASE WHEN MAD.EST_Q IN (4, 1234, 234, 34) THEN
																					(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																				ELSE
																					0
																				END
																			ELSE
																				CASE WHEN MAD.EST_Q IN (4, 1234, 234, 34) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																			END
																		END AS AMOUNT_Q4_0
																	,	CASE WHEN MAD.POTENSIAL_PROJECT = '1' THEN
																			CASE WHEN MAD.JML_TAHUN <> 0 THEN
																				CASE WHEN MAD.EST_Q IN (4, 1234, 234, 34) THEN
																					(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																				ELSE
																					0
																				END
																			ELSE
																				CASE WHEN MAD.EST_Q IN (4, 1234, 234, 34) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																			END
																		END AS AMOUNT_Q4_1
																	,MAD.EST_YEAR
															FROM	(
																		SELECT	CASE WHEN SOH.[No_ Series] = 'SBOM' THEN 'AJ' ELSE 'SP' END AS COMPANY
																				,SOH.[Salesperson Code] AS SALES_CODE
																				,SOH.[Responsibility Center] AS CABANG
																				,	CASE WHEN SOL.[Warranty Code] = '' THEN
																						CASE WHEN SUBSTRING(SOH.[Posting Description], 1,1) NOT IN ('L','R','M','H','A') THEN
																							'R'
																						ELSE
																							SUBSTRING(SOH.[Posting Description], 1,1)
																						END
																					ELSE
																						CASE WHEN SOL.[Warranty Code] = 'CLOSED' THEN 'H'
																						ELSE
																							CASE WHEN SUBSTRING(SOH.[Posting Description], 1,1) NOT IN ('L','R','M','H','A') THEN
																								'R'
																							ELSE
																								SUBSTRING(SOH.[Posting Description], 1,1)
																							END
																						END
																					END AS SIZE_PROJECT
																				, CASE WHEN SUBSTRING(SOH.[Posting Description], 2,1) NOT IN ('1','0') THEN '0' ELSE SUBSTRING(SOH.[Posting Description], 2,1) END AS POTENSIAL_PROJECT
																				,SOL.[No_] AS ITEM
																				,SOL.[Description] AS ITEM_DESC
																				,SOL.[Shortcut Dimension 2 Code] AS ITEM_CAT
																				,SUBSTRING(SOL.[Shortcut Dimension 2 Code] , 1,2) AS HW_PS
																				,SOL.[Quantity] AS QTY_OUT
																				,SOL.[Unit Price] AS UNIT_PRICE
																				,SOH.[Payment Discount _] AS JML_TAHUN
																				,	CASE WHEN SOH.[Payment Discount _] <> 0 THEN
																						CASE WHEN DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date]) <=  GETDATE() THEN
																							CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) > $P{Tahun} THEN
																								CASE WHEN DATEPART(year, GETDATE()) <= $P{Tahun} THEN
																									$P{Tahun}
																								ELSE
																									0
																								END
																							ELSE
																								CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) = $P{Tahun} THEN
																									CASE WHEN DATEPART(quarter, GETDATE()) IN (1,2) THEN 0 ELSE $P{Tahun} END
																								ELSE
																									0
																								END
																							END
																						ELSE
																							CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) - 1)) > $P{Tahun} THEN
																								CASE WHEN DATEPART(year, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) <= $P{Tahun} THEN
																									$P{Tahun}
																								ELSE
																									0
																								END
																							ELSE
																								CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) - 1)) = $P{Tahun} THEN
																									CASE WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) IN (1,2) THEN 0 ELSE $P{Tahun} END
																								ELSE
																									0
																								END
																							END
																						END
																					ELSE
																						CASE WHEN DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date]) <=  GETDATE() THEN
																							DATEPART(year, GETDATE()) ELSE DATEPART(year, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date]))
																						END
																					END AS EST_YEAR
																				,	CASE WHEN SOH.[Payment Discount _] <> 0 THEN
																						CASE WHEN DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date]) <=  GETDATE() THEN
																							CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) > $P{Tahun} THEN
																								CASE WHEN DATEPART(year, GETDATE()) < $P{Tahun} THEN
																									1234
																								ELSE
																									CASE WHEN DATEPART(year, GETDATE()) = $P{Tahun} THEN
																										CASE	WHEN DATEPART(quarter, GETDATE()) = 1 THEN 1234
																												WHEN DATEPART(quarter, GETDATE()) = 2 THEN 234
																												WHEN DATEPART(quarter, GETDATE()) = 3 THEN 34
																												ELSE 4
																										END
																									ELSE
																										0
																									END
																								END
																							ELSE
																								CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) = $P{Tahun} THEN
																									CASE WHEN DATEPART(quarter, GETDATE()) = 1 THEN 0
																										 WHEN DATEPART(quarter, GETDATE()) = 2 THEN 1
																										 WHEN DATEPART(quarter, GETDATE()) = 3 THEN 12
																										 ELSE 123
																									END
																								ELSE
																									0
																								END
																							END
																						ELSE
																							CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) - 1)) > $P{Tahun} THEN
																								CASE WHEN DATEPART(year, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) < $P{Tahun} THEN
																									1234
																								ELSE
																									CASE WHEN DATEPART(year, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = $P{Tahun} THEN
																										CASE	WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = 1 THEN 1234
																												WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = 2 THEN 234
																												WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = 3 THEN 34
																												ELSE 4
																										END
																									ELSE
																										0
																									END
																								END
																							ELSE
																								CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) - 1)) = $P{Tahun} THEN
																									CASE WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = 1 THEN 0
																										 WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = 2 THEN 1
																										 WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = 3 THEN 12
																										 ELSE 123
																									END
																								ELSE
																									0
																								END
																							END
																						END
																					ELSE
																						CASE WHEN DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date]) <=  GETDATE() THEN
																							DATEPART(quarter, GETDATE())
																						ELSE
																							DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date]))
																						END
																					END AS EST_Q
																		FROM [dbo].[BC_AUTOJAYA$Sales Header] SOH
																		INNER JOIN [dbo].[BC_AUTOJAYA$Sales Line] SOL ON SOL.[Document Type] = SOH.[Document Type] AND SOL.[Document No_] = SOH.[No_]
																		WHERE SOH.[No_ Series] IN ('SBOM', 'SBOMS')
																	) MAD
														) X
												WHERE	(X.POTENSIAL_PROJECT = $P{Potensi} OR $P{Potensi} = 'ALL') AND
														(X.SIZE_PROJECT = $P{Size} OR $P{Size} = 'ALL')
												GROUP BY X.SALES_CODE, X.EST_YEAR
											) XX
									LEFT JOIN [dbo].[BC_AUTOJAYA$Salesperson_Purchaser] SLS ON SLS.[Code] = XX.SALES_CODE
									WHERE XX.YEAR_SUM = $P{Tahun}
									GROUP BY SLS.[Global Dimension 1 Code], XX.SALES_CODE, SLS.[Name]
								) U
						UNION ALL
						SELECT	XOXO.BRANCHES
								,XOXO.SALES_CODE
								,XOXO.SALES_NAME
								,XOXO.AMOUNT_REALISASI_S1
								,XOXO.AMOUNT_REALISASI_S2
								, 0 AS AMOUNT_POTENSI
								, 0 AS AMOUNT_POTENSI_Q1_0
								, 0 AS AMOUNT_POTENSI_Q1_1
								, 0 AS AMOUNT_POTENSI_Q2_0
								, 0 AS AMOUNT_POTENSI_Q2_1
								, 0 AS AMOUNT_POTENSI_Q3_0
								, 0 AS AMOUNT_POTENSI_Q3_1
								, 0 AS AMOUNT_POTENSI_Q4_0
								, 0 AS AMOUNT_POTENSI_Q4_1
						FROM	(
									SELECT	XOX.BRANCHES
											,XOX.SALESMAN AS SALES_CODE
											, SLS.[Name] AS SALES_NAME
											,(SUM(XOX.AMOUNT_S1)) AS AMOUNT_REALISASI_S1
											,(SUM(XOX.AMOUNT_S2)) AS AMOUNT_REALISASI_S2
											,((SUM(XOX.AMOUNT_S1)) + (SUM(XOX.AMOUNT_S2))) AS AMOUNT_REALISASI
									FROM	(
												SELECT	GROUP_NO
														, GROUP_DESC
														, BRANCHES
														, SALESMAN
														, SUM(CASE WHEN Q_OUT_SO IN (1,2) THEN CASE WHEN GROUP_NO = 2 AND $P{Tahun} <> DATEPART(year, GETDATE()) THEN 0 ELSE AMOUNT END ELSE 0  END) AS AMOUNT_S1
														, SUM(CASE WHEN Q_OUT_SO IN (3,4) THEN CASE WHEN GROUP_NO = 2 AND $P{Tahun} <> DATEPART(year, GETDATE()) THEN 0 ELSE AMOUNT END ELSE 0  END) AS AMOUNT_S2
												FROM [dbo].[TEMP08_SALES_REVENUE]
												WHERE	PROSES_ID = $P{ProsesId} AND
														GROUP_NO = 1
												GROUP BY GROUP_NO, GROUP_DESC, BRANCHES, SALESMAN
												UNION ALL
												SELECT	GROUP_NO
														, GROUP_DESC
														, BRANCHES
														, SALESMAN
														, SUM(CASE WHEN Q_OUT_SO IN (1,2) THEN AMOUNT ELSE 0  END) AS AMOUNT_S1
														, SUM(CASE WHEN Q_OUT_SO IN (3,4) THEN AMOUNT ELSE 0  END) AS AMOUNT_S2
												FROM [dbo].[TEMP08_SALES_REVENUE]
												WHERE	PROSES_ID = $P{ProsesId} AND
														GROUP_NO = 2  AND
														YEAR_OUT_SO = $P{Tahun}
												GROUP BY GROUP_NO, GROUP_DESC, BRANCHES, SALESMAN
												UNION ALL
												SELECT	GROUP_NO
														, GROUP_DESC
														, CASE	WHEN BRANCHES = 'JAKARTA' THEN '10'
																WHEN BRANCHES = 'SURABAYA' THEN '15'
																WHEN BRANCHES = 'CIKARANG' THEN '16'
																WHEN BRANCHES = 'SEMARANG' THEN '17'
																WHEN BRANCHES = 'BALI' THEN '19'
																ELSE 'NA' END AS BRANCHES
														, SALESMAN
														, SUM(CASE WHEN Q_OUT_SO IN (1,2) THEN AMOUNT ELSE 0  END) AS AMOUNT_S1
														, SUM(CASE WHEN Q_OUT_SO IN (3,4) THEN AMOUNT ELSE 0  END) AS AMOUNT_S2
												FROM [dbo].[TEMP08_SALES_REVENUE]
												WHERE	PROSES_ID = $P{ProsesId}  AND
														GROUP_NO = 3 AND
														YEAR_OUT_SO = $P{Tahun}
												GROUP BY GROUP_NO, GROUP_DESC, BRANCHES, SALESMAN
											)XOX
									LEFT JOIN [dbo].[BC_AUTOJAYA$Salesperson_Purchaser] SLS ON SLS.[Code] = XOX.SALESMAN
									GROUP BY XOX.BRANCHES, XOX.SALESMAN, SLS.[Name]
								) XOXO
						WHERE XOXO.AMOUNT_REALISASI <> 0
					) FBR
			LEFT JOIN [dbo].[BC_AUTOJAYA$Responsibility Center] CAB ON CAB.[Code] = FBR.BRANCHES
			GROUP BY FBR.BRANCHES, CAB.[Name], FBR.SALES_CODE, FBR.SALES_NAME
		) XI
LEFT JOIN	(
				SELECT	TGT.BRANCHES
						,SUM(TGT.AMOUNT_TARGET) AS AMOUNT_TARGET
				FROM	(
							SELECT	M06.SLS_OR_CAB  AS BRANCHES
									,M06.[TARGET]  AS AMOUNT_TARGET
							FROM	[dbo].[M06_TARGET_PIPELINE] M06
							WHERE	M06.TAHUN = $P{Tahun} AND
									M06.JENIS <> 'SALES'
						) TGT
				GROUP BY TGT.BRANCHES
			) TRG ON TRG.BRANCHES = XI.CABANG
WHERE (XI.CABANG_NAMA = $P{Cabang} OR $P{Cabang} = 'ALL')]]>
	</queryString>
