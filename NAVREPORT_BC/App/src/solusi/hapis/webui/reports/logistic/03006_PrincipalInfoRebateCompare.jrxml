	<queryString>
		<![CDATA[SELECT	PIN.TGL_INVOICE
		,PIN.NO_INVIOCE_VENDOR
		,PIN.NO_INVOICE
		,PIN.NO_PO
		,PIN.ITEM_NO
		,PIN.ITEM_DESC
		,PIN.QTY
		,PIN.CURR_CODE
		,PIN.AMOUNT
		,PIN.VENDOR_NO
		,VDR.[Name] AS DISTI
		,PIN.DIM_PRINCIPAL
		,ISNULL(PRIN.[Name], 'NON PRINCIPAL') AS PRINCIPAL
		,PIN.TGL_PO
FROM	(
			SELECT	XXX.TGL_INVOICE
					,XXX.NO_INVIOCE_VENDOR
					,XXX.NO_INVOICE
					,XXX.NO_PO
					,XXX.ITEM_NO
					,XXX.ITEM_DESC
					,XXX.QTY
					,XXX.CURR_CODE
					,XXX.AMOUNT
					,XXX.VENDOR_NO
					,XXX.DIM_PRINCIPAL
					,XXX.TGL_PO
			FROM	(
						SELECT	XX.TGL_INVOICE
								,XX.NO_INVIOCE_VENDOR
								,XX.NO_INVOICE
								,XX.NO_PO
								,XX.ITEM_NO
								,XX.ITEM_DESC
								,XX.QTY
								,CASE WHEN XX.VENDOR_NO IN ('SUPP050', 'SUPP021') AND XX.CURR_CODE = 'USD' THEN 'IDR' ELSE XX.CURR_CODE END AS CURR_CODE
								,CASE WHEN XX.VENDOR_NO IN ('SUPP050', 'SUPP021') AND XX.CURR_CODE = 'USD' THEN ROUND((XX.AMOUNT / ISNULL(XX.CF, 1)),0)
									ELSE XX.AMOUNT
								 END AS AMOUNT
								,CASE WHEN XX.VENDOR_NO = 'SUPP050' THEN 'SUPP072' ELSE XX.VENDOR_NO END AS VENDOR_NO
								,XX.DIM_PRINCIPAL
								,PH.[Order Date] AS TGL_PO
						FROM	(
									SELECT	X.NO_INVOICE
											,X.NO_INVIOCE_VENDOR
											,X.TGL_INVOICE
											,SUBSTRING(CONVERT(varchar, X.TGL_INVOICE, 105), 7,4) AS TAHUN_INV
											,X.VENDOR_NO
											,MAX(X.DIM_PRINCIPAL) AS DIM_PRINCIPAL
											,MAX(X.NO_PO) AS NO_PO
											,CASE WHEN SUBSTRING(MAX(X.NO_PO),1,3) = 'BPO' THEN 4 ELSE 1 END AS PO_TYPE
											,X.CURR_CODE
											,X.CF
											,X.ITEM_NO
											,MAX(X.ITEM_DESC) AS ITEM_DESC
											,SUM(X.QTY) AS QTY
											,SUM(X.AMOUNT) AS AMOUNT
									FROM	(
												SELECT 	PIH.[No_] AS NO_INVOICE
														,PIH.[Vendor Invoice No_] AS NO_INVIOCE_VENDOR
														,PIH.[Posting Date] AS TGL_INVOICE
														,PIH.[Pay-to Vendor No_] AS VENDOR_NO
														,PIL.DIM_PRINCIPAL
														,	CASE WHEN ISNULL(BPO.BPO,'') <> '' THEN
																CASE WHEN SUBSTRING(BPO.BPO,6,1) = 'P' THEN BPO.BPO ELSE PRH.[Order No_] END
																ELSE PRH.[Order No_]
															END AS NO_PO
														,CASE WHEN PIH.[Currency Code] = '' THEN 'IDR' ELSE PIH.[Currency Code] END AS CURR_CODE
														,PIH.[Currency Factor] AS CF
														,PIL.AMOUNT
														,PIL.QTY
														,PIL.ITEM_NO
														,PIL.ITEM_DESC
												FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH
												LEFT JOIN	(
																SELECT PCL.[Document No_]
																		,PCL.[Receipt No_]
																		,PCL.[No_] AS ITEM_NO
																		,PCL.[Description] AS ITEM_DESC
																		,PCL.[Quantity] AS QTY
																		,PCL.[Amount] AS AMOUNT
																		,DIM.DIM_PRINCIPAL AS DIM_PRINCIPAL
																FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PCL
																LEFT JOIN	(
																				SELECT [Dimension Set ID]
																					  ,MAX(CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END) AS DIM_ITEM_CATEGORY
																					  ,MAX(CASE WHEN [Dimension Code] = 'PRINCIPAL' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRINCIPAL
																					  ,MAX(CASE WHEN [Dimension Code] = 'PRODUCT' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRODUCT
																				FROM [dbo].[BC_AUTOJAYA$Dimension Set Entry]
																				GROUP BY [Dimension Set ID]
																			) DIM ON DIM.[Dimension Set ID] = PCL.[Dimension Set ID]
																WHERE	[Amount] <> 0 AND
																		(
																			([Type] = 1  AND [No_] <> '230.120.00') OR
																			[Type] <> 1
																		)
															) PIL ON PIL.[Document No_] =  PIH.[No_]
												LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
												LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Header] PRH ON PRH.[No_] = PIL.[Receipt No_]
												LEFT JOIN	(
																SELECT PH.[No_], MAX(PD.[Blanket Order No_]) AS BPO
																FROM [dbo].[BC_AUTOJAYA$Purchase Header] PH
																INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Line] PD ON PD.[Document No_] = PH.[No_]
																WHERE PH.[Document Type] = 1
																GROUP BY PH.[No_]
															) BPO ON BPO.[No_] = PRH.[Order No_]
												WHERE CMH.[No_] IS NULL
											) X
									GROUP BY X.NO_INVOICE
											,X.NO_INVIOCE_VENDOR
											,X.TGL_INVOICE
											,X.VENDOR_NO
											,X.CURR_CODE
											,X.CF
											,X.ITEM_NO
								) XX
						LEFT JOIN  [dbo].[BC_AUTOJAYA$Purchase Header] PH ON PH.[No_] = XX.NO_PO AND PH.[Document Type] = XX.PO_TYPE
					) XXX
			UNION ALL
			SELECT	XXX.TGL_INVOICE
					,XXX.NO_INVIOCE_VENDOR
					,XXX.NO_INVOICE
					,XXX.NO_PO
					,XXX.ITEM_NO
					,XXX.ITEM_DESC
					,XXX.QTY
					,XXX.CURR_CODE
					,XXX.AMOUNT
					,XXX.VENDOR_NO
					,XXX.DIM_PRINCIPAL
					,XXX.TGL_PO
			FROM	(
						SELECT	XX.TGL_INVOICE
								,XX.NO_INVIOCE_VENDOR
								,XX.NO_INVOICE
								,XX.NO_PO
								,XX.ITEM_NO
								,XX.ITEM_DESC
								,XX.QTY
								,CASE WHEN XX.VENDOR_NO IN ('SUPP050', 'SUPP021') AND XX.CURR_CODE = 'USD' THEN 'IDR' ELSE XX.CURR_CODE END AS CURR_CODE
								,CASE WHEN XX.VENDOR_NO IN ('SUPP050', 'SUPP021') AND XX.CURR_CODE = 'USD' THEN ROUND((XX.AMOUNT / ISNULL(XX.CF, 1)),0)
									ELSE XX.AMOUNT
								 END AS AMOUNT
								,CASE WHEN XX.VENDOR_NO = 'SUPP050' THEN 'SUPP072' ELSE XX.VENDOR_NO END AS VENDOR_NO
								,XX.DIM_PRINCIPAL
								,PH.[Order Date] AS TGL_PO
						FROM	(
									SELECT	X.NO_INVOICE
											,X.NO_INVIOCE_VENDOR
											,X.TGL_INVOICE
											,SUBSTRING(CONVERT(varchar, X.TGL_INVOICE, 105), 7,4) AS TAHUN_INV
											,X.VENDOR_NO
											,MAX(X.DIM_PRINCIPAL) AS DIM_PRINCIPAL
											,MAX(X.NO_PO) AS NO_PO
											,CASE WHEN SUBSTRING(MAX(X.NO_PO),1,3) = 'BPO' THEN 4 ELSE 1 END AS PO_TYPE
											,X.CURR_CODE
											,X.CF
											,X.ITEM_NO
											,MAX(X.ITEM_DESC) AS ITEM_DESC
											,SUM(X.QTY) AS QTY
											,SUM(X.AMOUNT) AS AMOUNT
									FROM	(
												SELECT 	PIH.[No_] AS NO_INVOICE
														,PIH.[Vendor Invoice No_] AS NO_INVIOCE_VENDOR
														,PIH.[Posting Date] AS TGL_INVOICE
														,PIH.[Pay-to Vendor No_] AS VENDOR_NO
														,PIL.DIM_PRINCIPAL
														,	CASE WHEN ISNULL(BPO.BPO,'') <> '' THEN
																CASE WHEN SUBSTRING(BPO.BPO,6,1) = 'P' THEN BPO.BPO ELSE PRH.[Order No_] END
																ELSE PRH.[Order No_]
															END AS NO_PO
														,CASE WHEN PIH.[Currency Code] = '' THEN 'IDR' ELSE PIH.[Currency Code] END AS CURR_CODE
														,PIH.[Currency Factor] AS CF
														,PIL.AMOUNT
														,PIL.QTY
														,PIL.ITEM_NO
														,PIL.ITEM_DESC
												FROM [dbo].[BC_SOLUSI$Purch_ Inv_ Header] PIH
												LEFT JOIN	(
																SELECT PCL.[Document No_]
																		,PCL.[Receipt No_]
																		,PCL.[No_] AS ITEM_NO
																		,PCL.[Description] AS ITEM_DESC
																		,PCL.[Quantity] AS QTY
																		,PCL.[Amount] AS AMOUNT
																		,DIM.DIM_PRINCIPAL AS DIM_PRINCIPAL
																FROM [dbo].[BC_SOLUSI$Purch_ Inv_ Line] PCL
																LEFT JOIN	(
																				SELECT [Dimension Set ID]
																					  ,MAX(CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END) AS DIM_ITEM_CATEGORY
																					  ,MAX(CASE WHEN [Dimension Code] = 'PRINCIPAL' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRINCIPAL
																					  ,MAX(CASE WHEN [Dimension Code] = 'PRODUCT' THEN [Dimension Value Code] ELSE NULL END ) AS DIM_PRODUCT
																				FROM [dbo].[BC_SOLUSI$Dimension Set Entry]
																				GROUP BY [Dimension Set ID]
																			) DIM ON DIM.[Dimension Set ID] = PCL.[Dimension Set ID]
																WHERE	[Amount] <> 0 AND
																		(
																			([Type] = 1  AND [No_] <> '230.120.00') OR
																			[Type] <> 1
																		)
															) PIL ON PIL.[Document No_] =  PIH.[No_]
												LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
												LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Rcpt_ Header] PRH ON PRH.[No_] = PIL.[Receipt No_]
												LEFT JOIN	(
																SELECT PH.[No_], MAX(PD.[Blanket Order No_]) AS BPO
																FROM [dbo].[BC_SOLUSI$Purchase Header] PH
																INNER JOIN [dbo].[BC_SOLUSI$Purchase Line] PD ON PD.[Document No_] = PH.[No_]
																WHERE PH.[Document Type] = 1
																GROUP BY PH.[No_]
															) BPO ON BPO.[No_] = PRH.[Order No_]
												WHERE CMH.[No_] IS NULL
											) X
									GROUP BY X.NO_INVOICE
											,X.NO_INVIOCE_VENDOR
											,X.TGL_INVOICE
											,X.VENDOR_NO
											,X.CURR_CODE
											,X.CF
											,X.ITEM_NO
								) XX
						LEFT JOIN  [dbo].[BC_SOLUSI$Purchase Header] PH ON PH.[No_] = XX.NO_PO AND PH.[Document Type] = XX.PO_TYPE
					) XXX
		) PIN
LEFT JOIN	(
				SELECT	VEN.[No_], MAX([Name]) AS [Name]
						FROM	(
									SELECT [No_], [Name]
									FROM [dbo].[BC_AUTOJAYA$Vendor]
									UNION ALL
									SELECT [No_], [Name]
									FROM [dbo].[BC_SOLUSI$Vendor]
								) VEN
						GROUP BY VEN.[No_]
			) VDR ON VDR.[No_] = PIN.VENDOR_NO
LEFT JOIN	(
				SELECT	PRI.[Code], MAX(PRI.[Name]) AS [Name]
				FROM	(
							SELECT [Code]
								  ,[Name]
							FROM [dbo].[BC_AUTOJAYA$Dimension Value]
							WHERE [Dimension Code] = 'PRINCIPAL'
							UNION ALL
							SELECT [Code]
								  ,[Name]
							FROM [dbo].[BC_SOLUSI$Dimension Value]
							WHERE [Dimension Code] = 'PRINCIPAL'
						) PRI
				GROUP BY PRI.[Code]
			) PRIN ON PRIN.[Code] = PIN.DIM_PRINCIPAL
WHERE (PIN.DIM_PRINCIPAL = $P{Principal} OR $P{Principal} = 'ALL') AND
(PIN.VENDOR_NO = $P{Vendor} OR $P{Vendor}='ALL') AND
	(PIN.TGL_INVOICE >= $P{TglFrom} AND PIN.TGL_INVOICE <= $P{TglUpto})]]>
	</queryString>
