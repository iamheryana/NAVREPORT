	<queryString>
		<![CDATA[SELECT X.*
FROM	(
			SELECT	SOH.[Order Date] AS TGL_ORDER
					,SOH.[No_] AS NO_SO
					,SOH.[Sell-to Customer No_] AS SELL_TO_CODE
					,SOH.[Sell-to Customer Name] AS SELL_TO_NAME
					,SOH.[Bill-to Customer No_] AS BILL_TO_CODE
					,SOH.[Bill-to Name] AS BILL_TO_NAME
					,SOH.[External Document No_] AS NO_PO_CUST
					,SOH.[Posting Description] AS ALOKASI
					,SOD.[No_]  AS NO_ITEM
					,SOD.[Description] AS DESC_ITEM
					,SHL.[Quantity]	AS QTY
					,SOD.[Unit Price] AS UNIT_PRICE
					,(SHL.[Quantity] * SOD.[Unit Price]) AS LINE_AMOUNT
					,SHL.[Document No_] AS NO_DO
					,SHL.[Posting Date] AS TGL_DO
			FROM [dbo].[BC_AUTOJAYA$Sales Header] SOH
			INNER JOIN [dbo].[BC_AUTOJAYA$Sales Line] SOD ON SOD.[Document No_] = SOH.[No_] AND SOH.[Document Type] = SOD.[Document Type]
			LEFT JOIN	(
							SELECT	DO.[Order No_],
									DO.[Order Line No_],
									DO.[Document No_],
									DO.[Posting Date],
									DO.[Quantity]
							FROM	(
										SELECT	[Order No_],
												[Order Line No_],
												[Document No_],
												[Posting Date],
												SUM([Quantity]) AS [Quantity]
										FROM [dbo].[BC_AUTOJAYA$Sales Shipment Line]
										WHERE [Type] = 2 AND
												([Order No_]  LIKE '8SO%' OR [Order No_]  LIKE '9SO%')
										GROUP BY [Order No_], [Order Line No_], [Document No_],	[Posting Date]
									) DO
							WHERE DO.[Quantity] <> 0
						) SHL ON SHL.[Order No_] = SOD.[Document No_] AND SHL.[Order Line No_] = SOD.[Line No_]
			WHERE	(SOH.[No_]  LIKE '8SO%' OR SOH.[No_]  LIKE '9SO%') AND
					SOH.[Document Type] = 1 AND SOD.[Type] = 2 AND
					SHL.[Document No_] IS NOT NULL
			UNION ALL
			SELECT	SOH.[Order Date] AS TGL_ORDER
					,SOH.[No_] AS NO_SO
					,SOH.[Sell-to Customer No_] AS SELL_TO_CODE
					,SOH.[Sell-to Customer Name] AS SELL_TO_NAME
					,SOH.[Bill-to Customer No_] AS BILL_TO_CODE
					,SOH.[Bill-to Name] AS BILL_TO_NAME
					,SOH.[External Document No_] AS NO_PO_CUST
					,SOH.[Posting Description] AS ALOKASI
					,SOD.[No_]  AS NO_ITEM
					,SOD.[Description] AS DESC_ITEM
					,SOD.[Quantity]	- SOD.[Quantity Shipped] AS QTY
					,SOD.[Unit Price] AS UNIT_PRICE
					,((SOD.[Quantity]	- SOD.[Quantity Shipped]) * SOD.[Unit Price]) AS LINE_AMOUNT
					,NULL AS NO_DO
					,NULL AS TGL_DO
			FROM [dbo].[BC_AUTOJAYA$Sales Header] SOH
			INNER JOIN [dbo].[BC_AUTOJAYA$Sales Line] SOD ON SOD.[Document No_] = SOH.[No_] AND SOH.[Document Type] = SOD.[Document Type]
			WHERE	(SOH.[No_]  LIKE '8SO%' OR SOH.[No_]  LIKE '9SO%') AND
					SOH.[Document Type] = 1 AND SOD.[Type] = 2 AND
					((SOD.[Quantity]	- SOD.[Quantity Shipped]) <> 0 )
			UNION ALL
			SELECT	CASE WHEN T15.ORDER_DATE IS NULL THEN SIH.[Order Date] ELSE T15.ORDER_DATE END AS TGL_ORDER
					,SIH.[Order No_] AS NO_SO
					,SIH.[Sell-to Customer No_] AS SELL_TO_CODE
					,SIH.[Sell-to Customer Name] AS SELL_TO_NAME
					,SIH.[Bill-to Customer No_] AS BILL_TO_CODE
					,SIH.[Bill-to Name] AS BILL_TO_NAME
					,SIH.[External Document No_] AS NO_PO_CUST
					,SIH.[Posting Description] AS ALOKASI
					,SIL.[No_]  AS NO_ITEM
					,SIL.[Description] AS DESC_ITEM
					,ISNULL(SHL.[Quantity], SIL.[Quantity]) AS QTY
					,SIL.[Unit Price] AS UNIT_PRICE
					,(ISNULL(SHL.[Quantity], SIL.[Quantity]) * SIL.[Unit Price]) AS LINE_AMOUNT
					,ISNULL(SHL.[Document No_], '') AS NO_DO
					,SHL.[Posting Date] AS TGL_DO
			FROM [dbo].[BC_AUTOJAYA$Sales Invoice Header]  SIH
			LEFT JOIN [dbo].[BC_AUTOJAYA$Sales Invoice Line] SIL ON  SIL.[Document No_] = SIH.[No_]
			LEFT JOIN [dbo].[BC_AUTOJAYA$Sales Cr_Memo Header] CRM ON CRM.[Applies-to Doc_ No_] = SIH.[No_]
			LEFT JOIN [dbo].[BC_AUTOJAYA$Sales Header] SOH ON SOH.[No_] = SIH.[Order No_] AND SOH.[Document Type] = 1
			LEFT JOIN [dbo].[T15_SATINDO_ADJ] T15 ON T15.NO_INVOICE = SIH.[No_]
			LEFT JOIN	(
										SELECT	DO.[Order No_],
												DO.[Order Line No_],
												DO.[Document No_],
												DO.[Posting Date],
												DO.[Quantity]
										FROM	(
													SELECT	[Order No_],
															[Order Line No_],
															[Document No_],
															[Posting Date],
															SUM([Quantity]) AS [Quantity]
													FROM [dbo].[BC_AUTOJAYA$Sales Shipment Line]
													WHERE [Type] = 2 AND
															([Order No_]  LIKE '8SO%' OR [Order No_]  LIKE '9SO%')
													GROUP BY [Order No_], [Order Line No_], [Document No_],	[Posting Date]
												) DO
										WHERE DO.[Quantity] <> 0
									) SHL ON SHL.[Order No_] = SIH.[Order No_] AND SHL.[Order Line No_] = SIL.[Line No_]
			WHERE (SIH.[No_]  LIKE '8SI%' OR SIH.[No_]  LIKE '9SI%') AND
					SIL.[Type] = 2 AND CRM.[No_] IS NULL AND
					SOH.[No_] IS NULL
		) X
WHERE X.TGL_ORDER BETWEEN $P{TglOrderFrom} AND $P{TglOrderUpto}]]>
	</queryString>
