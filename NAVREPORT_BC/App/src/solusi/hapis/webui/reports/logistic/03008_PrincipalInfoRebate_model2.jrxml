	<parameter name="Principal" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Vendor" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="TglFrom" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="TglUpto" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Product" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT	PIN.NO_INVOICE
		,PIN.NO_INVIOCE_VENDOR
		,PIN.TGL_INVOICE
		,PIN.TAHUN_INV
		,PIN.VENDOR_NO
		,VDR.[Name] AS DISTI
		,PIN.DIM_PRINCIPAL
		,ISNULL(PRIN.[Name], 'NON PRINCIPAL') AS PRINCIPAL
		,PIN.NO_PO
		,CASE WHEN SUM(PIN.AMOUNT_PO) OVER (PARTITION BY PIN.NO_INVOICE ORDER BY PIN.NO_INVOICE) <> (PIN.AMOUNT_THN_INI + PIN.AMOUNT_THN_LALU) THEN '***' ELSE '' END AS TIDAK_SAMA
		,PIN.AMOUNT_PO
		,PIN.TGL_PO
		,PIN.PO_REF
		,PIN.CURR_CODE
		,PIN.AMOUNT_THN_INI
		,PIN.AMOUNT_THN_LALU
		,(PIN.AMOUNT_REBATE * -1) AS AMOUNT_REBATE
		,(PIN.AMOUNT_THN_INI + PIN.AMOUNT_THN_LALU + (PIN.AMOUNT_REBATE * -1)) AS AMOUNT_TANPA_REBATE
FROM	(
			SELECT	XXX.NO_INVOICE
					,XXX.NO_INVIOCE_VENDOR
					,XXX.TGL_INVOICE
					,XXX.TAHUN_INV
					,XXX.VENDOR_NO
					,XXX.DIM_PRINCIPAL
					,XXX.NO_PO
					,XXX.AMOUNT_PO
					,XXX.TGL_PO
					,XXX.PO_REF
					,XXX.CURR_CODE
					, CASE WHEN XXX.BARIS = 1 THEN XXX.AMOUNT_THN_INI ELSE 0 END AS AMOUNT_THN_INI
					, CASE WHEN XXX.BARIS = 1 THEN XXX.AMOUNT_THN_LALU ELSE 0 END AS AMOUNT_THN_LALU
					,XXX.AMOUNT_REBATE
			FROM	(
						SELECT  ROW_NUMBER() OVER (PARTITION BY XX.NO_INVOICE ORDER BY XX.NO_INVOICE) AS BARIS
								,XX.NO_INVOICE
								,XX.NO_INVIOCE_VENDOR
								,XX.TGL_INVOICE
								,XX.TAHUN_INV
								,XX.VENDOR_NO
								,XX.DIM_PRINCIPAL
								,XX.NO_PO
								,XX.AMOUNT_PO
								,XX.TGL_PO
								,XX.PO_REF
								,XX.CURR_CODE
								,CASE WHEN XX.TAHUN_INV = XX.TAHUN_PO THEN XX.AMOUNT ELSE 0 END AS AMOUNT_THN_INI
								,CASE WHEN XX.TAHUN_INV <> XX.TAHUN_PO THEN XX.AMOUNT ELSE 0 END AS AMOUNT_THN_LALU
								,XX.AMOUNT_REBATE
						FROM	(
									SELECT 	PIH.[No_] AS NO_INVOICE
											,PIH.[Vendor Invoice No_] AS NO_INVIOCE_VENDOR
											,PIH.[Posting Date] AS TGL_INVOICE
											,SUBSTRING(CONVERT(varchar, PIH.[Posting Date], 105), 7,4) AS TAHUN_INV
											,CASE WHEN PIH.[Pay-to Vendor No_]  = 'SUPP050' THEN 'SUPP072' ELSE PIH.[Pay-to Vendor No_]  END AS VENDOR_NO
											,PIL.DIM_PRINCIPAL
											,POO.NO_PO
											,PH.[Order Date] AS TGL_PO
											,PH.[Your Reference] AS PO_REF
											,CASE WHEN PIH.[Pay-to Vendor No_] IN ('SUPP050', 'SUPP021') AND PIH.[Currency Code] = 'USD' THEN ROUND((ISNULL(POO.AMT_PO,0) / ISNULL(PIH.[Currency Factor], 1)),0)
												ELSE ISNULL(POO.AMT_PO,0)
											 END AS AMOUNT_PO
											,SUBSTRING(CONVERT(varchar, ISNULL(PH.[Order Date], PIH.[Posting Date]) , 105), 7,4) AS TAHUN_PO
											,CASE WHEN PIH.[Pay-to Vendor No_] IN ('SUPP050', 'SUPP021') AND PIH.[Currency Code] = 'USD' THEN 'IDR' ELSE CASE WHEN PIH.[Currency Code] = '' THEN 'IDR' ELSE PIH.[Currency Code] END END AS CURR_CODE
											,CASE WHEN PIH.[Pay-to Vendor No_] IN ('SUPP050', 'SUPP021') AND PIH.[Currency Code] = 'USD' THEN ROUND((PIL.AMOUNT / ISNULL(PIH.[Currency Factor], 1)),0)
												ELSE PIL.AMOUNT
											 END AS AMOUNT
											,CASE WHEN PIH.[Pay-to Vendor No_] IN ('SUPP050', 'SUPP021') AND PIH.[Currency Code] = 'USD' THEN ROUND((PIL.AMOUNT_REBATE / ISNULL(PIH.[Currency Factor], 1)),0)
												ELSE PIL.AMOUNT_REBATE
											 END AS AMOUNT_REBATE
									FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH
									LEFT JOIN	(
													SELECT	PCL.[Document No_]
															,MAX(DIM.DIM_PRINCIPAL) AS DIM_PRINCIPAL
															,SUM(PCL.[Amount]) AS AMOUNT
															,SUM(CASE WHEN PCL.No_ = '600.110.00' THEN PCL.[Amount] ELSE 0 END) AS AMOUNT_REBATE
													FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PCL
													LEFT JOIN	(
																	SELECT [Dimension Set ID]
																			,MAX(CASE WHEN [Dimension Code] = 'PRINCIPAL' THEN [Dimension Value Code] ELSE NULL END) AS DIM_PRINCIPAL
																			,MAX(CASE WHEN [Dimension Code] = 'PRODUCT' THEN [Dimension Value Code] ELSE NULL END) AS DIM_PRODUCT
																	FROM [dbo].[BC_AUTOJAYA$Dimension Set Entry]
																	GROUP BY [Dimension Set ID]
																) DIM ON DIM.[Dimension Set ID] = PCL.[Dimension Set ID]
													WHERE
															(
																(PCL.[Type] = 1  AND PCL.[No_] <> '230.120.00') OR
																PCL.[Type] <> 1
															)  AND
															(
																$P{Product}='ALL' OR
																($P{Product}='MC' AND DIM.DIM_PRODUCT = 'S405') OR
																($P{Product}='NMC' AND DIM.DIM_PRODUCT <> 'S405')
															)
													GROUP BY [Document No_]
												) PIL ON PIL.[Document No_] =  PIH.[No_]
									LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
									LEFT JOIN	(
													SELECT	DISTINCT PLN.NO_INV
															,	CASE WHEN ISNULL(POD.NO_BPO,'') <> '' THEN
																	CASE WHEN SUBSTRING(POD.NO_BPO,6,1) = 'P' THEN POD.NO_BPO ELSE PLN.NO_PO END
																ELSE
																	PLN.NO_PO
																END AS NO_PO
															,	CASE WHEN ISNULL(POD.NO_BPO,'') <> '' THEN
																	CASE WHEN SUBSTRING(POD.NO_BPO,6,1) = 'P' THEN 4 ELSE 1 END
																ELSE
																	1
																END AS TYPE_NO_PO
															,	CASE WHEN ISNULL(POD.NO_BPO,'') <> '' THEN
																	CASE WHEN SUBSTRING(POD.NO_BPO,6,1) = 'P' THEN POD.AMT_BPO ELSE POD.AMT_PO END
																ELSE
																	ISNULL(POD.AMT_PO,0)
																END AS AMT_PO
													FROM	(
																SELECT	 DISTINCT PIL.[Document No_] AS NO_INV
																		, ISNULL(PRH.[Order No_], PIH.[Prepayment Order No_]) AS NO_PO
																FROM [dbo].[BC_AUTOJAYA$Purch_ Inv_ Line] PIL
																INNER JOIN [dbo].[BC_AUTOJAYA$Purch_ Inv_ Header] PIH ON PIH.[No_] = PIL.[Document No_]
																LEFT JOIN [dbo].[BC_AUTOJAYA$Purch_ Rcpt_ Header] PRH ON PRH.[No_] = PIL.[Receipt No_]
																WHERE (PIL.[Receipt No_] <> '' OR PIH.[Prepayment Order No_] <> '')
															) PLN
													LEFT JOIN	(
																	SELECT	PO.NO_PO
																			, PO.AMT_PO
																			, PO.NO_BPO
																			, ISNULL(BPO.AMT_BPO,0) AS AMT_BPO
																	FROM	(
																				SELECT	PH.[No_] AS NO_PO
																						, SUM(PD.[Amount]) AS AMT_PO
																						, MAX(PD.[Blanket Order No_]) AS NO_BPO
																				FROM [dbo].[BC_AUTOJAYA$Purchase Header] PH
																				INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Line] PD ON PD.[Document No_] = PH.[No_]
																				WHERE PH.[Document Type] = 1
																				GROUP BY PH.[No_]
																			) PO
																	LEFT JOIN	(
																					SELECT	PH.[No_] AS NO_BPO
																							, SUM(PD.[Amount]) AS AMT_BPO
																					FROM [dbo].[BC_AUTOJAYA$Purchase Header] PH
																					INNER JOIN [dbo].[BC_AUTOJAYA$Purchase Line] PD ON PD.[Document No_] = PH.[No_]
																					WHERE PH.[Document Type] = 4
																					GROUP BY PH.[No_]
																				) BPO ON BPO.NO_BPO = PO.NO_BPO
																) POD ON POD.NO_PO = PLN.NO_PO
												) POO ON POO.NO_INV = PIH.[No_]
									LEFT JOIN  [dbo].[BC_AUTOJAYA$Purchase Header] PH ON PH.[No_] = POO.NO_PO AND PH.[Document Type] = POO.TYPE_NO_PO
									WHERE CMH.[No_] IS NULL
								) XX
					) XXX
			UNION ALL
			SELECT	XXX.NO_INVOICE
					,XXX.NO_INVIOCE_VENDOR
					,XXX.TGL_INVOICE
					,XXX.TAHUN_INV
					,XXX.VENDOR_NO
					,XXX.DIM_PRINCIPAL
					,XXX.NO_PO
					,XXX.AMOUNT_PO
					,XXX.TGL_PO
					,XXX.PO_REF
					,XXX.CURR_CODE
					, CASE WHEN XXX.BARIS = 1 THEN XXX.AMOUNT_THN_INI ELSE 0 END AS AMOUNT_THN_INI
					, CASE WHEN XXX.BARIS = 1 THEN XXX.AMOUNT_THN_LALU ELSE 0 END AS AMOUNT_THN_LALU
					,XXX.AMOUNT_REBATE
			FROM	(
						SELECT  ROW_NUMBER() OVER (PARTITION BY XX.NO_INVOICE ORDER BY XX.NO_INVOICE) AS BARIS
								,XX.NO_INVOICE
								,XX.NO_INVIOCE_VENDOR
								,XX.TGL_INVOICE
								,XX.TAHUN_INV
								,XX.VENDOR_NO
								,XX.DIM_PRINCIPAL
								,XX.NO_PO
								,XX.AMOUNT_PO
								,XX.TGL_PO
								,XX.PO_REF
								,XX.CURR_CODE
								,CASE WHEN XX.TAHUN_INV = XX.TAHUN_PO THEN XX.AMOUNT ELSE 0 END AS AMOUNT_THN_INI
								,CASE WHEN XX.TAHUN_INV <> XX.TAHUN_PO THEN XX.AMOUNT ELSE 0 END AS AMOUNT_THN_LALU
								,XX.AMOUNT_REBATE
						FROM	(
									SELECT 	PIH.[No_] AS NO_INVOICE
											,PIH.[Vendor Invoice No_] AS NO_INVIOCE_VENDOR
											,PIH.[Posting Date] AS TGL_INVOICE
											,SUBSTRING(CONVERT(varchar, PIH.[Posting Date], 105), 7,4) AS TAHUN_INV
											,CASE WHEN PIH.[Pay-to Vendor No_]  = 'SUPP050' THEN 'SUPP072' ELSE PIH.[Pay-to Vendor No_]  END AS VENDOR_NO
											,PIL.DIM_PRINCIPAL
											,POO.NO_PO
											,PH.[Order Date] AS TGL_PO
											,PH.[Your Reference] AS PO_REF
											,CASE WHEN PIH.[Pay-to Vendor No_] IN ('SUPP050', 'SUPP021') AND PIH.[Currency Code] = 'USD' THEN ROUND((ISNULL(POO.AMT_PO,0) / ISNULL(PIH.[Currency Factor], 1)),0)
												ELSE ISNULL(POO.AMT_PO,0)
											 END AS AMOUNT_PO
											,SUBSTRING(CONVERT(varchar, ISNULL(PH.[Order Date], PIH.[Posting Date]) , 105), 7,4) AS TAHUN_PO
											,CASE WHEN PIH.[Pay-to Vendor No_] IN ('SUPP050', 'SUPP021') AND PIH.[Currency Code] = 'USD' THEN 'IDR' ELSE CASE WHEN PIH.[Currency Code] = '' THEN 'IDR' ELSE PIH.[Currency Code] END END AS CURR_CODE
											,CASE WHEN PIH.[Pay-to Vendor No_] IN ('SUPP050', 'SUPP021') AND PIH.[Currency Code] = 'USD' THEN ROUND((PIL.AMOUNT / ISNULL(PIH.[Currency Factor], 1)),0)
												ELSE PIL.AMOUNT
											 END AS AMOUNT
											,CASE WHEN PIH.[Pay-to Vendor No_] IN ('SUPP050', 'SUPP021') AND PIH.[Currency Code] = 'USD' THEN ROUND((PIL.AMOUNT_REBATE / ISNULL(PIH.[Currency Factor], 1)),0)
												ELSE PIL.AMOUNT_REBATE
											 END AS AMOUNT_REBATE
									FROM [dbo].[BC_SOLUSI$Purch_ Inv_ Header] PIH
									LEFT JOIN	(
													SELECT	PCL.[Document No_]
															,MAX(DIM.DIM_PRINCIPAL) AS DIM_PRINCIPAL
															,SUM(PCL.[Amount]) AS AMOUNT
															,SUM(CASE WHEN PCL.No_ = '600.110.00' THEN PCL.[Amount] ELSE 0 END) AS AMOUNT_REBATE
													FROM [dbo].[BC_SOLUSI$Purch_ Inv_ Line] PCL
													LEFT JOIN	(
																	SELECT [Dimension Set ID]
																			,MAX(CASE WHEN [Dimension Code] = 'PRINCIPAL' THEN [Dimension Value Code] ELSE NULL END) AS DIM_PRINCIPAL
																			,MAX(CASE WHEN [Dimension Code] = 'PRODUCT' THEN [Dimension Value Code] ELSE NULL END) AS DIM_PRODUCT
																	FROM [dbo].[BC_SOLUSI$Dimension Set Entry]
																	GROUP BY [Dimension Set ID]
																) DIM ON DIM.[Dimension Set ID] = PCL.[Dimension Set ID]
													WHERE
															(
																(PCL.[Type] = 1  AND PCL.[No_] <> '230.120.00') OR
																PCL.[Type] <> 1
															) AND
															(
																$P{Product}='ALL' OR
																($P{Product}='MC' AND DIM.DIM_PRODUCT = 'S405') OR
																($P{Product}='NMC' AND DIM.DIM_PRODUCT <> 'S405')
															)
													GROUP BY [Document No_]
												) PIL ON PIL.[Document No_] =  PIH.[No_]
									LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
									LEFT JOIN	(
													SELECT	DISTINCT PLN.NO_INV
															,	CASE WHEN ISNULL(POD.NO_BPO,'') <> '' THEN
																	CASE WHEN SUBSTRING(POD.NO_BPO,6,1) = 'P' THEN POD.NO_BPO ELSE PLN.NO_PO END
																ELSE
																	PLN.NO_PO
																END AS NO_PO
															,	CASE WHEN ISNULL(POD.NO_BPO,'') <> '' THEN
																	CASE WHEN SUBSTRING(POD.NO_BPO,6,1) = 'P' THEN 4 ELSE 1 END
																ELSE
																	1
																END AS TYPE_NO_PO
															,	CASE WHEN ISNULL(POD.NO_BPO,'') <> '' THEN
																	CASE WHEN SUBSTRING(POD.NO_BPO,6,1) = 'P' THEN POD.AMT_BPO ELSE POD.AMT_PO END
																ELSE
																	ISNULL(POD.AMT_PO,0)
																END AS AMT_PO
													FROM	(
																SELECT	 DISTINCT PIL.[Document No_] AS NO_INV
																		, ISNULL(PRH.[Order No_], PIH.[Prepayment Order No_]) AS NO_PO
																FROM [dbo].[BC_SOLUSI$Purch_ Inv_ Line] PIL
																INNER JOIN [dbo].[BC_SOLUSI$Purch_ Inv_ Header] PIH ON PIH.[No_] = PIL.[Document No_]
																LEFT JOIN [dbo].[BC_SOLUSI$Purch_ Rcpt_ Header] PRH ON PRH.[No_] = PIL.[Receipt No_]
																WHERE (PIL.[Receipt No_] <> '' OR PIH.[Prepayment Order No_] <> '')
															) PLN
													LEFT JOIN	(
																	SELECT	PO.NO_PO
																			, PO.AMT_PO
																			, PO.NO_BPO
																			, ISNULL(BPO.AMT_BPO,0) AS AMT_BPO
																	FROM	(
																				SELECT	PH.[No_] AS NO_PO
																						, SUM(PD.[Amount]) AS AMT_PO
																						, MAX(PD.[Blanket Order No_]) AS NO_BPO
																				FROM [dbo].[BC_SOLUSI$Purchase Header] PH
																				INNER JOIN [dbo].[BC_SOLUSI$Purchase Line] PD ON PD.[Document No_] = PH.[No_]
																				WHERE PH.[Document Type] = 1
																				GROUP BY PH.[No_]
																			) PO
																	LEFT JOIN	(
																					SELECT	PH.[No_] AS NO_BPO
																							, SUM(PD.[Amount]) AS AMT_BPO
																					FROM [dbo].[BC_SOLUSI$Purchase Header] PH
																					INNER JOIN [dbo].[BC_SOLUSI$Purchase Line] PD ON PD.[Document No_] = PH.[No_]
																					WHERE PH.[Document Type] = 4
																					GROUP BY PH.[No_]
																				) BPO ON BPO.NO_BPO = PO.NO_BPO
																) POD ON POD.NO_PO = PLN.NO_PO
												) POO ON POO.NO_INV = PIH.[No_]
									LEFT JOIN  [dbo].[BC_SOLUSI$Purchase Header] PH ON PH.[No_] = POO.NO_PO AND PH.[Document Type] = POO.TYPE_NO_PO
									WHERE CMH.[No_] IS NULL
								) XX
					) XXX
		) PIN
LEFT JOIN	(
				SELECT	VEN.[No_], MAX([Name]) AS [Name]
						FROM	(
									SELECT [No_], [Name]
									FROM [dbo].[BC_AUTOJAYA$Vendor]
									UNION ALL
									SELECT [No_], [Name]
									FROM [dbo].[BC_SOLUSI$Vendor]
								) VEN
						GROUP BY VEN.[No_]
			) VDR ON VDR.[No_] = PIN.VENDOR_NO
LEFT JOIN	(
				SELECT	PRI.[Code], MAX(PRI.[Name]) AS [Name]
				FROM	(
							SELECT [Code]
								  ,[Name]
							FROM [dbo].[BC_AUTOJAYA$Dimension Value]
							WHERE [Dimension Code] = 'PRINCIPAL'
							UNION ALL
							SELECT [Code]
								  ,[Name]
							FROM [dbo].[BC_SOLUSI$Dimension Value]
							WHERE [Dimension Code] = 'PRINCIPAL'
						) PRI
				GROUP BY PRI.[Code]
			) PRIN ON PRIN.[Code] = PIN.DIM_PRINCIPAL
WHERE (PIN.DIM_PRINCIPAL = $P{Principal} OR $P{Principal} = 'ALL') AND
(PIN.VENDOR_NO = $P{Vendor} OR $P{Vendor}='ALL') AND
	(PIN.TGL_INVOICE >= $P{TglFrom} AND PIN.TGL_INVOICE <= $P{TglUpto})]]>
	</queryString>
