	<queryString>
		<![CDATA[SELECT	X.*
FROM	(
			SELECT	'AUTOJAYA' AS COMPANY
					, PH.[Assigned User ID] AS ASSIGN_USER
					,	CASE WHEN MAX(PL.NO_BPO) OVER (PARTITION BY PH.[No_] ORDER BY PH.[No_]) = '' THEN '.'
						ELSE
							MAX(PL.NO_BPO) OVER (PARTITION BY PH.[No_] ORDER BY PH.[No_])
						END AS NO_BPO
					, PH.[No_] AS NO_PO
					,	CASE WHEN SUBSTRING(PH.[No_],5,1) = 'P'  THEN 'OTP' ELSE
							CASE	WHEN SUBSTRING(PH.[No_],4,1) = 'S'  THEN 'SERVICE'
									WHEN SUBSTRING(PH.[No_],4,1) = 'N'  THEN 'NON TRADE'
									WHEN SUBSTRING(PH.[No_],4,1) = 'T'  THEN 'TRADE'
									ELSE 'UNKNOWN' END
						END AS JENIS_PO
					, PH.[Order Date] AS TGL_PO
					, PH.[Pay-to Vendor No_] AS VENDOR_NO
					, PH.[Pay-to Name] AS VENDOR_NAME
					, PH.[Your Reference] AS YOUR_REF
					, CASE WHEN PH.[Currency Code] = '' THEN 'IDR' ELSE PH.[Currency Code] END AS CURR_CODE
					, PL.TIPE
					, PL.LINE_NO
					, PL.NO_ITEM
					, PL.DESC_1
					, PL.DESC_2
					, PL.LOC_CODE
					, PL.UNIT_PRICE
					, PL.QTY_ORDER
					, PL.UOM
					, PL.QTY_RCV
					, PL.QTY_ORDER - PL.QTY_RCV AS QTY_OUT
					, PL.QTY_INV
					, PL.EXP_RCP_DATE
					, PL.DPP
					, PL.PPN
					, PL.TOTAL
					, SUM(PL.DPP) OVER (PARTITION BY PH.[No_] ORDER BY PH.[No_]) AS DPP_H
					, SUM(PL.PPN) OVER (PARTITION BY PH.[No_] ORDER BY PH.[No_]) AS PPN_H
					, SUM(PL.TOTAL) OVER (PARTITION BY PH.[No_] ORDER BY PH.[No_]) AS TOTAL_H
					, PL.NO_SO
					, PL.CUST_NAME
					, PL.RSRV_QTY
			FROM [dbo].[BC_AUTOJAYA$Purchase Header] PH
			INNER JOIN	(
							SELECT	POL.NO_PO
									,POL.TIPE
									,POL.LINE_NO
									,POL.NO_ITEM
									,POL.DESC_1
									,POL.DESC_2
									,POL.LOC_CODE
									,POL.UOM
									,POL.UNIT_PRICE
									,POL.QTY_ORDER
									,POL.QTY_RCV
									,POL.QTY_INV
									,POL.EXP_RCP_DATE
									,POL.NO_BPO
									,POL.DPP
									,POL.PPN
									,POL.TOTAL
									,RSV.NO_SO
									,RSV.CUST_NAME
									,RSV.RSRV_QTY
							FROM	(
										SELECT
												[Document No_] AS NO_PO
												,CASE	WHEN [Type] = 1 THEN 'G/L Account'
													WHEN [Type] = 2 THEN 'Item'
													WHEN [Type] = 3 THEN '?'
													WHEN [Type] = 4 THEN 'Fixed Asset'
													WHEN [Type] = 5 THEN 'Freight'
													ELSE 'Others'
												END AS TIPE
												,[Line No_] AS LINE_NO
												,[No_] AS NO_ITEM
												,[Description] AS DESC_1
												,[Description 2] AS DESC_2
												,[Location Code] AS LOC_CODE
												,[Unit of Measure] AS UOM
												,[Quantity] AS QTY_ORDER
												,[Direct Unit Cost] AS UNIT_PRICE
												,([Quantity Received])  AS QTY_RCV
												,([Quantity Invoiced]) AS QTY_INV
												,[Expected Receipt Date] AS EXP_RCP_DATE
												,[Amount] AS DPP
												,[Amount Including VAT] - [Amount] AS PPN
												,[Amount Including VAT] AS TOTAL
												,[Blanket Order No_] AS NO_BPO
										FROM [dbo].[BC_AUTOJAYA$Purchase Line]
										WHERE [Document Type] = 1
									) POL
							LEFT JOIN	(
											SELECT	DTL.ITEM_NO
													, DTL.NO_SO
													, DTL.CUST_NAME
													, DTL.QTY_ORDER
													, SUM(DTL.RSRV_QTY) AS RSRV_QTY
													, DTL.SOURCE_RSRV_LINENO
													, DTL.SOURCE_RSRV
											FROM	(
														SELECT  PO.[Item No_] AS ITEM_NO
																, SO.[Source ID] AS NO_SO
																, SOH.[Order Date] AS TGL_ORDER
																, SOH.[Bill-to Customer No_] AS CUST_CODE
																, CASE WHEN SUBSTRING(SO.[Source ID], 1, 2) = 'TO' THEN
																		'TRANSFER FROM '+TH.[Transfer-from Code]+' TO '+TH.[Transfer-to Code]
																	ELSE SOH.[Bill-to Name] END AS CUST_NAME
																, ISNULL(SOD.[Quantity (Base)], 0) AS QTY_ORDER
																, PO.[Quantity (Base)] AS RSRV_QTY
																, CASE WHEN PO.[Source ID] = '' THEN 'ON HAND' ELSE PO.[Source ID] END AS SOURCE_RSRV
																, PO.[Source Ref_ No_] AS SOURCE_RSRV_LINENO
																, SOD.[Requested Delivery Date] AS EXP_DELIVERY_DATE
																, SOH.[External Document No_] AS EXT_DOC_NO
																, SO.[Location Code] AS LOC_CODE
														FROM [dbo].[BC_AUTOJAYA$Reservation Entry] PO
														LEFT JOIN [dbo].[BC_AUTOJAYA$Reservation Entry] SO ON PO.[Entry No_] = SO.[Entry No_] AND SO.[Positive] = 0
														LEFT JOIN [dbo].[BC_AUTOJAYA$Sales Header] SOH ON SOH.[No_] = SO.[Source ID]
														LEFT JOIN [dbo].[BC_AUTOJAYA$Sales Line] SOD ON SOD.[Document No_] = SO.[Source ID] AND SOD.[Line No_] = SO.[Source Ref_ No_]
														LEFT JOIN [dbo].[BC_AUTOJAYA$Transfer Header] TH ON TH.[No_] = SO.[Source ID]
														WHERE	PO.[Reservation Status] = 0 AND
																PO.[Positive] = 1 AND
																SUBSTRING(PO.[Source ID], 1, 2) <> 'TO'
													) DTL
											WHERE	DTL.SOURCE_RSRV <> 'ON HAND'
											GROUP BY DTL.ITEM_NO, DTL.NO_SO, DTL.TGL_ORDER, DTL.CUST_CODE
													, DTL.CUST_NAME, DTL.QTY_ORDER, DTL.SOURCE_RSRV_LINENO
													, DTL.SOURCE_RSRV, DTL.EXP_DELIVERY_DATE, DTL.EXT_DOC_NO, DTL.LOC_CODE
										) RSV ON RSV.ITEM_NO = POL.NO_ITEM AND RSV.SOURCE_RSRV_LINENO = POL.LINE_NO AND RSV.SOURCE_RSRV = POL.NO_PO
						) PL ON PL.NO_PO = PH.[No_] AND PH.[Document Type] = PH.[Document Type]
			WHERE	PH.[Document Type] = 1
			UNION ALL
			SELECT	'SOLUSI' AS COMPANY
					, PH.[Assigned User ID] AS ASSIGN_USER
					,	CASE WHEN MAX(PL.NO_BPO) OVER (PARTITION BY PH.[No_] ORDER BY PH.[No_]) = '' THEN '.'
						ELSE
							MAX(PL.NO_BPO) OVER (PARTITION BY PH.[No_] ORDER BY PH.[No_])
						END AS NO_BPO
					, PH.[No_] AS NO_PO
					,	CASE WHEN SUBSTRING(PH.[No_],5,1) = 'P'  THEN 'OTP' ELSE
							CASE	WHEN SUBSTRING(PH.[No_],4,1) = 'S'  THEN 'SERVICE'
									WHEN SUBSTRING(PH.[No_],4,1) = 'N'  THEN 'NON TRADE'
									WHEN SUBSTRING(PH.[No_],4,1) = 'T'  THEN 'TRADE'
									ELSE 'UNKNOWN' END
						END AS JENIS_PO
					, PH.[Order Date] AS TGL_PO
					, PH.[Pay-to Vendor No_] AS VENDOR_NO
					, PH.[Pay-to Name] AS VENDOR_NAME
					, PH.[Your Reference] AS YOUR_REF
					, CASE WHEN PH.[Currency Code] = '' THEN 'IDR' ELSE PH.[Currency Code] END AS CURR_CODE
					, PL.TIPE
					, PL.LINE_NO
					, PL.NO_ITEM
					, PL.DESC_1
					, PL.DESC_2
					, PL.LOC_CODE
					, PL.UNIT_PRICE
					, PL.QTY_ORDER
					, PL.UOM
					, PL.QTY_RCV
					, PL.QTY_ORDER - PL.QTY_RCV AS QTY_OUT
					, PL.QTY_INV
					, PL.EXP_RCP_DATE
					, PL.DPP
					, PL.PPN
					, PL.TOTAL
					, SUM(PL.DPP) OVER (PARTITION BY PH.[No_] ORDER BY PH.[No_]) AS DPP_H
					, SUM(PL.PPN) OVER (PARTITION BY PH.[No_] ORDER BY PH.[No_]) AS PPN_H
					, SUM(PL.TOTAL) OVER (PARTITION BY PH.[No_] ORDER BY PH.[No_]) AS TOTAL_H
					, PL.NO_SO
					, PL.CUST_NAME
					, PL.RSRV_QTY
			FROM [dbo].[BC_SOLUSI$Purchase Header] PH
			INNER JOIN	(
							SELECT	POL.NO_PO
									,POL.TIPE
									,POL.LINE_NO
									,POL.NO_ITEM
									,POL.DESC_1
									,POL.DESC_2
									,POL.LOC_CODE
									,POL.UOM
									,POL.UNIT_PRICE
									,POL.QTY_ORDER
									,POL.QTY_RCV
									,POL.QTY_INV
									,POL.EXP_RCP_DATE
									,POL.NO_BPO
									,POL.DPP
									,POL.PPN
									,POL.TOTAL
									,RSV.NO_SO
									,RSV.CUST_NAME
									,RSV.RSRV_QTY
							FROM	(
										SELECT
												[Document No_] AS NO_PO
												,CASE	WHEN [Type] = 1 THEN 'G/L Account'
													WHEN [Type] = 2 THEN 'Item'
													WHEN [Type] = 3 THEN '?'
													WHEN [Type] = 4 THEN 'Fixed Asset'
													WHEN [Type] = 5 THEN 'Freight'
													ELSE 'Others'
												END AS TIPE
												,[Line No_] AS LINE_NO
												,[No_] AS NO_ITEM
												,[Description] AS DESC_1
												,[Description 2] AS DESC_2
												,[Location Code] AS LOC_CODE
												,[Unit of Measure] AS UOM
												,[Quantity] AS QTY_ORDER
												,[Direct Unit Cost] AS UNIT_PRICE
												,([Quantity Received])  AS QTY_RCV
												,([Quantity Invoiced]) AS QTY_INV
												,[Expected Receipt Date] AS EXP_RCP_DATE
												,[Amount] AS DPP
												,[Amount Including VAT] - [Amount] AS PPN
												,[Amount Including VAT] AS TOTAL
												,[Blanket Order No_] AS NO_BPO
										FROM [dbo].[BC_SOLUSI$Purchase Line]
										WHERE [Document Type] = 1
									) POL
							LEFT JOIN	(
											SELECT	DTL.ITEM_NO
													, DTL.NO_SO
													, DTL.CUST_NAME
													, DTL.QTY_ORDER
													, SUM(DTL.RSRV_QTY) AS RSRV_QTY
													, DTL.SOURCE_RSRV_LINENO
													, DTL.SOURCE_RSRV
											FROM	(
														SELECT  PO.[Item No_] AS ITEM_NO
																, SO.[Source ID] AS NO_SO
																, SOH.[Order Date] AS TGL_ORDER
																, SOH.[Bill-to Customer No_] AS CUST_CODE
																, CASE WHEN SUBSTRING(SO.[Source ID], 1, 2) = 'TO' THEN
																		'TRANSFER FROM '+TH.[Transfer-from Code]+' TO '+TH.[Transfer-to Code]
																	ELSE SOH.[Bill-to Name] END AS CUST_NAME
																, ISNULL(SOD.[Quantity (Base)], 0) AS QTY_ORDER
																, PO.[Quantity (Base)] AS RSRV_QTY
																, CASE WHEN PO.[Source ID] = '' THEN 'ON HAND' ELSE PO.[Source ID] END AS SOURCE_RSRV
																, PO.[Source Ref_ No_] AS SOURCE_RSRV_LINENO
																, SOD.[Requested Delivery Date] AS EXP_DELIVERY_DATE
																, SOH.[External Document No_] AS EXT_DOC_NO
																, SO.[Location Code] AS LOC_CODE
														FROM [dbo].[BC_SOLUSI$Reservation Entry] PO
														LEFT JOIN [dbo].[BC_SOLUSI$Reservation Entry] SO ON PO.[Entry No_] = SO.[Entry No_] AND SO.[Positive] = 0
														LEFT JOIN [dbo].[BC_SOLUSI$Sales Header] SOH ON SOH.[No_] = SO.[Source ID]
														LEFT JOIN [dbo].[BC_SOLUSI$Sales Line] SOD ON SOD.[Document No_] = SO.[Source ID] AND SOD.[Line No_] = SO.[Source Ref_ No_]
														LEFT JOIN [dbo].[BC_SOLUSI$Transfer Header] TH ON TH.[No_] = SO.[Source ID]
														WHERE	PO.[Reservation Status] = 0 AND
																PO.[Positive] = 1 AND
																SUBSTRING(PO.[Source ID], 1, 2) <> 'TO'
													) DTL
											WHERE	DTL.SOURCE_RSRV <> 'ON HAND'
											GROUP BY DTL.ITEM_NO, DTL.NO_SO, DTL.TGL_ORDER, DTL.CUST_CODE
													, DTL.CUST_NAME, DTL.QTY_ORDER, DTL.SOURCE_RSRV_LINENO
													, DTL.SOURCE_RSRV, DTL.EXP_DELIVERY_DATE, DTL.EXT_DOC_NO, DTL.LOC_CODE
										) RSV ON RSV.ITEM_NO = POL.NO_ITEM AND RSV.SOURCE_RSRV_LINENO = POL.LINE_NO AND RSV.SOURCE_RSRV = POL.NO_PO
						) PL ON PL.NO_PO = PH.[No_] AND PH.[Document Type] = PH.[Document Type]
			WHERE	PH.[Document Type] = 1
		) X
WHERE	($P{Company} = 'ALL' OR X.COMPANY =  $P{Company}) AND
		($P{AssignUserID} = 'ALL' OR X.ASSIGN_USER = $P{AssignUserID}) AND
		(X.NO_BPO BETWEEN $P{NoBPOFrom} AND $P{NoBPOUpto}) AND
		(X.NO_PO BETWEEN $P{NoPOFrom} AND $P{NoPOUpto}) AND
		(X.TGL_PO >= $P{TglPOFrom} AND X.TGL_PO <= $P{TglPOUpto}) AND
		(X.VENDOR_NO BETWEEN $P{VendorFrom} AND $P{VendorUpto})]]>
	</queryString>
