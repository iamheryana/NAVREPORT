<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="solusi.hapis.backend.model.Nextidview" table="nextidview">
        <id name="nextval" type="java.lang.Long">
            <column name="nextval"/>
            <generator class="sequence">
                <param name="sequence"></param>
            </generator>
        </id>
    </class>
</hibernate-mapping>
