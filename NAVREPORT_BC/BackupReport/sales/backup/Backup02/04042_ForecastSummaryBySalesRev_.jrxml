<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="04042_ForecastSummaryBySalesRev" language="groovy" pageWidth="1600" pageHeight="595" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="1560" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="b406b291-7ccc-4e85-8d33-6ed4d9da73cd">
	<property name="ireport.zoom" value="1.3310000000001325"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="240"/>
	<parameter name="Tahun" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Potensi" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Size" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Cabang" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT	1 AS GRP
		, XI.CABANG
		, XI.CABANG_NAMA
		, XI.SALES_CODE
		, XI.SALES_NAME
		, XI.AMOUNT_POTENSI_Q1_0
		, XI.AMOUNT_POTENSI_Q1_1
		, XI.AMOUNT_POTENSI_Q2_0
		, XI.AMOUNT_POTENSI_Q2_1
		, XI.AMOUNT_POTENSI_S1_0
		, XI.AMOUNT_POTENSI_S1_1
		, XI.AMOUNT_REALISASI_S1
		, XI.AMOUNT_POTENSI_Q3_0
		, XI.AMOUNT_POTENSI_Q3_1
		, XI.AMOUNT_POTENSI_Q4_0
		, XI.AMOUNT_POTENSI_Q4_1
		, XI.AMOUNT_POTENSI_S2_0
		, XI.AMOUNT_POTENSI_S2_1
		, XI.AMOUNT_REALISASI_S2
		, XI.AMOUNT_REALISASI
		, XI.AMOUNT_POTENSI
		, ISNULL(TRG.AMOUNT_TARGET, 0) AS AMOUNT_TARGET
		, CASE WHEN $P{Tahun} <> DATEPART(year, GETDATE()) THEN 1 ELSE DATEPART(quarter, GETDATE()) END AS CURR_Q
FROM	(
			SELECT	FBR.BRANCHES AS CABANG
					,CAB.[Name] AS CABANG_NAMA
					, FBR.SALES_CODE
					, FBR.SALES_NAME
					, SUM(FBR.AMOUNT_POTENSI_Q1_0) AS AMOUNT_POTENSI_Q1_0
					, SUM(FBR.AMOUNT_POTENSI_Q1_1) AS AMOUNT_POTENSI_Q1_1
					, SUM(FBR.AMOUNT_POTENSI_Q2_0) AS AMOUNT_POTENSI_Q2_0
					, SUM(FBR.AMOUNT_POTENSI_Q2_1) AS AMOUNT_POTENSI_Q2_1
					, (	SUM(FBR.AMOUNT_POTENSI_Q1_0) + SUM(FBR.AMOUNT_POTENSI_Q2_0) ) AS AMOUNT_POTENSI_S1_0
					, (	SUM(FBR.AMOUNT_POTENSI_Q1_1) + SUM(FBR.AMOUNT_POTENSI_Q2_1) ) AS AMOUNT_POTENSI_S1_1
					, SUM(FBR.AMOUNT_REALISASI_S1) AS AMOUNT_REALISASI_S1
					, SUM(FBR.AMOUNT_POTENSI_Q3_0) AS AMOUNT_POTENSI_Q3_0
					, SUM(FBR.AMOUNT_POTENSI_Q3_1) AS AMOUNT_POTENSI_Q3_1
					, SUM(FBR.AMOUNT_POTENSI_Q4_0) AS AMOUNT_POTENSI_Q4_0
					, SUM(FBR.AMOUNT_POTENSI_Q4_1) AS AMOUNT_POTENSI_Q4_1
					, (	SUM(FBR.AMOUNT_POTENSI_Q3_0) + SUM(FBR.AMOUNT_POTENSI_Q4_0) ) AS AMOUNT_POTENSI_S2_0
					, (	SUM(FBR.AMOUNT_POTENSI_Q3_1) + SUM(FBR.AMOUNT_POTENSI_Q4_1) ) AS AMOUNT_POTENSI_S2_1
					, SUM(FBR.AMOUNT_REALISASI_S2) AS AMOUNT_REALISASI_S2
					, (SUM(FBR.AMOUNT_REALISASI_S1) + SUM(FBR.AMOUNT_REALISASI_S2)) AS AMOUNT_REALISASI
					, SUM(FBR.AMOUNT_POTENSI) AS AMOUNT_POTENSI
			FROM	(
						SELECT	SLS.[Global Dimension 1 Code] AS BRANCHES
								, XX.SALES_CODE
								, SLS.[Name] AS SALES_NAME
								, 0 AS AMOUNT_REALISASI_S1
								, 0 AS AMOUNT_REALISASI_S2
								, SUM(XX.AMOUNT_POTENSI) AS AMOUNT_POTENSI
								, SUM(XX.AMOUNT_POTENSI_Q1_0) AS AMOUNT_POTENSI_Q1_0
								, SUM(XX.AMOUNT_POTENSI_Q1_1) AS AMOUNT_POTENSI_Q1_1
								, SUM(XX.AMOUNT_POTENSI_Q2_0) AS AMOUNT_POTENSI_Q2_0
								, SUM(XX.AMOUNT_POTENSI_Q2_1) AS AMOUNT_POTENSI_Q2_1
								, SUM(XX.AMOUNT_POTENSI_Q3_0) AS AMOUNT_POTENSI_Q3_0
								, SUM(XX.AMOUNT_POTENSI_Q3_1) AS AMOUNT_POTENSI_Q3_1
								, SUM(XX.AMOUNT_POTENSI_Q4_0) AS AMOUNT_POTENSI_Q4_0
								, SUM(XX.AMOUNT_POTENSI_Q4_1) AS AMOUNT_POTENSI_Q4_1
						FROM	(
									SELECT	CMP.[Salesperson Code] AS SALES_CODE
											, DATEPART(year, GETDATE()) AS YEAR_SUM
											, 0 AS AMOUNT_POTENSI
											, 0 AS AMOUNT_POTENSI_Q1_0
											, 0 AS AMOUNT_POTENSI_Q1_1
											, 0 AS AMOUNT_POTENSI_Q2_0
											, 0 AS AMOUNT_POTENSI_Q2_1
											, 0 AS AMOUNT_POTENSI_Q3_0
											, 0 AS AMOUNT_POTENSI_Q3_1
											, 0 AS AMOUNT_POTENSI_Q4_0
											, 0 AS AMOUNT_POTENSI_Q4_1
									FROM [AUTOJAYA].[dbo].[AUTOJAYA$Campaign]  CMP
									WHERE CMP.[Status Code] = 'ACTIVE' AND CMP.[Salesperson Code] <> ''
									UNION ALL
									SELECT	X.SALES_CODE
											,X.EST_YEAR AS YEAR_SUM
											,SUM(X.AMOUNT) AS AMOUNT_POTENSI
											, SUM(X.AMOUNT_Q1_0) AS AMOUNT_POTENSI_Q1_0
											, SUM(X.AMOUNT_Q1_1) AS AMOUNT_POTENSI_Q1_1
											, SUM(X.AMOUNT_Q2_0) AS AMOUNT_POTENSI_Q2_0
											, SUM(X.AMOUNT_Q2_1) AS AMOUNT_POTENSI_Q2_1
											, SUM(X.AMOUNT_Q3_0) AS AMOUNT_POTENSI_Q3_0
											, SUM(X.AMOUNT_Q3_1) AS AMOUNT_POTENSI_Q3_1
											, SUM(X.AMOUNT_Q4_0) AS AMOUNT_POTENSI_Q4_0
											, SUM(X.AMOUNT_Q4_1) AS AMOUNT_POTENSI_Q4_1
									FROM	(
												SELECT	'AJ' AS COMPANY
														,MAD.SALES_CODE
														,MAD.CABANG
														,MAD.SIZE_PROJECT
														,MAD.POTENSIAL_PROJECT
														,MAD.ITEM
														,MAD.ITEM_CAT
														,MAD.HW_PS
														,(MAD.UNIT_PRICE * MAD.QTY_OUT) AS AMOUNT
														,CASE WHEN MAD.POTENSIAL_PROJECT = '0' THEN CASE WHEN MAD.EST_Q = 1 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q1_0
														,CASE WHEN MAD.POTENSIAL_PROJECT = '1' THEN CASE WHEN MAD.EST_Q = 1 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q1_1
														,CASE WHEN MAD.POTENSIAL_PROJECT = '0' THEN CASE WHEN MAD.EST_Q = 2 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q2_0
														,CASE WHEN MAD.POTENSIAL_PROJECT = '1' THEN CASE WHEN MAD.EST_Q = 2 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q2_1
														,CASE WHEN MAD.POTENSIAL_PROJECT = '0' THEN CASE WHEN MAD.EST_Q = 3 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q3_0
														,CASE WHEN MAD.POTENSIAL_PROJECT = '1' THEN CASE WHEN MAD.EST_Q = 3 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q3_1
														,CASE WHEN MAD.POTENSIAL_PROJECT = '0' THEN CASE WHEN MAD.EST_Q = 4 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q4_0
														,CASE WHEN MAD.POTENSIAL_PROJECT = '1' THEN CASE WHEN MAD.EST_Q = 4 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q4_1
														,MAD.EST_YEAR
												FROM	(
															SELECT	SOH.[Salesperson Code] AS SALES_CODE
																	,SOH.[Responsibility Center] AS CABANG
																	, SUBSTRING(SOH.[Posting Description], 1,1) AS SIZE_PROJECT
																	, CASE WHEN SUBSTRING(SOH.[Posting Description], 2,1) NOT IN ('1','0') THEN '0' ELSE SUBSTRING(SOH.[Posting Description], 2,1) END AS POTENSIAL_PROJECT
																	,SOL.[No_] AS ITEM
																	,SOL.[Description] AS ITEM_DESC
																	,SOL.[Shortcut Dimension 2 Code] AS ITEM_CAT
																	,SUBSTRING(SOL.[Shortcut Dimension 2 Code] , 1,2) AS HW_PS
																	,SOL.[Quantity] AS QTY_OUT
																	,SOL.[Unit Price] AS UNIT_PRICE
																	,CASE WHEN SOL.[Shipment Date] <=  GETDATE() THEN
																		DATEPART(year, GETDATE()) ELSE DATEPART(year, SOL.[Shipment Date])
																	 END AS EST_YEAR
																	,CASE WHEN SOL.[Shipment Date] <=  GETDATE() THEN
																		DATEPART(quarter, GETDATE()) ELSE DATEPART(quarter, SOL.[Shipment Date])
																	 END AS EST_Q
															FROM [AUTOJAYA].[dbo].[AUTOJAYA$Sales Header] SOH
															INNER JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Sales Line] SOL ON SOL.[Document Type] = SOH.[Document Type] AND SOL.[Document No_] = SOH.[No_]
															WHERE SOH.[No_ Series] = 'SBOM'
														) MAD
												UNION ALL
												SELECT	'SP' AS COMPANY
														,MAD.SALES_CODE
														,MAD.CABANG
														,MAD.SIZE_PROJECT
														,MAD.POTENSIAL_PROJECT
														,MAD.ITEM
														,MAD.ITEM_CAT
														,MAD.HW_PS
														,(MAD.UNIT_PRICE * MAD.QTY_OUT) AS AMOUNT
														,CASE WHEN MAD.POTENSIAL_PROJECT = '0' THEN CASE WHEN MAD.EST_Q = 1 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q1_0
														,CASE WHEN MAD.POTENSIAL_PROJECT = '1' THEN CASE WHEN MAD.EST_Q = 1 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q1_1
														,CASE WHEN MAD.POTENSIAL_PROJECT = '0' THEN CASE WHEN MAD.EST_Q = 2 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q2_0
														,CASE WHEN MAD.POTENSIAL_PROJECT = '1' THEN CASE WHEN MAD.EST_Q = 2 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q2_1
														,CASE WHEN MAD.POTENSIAL_PROJECT = '0' THEN CASE WHEN MAD.EST_Q = 3 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q3_0
														,CASE WHEN MAD.POTENSIAL_PROJECT = '1' THEN CASE WHEN MAD.EST_Q = 3 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q3_1
														,CASE WHEN MAD.POTENSIAL_PROJECT = '0' THEN CASE WHEN MAD.EST_Q = 4 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q4_0
														,CASE WHEN MAD.POTENSIAL_PROJECT = '1' THEN CASE WHEN MAD.EST_Q = 4 THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END ELSE 0 END AS AMOUNT_Q4_1
														,MAD.EST_YEAR
												FROM	(
															SELECT	SOH.[Salesperson Code] AS SALES_CODE
																	,SOH.[Responsibility Center] AS CABANG
																	, SUBSTRING(SOH.[Posting Description], 1,1) AS SIZE_PROJECT
																	, CASE WHEN SUBSTRING(SOH.[Posting Description], 2,1) NOT IN ('1','0') THEN '0' ELSE SUBSTRING(SOH.[Posting Description], 2,1) END AS POTENSIAL_PROJECT
																	,SOL.[No_] AS ITEM
																	,SOL.[Description] AS ITEM_DESC
																	,SOL.[Shortcut Dimension 2 Code] AS ITEM_CAT
																	,SUBSTRING(SOL.[Shortcut Dimension 2 Code] , 1,2) AS HW_PS
																	,SOL.[Quantity] AS QTY_OUT
																	,SOL.[Unit Price] AS UNIT_PRICE
																	,CASE WHEN SOL.[Shipment Date] <=  GETDATE() THEN
																		DATEPART(year, GETDATE()) ELSE DATEPART(year, SOL.[Shipment Date])
																	 END AS EST_YEAR
																	,CASE WHEN SOL.[Shipment Date] <=  GETDATE() THEN
																		DATEPART(quarter, GETDATE()) ELSE DATEPART(quarter, SOL.[Shipment Date])
																	 END AS EST_Q
															FROM [AUTOJAYA].[dbo].[SOLUSI$Sales Header] SOH
															INNER JOIN [AUTOJAYA].[dbo].[SOLUSI$Sales Line] SOL ON SOL.[Document Type] = SOH.[Document Type] AND SOL.[Document No_] = SOH.[No_]
															WHERE SOH.[No_ Series] = 'SBOM'
														) MAD
											) X
									WHERE	(X.POTENSIAL_PROJECT = $P{Potensi} OR $P{Potensi} = 'ALL') AND
											(X.SIZE_PROJECT = $P{Size} OR $P{Size} = 'ALL')
									GROUP BY X.SALES_CODE, X.EST_YEAR
								) XX
						LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Salesperson_Purchaser] SLS ON SLS.[Code] = XX.SALES_CODE
						WHERE XX.YEAR_SUM = $P{Tahun}
						GROUP BY SLS.[Global Dimension 1 Code], XX.SALES_CODE, SLS.[Name]
						UNION ALL
						SELECT	XOXO.BRANCHES
								,XOXO.SALES_CODE
								,XOXO.SALES_NAME
								,XOXO.AMOUNT_REALISASI_S1
								,XOXO.AMOUNT_REALISASI_S2
								, 0 AS AMOUNT_POTENSI
								, 0 AS AMOUNT_POTENSI_Q1_0
								, 0 AS AMOUNT_POTENSI_Q1_1
								, 0 AS AMOUNT_POTENSI_Q2_0
								, 0 AS AMOUNT_POTENSI_Q2_1
								, 0 AS AMOUNT_POTENSI_Q3_0
								, 0 AS AMOUNT_POTENSI_Q3_1
								, 0 AS AMOUNT_POTENSI_Q4_0
								, 0 AS AMOUNT_POTENSI_Q4_1
						FROM	(
									SELECT	XOX.BRANCHES
											,XOX.SALESMAN AS SALES_CODE
											, SLS.[Name] AS SALES_NAME
											,(SUM(XOX.AMOUNT_S1)) AS AMOUNT_REALISASI_S1
											,(SUM(XOX.AMOUNT_S2)) AS AMOUNT_REALISASI_S2
											,((SUM(XOX.AMOUNT_S1)) + (SUM(XOX.AMOUNT_S2))) AS AMOUNT_REALISASI
									FROM	(
												SELECT	1 AS GROUP_NO
														,'Sales Revenue' AS GROUP_DESC
														,XX.BRANCHES
														,XX.SALESMAN
														,(SUM(XX.AMOUNT_S1)) AS AMOUNT_S1
														,(SUM(XX.AMOUNT_S2)) AS AMOUNT_S2
												FROM	(
															SELECT	X.BRANCHES
																	,X.SALESMAN
																	,X.AMOUNT_S1
																	,X.AMOUNT_S2
															FROM	(
																		SELECT	'SALES ALL' AS NOTE
																				,GLL.BRANCHES
																				,GLL.ITEM_CAT
																				,GLL.SALESMAN
																				,SUM(GLL.AMOUNT_S1) AS AMOUNT_S1
																				,SUM(GLL.AMOUNT_S2) AS AMOUNT_S2
																		FROM	(
																					SELECT	DIMS.BRANCHES
																							,DIMS.ITEM_CAT
																							,CASE WHEN GLE.[Document No_] = 'PCJR1808/035' THEN 'NGM' ELSE DIMS.SALESMAN END AS SALESMAN
																							, SUM((CASE WHEN DATEPART(quarter, GLE.[Posting Date]) IN (1,2) THEN GLE.[Amount] ELSE 0 END)) * -1 AS AMOUNT_S1
																							, SUM((CASE WHEN DATEPART(quarter, GLE.[Posting Date]) IN (3,4) THEN GLE.[Amount] ELSE 0 END)) * -1 AS AMOUNT_S2
																					FROM [AUTOJAYA].[dbo].[AUTOJAYA$G_L Entry] GLE
																					LEFT JOIN	(
																									SELECT	DIM.DIM_ID
																											,MAX(DIM.BRANCHES) AS BRANCHES
																											,MAX(DIM.ITEM_CAT) AS ITEM_CAT
																											,MAX(DIM.SALESMAN) AS SALESMAN
																									FROM	(
																												SELECT [Dimension Set ID] AS DIM_ID
																													  ,CASE WHEN [Dimension Code] = 'BRANCHES' THEN [Dimension Value Code] ELSE NULL END AS BRANCHES
																													  ,CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END AS ITEM_CAT
																													  ,CASE WHEN [Dimension Code] = 'SALESMAN' THEN [Dimension Value Code] ELSE NULL END AS SALESMAN
																												FROM [AUTOJAYA].[dbo].[AUTOJAYA$Dimension Set Entry]
																											) DIM
																									GROUP BY DIM.DIM_ID
																								) DIMS ON DIMS.DIM_ID = GLE.[Dimension Set ID]
																					WHERE	(GLE.[G_L Account No_] BETWEEN '400.110.00'  AND '420.110.01') AND
																							GLE.[G_L Account No_] <> '400.110.04' AND
																							DIMS.ITEM_CAT <> '' AND
																							(GLE.[Posting Date] >= (CAST((CAST($P{Tahun} as varchar(4))+'-01-01') AS datetime)) AND GLE.[Posting Date] <=  (CAST((CAST($P{Tahun} as varchar(4))+'-12-31') AS datetime)))
																					GROUP BY DIMS.BRANCHES, DIMS.ITEM_CAT, DIMS.SALESMAN, GLE.[Document No_]
																				) GLL
																		GROUP BY GLL.BRANCHES, GLL.ITEM_CAT, GLL.SALESMAN
																		UNION ALL
																		SELECT	'Sales - LAINNYA' AS NOTE
																				,'10' BRANCHES
																				,'HW01' AS ITEM_CAT
																				,'OFC' AS SALESMAN
																				,SUM(CASE WHEN DATEPART(quarter, GLE.[Posting Date]) IN (1,2) THEN GLE.[Amount] ELSE 0 END) * -1 AS AMOUNT_S1
																				,SUM(CASE WHEN DATEPART(quarter, GLE.[Posting Date]) IN (3,4) THEN GLE.[Amount] ELSE 0 END) * -1 AS AMOUNT_S2
																		FROM [AUTOJAYA].[dbo].[AUTOJAYA$G_L Entry] GLE
																		LEFT JOIN	(
																						SELECT	DIM.DIM_ID
																								,MAX(DIM.BRANCHES) AS BRANCHES
																								,MAX(DIM.ITEM_CAT) AS ITEM_CAT
																								,MAX(DIM.SALESMAN) AS SALESMAN
																						FROM	(
																									SELECT [Dimension Set ID] AS DIM_ID
																										  ,CASE WHEN [Dimension Code] = 'BRANCHES' THEN [Dimension Value Code] ELSE NULL END AS BRANCHES
																										  ,CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END AS ITEM_CAT
																										  ,CASE WHEN [Dimension Code] = 'SALESMAN' THEN [Dimension Value Code] ELSE NULL END AS SALESMAN
																									FROM [AUTOJAYA].[dbo].[AUTOJAYA$Dimension Set Entry]
																								) DIM
																						GROUP BY DIM.DIM_ID
																					) DIMS ON DIMS.DIM_ID = GLE.[Dimension Set ID]
																		WHERE	GLE.[G_L Account No_] = '400.110.04' AND
																				(GLE.[Posting Date] >= (CAST((CAST($P{Tahun} as varchar(4))+'-01-01') AS datetime)) AND GLE.[Posting Date] <=  (CAST((CAST($P{Tahun} as varchar(4))+'-12-31') AS datetime)))
																		UNION ALL
																		SELECT	'(-) Accured Sales Current Year Semester 1' AS NOTE
																				,ASCY.BRANCHES
																				,ASCY.ITEM_CAT
																				,ASCY.SALESMAN
																				,SUM(ASCY.AMOUNT) * -1 AS AMOUNT_S1
																				,0 AS AMOUNT_S2
																		FROM	(
																					SELECT	OUM.ITEM_CAT
																							,OUM.BRANCHES
																							,OUM.SALESMAN
																							,OUM.AMOUNT
																					FROM	(
																								SELECT	UM.ITEM_CAT
																										,UM.BRANCHES
																										,UM.SALESMAN
																										,UM.TGL_UM
																										,	CASE WHEN ISNULL(INV.AMOUNT,0) = 0 THEN UM.AMOUNT ELSE
																												CASE WHEN SUM(UM.AMOUNT)OVER(PARTITION BY UM.NO_SO, UM.BRANCHES, UM.ITEM_CAT, UM.SALESMAN  ORDER BY UM.TGL_UM) <= ISNULL(INV.AMOUNT,0) THEN 0 ELSE
																													SUM(UM.AMOUNT)OVER(PARTITION BY UM.NO_SO, UM.BRANCHES, UM.ITEM_CAT, UM.SALESMAN  ORDER BY UM.TGL_UM) - ISNULL(INV.AMOUNT,0)
																												END
																											END AS AMOUNT
																								FROM	(
																											SELECT	DIMS.ITEM_CAT
																													,DIMS.BRANCHES
																													,DIMS.SALESMAN
																													,SIH.[Posting Date] AS TGL_UM
																													,SIH.[No_] AS NO_UM
																													,SIH.[External Document No_] AS NO_PO_CUST
																													,SIH.[Bill-to Customer No_] AS CUST_NO
																													,SIH.[Bill-to Name] AS CUST_NAME
																													,SIH.[Prepayment Order No_] AS NO_SO
																													, (SUM(GLE.[Amount]) * -1) AS AMOUNT
																											FROM [AUTOJAYA].[dbo].[AUTOJAYA$G_L Entry] GLE
																											LEFT JOIN	(
																															SELECT	DIM.DIM_ID
																																	,MAX(DIM.BRANCHES) AS BRANCHES
																																	,MAX(DIM.ITEM_CAT) AS ITEM_CAT
																																	,MAX(DIM.SALESMAN) AS SALESMAN
																															FROM	(
																																		SELECT [Dimension Set ID] AS DIM_ID
																																			  ,CASE WHEN [Dimension Code] = 'BRANCHES' THEN [Dimension Value Code] ELSE NULL END AS BRANCHES
																																			  ,CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END AS ITEM_CAT
																																			  ,CASE WHEN [Dimension Code] = 'SALESMAN' THEN [Dimension Value Code] ELSE NULL END AS SALESMAN
																																		FROM [AUTOJAYA].[dbo].[AUTOJAYA$Dimension Set Entry]
																																	) DIM
																															GROUP BY DIM.DIM_ID
																														) DIMS ON DIMS.DIM_ID = GLE.[Dimension Set ID]
																											LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Sales Invoice Header] SIH ON SIH.[No_] = GLE.[Document No_]
																											LEFT JOIN	(
																															SELECT CLE.[Entry No_]
																																	, CASE WHEN CLE.[Document No_] LIKE 'UM%' THEN CLE.[Document No_] ELSE CLE2.[Document No_] END AS NO_UM
