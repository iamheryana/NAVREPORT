																							AND GLE.[Posting Date] >= (CAST((CAST($P{Tahun} as varchar(4))+'-01-01') AS datetime)) AND GLE.[Posting Date] <=  (CAST((CAST($P{Tahun} as varchar(4))+'-12-31') AS datetime))
																				) SLC
