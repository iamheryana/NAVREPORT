	<queryString>
		<![CDATA[SELECT POSS.COMPANY
		, POSS.TIPE
		, POSS.CABANG
		, POSS.CABANG_NAME
		, POSS.SALES_CODE
		, POSS.SALES_NAME
		, POSS.CUST_CODE
		, POSS.CUST_NAME
		, POSS.ORDER_DATE
		, POSS.NO_SO
		, POSS.NO_PO_CUST
		, POSS.CURR_CODE
		, POSS.AMOUNT_IDR
		, POSS.AMOUNT_IDR_HW01
		, POSS.AMOUNT_IDR_PS01
		, POSS.AMOUNT_IDR_PS02
		, POSS.AMOUNT_IDR_PS03
		, POSS.AMOUNT_IDR_PS04
		, POSS.AMOUNT_IDR_PS05
FROM	(
SELECT
		'AUTOJAYA' AS COMPANY
		, CASE WHEN SUBSTRING( POS.NO_SO, 1, 4) = 'SOAS' THEN 'SC' ELSE
			CASE WHEN SUBSTRING( POS.NO_SO, 1, 5) = '8SOAT' THEN 'AI' ELSE 'TD' END
		  END AS TIPE
		, POS.CABANG
		, CAB.[Name] AS CABANG_NAME
		, POS.SALES_CODE
		, SLS.[Name] AS SALES_NAME
		, POS.CUST_CODE
		, POS.CUST_NAME
		, POS.ORDER_DATE
		, POS.NO_SO
		, POS.NO_PO_CUST
		, POS.CURR_CODE
		, POS.AMOUNT_IDR
		, POS.AMOUNT_IDR_HW01
		, POS.AMOUNT_IDR_PS01
		, POS.AMOUNT_IDR_PS02
		, POS.AMOUNT_IDR_PS03
		, POS.AMOUNT_IDR_PS04
		, POS.AMOUNT_IDR_PS05
		, POS.NO_ITEM
		, POS.DESC_ITEM
		, POS.QTY
		, POS.UOM
		, POS.UNIT_PRICE
		, POS.DISC_AMOUNT
		, POS.ITEM_CATEGORY
FROM	(
SELECT
		SH.[Shortcut Dimension 1 Code] AS CABANG
		, SH.[Salesperson Code] AS SALES_CODE
		, SH.[Bill-to Customer No_] AS CUST_CODE
		, SH.[Bill-to Name] AS CUST_NAME
		, SH.[Order Date] AS ORDER_DATE
		, SH.[No_] AS NO_SO
		, SH.[External Document No_] AS NO_PO_CUST
		, SH.[Currency Code] AS CURR_CODE
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT / SH.[Currency Factor],0) ELSE SL.AMOUNT END AS AMOUNT_IDR
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT_HW01 / SH.[Currency Factor],0) ELSE SL.AMOUNT_HW01 END AS AMOUNT_IDR_HW01
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT_PS01 / SH.[Currency Factor],0) ELSE SL.AMOUNT_PS01 END AS AMOUNT_IDR_PS01
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT_PS02 / SH.[Currency Factor],0) ELSE SL.AMOUNT_PS02 END AS AMOUNT_IDR_PS02
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT_PS03 / SH.[Currency Factor],0) ELSE SL.AMOUNT_PS03 END AS AMOUNT_IDR_PS03
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT_PS04 / SH.[Currency Factor],0) ELSE SL.AMOUNT_PS04 END AS AMOUNT_IDR_PS04
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT_PS05 / SH.[Currency Factor],0) ELSE SL.AMOUNT_PS05 END AS AMOUNT_IDR_PS05
		, SL.NO_ITEM
		, SL.DESC_ITEM
		, SL.QTY
		, SL.UOM
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.UNIT_PRICE / SH.[Currency Factor],0) ELSE SL.UNIT_PRICE END AS UNIT_PRICE
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.DISC_AMOUNT / SH.[Currency Factor],0) ELSE SL.DISC_AMOUNT END AS DISC_AMOUNT
		, SL.ITEM_CATEGORY
FROM [AUTOJAYA].[dbo].[AUTOJAYA$Sales Header] SH
INNER JOIN  (
				SELECT [Document No_] AS NO_SO
						,[No_] AS NO_ITEM
						,[Description] AS DESC_ITEM
						,[Quantity] AS QTY
						,[Unit of Measure] AS UOM
						,[Unit Price] AS UNIT_PRICE
						,[Amount] AS AMOUNT
						,CASE WHEN [Item Category Code] = 'HW01' THEN [Amount] ELSE 0 END AS AMOUNT_HW01
						,CASE WHEN [Item Category Code] = 'PS01' THEN [Amount] ELSE 0 END AS AMOUNT_PS01
						,CASE WHEN [Item Category Code] = 'PS02' THEN [Amount] ELSE 0 END AS AMOUNT_PS02
						,CASE WHEN [Item Category Code] = 'PS03' THEN [Amount] ELSE 0 END AS AMOUNT_PS03
						,CASE WHEN [Item Category Code] = 'PS04' THEN [Amount] ELSE 0 END AS AMOUNT_PS04
						,CASE WHEN [Item Category Code] = 'PS05' THEN [Amount] ELSE 0 END AS AMOUNT_PS05
						,[Item Category Code] AS ITEM_CATEGORY
						,[Line Discount Amount] AS DISC_AMOUNT
				FROM [AUTOJAYA].[dbo].[AUTOJAYA$Sales Line]
				WHERE [Document Type] = 1 AND
					  [Type] = 2
			)SL ON SL.NO_SO = SH.[No_]
WHERE SH.[Document Type] = 1 AND SH.[Status] IN (1, 3)
UNION ALL
SELECT	XX.CABANG
		, XX.SALES_CODE
		, XX.CUST_CODE
		, XX.CUST_NAME
		, XX.ORDER_DATE
		, XX.NO_SO
		, XX.NO_PO_CUST
		, XX.CURR_CODE
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END ,0) ELSE XXD.AMOUNT END AS AMOUNT_IDR
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT_HW01 / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.AMOUNT_HW01 END AS AMOUNT_IDR_HW01
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT_PS01 / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.AMOUNT_PS01 END AS AMOUNT_IDR_PS01
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT_PS02 / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.AMOUNT_PS02 END AS AMOUNT_IDR_PS02
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT_PS03 / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.AMOUNT_PS03 END AS AMOUNT_IDR_PS03
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT_PS04 / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.AMOUNT_PS04 END AS AMOUNT_IDR_PS04
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT_PS05 / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.AMOUNT_PS05 END AS AMOUNT_IDR_PS05
		, XXD.NO_ITEM
		, XXD.DESC_ITEM
		, XXD.QTY
		, XXD.UOM
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.UNIT_PRICE / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.UNIT_PRICE END AS UNIT_PRICE
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.DISC_AMOUNT / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.DISC_AMOUNT END AS DISC_AMOUNT
		, XXD.ITEM_CATEGORY
FROM	(
			SELECT	X.CABANG
					, X.SALES_CODE
					, X.CUST_CODE
					, X.CUST_NAME
					, X.ORDER_DATE
					, X.NO_SO
					, X.NO_PO_CUST
					, X.CURR_CODE
			FROM (
					SELECT	SIH.[Shortcut Dimension 1 Code] AS CABANG
							, SIH.[Salesperson Code] AS SALES_CODE
							, SIH.[Bill-to Customer No_] AS CUST_CODE
							, SIH.[Bill-to Name] AS CUST_NAME
							, SIH.[Order Date] AS ORDER_DATE
							, CASE WHEN SUBSTRING(SIH.[No_], 1, 2) = 'UM' THEN  SIH.[Prepayment Order No_] ELSE SIH.[Order No_] END AS NO_SO
							, SIH.[No_] AS NO_INV
							, SIH.[External Document No_] AS NO_PO_CUST
							, SIH.[Currency Code] AS CURR_CODE
							FROM [AUTOJAYA].[dbo].[AUTOJAYA$Sales Invoice Header] SIH
					LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Sales Cr_Memo Header] CRM ON CRM.[Applies-to Doc_ No_] = SIH.[No_]
					WHERE CRM.[No_] IS NULL
				) X
			GROUP BY X.CABANG
					, X.SALES_CODE
					, X.CUST_CODE
					, X.CUST_NAME
					, X.ORDER_DATE
					, X.NO_SO
					, X.NO_PO_CUST
					, X.CURR_CODE

		) XX
LEFT JOIN (
			SELECT AL.[Version No_], AL.[Document No_] AS NO_SO
					,AL.[No_] AS NO_ITEM
					,AL.[Description] AS DESC_ITEM
					,AL.[Quantity] AS QTY
					,AL.[Unit of Measure] AS UOM
					,AL.[Unit Price] AS UNIT_PRICE
					,AL.[Line Discount Amount] AS DISC_AMOUNT
					,AL.[Amount] AS AMOUNT
					,CASE WHEN AL.[Item Category Code] = 'HW01' THEN AL.[Amount] ELSE 0 END AS AMOUNT_HW01
					,CASE WHEN AL.[Item Category Code] = 'PS01' THEN AL.[Amount] ELSE 0 END AS AMOUNT_PS01
					,CASE WHEN AL.[Item Category Code] = 'PS02' THEN AL.[Amount] ELSE 0 END AS AMOUNT_PS02
					,CASE WHEN AL.[Item Category Code] = 'PS03' THEN AL.[Amount] ELSE 0 END AS AMOUNT_PS03
					,CASE WHEN AL.[Item Category Code] = 'PS04' THEN AL.[Amount] ELSE 0 END AS AMOUNT_PS04
					,CASE WHEN AL.[Item Category Code] = 'PS05' THEN AL.[Amount] ELSE 0 END AS AMOUNT_PS05
					,AL.[Item Category Code] AS ITEM_CATEGORY
					,ALH.[Currency Factor] AS CURR_FACTOR
			FROM [AUTOJAYA].[dbo].[AUTOJAYA$Sales Line Archive] AL
			LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Sales Header Archive] ALH ON ALH.[No_] = AL.[Document No_] AND ALH.[Version No_] = AL.[Version No_]
			WHERE	AL.[Type] = 2 AND
					AL.[Version No_] = (SELECT MAX(MAL.[Version No_]) FROM [AUTOJAYA].[dbo].[AUTOJAYA$Sales Line Archive] MAL WHERE MAL.[Document No_] = AL.[Document No_])
		  ) XXD ON XXD.NO_SO = XX.NO_SO
LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Sales Header] SOO ON SOO.[No_] = XX.NO_SO
WHERE SOO.[No_] IS NULL
		) POS
LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Responsibility Center] CAB ON CAB.[Code] = POS.CABANG
LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Salesperson_Purchaser] SLS ON SLS.[Code] = POS.SALES_CODE
UNION ALL
SELECT
		'SOLUSI' AS COMPANY
		, CASE WHEN SUBSTRING( POS.NO_SO, 1, 4) = 'SOSS' THEN 'SC' ELSE
			CASE WHEN SUBSTRING( POS.NO_SO, 1, 5) = '9SOST' THEN 'AI' ELSE 'TD' END
		  END AS TIPE
		, POS.CABANG
		, CAB.[Name] AS CABANG_NAME
		, POS.SALES_CODE
		, SLS.[Name] AS SALES_NAME
		, POS.CUST_CODE
		, POS.CUST_NAME
		, POS.ORDER_DATE
		, POS.NO_SO
		, POS.NO_PO_CUST
		, POS.CURR_CODE
		, POS.AMOUNT_IDR
		, POS.AMOUNT_IDR_HW01
		, POS.AMOUNT_IDR_PS01
		, POS.AMOUNT_IDR_PS02
		, POS.AMOUNT_IDR_PS03
		, POS.AMOUNT_IDR_PS04
		, POS.AMOUNT_IDR_PS05
		, POS.NO_ITEM
		, POS.DESC_ITEM
		, POS.QTY
		, POS.UOM
		, POS.UNIT_PRICE
		, POS.DISC_AMOUNT
		, POS.ITEM_CATEGORY
FROM	(
SELECT
		SH.[Shortcut Dimension 1 Code] AS CABANG
		, SH.[Salesperson Code] AS SALES_CODE
		, SH.[Bill-to Customer No_] AS CUST_CODE
		, SH.[Bill-to Name] AS CUST_NAME
		, SH.[Order Date] AS ORDER_DATE
		, SH.[No_] AS NO_SO
		, SH.[External Document No_] AS NO_PO_CUST
		, SH.[Currency Code] AS CURR_CODE
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT / SH.[Currency Factor],0) ELSE SL.AMOUNT END AS AMOUNT_IDR
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT_HW01 / SH.[Currency Factor],0) ELSE SL.AMOUNT_HW01 END AS AMOUNT_IDR_HW01
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT_PS01 / SH.[Currency Factor],0) ELSE SL.AMOUNT_PS01 END AS AMOUNT_IDR_PS01
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT_PS02 / SH.[Currency Factor],0) ELSE SL.AMOUNT_PS02 END AS AMOUNT_IDR_PS02
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT_PS03 / SH.[Currency Factor],0) ELSE SL.AMOUNT_PS03 END AS AMOUNT_IDR_PS03
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT_PS04 / SH.[Currency Factor],0) ELSE SL.AMOUNT_PS04 END AS AMOUNT_IDR_PS04
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.AMOUNT_PS05 / SH.[Currency Factor],0) ELSE SL.AMOUNT_PS05 END AS AMOUNT_IDR_PS05
		, SL.NO_ITEM
		, SL.DESC_ITEM
		, SL.QTY
		, SL.UOM
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.UNIT_PRICE / SH.[Currency Factor],0) ELSE SL.UNIT_PRICE END AS UNIT_PRICE
		, CASE WHEN SH.[Currency Code] <> '' THEN ROUND(SL.DISC_AMOUNT / SH.[Currency Factor],0) ELSE SL.DISC_AMOUNT END AS DISC_AMOUNT
		, SL.ITEM_CATEGORY
FROM [AUTOJAYA].[dbo].[SOLUSI$Sales Header] SH
INNER JOIN  (
				SELECT [Document No_] AS NO_SO
						,[No_] AS NO_ITEM
						,[Description] AS DESC_ITEM
						,[Quantity] AS QTY
						,[Unit of Measure] AS UOM
						,[Unit Price] AS UNIT_PRICE
						,[Amount] AS AMOUNT
						,CASE WHEN [Item Category Code] = 'HW01' THEN [Amount] ELSE 0 END AS AMOUNT_HW01
						,CASE WHEN [Item Category Code] = 'PS01' THEN [Amount] ELSE 0 END AS AMOUNT_PS01
						,CASE WHEN [Item Category Code] = 'PS02' THEN [Amount] ELSE 0 END AS AMOUNT_PS02
						,CASE WHEN [Item Category Code] = 'PS03' THEN [Amount] ELSE 0 END AS AMOUNT_PS03
						,CASE WHEN [Item Category Code] = 'PS04' THEN [Amount] ELSE 0 END AS AMOUNT_PS04
						,CASE WHEN [Item Category Code] = 'PS05' THEN [Amount] ELSE 0 END AS AMOUNT_PS05
						,[Item Category Code] AS ITEM_CATEGORY
						,[Line Discount Amount] AS DISC_AMOUNT
				FROM [AUTOJAYA].[dbo].[SOLUSI$Sales Line]
				WHERE [Document Type] = 1 AND
					  [Type] = 2
			)SL ON SL.NO_SO = SH.[No_]
WHERE SH.[Document Type] = 1 AND SH.[Status] IN (1, 3)
UNION ALL
SELECT	XX.CABANG
		, XX.SALES_CODE
		, XX.CUST_CODE
		, XX.CUST_NAME
		, XX.ORDER_DATE
		, XX.NO_SO
		, XX.NO_PO_CUST
		, XX.CURR_CODE
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END ,0) ELSE XXD.AMOUNT END AS AMOUNT_IDR
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT_HW01 / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.AMOUNT_HW01 END AS AMOUNT_IDR_HW01
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT_PS01 / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.AMOUNT_PS01 END AS AMOUNT_IDR_PS01
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT_PS02 / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.AMOUNT_PS02 END AS AMOUNT_IDR_PS02
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT_PS03 / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.AMOUNT_PS03 END AS AMOUNT_IDR_PS03
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT_PS04 / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.AMOUNT_PS04 END AS AMOUNT_IDR_PS04
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.AMOUNT_PS05 / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.AMOUNT_PS05 END AS AMOUNT_IDR_PS05
		, XXD.NO_ITEM
		, XXD.DESC_ITEM
		, XXD.QTY
		, XXD.UOM
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.UNIT_PRICE / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.UNIT_PRICE END AS UNIT_PRICE
		, CASE WHEN XX.CURR_CODE <> '' THEN ROUND(XXD.DISC_AMOUNT / CASE WHEN XXD.CURR_FACTOR = 0 THEN 1 ELSE XXD.CURR_FACTOR END,0) ELSE XXD.DISC_AMOUNT END AS DISC_AMOUNT
		, XXD.ITEM_CATEGORY
FROM	(
			SELECT	X.CABANG
					, X.SALES_CODE
					, X.CUST_CODE
					, X.CUST_NAME
					, X.ORDER_DATE
					, X.NO_SO
					, X.NO_PO_CUST
					, X.CURR_CODE
			FROM (
					SELECT	SIH.[Shortcut Dimension 1 Code] AS CABANG
							, SIH.[Salesperson Code] AS SALES_CODE
							, SIH.[Bill-to Customer No_] AS CUST_CODE
							, SIH.[Bill-to Name] AS CUST_NAME
							, SIH.[Order Date] AS ORDER_DATE
							, CASE WHEN SUBSTRING(SIH.[No_], 1, 2) = 'UM' THEN  SIH.[Prepayment Order No_] ELSE SIH.[Order No_] END AS NO_SO
							, SIH.[No_] AS NO_INV
							, SIH.[External Document No_] AS NO_PO_CUST
							, SIH.[Currency Code] AS CURR_CODE
							FROM [AUTOJAYA].[dbo].[SOLUSI$Sales Invoice Header] SIH
					LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Sales Cr_Memo Header] CRM ON CRM.[Applies-to Doc_ No_] = SIH.[No_]
					WHERE CRM.[No_] IS NULL
				) X
			GROUP BY X.CABANG
					, X.SALES_CODE
					, X.CUST_CODE
					, X.CUST_NAME
					, X.ORDER_DATE
					, X.NO_SO
					, X.NO_PO_CUST
					, X.CURR_CODE
		) XX
LEFT JOIN (
			SELECT AL.[Document No_] AS NO_SO
					,AL.[No_] AS NO_ITEM
					,AL.[Description] AS DESC_ITEM
					,AL.[Quantity] AS QTY
					,AL.[Unit of Measure] AS UOM
					,AL.[Unit Price] AS UNIT_PRICE
					,AL.[Line Discount Amount] AS DISC_AMOUNT
					,AL.[Amount] AS AMOUNT
					,CASE WHEN AL.[Item Category Code] = 'HW01' THEN AL.[Amount] ELSE 0 END AS AMOUNT_HW01
					,CASE WHEN AL.[Item Category Code] = 'PS01' THEN AL.[Amount] ELSE 0 END AS AMOUNT_PS01
					,CASE WHEN AL.[Item Category Code] = 'PS02' THEN AL.[Amount] ELSE 0 END AS AMOUNT_PS02
					,CASE WHEN AL.[Item Category Code] = 'PS03' THEN AL.[Amount] ELSE 0 END AS AMOUNT_PS03
					,CASE WHEN AL.[Item Category Code] = 'PS04' THEN AL.[Amount] ELSE 0 END AS AMOUNT_PS04
					,CASE WHEN AL.[Item Category Code] = 'PS05' THEN AL.[Amount] ELSE 0 END AS AMOUNT_PS05
					,AL.[Item Category Code] AS ITEM_CATEGORY
					,ALH.[Currency Factor] AS CURR_FACTOR
			FROM [AUTOJAYA].[dbo].[SOLUSI$Sales Line Archive] AL
			LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Sales Header Archive] ALH ON ALH.[No_] = AL.[Document No_]
			WHERE	AL.[Type] = 2 AND
					AL.[Version No_] = (SELECT MAX(MAL.[Version No_]) FROM [AUTOJAYA].[dbo].[SOLUSI$Sales Line Archive] MAL WHERE MAL.[Document No_] = AL.[Document No_])
		  ) XXD ON XXD.NO_SO = XX.NO_SO
LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Sales Header] SOO ON SOO.[No_] = XX.NO_SO
WHERE SOO.[No_] IS NULL
		) POS
LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Responsibility Center] CAB ON CAB.[Code] = POS.CABANG
LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Salesperson_Purchaser] SLS ON SLS.[Code] = POS.SALES_CODE
) POSS
WHERE (POSS.COMPANY = $P{Company} OR $P{Company} = 'ALL') AND
      (POSS.CABANG_NAME = $P{Cabang} OR $P{Cabang} = 'ALL') AND
      (POSS.TIPE = $P{JenisSO} OR $P{JenisSO} = 'ALL') AND
      (POSS.SALES_CODE >= $P{SalesFrom} AND POSS.SALES_CODE <= $P{SalesUpto}) AND
      (POSS.ORDER_DATE >= $P{TglFrom} AND POSS.ORDER_DATE <=$P{TglUpto}) AND
	POSS.AMOUNT_IDR <> 0]]>
	</queryString>
