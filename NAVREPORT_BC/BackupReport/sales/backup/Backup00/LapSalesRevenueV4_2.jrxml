					, 'PS03 - Technology Service' AS ITEM_CAT
					, $P{aJKT04um} * -1 AS AMT_HO
					, $P{aSBY04um} * -1 AS AMT_SBY
					, $P{aDPS04um} * -1 AS AMT_DPS
					, $P{aCKR04um} * -1 AS AMT_CKR
					, $P{aSMR04um} * -1 AS AMT_SMR
			UNION ALL
