		, OSO.GROUP_ITEM_CAT
		, OSO.ITEM_CAT
		,ROUND((SUM(OSO.AMT_HO)),0) AS AMT_HO
		,ROUND((SUM(OSO.AMT_SBY)),0) AS AMT_SBY
