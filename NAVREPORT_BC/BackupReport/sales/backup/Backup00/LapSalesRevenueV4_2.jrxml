			UNION ALL
			SELECT	CASE WHEN DIMS.ITEM_CAT = 'HW01' THEN 'HW' ELSE 'PS' END AS GROUP_ITEM_CAT
					,CASE	WHEN DIMS.ITEM_CAT = 'HW01' THEN 'HW01 - Hardware'
							WHEN DIMS.ITEM_CAT = 'PS01' THEN 'PS01 - 3rd Party Software'
							WHEN DIMS.ITEM_CAT = 'PS02' THEN 'PS02 - Own Software'
							WHEN DIMS.ITEM_CAT = 'PS03' THEN 'PS03 - Technology Service'
							WHEN DIMS.ITEM_CAT = 'PS04' THEN 'PS04 - Hardware Service'
							WHEN DIMS.ITEM_CAT = 'PS05' THEN 'PS05 - Manage Service'
						END AS ITEM_CAT
					,CASE WHEN DIMS.BRANCHES = '10' THEN GLE.[Amount] ELSE 0 END AS AMT_HO
					,CASE WHEN DIMS.BRANCHES = '15' THEN GLE.[Amount] ELSE 0 END AS AMT_SBY
					,CASE WHEN DIMS.BRANCHES = '19' THEN GLE.[Amount] ELSE 0 END AS AMT_DPS
					,CASE WHEN DIMS.BRANCHES = '16' THEN GLE.[Amount] ELSE 0 END AS AMT_CKR
					,CASE WHEN DIMS.BRANCHES = '17' THEN GLE.[Amount] ELSE 0 END AS AMT_SMR
			FROM [AUTOJAYA].[dbo].[SOLUSI$G_L Entry] GLE
			LEFT JOIN	(
							SELECT	DIM.DIM_ID
									,MAX(DIM.BRANCHES) AS BRANCHES
									,MAX(DIM.ITEM_CAT) AS ITEM_CAT
									,MAX(DIM.SALESMAN) AS SALESMAN
							FROM	(
										SELECT [Dimension Set ID] AS DIM_ID
											  ,CASE WHEN [Dimension Code] = 'BRANCHES' THEN [Dimension Value Code] ELSE NULL END AS BRANCHES
											  ,CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END AS ITEM_CAT
											  ,CASE WHEN [Dimension Code] = 'SALESMAN' THEN [Dimension Value Code] ELSE NULL END AS SALESMAN
										FROM [AUTOJAYA].[dbo].[SOLUSI$Dimension Set Entry]
									) DIM
							GROUP BY DIM.DIM_ID
						) DIMS ON DIMS.DIM_ID = GLE.[Dimension Set ID]
			WHERE GLE.[G_L Account No_] IN ('400.110.00', '410.110.00', '420.110.00') AND
				  (GLE.[Posting Date] >= $P{TglInvFrom} AND GLE.[Posting Date] <= $P{TglInvTo}) AND
				  ($P{Company} = 'SOLUSI' OR $P{Company} = 'ALL')
			UNION ALL
			SELECT	'HW' AS GROUP_ITEM_CAT
					, 'HW01 - Hardware' AS ITEM_CAT
					, $P{aJKT01} * -1 AS AMT_HO
					, $P{aSBY01} * -1 AS AMT_SBY
					, $P{aDPS01} * -1 AS AMT_DPS
					, $P{aCKR01} * -1 AS AMT_CKR
					, $P{aSMR01} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS01 - 3rd Party Software' AS ITEM_CAT
					, $P{aJKT02} * -1 AS AMT_HO
					, $P{aSBY02} * -1 AS AMT_SBY
					, $P{aDPS02} * -1 AS AMT_DPS
					, $P{aCKR02} * -1 AS AMT_CKR
					, $P{aSMR02} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS02 - Own Software' AS ITEM_CAT
					, $P{aJKT03} * -1 AS AMT_HO
					, $P{aSBY03} * -1 AS AMT_SBY
					, $P{aDPS03} * -1 AS AMT_DPS
					, $P{aCKR03} * -1 AS AMT_CKR
					, $P{aSMR03} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS03 - Technology Service' AS ITEM_CAT
					, $P{aJKT04} * -1 AS AMT_HO
					, $P{aSBY04} * -1 AS AMT_SBY
					, $P{aDPS04} * -1 AS AMT_DPS
					, $P{aCKR04} * -1 AS AMT_CKR
					, $P{aSMR04} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS04 - Hardware Service' AS ITEM_CAT
					, $P{aJKT05} * -1 AS AMT_HO
					, $P{aSBY05} * -1 AS AMT_SBY
					, $P{aDPS05} * -1 AS AMT_DPS
					, $P{aCKR05} * -1 AS AMT_CKR
					, $P{aSMR05} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS05 - Manage Service' AS ITEM_CAT
					, $P{aJKT06} * -1 AS AMT_HO
					, $P{aSBY06} * -1 AS AMT_SBY
					, $P{aDPS06} * -1 AS AMT_DPS
					, $P{aCKR06} * -1 AS AMT_CKR
					, $P{aSMR06} * -1 AS AMT_SMR
		) X
GROUP BY X.GROUP_ITEM_CAT, X.ITEM_CAT
