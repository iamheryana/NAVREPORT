					, $P{aJKT01um} * -1 AS AMT_HO
					, $P{aSBY01um} * -1 AS AMT_SBY
					, $P{aDPS01um} * -1 AS AMT_DPS
					, $P{aCKR01um} * -1 AS AMT_CKR
					, $P{aSMR01um} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS01 - 3rd Party Software' AS ITEM_CAT
					, $P{aJKT02um} * -1 AS AMT_HO
					, $P{aSBY02um} * -1 AS AMT_SBY
					, $P{aDPS02um} * -1 AS AMT_DPS
					, $P{aCKR02um} * -1 AS AMT_CKR
					, $P{aSMR02um} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS02 - Own Software' AS ITEM_CAT
					, $P{aJKT03um} * -1 AS AMT_HO
					, $P{aSBY03um} * -1 AS AMT_SBY
					, $P{aDPS03um} * -1 AS AMT_DPS
					, $P{aCKR03um} * -1 AS AMT_CKR
					, $P{aSMR03um} * -1 AS AMT_SMR
			UNION ALL
