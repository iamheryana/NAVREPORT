	<queryString>
		<![CDATA[SELECT	SRV.GROUP_NO
		,SRV.GROUP_DESC
		,SRV.GROUP_ITEM_CAT
		,SRV.ITEM_CAT
		,ROUND((SRV.AMT_HO / 1),2) AS AMT_HO
		,ROUND((SRV.AMT_SBY / 1),2) AS AMT_SBY
		,ROUND((SRV.AMT_DPS / 1),2) AS AMT_DPS
		,ROUND((SRV.AMT_CKR / 1),2) AS AMT_CKR
		,ROUND((SRV.AMT_SMR / 1),2) AS AMT_SMR
		,	(ROUND((SRV.AMT_HO / 1),2) + ROUND((SRV.AMT_SBY / 1),2) + ROUND((SRV.AMT_DPS / 1),2)
			+ROUND((SRV.AMT_CKR / 1),2) +ROUND((SRV.AMT_SMR / 1),2) )AS TOTAL
FROM	(
SELECT	1 AS GROUP_NO
		,'Sales Revenue' AS GROUP_DESC
		,X.GROUP_ITEM_CAT
		,X.ITEM_CAT
		,(SUM(X.AMT_HO) * -1) AS AMT_HO
		,(SUM(X.AMT_SBY) * -1) AS AMT_SBY
		,(SUM(X.AMT_DPS) * -1) AS AMT_DPS
		,(SUM(X.AMT_CKR) * -1) AS AMT_CKR
		,(SUM(X.AMT_SMR) * -1) AS AMT_SMR
FROM	(
			SELECT	CASE WHEN DIMS.ITEM_CAT = 'HW01' THEN 'HW' ELSE 'PS' END AS GROUP_ITEM_CAT
					,CASE	WHEN DIMS.ITEM_CAT = 'HW01' THEN 'HW01 - Hardware'
							WHEN DIMS.ITEM_CAT = 'PS01' THEN 'PS01 - 3rd Party Software'
							WHEN DIMS.ITEM_CAT = 'PS02' THEN 'PS02 - Own Software'
							WHEN DIMS.ITEM_CAT = 'PS03' THEN 'PS03 - Technology Service'
							WHEN DIMS.ITEM_CAT = 'PS04' THEN 'PS04 - Hardware Service'
							WHEN DIMS.ITEM_CAT = 'PS05' THEN 'PS05 - Manage Service'
						END AS ITEM_CAT
					,CASE WHEN DIMS.BRANCHES = '10' THEN GLE.[Amount] ELSE 0 END AS AMT_HO
					,CASE WHEN DIMS.BRANCHES = '15' THEN GLE.[Amount] ELSE 0 END AS AMT_SBY
					,CASE WHEN DIMS.BRANCHES = '19' THEN GLE.[Amount] ELSE 0 END AS AMT_DPS
					,CASE WHEN DIMS.BRANCHES = '16' THEN GLE.[Amount] ELSE 0 END AS AMT_CKR
					,CASE WHEN DIMS.BRANCHES = '17' THEN GLE.[Amount] ELSE 0 END AS AMT_SMR
			FROM [AUTOJAYA].[dbo].[AUTOJAYA$G_L Entry] GLE
			LEFT JOIN	(
							SELECT	DIM.DIM_ID
									,MAX(DIM.BRANCHES) AS BRANCHES
									,MAX(DIM.ITEM_CAT) AS ITEM_CAT
									,MAX(DIM.SALESMAN) AS SALESMAN
							FROM	(
										SELECT [Dimension Set ID] AS DIM_ID
											  ,CASE WHEN [Dimension Code] = 'BRANCHES' THEN [Dimension Value Code] ELSE NULL END AS BRANCHES
											  ,CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END AS ITEM_CAT
											  ,CASE WHEN [Dimension Code] = 'SALESMAN' THEN [Dimension Value Code] ELSE NULL END AS SALESMAN
										FROM [AUTOJAYA].[dbo].[AUTOJAYA$Dimension Set Entry]
									) DIM
							GROUP BY DIM.DIM_ID
						) DIMS ON DIMS.DIM_ID = GLE.[Dimension Set ID]
			WHERE GLE.[G_L Account No_] IN ('400.110.00', '410.110.00', '420.110.00') AND
				  (GLE.[Posting Date] >= $P{TglInvFrom} AND GLE.[Posting Date] <= $P{TglInvTo}) AND
				  ($P{Company} = 'AUTOJAYA' OR $P{Company} = 'ALL')
			UNION ALL
			SELECT	CASE WHEN DIMS.ITEM_CAT = 'HW01' THEN 'HW' ELSE 'PS' END AS GROUP_ITEM_CAT
					,CASE	WHEN DIMS.ITEM_CAT = 'HW01' THEN 'HW01 - Hardware'
							WHEN DIMS.ITEM_CAT = 'PS01' THEN 'PS01 - 3rd Party Software'
							WHEN DIMS.ITEM_CAT = 'PS02' THEN 'PS02 - Own Software'
							WHEN DIMS.ITEM_CAT = 'PS03' THEN 'PS03 - Technology Service'
							WHEN DIMS.ITEM_CAT = 'PS04' THEN 'PS04 - Hardware Service'
							WHEN DIMS.ITEM_CAT = 'PS05' THEN 'PS05 - Manage Service'
						END AS ITEM_CAT
					,CASE WHEN DIMS.BRANCHES = '10' THEN GLE.[Amount] ELSE 0 END AS AMT_HO
					,CASE WHEN DIMS.BRANCHES = '15' THEN GLE.[Amount] ELSE 0 END AS AMT_SBY
					,CASE WHEN DIMS.BRANCHES = '19' THEN GLE.[Amount] ELSE 0 END AS AMT_DPS
					,CASE WHEN DIMS.BRANCHES = '16' THEN GLE.[Amount] ELSE 0 END AS AMT_CKR
					,CASE WHEN DIMS.BRANCHES = '17' THEN GLE.[Amount] ELSE 0 END AS AMT_SMR
			FROM [AUTOJAYA].[dbo].[SOLUSI$G_L Entry] GLE
			LEFT JOIN	(
							SELECT	DIM.DIM_ID
									,MAX(DIM.BRANCHES) AS BRANCHES
									,MAX(DIM.ITEM_CAT) AS ITEM_CAT
									,MAX(DIM.SALESMAN) AS SALESMAN
							FROM	(
										SELECT [Dimension Set ID] AS DIM_ID
											  ,CASE WHEN [Dimension Code] = 'BRANCHES' THEN [Dimension Value Code] ELSE NULL END AS BRANCHES
											  ,CASE WHEN [Dimension Code] = 'ITEM CATEGORY' THEN [Dimension Value Code] ELSE NULL END AS ITEM_CAT
											  ,CASE WHEN [Dimension Code] = 'SALESMAN' THEN [Dimension Value Code] ELSE NULL END AS SALESMAN
										FROM [AUTOJAYA].[dbo].[SOLUSI$Dimension Set Entry]
									) DIM
							GROUP BY DIM.DIM_ID
						) DIMS ON DIMS.DIM_ID = GLE.[Dimension Set ID]
			WHERE GLE.[G_L Account No_] IN ('400.110.00', '410.110.00', '420.110.00') AND
				  (GLE.[Posting Date] >= $P{TglInvFrom} AND GLE.[Posting Date] <= $P{TglInvTo}) AND
				  ($P{Company} = 'SOLUSI' OR $P{Company} = 'ALL')
			UNION ALL
			SELECT	'HW' AS GROUP_ITEM_CAT
					, 'HW01 - Hardware' AS ITEM_CAT
					, $P{aJKT01} * -1 AS AMT_HO
					, $P{aSBY01} * -1 AS AMT_SBY
					, $P{aDPS01} * -1 AS AMT_DPS
					, $P{aCKR01} * -1 AS AMT_CKR
					, $P{aSMR01} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS01 - 3rd Party Software' AS ITEM_CAT
					, $P{aJKT02} * -1 AS AMT_HO
					, $P{aSBY02} * -1 AS AMT_SBY
					, $P{aDPS02} * -1 AS AMT_DPS
					, $P{aCKR02} * -1 AS AMT_CKR
					, $P{aSMR02} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS02 - Own Software' AS ITEM_CAT
					, $P{aJKT03} * -1 AS AMT_HO
					, $P{aSBY03} * -1 AS AMT_SBY
					, $P{aDPS03} * -1 AS AMT_DPS
					, $P{aCKR03} * -1 AS AMT_CKR
					, $P{aSMR03} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS03 - Technology Service' AS ITEM_CAT
					, $P{aJKT04} * -1 AS AMT_HO
					, $P{aSBY04} * -1 AS AMT_SBY
					, $P{aDPS04} * -1 AS AMT_DPS
					, $P{aCKR04} * -1 AS AMT_CKR
					, $P{aSMR04} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS04 - Hardware Service' AS ITEM_CAT
					, $P{aJKT05} * -1 AS AMT_HO
					, $P{aSBY05} * -1 AS AMT_SBY
					, $P{aDPS05} * -1 AS AMT_DPS
					, $P{aCKR05} * -1 AS AMT_CKR
					, $P{aSMR05} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS05 - Manage Service' AS ITEM_CAT
					, $P{aJKT06} * -1 AS AMT_HO
					, $P{aSBY06} * -1 AS AMT_SBY
					, $P{aDPS06} * -1 AS AMT_DPS
					, $P{aCKR06} * -1 AS AMT_CKR
					, $P{aSMR06} * -1 AS AMT_SMR
		) X
GROUP BY X.GROUP_ITEM_CAT, X.ITEM_CAT
UNION ALL
SELECT	2 AS GROUP_NO
		,'Uang Muka' AS GROUP_DESC
		, OSO.GROUP_ITEM_CAT
		, OSO.ITEM_CAT
		,ROUND((SUM(OSO.AMT_HO)),0) AS AMT_HO
		,ROUND((SUM(OSO.AMT_SBY)),0) AS AMT_SBY
		,ROUND((SUM(OSO.AMT_DPS)),0) AS AMT_DPS
		,ROUND((SUM(OSO.AMT_CKR)),0) AS AMT_CKR
		,ROUND((SUM(OSO.AMT_SMR)),0) AS AMT_SMR
FROM	(
			SELECT	'HW' AS GROUP_ITEM_CAT
					, 'HW01 - Hardware' AS ITEM_CAT
					, $P{aJKT01um} * -1 AS AMT_HO
					, $P{aSBY01um} * -1 AS AMT_SBY
					, $P{aDPS01um} * -1 AS AMT_DPS
					, $P{aCKR01um} * -1 AS AMT_CKR
					, $P{aSMR01um} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS01 - 3rd Party Software' AS ITEM_CAT
					, $P{aJKT02um} * -1 AS AMT_HO
					, $P{aSBY02um} * -1 AS AMT_SBY
					, $P{aDPS02um} * -1 AS AMT_DPS
					, $P{aCKR02um} * -1 AS AMT_CKR
					, $P{aSMR02um} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS02 - Own Software' AS ITEM_CAT
					, $P{aJKT03um} * -1 AS AMT_HO
					, $P{aSBY03um} * -1 AS AMT_SBY
					, $P{aDPS03um} * -1 AS AMT_DPS
					, $P{aCKR03um} * -1 AS AMT_CKR
					, $P{aSMR03um} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS03 - Technology Service' AS ITEM_CAT
					, $P{aJKT04um} * -1 AS AMT_HO
					, $P{aSBY04um} * -1 AS AMT_SBY
					, $P{aDPS04um} * -1 AS AMT_DPS
					, $P{aCKR04um} * -1 AS AMT_CKR
					, $P{aSMR04um} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS04 - Hardware Service' AS ITEM_CAT
					, $P{aJKT05um} * -1 AS AMT_HO
					, $P{aSBY05um} * -1 AS AMT_SBY
					, $P{aDPS05um} * -1 AS AMT_DPS
					, $P{aCKR05um} * -1 AS AMT_CKR
					, $P{aSMR05um} * -1 AS AMT_SMR
			UNION ALL
			SELECT	'PS' AS GROUP_ITEM_CAT
					, 'PS05 - Manage Service' AS ITEM_CAT
					, $P{aJKT06um} * -1 AS AMT_HO
					, $P{aSBY06um} * -1 AS AMT_SBY
					, $P{aDPS06um} * -1 AS AMT_DPS
					, $P{aCKR06um} * -1 AS AMT_CKR
					, $P{aSMR06um} * -1 AS AMT_SMR
			UNION ALL
			SELECT	CASE WHEN XXX.ITEM_CATEGORY = 'HW01' THEN 'HW' ELSE 'PS' END AS GROUP_ITEM_CAT
					,CASE	WHEN XXX.ITEM_CATEGORY = 'HW01' THEN 'HW01 - Hardware'
							WHEN XXX.ITEM_CATEGORY = 'PS01' THEN 'PS01 - 3rd Party Software'
							WHEN XXX.ITEM_CATEGORY = 'PS02' THEN 'PS02 - Own Software'
							WHEN XXX.ITEM_CATEGORY = 'PS03' THEN 'PS03 - Technology Service'
							WHEN XXX.ITEM_CATEGORY = 'PS04' THEN 'PS04 - Hardware Service'
							WHEN XXX.ITEM_CATEGORY = 'PS05' THEN 'PS05 - Manage Service'
						END AS ITEM_CAT
					, CASE WHEN XXX.CABANG = 'JAKARTA' THEN XXX.AMOUNT_UM_IDR ELSE 0 END AS AMT_HO
					, CASE WHEN XXX.CABANG = 'SURABAYA' THEN XXX.AMOUNT_UM_IDR ELSE 0 END AS AMT_SBY
					, CASE WHEN XXX.CABANG = 'BALI' THEN XXX.AMOUNT_UM_IDR ELSE 0 END AS AMT_DPS
					, CASE WHEN XXX.CABANG = 'CIKARANG' THEN XXX.AMOUNT_UM_IDR ELSE 0 END AS AMT_CKR
					, CASE WHEN XXX.CABANG = 'SEMARANG' THEN XXX.AMOUNT_UM_IDR ELSE 0 END AS AMT_SMR
			FROM	(
						SELECT	'AUTOJAYA' AS COMPANY
								, XY.NO_SO
								, XY.TIPE
								, XY.CABANG
								, XY.LOC_CODE
								, XY.SALES_CODE
								, XY.SALES_NAME
								, XY.CUST_CODE
								, XY.CUST_NAME
								, XY.ORDER_DATE
								, XY.STATUS_SO
								, XY.TERM_OF_PAY
								, XY.NO_PO_CUST
								, XY.CURR_CODE
								, XY.CURR_FAKTOR
								, XY.ITEM_CATEGORY
								, CASE WHEN XY.AMOUNT_IDR_TTL = 0 THEN
									(XY.AMOUNT_IDR - XY.UM_AMOUNT_IDR)
									ELSE
									(XY.AMOUNT_IDR - ((XY.UM_AMOUNT_IDR*XY.AMOUNT_IDR)/(XY.AMOUNT_IDR_TTL)))
									END AS AMOUNT_IDR
								, CASE WHEN XY.AMOUNT_IDR_TTL = 0 THEN
									(XY.UM_AMOUNT_IDR)
									ELSE
									(((XY.UM_AMOUNT_IDR*XY.AMOUNT_IDR)/(XY.AMOUNT_IDR_TTL)))
									END AS AMOUNT_UM_IDR
								, CASE WHEN XY.AMOUNT_IDR_TTL = 0 THEN
									(XY.AMOUNT - XY.UM_AMOUNT)
									ELSE
									(XY.AMOUNT - ((XY.UM_AMOUNT*XY.AMOUNT) /(XY.AMOUNT_TTL)))
									END AS AMOUNT
						FROM	(
									SELECT SH.[No_] AS NO_SO
											, CASE WHEN SUBSTRING( SH.[No_], 1, 4) = 'SOAS' THEN 'SC' ELSE
												CASE WHEN SUBSTRING( SH.[No_], 1, 5) = '8SOAT' THEN 'AI' ELSE 'TD' END
											  END AS TIPE
											, CASE WHEN SH.[Location Code] = '' THEN 'HO-TD' ELSE SH.[Location Code] END AS LOC_CODE
