<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="04045_ForecastSummaryBySalesRevPillar" language="groovy" pageWidth="900" pageHeight="595" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="860" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="b406b291-7ccc-4e85-8d33-6ed4d9da73cd">
	<property name="ireport.zoom" value="1.100000000000112"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="192"/>
	<parameter name="Tahun" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Potensi" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Size" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Cabang" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Sem" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Sem1Akhir" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT	XXI.GRP
		, XXI.CABANG
		, XXI.CABANG_NAMA
		, XXI.SALES_CODE
		, XXI.SALES_NAME
		, XXI.AMOUNT_POTENSI
		, XXI.AMOUNT_POTENSI_P1
		, XXI.AMOUNT_POTENSI_P2
		, XXI.AMOUNT_POTENSI_P3
		, XXI.AMOUNT_POTENSI_P4
		, XXI.AMOUNT_POTENSI_P5
		, CASE WHEN XXI.AMOUNT_POTENSI = 0 OR XXI.AMOUNT_POTENSI_P1 = 0 THEN 0 ELSE
			 CASE WHEN XXI.AMOUNT_POTENSI = XXI.AMOUNT_POTENSI_P1 THEN 100 ELSE XXI.PCN_POTENSI_P1 END
		  END AS PCN_POTENSI_P1
		, CASE WHEN XXI.AMOUNT_POTENSI = 0 OR XXI.AMOUNT_POTENSI_P2 = 0 THEN 0 ELSE
			 CASE WHEN XXI.AMOUNT_POTENSI = (XXI.AMOUNT_POTENSI_P1 + XXI.AMOUNT_POTENSI_P2) THEN 100 - XXI.PCN_POTENSI_P1 ELSE XXI.PCN_POTENSI_P2 END
		  END AS PCN_POTENSI_P2
		 , CASE WHEN XXI.AMOUNT_POTENSI = 0 OR XXI.AMOUNT_POTENSI_P3 = 0 THEN 0 ELSE
			 CASE WHEN XXI.AMOUNT_POTENSI = (XXI.AMOUNT_POTENSI_P1 + XXI.AMOUNT_POTENSI_P2 + XXI.AMOUNT_POTENSI_P3) THEN
				100 - (XXI.PCN_POTENSI_P1 + XXI.PCN_POTENSI_P2) ELSE XXI.PCN_POTENSI_P3
			 END
		   END AS PCN_POTENSI_P3
		, CASE WHEN XXI.AMOUNT_POTENSI = 0 OR XXI.AMOUNT_POTENSI_P4 = 0 THEN 0 ELSE
			 CASE WHEN XXI.AMOUNT_POTENSI = (XXI.AMOUNT_POTENSI_P1 + XXI.AMOUNT_POTENSI_P2 + XXI.AMOUNT_POTENSI_P3 + XXI.AMOUNT_POTENSI_P4) THEN
				100 - (XXI.PCN_POTENSI_P1 + XXI.PCN_POTENSI_P2 + XXI.PCN_POTENSI_P3) ELSE XXI.PCN_POTENSI_P4
			 END
		   END AS PCN_POTENSI_P4
		, CASE WHEN XXI.AMOUNT_POTENSI = 0 OR XXI.AMOUNT_POTENSI_P5 = 0 THEN 0 ELSE
			 CASE WHEN XXI.AMOUNT_POTENSI = (XXI.AMOUNT_POTENSI_P1 + XXI.AMOUNT_POTENSI_P2 + XXI.AMOUNT_POTENSI_P3 + XXI.AMOUNT_POTENSI_P4 + XXI.AMOUNT_POTENSI_P5) THEN
				100 - (XXI.PCN_POTENSI_P1 + XXI.PCN_POTENSI_P2 + XXI.PCN_POTENSI_P3 + XXI.PCN_POTENSI_P4) ELSE XXI.PCN_POTENSI_P5
			 END
		   END AS PCN_POTENSI_P5
FROM	(
SELECT	1 AS GRP
		, XI.CABANG
		, XI.CABANG_NAMA
		, XI.SALES_CODE
		, XI.SALES_NAME
		, XI.AMOUNT_POTENSI
		, XI.AMOUNT_POTENSI_P1
		, XI.AMOUNT_POTENSI_P2
		, XI.AMOUNT_POTENSI_P3
		, XI.AMOUNT_POTENSI_P4
		, XI.AMOUNT_POTENSI_P5
		, ROUND((CASE WHEN  XI.AMOUNT_POTENSI  = 0 THEN 0 ELSE (XI.AMOUNT_POTENSI_P1 /  XI.AMOUNT_POTENSI) * 100 END),2) AS PCN_POTENSI_P1
		, ROUND((CASE WHEN  XI.AMOUNT_POTENSI  = 0 THEN 0 ELSE (XI.AMOUNT_POTENSI_P2 /  XI.AMOUNT_POTENSI) * 100 END),2) AS PCN_POTENSI_P2
		, ROUND((CASE WHEN  XI.AMOUNT_POTENSI  = 0 THEN 0 ELSE (XI.AMOUNT_POTENSI_P3 /  XI.AMOUNT_POTENSI) * 100 END),2) AS PCN_POTENSI_P3
		, ROUND((CASE WHEN  XI.AMOUNT_POTENSI  = 0 THEN 0 ELSE (XI.AMOUNT_POTENSI_P4 /  XI.AMOUNT_POTENSI) * 100 END),2) AS PCN_POTENSI_P4
		, ROUND((CASE WHEN  XI.AMOUNT_POTENSI  = 0 THEN 0 ELSE (XI.AMOUNT_POTENSI_P5 /  XI.AMOUNT_POTENSI) * 100 END),2) AS PCN_POTENSI_P5

FROM	(
			SELECT	FBR.BRANCHES AS CABANG
					,CAB.[Name] AS CABANG_NAMA
					, FBR.SALES_CODE
					, FBR.SALES_NAME
					, CASE	WHEN $P{Sem} = 'ALL' THEN
							(	FBR.AMOUNT_P1_SEM1 + FBR.AMOUNT_P2_SEM1 + FBR.AMOUNT_P3_SEM1 + FBR.AMOUNT_P4_SEM1 + FBR.AMOUNT_P5_SEM1 +
								FBR.AMOUNT_P1_SEM2 + FBR.AMOUNT_P2_SEM2 + FBR.AMOUNT_P3_SEM2 + FBR.AMOUNT_P4_SEM2 + FBR.AMOUNT_P5_SEM2)
							WHEN $P{Sem} = 'S1' THEN
							(
								FBR.AMOUNT_P1_SEM1 + FBR.AMOUNT_P2_SEM1 + FBR.AMOUNT_P3_SEM1 + FBR.AMOUNT_P4_SEM1 + FBR.AMOUNT_P5_SEM1
							)
							WHEN $P{Sem} = 'S2' THEN
							(
								FBR.AMOUNT_P1_SEM2 + FBR.AMOUNT_P2_SEM2 + FBR.AMOUNT_P3_SEM2 + FBR.AMOUNT_P4_SEM2 + FBR.AMOUNT_P5_SEM2
							)
							ELSE 0
					  END AMOUNT_POTENSI
					, CASE	WHEN $P{Sem} = 'ALL' THEN (FBR.AMOUNT_P1_SEM1 + FBR.AMOUNT_P1_SEM2)
							WHEN $P{Sem} = 'S1' THEN FBR.AMOUNT_P1_SEM1
							WHEN $P{Sem} = 'S2' THEN FBR.AMOUNT_P1_SEM2
							ELSE 0
					  END AMOUNT_POTENSI_P1
					, CASE	WHEN $P{Sem} = 'ALL' THEN (FBR.AMOUNT_P2_SEM1 + FBR.AMOUNT_P2_SEM2)
							WHEN $P{Sem} = 'S1' THEN FBR.AMOUNT_P2_SEM1
							WHEN $P{Sem} = 'S2' THEN FBR.AMOUNT_P2_SEM2
							ELSE 0
					  END AMOUNT_POTENSI_P2
					, CASE	WHEN $P{Sem} = 'ALL' THEN (FBR.AMOUNT_P3_SEM1 + FBR.AMOUNT_P3_SEM2)
							WHEN $P{Sem} = 'S1' THEN FBR.AMOUNT_P3_SEM1
							WHEN $P{Sem} = 'S2' THEN FBR.AMOUNT_P3_SEM2
							ELSE 0
					  END AMOUNT_POTENSI_P3
					, CASE	WHEN $P{Sem} = 'ALL' THEN (FBR.AMOUNT_P4_SEM1 + FBR.AMOUNT_P4_SEM2)
							WHEN $P{Sem} = 'S1' THEN FBR.AMOUNT_P4_SEM1
							WHEN $P{Sem} = 'S2' THEN FBR.AMOUNT_P4_SEM2
							ELSE 0
					  END AMOUNT_POTENSI_P4
					, CASE	WHEN $P{Sem} = 'ALL' THEN (FBR.AMOUNT_P5_SEM1 + FBR.AMOUNT_P5_SEM2)
							WHEN $P{Sem} = 'S1' THEN FBR.AMOUNT_P5_SEM1
							WHEN $P{Sem} = 'S2' THEN FBR.AMOUNT_P5_SEM2
							ELSE 0
					  END AMOUNT_POTENSI_P5
			FROM	(
						SELECT	SLS.[Global Dimension 1 Code] AS BRANCHES
								, XXX.SALES_CODE
								, SLS.[Name] AS SALES_NAME
								, SUM(XXX.AMOUNT_P1_SEM1)AS AMOUNT_P1_SEM1
								, SUM(XXX.AMOUNT_P2_SEM1)AS AMOUNT_P2_SEM1
								, SUM(XXX.AMOUNT_P3_SEM1)AS AMOUNT_P3_SEM1
								, SUM(XXX.AMOUNT_P4_SEM1)AS AMOUNT_P4_SEM1
								, SUM(XXX.AMOUNT_P5_SEM1)AS AMOUNT_P5_SEM1
								, SUM(XXX.AMOUNT_P1_SEM2)AS AMOUNT_P1_SEM2
								, SUM(XXX.AMOUNT_P2_SEM2)AS AMOUNT_P2_SEM2
								, SUM(XXX.AMOUNT_P3_SEM2)AS AMOUNT_P3_SEM2
								, SUM(XXX.AMOUNT_P4_SEM2)AS AMOUNT_P4_SEM2
								, SUM(XXX.AMOUNT_P5_SEM2)AS AMOUNT_P5_SEM2
						FROM	(
									SELECT	CMP.[Salesperson Code] AS SALES_CODE
											, DATEPART(year, GETDATE()) AS YEAR_SUM
											, 0 AS AMOUNT_P1_SEM1
											, 0 AS AMOUNT_P2_SEM1
											, 0 AS AMOUNT_P3_SEM1
											, 0 AS AMOUNT_P4_SEM1
											, 0 AS AMOUNT_P5_SEM1
											, 0 AS AMOUNT_P1_SEM2
											, 0 AS AMOUNT_P2_SEM2
											, 0 AS AMOUNT_P3_SEM2
											, 0 AS AMOUNT_P4_SEM2
											, 0 AS AMOUNT_P5_SEM2
									FROM [AUTOJAYA].[dbo].[AUTOJAYA$Campaign]  CMP
									WHERE CMP.[Status Code] = 'ACTIVE' AND CMP.[Salesperson Code] <> ''
									UNION ALL
									SELECT	 XX.SALES_CODE
											, XX.YEAR_SUM
											, SUM(XX.AMOUNT_P1_SEM1) AS AMOUNT_P1_SEM1
											, SUM(XX.AMOUNT_P2_SEM1) AS AMOUNT_P2_SEM1
											, SUM(XX.AMOUNT_P3_SEM1) AS AMOUNT_P3_SEM1
											, SUM(XX.AMOUNT_P4_SEM1) AS AMOUNT_P4_SEM1
											, SUM(XX.AMOUNT_P5_SEM1) AS AMOUNT_P5_SEM1
											, SUM(XX.AMOUNT_P1_SEM2) AS AMOUNT_P1_SEM2
											, SUM(XX.AMOUNT_P2_SEM2) AS AMOUNT_P2_SEM2
											, SUM(XX.AMOUNT_P3_SEM2) AS AMOUNT_P3_SEM2
											, SUM(XX.AMOUNT_P4_SEM2) AS AMOUNT_P4_SEM2
											, SUM(XX.AMOUNT_P5_SEM2) AS AMOUNT_P5_SEM2
									FROM	(
												SELECT	X.SALES_CODE
														,X.EST_YEAR AS YEAR_SUM
														,CASE WHEN X.DIM_PRINCIPAL IN	(
																							'AGILYSYS', 'ANEW','BLUE','BROT','CILI',
																							'EVOL','FARG','GODE','HONE','IVANTI',
																							'MOTO','POIN','PROT','SAMSUNG','SEAGULL',
																							'SEUIC','UNI','WINM','WOOS','ZEBR','JANZ'
																						) THEN X.AMOUNT_S1 ELSE 0 END AS AMOUNT_P1_SEM1
														,CASE WHEN X.DIM_PRINCIPAL IN	(
																							'AGILYSYS', 'ANEW','BLUE','BROT','CILI',
																							'EVOL','FARG','GODE','HONE','IVANTI',
																							'MOTO','POIN','PROT','SAMSUNG','SEAGULL',
																							'SEUIC','UNI','WINM','WOOS','ZEBR','JANZ'
																						) THEN X.AMOUNT_S2 ELSE 0 END AS AMOUNT_P1_SEM2
														,CASE WHEN X.DIM_PRINCIPAL IN	(
																							'APC','CAMB','CISC',
																							'DELL','FORTI','HP','XTRN','NUTANIX'
																						) THEN X.AMOUNT_S1 ELSE 0 END AS AMOUNT_P2_SEM1
														,CASE WHEN X.DIM_PRINCIPAL IN	(
																							'APC','CAMB','CISC',
																							'DELL','FORTI','HP','XTRN','NUTANIX'
																						) THEN X.AMOUNT_S2 ELSE 0 END AS AMOUNT_P2_SEM2
														,CASE WHEN X.DIM_PRINCIPAL IN	(
																							'ALCAT','AXIS','HIKV','HONS','TELV'
																						) THEN X.AMOUNT_S1 ELSE 0 END AS AMOUNT_P3_SEM1
														,CASE WHEN X.DIM_PRINCIPAL IN	(
																							'ALCAT','AXIS','HIKV','HONS','TELV'
																						) THEN X.AMOUNT_S2 ELSE 0 END AS AMOUNT_P3_SEM2
														,CASE WHEN X.DIM_PRINCIPAL IN	(
																							'ACS'
																						) THEN X.AMOUNT_S1 ELSE 0 END AS AMOUNT_P4_SEM1
														,CASE WHEN X.DIM_PRINCIPAL IN	(
																							'ACS'
																						) THEN X.AMOUNT_S2 ELSE 0 END AS AMOUNT_P4_SEM2
														,CASE WHEN X.DIM_PRINCIPAL IS NULL OR X.DIM_PRINCIPAL IN
																						(
																							'LOCAL','MICROSOFT','OPGLB',''
																						) THEN X.AMOUNT_S1 ELSE 0 END AS AMOUNT_P5_SEM1
														,CASE WHEN X.DIM_PRINCIPAL IS NULL OR X.DIM_PRINCIPAL IN
																						(
																							'LOCAL','MICROSOFT','OPGLB',''
																						) THEN X.AMOUNT_S2 ELSE 0 END AS AMOUNT_P5_SEM2
												FROM	(
															SELECT	'AJ' AS COMPANY
																	,MAD.SALES_CODE
																	,MAD.CABANG
																	,MAD.SIZE_PROJECT
																	,MAD.POTENSIAL_PROJECT
																	,MAD.ITEM
																	,MAD.ITEM_CAT
																	,MAD.HW_PS
																	,ROUND((
																		CASE WHEN MAD.JML_TAHUN <> 0 THEN
																			CASE WHEN  MAD.EST_Q = 1234 THEN
																				((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN)
																			ELSE
																				CASE	WHEN MAD.EST_Q IN (123, 234) THEN (((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4) * 3
																						WHEN MAD.EST_Q IN (12, 34) THEN ((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 2
																						WHEN MAD.EST_Q IN (1,2,3,4) THEN (((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																						ELSE 0
																				END
																			END
																		ELSE
																			(MAD.UNIT_PRICE * MAD.QTY_OUT)
																		END
																		/ 1000000 ),2) AS AMOUNT
																	,ROUND(((
																		CASE WHEN MAD.JML_TAHUN <> 0 THEN
																			CASE WHEN MAD.EST_Q IN (1, 1234, 123, 12) THEN
																				(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																			ELSE
																				0
																			END
																		ELSE
																			CASE WHEN MAD.EST_Q IN (1, 1234, 123, 12) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																		END
																		+
																		CASE WHEN MAD.JML_TAHUN <> 0 THEN
																			CASE WHEN MAD.EST_Q IN (2, 1234, 123, 12, 234) THEN
																				(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																			ELSE
																				0
																			END
																		ELSE
																			CASE WHEN MAD.EST_Q IN (2, 1234, 123, 12, 234) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																		END
																		) / 1000000),2) AS AMOUNT_S1
																	,ROUND(((
																		CASE WHEN MAD.JML_TAHUN <> 0 THEN
																			CASE WHEN MAD.EST_Q IN (3, 1234, 123, 234, 34) THEN
																				(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																			ELSE
																				0
																			END
																		ELSE
																			CASE WHEN MAD.EST_Q IN (3, 1234, 123, 234, 34) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																		END
																		+
																		CASE WHEN MAD.JML_TAHUN <> 0 THEN
																			CASE WHEN MAD.EST_Q IN (4, 1234, 234, 34) THEN
																				(((MAD.UNIT_PRICE * MAD.QTY_OUT) / MAD.JML_TAHUN) / 4)
																			ELSE
																				0
																			END
																		ELSE
																			CASE WHEN MAD.EST_Q IN (4, 1234, 234, 34) THEN (MAD.UNIT_PRICE * MAD.QTY_OUT) ELSE 0 END
																		END
																		) / 1000000),2) AS AMOUNT_S2
																	,MAD.DIM_PRINCIPAL
																	,MAD.EST_YEAR
															FROM	(
																		SELECT	SOH.[Salesperson Code] AS SALES_CODE
																				,SOH.[Responsibility Center] AS CABANG
																				, 	CASE WHEN SOL.[Warranty Code] = '' THEN
																						SUBSTRING(SOH.[Posting Description], 1,1)
																					ELSE
																						CASE WHEN SOL.[Warranty Code] = 'CLOSED' THEN 'C' ELSE SUBSTRING(SOH.[Posting Description], 1,1) END
																					END AS SIZE_PROJECT
																				, CASE WHEN SUBSTRING(SOH.[Posting Description], 2,1) NOT IN ('1','0') THEN '0' ELSE SUBSTRING(SOH.[Posting Description], 2,1) END AS POTENSIAL_PROJECT
																				,SOL.[No_] AS ITEM
																				,SOL.[Description] AS ITEM_DESC
																				,SOL.[Shortcut Dimension 2 Code] AS ITEM_CAT
																				,SUBSTRING(SOL.[Shortcut Dimension 2 Code] , 1,2) AS HW_PS
																				,SOL.[Quantity] AS QTY_OUT
																				,SOL.[Unit Price] AS UNIT_PRICE
																				,DIMD.DIM_PRINCIPAL
																				,SOH.[Payment Discount _] AS JML_TAHUN
																				,	CASE WHEN SOH.[Payment Discount _] <> 0 THEN
																						CASE WHEN DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date]) <=  GETDATE() THEN
																							CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) > $P{Tahun} THEN
																								CASE WHEN DATEPART(year, GETDATE()) <= $P{Tahun} THEN
																									$P{Tahun}
																								ELSE
																									0
																								END
																							ELSE
																								CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) = $P{Tahun} THEN
																									CASE WHEN DATEPART(quarter, GETDATE()) IN (1,2) THEN 0 ELSE $P{Tahun} END
																								ELSE
																									0
																								END
																							END
																						ELSE
																							CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) - 1)) > $P{Tahun} THEN
																								CASE WHEN DATEPART(year, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) <= $P{Tahun} THEN
																									$P{Tahun}
																								ELSE
																									0
																								END
																							ELSE
																								CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) - 1)) = $P{Tahun} THEN
																									CASE WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) IN (1,2) THEN 0 ELSE $P{Tahun} END
																								ELSE
																									0
																								END
																							END
																						END
																					ELSE
																						CASE WHEN DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date]) <=  GETDATE() THEN
																							DATEPART(year, GETDATE()) ELSE DATEPART(year, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date]))
																						END
																					END AS EST_YEAR
																				,	CASE WHEN SOH.[Payment Discount _] <> 0 THEN
																						CASE WHEN DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date]) <=  GETDATE() THEN
																							CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) > $P{Tahun} THEN
																								CASE WHEN DATEPART(year, GETDATE()) < $P{Tahun} THEN
																									1234
																								ELSE
																									CASE WHEN DATEPART(year, GETDATE()) = $P{Tahun} THEN
																										CASE	WHEN DATEPART(quarter, GETDATE()) = 1 THEN 1234
																												WHEN DATEPART(quarter, GETDATE()) = 2 THEN 234
																												WHEN DATEPART(quarter, GETDATE()) = 3 THEN 34
																												ELSE 4
																										END
																									ELSE
																										0
																									END
																								END
																							ELSE
																								CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], GETDATE()) - 1)) = $P{Tahun} THEN
																									CASE WHEN DATEPART(quarter, GETDATE()) = 1 THEN 0
																										 WHEN DATEPART(quarter, GETDATE()) = 2 THEN 1
																										 WHEN DATEPART(quarter, GETDATE()) = 3 THEN 12
																										 ELSE 123
																									END
																								ELSE
																									0
																								END
																							END
																						ELSE
																							CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) - 1)) > $P{Tahun} THEN
																								CASE WHEN DATEPART(year, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) < $P{Tahun} THEN
																									1234
																								ELSE
																									CASE WHEN DATEPART(year, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = $P{Tahun} THEN
																										CASE	WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = 1 THEN 1234
																												WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = 2 THEN 234
																												WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = 3 THEN 34
																												ELSE 4
																										END
																									ELSE
																										0
																									END
																								END
																							ELSE
																								CASE WHEN DATEPART(year,(DATEADD(year, SOH.[Payment Discount _], DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) - 1)) = $P{Tahun} THEN
																									CASE WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = 1 THEN 0
																										 WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = 2 THEN 1
																										 WHEN DATEPART(quarter, DATEADD(MONTH, (6-$P{Sem1Akhir}) , SOL.[Shipment Date])) = 3 THEN 12
																										 ELSE 123
																									END
																								ELSE
																									0
																								END
																							END
																						END
																					ELSE
