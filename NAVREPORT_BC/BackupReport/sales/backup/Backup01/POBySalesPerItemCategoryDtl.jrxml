		,SUM(POSS.JML_PS) OVER(PARTITION BY POSS.NO_SO, POSS.NO_PO_CUST ORDER BY POSS.NO_SO, POSS.NO_PO_CUST) AS JML_PS_TTL
FROM	(
SELECT
		'AUTOJAYA' AS COMPANY
		, CASE WHEN SUBSTRING( POS.NO_SO, 1, 4) = 'SOAS' THEN 'SC' ELSE
			CASE WHEN SUBSTRING( POS.NO_SO, 1, 5) = '8SOAT' THEN 'AI' ELSE 'TD' END
		  END AS TIPE
		, POS.CABANG
		, CAB.[Name] AS CABANG_NAME
		, POS.SALES_CODE
		, SLS.[Name] AS SALES_NAME
		, POS.CUST_CODE
		, POS.CUST_NAME
		, POS.ORDER_DATE
		, POS.NO_SO
		, POS.NO_PO_CUST
		, POS.CURR_CODE
		, POS.AMOUNT_IDR
		, POS.AMOUNT_IDR_HW01
		, POS.AMOUNT_IDR_PS01
		, POS.AMOUNT_IDR_PS02
		, POS.AMOUNT_IDR_PS03
		, POS.AMOUNT_IDR_PS04
		, POS.AMOUNT_IDR_PS05
		, POS.NO_ITEM
		, POS.DESC_ITEM
