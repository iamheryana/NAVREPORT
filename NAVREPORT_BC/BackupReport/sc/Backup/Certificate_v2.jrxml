	<queryString>
		<![CDATA[SELECT 'A' AS GROUP_TEXT
		,'A' AS LINE_TEXT
		,X.NO_DO
		,X.ITEM_NO
		,X.PRODUCT_CODE
		,X.TGL_DO
		,X.QTY
		,X.EXP_DATE
		,X.SN
FROM
(
SELECT	DOH.[No_] AS NO_DO
		,DOL.[No_] AS ITEM_NO
		,DOL.[Description 2] AS PRODUCT_CODE
		,DOH.[Posting Date] AS TGL_DO
		,DOL.[Quantity] AS QTY
		,CASE WHEN CASE WHEN SUBSTRING(DET.WC,1,1) = 'W' THEN 'W' ELSE 'D' END = 'W' THEN  dateadd(MONTH, cast(ISNULL(ISNULL(SUBSTRING(DET.DDC, LEN(DET.DDC)-2, LEN(DET.DDC)-1),0) ,0) AS INT), DOH.[Posting Date])
			ELSE  DOH.[Posting Date] END AS EXP_DATE
		,(
			SELECT '|'+IL.[Serial No_] + '| ' as 'data()'
			FROM [AUTOJAYA].[dbo].[AUTOJAYA$Item Ledger Entry] IL
			WHERE	IL.[Document No_] = DOH.[No_] AND
					IL.[Document Line No_] = DOL.[Order Line No_] AND
					IL.[Item No_] = DOL.[No_]
			for xml path('')

			) AS SN
FROM [AUTOJAYA].[dbo].[AUTOJAYA$Sales Shipment Header] DOH
INNER JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Sales Shipment Line] DOL ON DOL.[Document No_] = DOH.[No_] AND DOL.[Type] = 2
LEFT JOIN	(
				SELECT ILE.[Item No_] AS NO_ITEM
					  ,ILE.[Document No_] AS NO_DO
					  ,ILE.[Document Line No_]  AS NO_DO_LINE
					  ,MAX(ILE.[Warranty Code]) AS WC
					  ,MAX(PYT.[Due Date Calculation]) AS DDC
				FROM [AUTOJAYA].[dbo].[AUTOJAYA$Item Ledger Entry] ILE
				LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Payment Terms] PYT ON PYT.[Code] = ILE.[Warranty Code]
				GROUP BY ILE.[Item No_], ILE.[Document No_], ILE.[Document Line No_]
			) DET ON DET.NO_DO = DOH.[No_] AND DET.NO_DO_LINE = DOL.[Order Line No_] AND DET.NO_ITEM = DOL.[No_]
WHERE DOH.[No_] = $P{NoDo} AND
	(DOL.[No_] = $P{Item1} OR DOL.[No_] = $P{Item2} OR DOL.[No_] = $P{Item2}) AND $P{Company} = 'AJ'
UNION ALL
SELECT	DOH.[No_] AS NO_DO
		,DOL.[No_] AS ITEM_NO
		,DOL.[Description 2] AS PRODUCT_CODE
		,DOH.[Posting Date] AS TGL_DO
		,DOL.[Quantity] AS QTY
		,CASE WHEN CASE WHEN SUBSTRING(DET.WC,1,1) = 'W' THEN 'W' ELSE 'D' END = 'W' THEN  dateadd(MONTH, cast(ISNULL(ISNULL(SUBSTRING(DET.DDC, LEN(DET.DDC)-2, LEN(DET.DDC)-1),0) ,0) AS INT), DOH.[Posting Date])
			ELSE  DOH.[Posting Date] END AS EXP_DATE
		,(
			SELECT '|'+IL.[Serial No_] + '| ' as 'data()'
			FROM [AUTOJAYA].[dbo].[AUTOJAYA$Item Ledger Entry] IL
			WHERE	IL.[Document No_] = DOH.[No_] AND
					IL.[Document Line No_] = DOL.[Order Line No_] AND
					IL.[Item No_] = DOL.[No_]
			for xml path('')

			) AS SN
FROM [AUTOJAYA].[dbo].[SOLUSI$Sales Shipment Header] DOH
INNER JOIN [AUTOJAYA].[dbo].[SOLUSI$Sales Shipment Line] DOL ON DOL.[Document No_] = DOH.[No_] AND DOL.[Type] = 2
LEFT JOIN	(
				SELECT ILE.[Item No_] AS NO_ITEM
					  ,ILE.[Document No_] AS NO_DO
					  ,ILE.[Document Line No_]  AS NO_DO_LINE
					  ,MAX(ILE.[Warranty Code]) AS WC
					  ,MAX(PYT.[Due Date Calculation]) AS DDC
				FROM [AUTOJAYA].[dbo].[SOLUSI$Item Ledger Entry] ILE
				LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Payment Terms] PYT ON PYT.[Code] = ILE.[Warranty Code]
				GROUP BY ILE.[Item No_], ILE.[Document No_], ILE.[Document Line No_]
			) DET ON DET.NO_DO = DOH.[No_] AND DET.NO_DO_LINE = DOL.[Order Line No_] AND DET.NO_ITEM = DOL.[No_]
WHERE DOH.[No_] = $P{NoDo} AND
	(DOL.[No_] = $P{Item1} OR DOL.[No_] = $P{Item2} OR DOL.[No_] = $P{Item2}) AND $P{Company} = 'SP'
) X
UNION ALL
SELECT 'B1' AS GROUP_TEXT, 'B101' AS LINE_TEXT, NULL AS NO_DO, 'ZZZZZZZZZZZZZZZZZZZZ' AS ITEM_NO, NULL AS PRODUCT_CODE, NULL AS TGL_DO,0 AS QTY, NULL AS EXP_DATE, NULL AS SN WHERE $P{sec1} ='Y'
UNION ALL
SELECT 'B1' AS GROUP_TEXT, 'B102' AS LINE_TEXT, NULL AS NO_DO, 'ZZZZZZZZZZZZZZZZZZZZ' AS ITEM_NO, NULL AS PRODUCT_CODE, NULL AS TGL_DO,0 AS QTY, NULL AS EXP_DATE, NULL AS SN WHERE $P{sec1} ='Y'
UNION ALL
SELECT 'B1' AS GROUP_TEXT, 'C101' AS LINE_TEXT, NULL AS NO_DO, 'ZZZZZZZZZZZZZZZZZZZZ' AS ITEM_NO, NULL AS PRODUCT_CODE, NULL AS TGL_DO,0 AS QTY, NULL AS EXP_DATE, NULL AS SN WHERE $P{sec2} ='Y'
UNION ALL
SELECT 'B1' AS GROUP_TEXT, 'C102' AS LINE_TEXT, NULL AS NO_DO, 'ZZZZZZZZZZZZZZZZZZZZ' AS ITEM_NO, NULL AS PRODUCT_CODE, NULL AS TGL_DO,0 AS QTY, NULL AS EXP_DATE, NULL AS SN WHERE $P{sec2} ='Y'
UNION ALL
SELECT 'B1' AS GROUP_TEXT, 'D101' AS LINE_TEXT, NULL AS NO_DO, 'ZZZZZZZZZZZZZZZZZZZZ' AS ITEM_NO, NULL AS PRODUCT_CODE, NULL AS TGL_DO,0 AS QTY, NULL AS EXP_DATE, NULL AS SN WHERE $P{sec3} ='Y'
UNION ALL
SELECT 'B1' AS GROUP_TEXT, 'D102' AS LINE_TEXT, NULL AS NO_DO, 'ZZZZZZZZZZZZZZZZZZZZ' AS ITEM_NO, NULL AS PRODUCT_CODE, NULL AS TGL_DO,0 AS QTY, NULL AS EXP_DATE, NULL AS SN WHERE $P{sec3} ='Y'
UNION ALL
SELECT 'B1' AS GROUP_TEXT, 'E101' AS LINE_TEXT, NULL AS NO_DO, 'ZZZZZZZZZZZZZZZZZZZZ' AS ITEM_NO, NULL AS PRODUCT_CODE, NULL AS TGL_DO,0 AS QTY, NULL AS EXP_DATE, NULL AS SN WHERE $P{sec4} ='Y'
UNION ALL
SELECT 'B1' AS GROUP_TEXT, 'E102' AS LINE_TEXT, NULL AS NO_DO, 'ZZZZZZZZZZZZZZZZZZZZ' AS ITEM_NO, NULL AS PRODUCT_CODE, NULL AS TGL_DO,0 AS QTY, NULL AS EXP_DATE, NULL AS SN WHERE $P{sec4} ='Y'
UNION ALL
SELECT 'B1' AS GROUP_TEXT, 'F101' AS LINE_TEXT, NULL AS NO_DO, 'ZZZZZZZZZZZZZZZZZZZZ' AS ITEM_NO, NULL AS PRODUCT_CODE, NULL AS TGL_DO,0 AS QTY, NULL AS EXP_DATE, NULL AS SN WHERE $P{sec5} ='Y'
UNION ALL
SELECT 'B1' AS GROUP_TEXT, 'F102' AS LINE_TEXT, NULL AS NO_DO, 'ZZZZZZZZZZZZZZZZZZZZ' AS ITEM_NO, NULL AS PRODUCT_CODE, NULL AS TGL_DO,0 AS QTY, NULL AS EXP_DATE, NULL AS SN WHERE $P{sec5} ='Y'
UNION ALL
SELECT 'B1' AS GROUP_TEXT, 'G101' AS LINE_TEXT, NULL AS NO_DO, 'ZZZZZZZZZZZZZZZZZZZZ' AS ITEM_NO, NULL AS PRODUCT_CODE, NULL AS TGL_DO,0 AS QTY, NULL AS EXP_DATE, NULL AS SN WHERE $P{sec6} ='Y'
UNION ALL
SELECT 'B1' AS GROUP_TEXT, 'G102' AS LINE_TEXT, NULL AS NO_DO, 'ZZZZZZZZZZZZZZZZZZZZ' AS ITEM_NO, NULL AS PRODUCT_CODE, NULL AS TGL_DO,0 AS QTY, NULL AS EXP_DATE, NULL AS SN WHERE $P{sec6} ='Y']]>
	</queryString>
