	<queryString>
		<![CDATA[SELECT	X.REG
		, CASE WHEN X.AMOUNT_PPN <> 0 THEN 'PPN' ELSE 'NON PPN' END AS TIPE_PPN
		, CASE WHEN X.TIPE = 'TRADE' THEN X.TIPE ELSE 'NTRADE' END AS TIPE
		, X.NO_INV
		, X.TGL_INV
		, X.NO_INV_CANCEL
		, X.KODE_SUPP
		, X.NAMA_SUPP
		, X.NO_INV_SUPP
		, X.NO_FP
		, X.AMOUNT_OTHERS
		, X.AMOUNT_FGOODS
		, X.AMOUNT_DCOST
		, X.AMOUNT_PEMBELIAN
	    , X.AMOUNT_UM
	    , (X.AMOUNT_OTHERS + X.AMOUNT_FGOODS + X.AMOUNT_DCOST + X.AMOUNT_PEMBELIAN + X.AMOUNT_UM) AS DPP
	 	, X.AMOUNT_PPN
	 	, (X.AMOUNT_OTHERS + X.AMOUNT_FGOODS + X.AMOUNT_DCOST + X.AMOUNT_PEMBELIAN + X.AMOUNT_UM + X.AMOUNT_PPN) AS TOTAL
FROM (
SELECT	CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' THEN 'INVOICE UM' ELSE 'INVOICE' END AS REG
		, MAX(GLE.[Gen_ Bus_ Posting Group]) AS TIPE
		, GLE.[Document No_] AS NO_INV
		, PIH.[Posting Date] AS TGL_INV
		, '' AS NO_INV_CANCEL
		, PIH.[Pay-to Vendor No_] AS KODE_SUPP
		, PIH.[Pay-to Name] AS NAMA_SUPP
		, PIH.[Vendor Invoice No_] AS NO_INV_SUPP
		, PIH.[Vendor Tax Invoice No_] AS NO_FP
		, SUM(CASE WHEN GLE.[G_L Account No_] NOT IN ('210.111.00', '510.200.00', '116.199.00','510.100.00','118.150.00', '118.290.00')  THEN
	 		GLE.[Amount] ELSE 0
	 	  END ) AS AMOUNT_OTHERS
		, SUM(CASE WHEN GLE.[G_L Account No_] = '116.199.00' THEN
	 		GLE.[Amount] ELSE 0
	 	  END ) AS AMOUNT_FGOODS
	 	, SUM(CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN
	 		GLE.[Amount] ELSE 0
	 	  END ) AS AMOUNT_DCOST
		, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.00' THEN
	 		GLE.[Amount] ELSE 0
	 	  END ) AS AMOUNT_PEMBELIAN
	    , SUM(CASE WHEN GLE.[G_L Account No_] = '118.150.00' THEN
	 		GLE.[Amount] ELSE 0
	 	  END ) AS AMOUNT_UM
	 	, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
	 		GLE.[Amount] ELSE 0
	 	  END ) AS AMOUNT_PPN
FROM [AUTOJAYA].[dbo].[SOLUSI$G_L Entry] GLE
INNER JOIN [AUTOJAYA].[dbo].[SOLUSI$Purch_ Inv_ Header] PIH ON PIH.[No_] = GLE.[Document No_]
WHERE GLE.[G_L Account No_] NOT IN ('210.110.00', '220.130.00')
GROUP BY GLE.[Document No_], PIH.[Pay-to Vendor No_], PIH.[Pay-to Name] , PIH.[Vendor Invoice No_], PIH.[Vendor Tax Invoice No_], PIH.[Posting Date]
UNION ALL
SELECT	CASE WHEN SUBSTRING(GLE.[Document No_],1,2) = 'PP' THEN 'CREDIT MEMO UM' ELSE 'CREDIT MEMO' END AS REG
		, MAX(GLE.[Gen_ Bus_ Posting Group]) AS TIPE
		, GLE.[Document No_] AS NO_INV
		, CIH.[Posting Date] AS TGL_INV
		, CIH.[Applies-to Doc_ No_]  AS NO_INV_CANCEL
		, CIH.[Pay-to Vendor No_] AS KODE_SUPP
		, CIH.[Pay-to Name] AS NAMA_SUPP
		, '' AS NO_INV_SUPP
		, CIH.[Vendor Tax Invoice No_] AS NO_FP
		, SUM(CASE WHEN GLE.[G_L Account No_] NOT IN ('210.111.00', '510.200.00', '116.199.00','510.100.00','118.150.00', '118.290.00')  THEN
	 		GLE.[Amount] ELSE 0
	 	  END ) AS AMOUNT_OTHERS
		, SUM(CASE WHEN GLE.[G_L Account No_] = '116.199.00' THEN
	 		GLE.[Amount] ELSE 0
	 	  END ) AS AMOUNT_FGOODS
	 	, SUM(CASE WHEN GLE.[G_L Account No_] IN ('210.111.00', '510.200.00') THEN
	 		GLE.[Amount] ELSE 0
	 	  END ) AS AMOUNT_DCOST
		, SUM(CASE WHEN GLE.[G_L Account No_] = '510.100.00' THEN
	 		GLE.[Amount] ELSE 0
	 	  END ) AS AMOUNT_PEMBELIAN
	    , SUM(CASE WHEN GLE.[G_L Account No_] = '118.150.00' THEN
	 		GLE.[Amount] ELSE 0
	 	  END ) AS AMOUNT_UM
	 	, SUM(CASE WHEN GLE.[G_L Account No_] = '118.290.00' THEN
	 		GLE.[Amount] ELSE 0
	 	  END ) AS AMOUNT_PPN
FROM [AUTOJAYA].[dbo].[SOLUSI$G_L Entry] GLE
INNER JOIN [AUTOJAYA].[dbo].[SOLUSI$Purch_ Cr_ Memo Hdr_] CIH ON CIH.[No_] = GLE.[Document No_]
WHERE GLE.[G_L Account No_] NOT IN ( '210.110.00', '220.130.00')
GROUP BY GLE.[Document No_],  CIH.[Pay-to Vendor No_], CIH.[Pay-to Name] , CIH.[Vendor Tax Invoice No_], CIH.[Applies-to Doc_ No_] , CIH.[Posting Date]
) X
WHERE  (X.TGL_INV >= $P{TglFrom} AND X.TGL_INV <= $P{TglUpto}) AND
	($P{Tipe} = 'ALL' OR X.TIPE = $P{Tipe}) AND
	(X.KODE_SUPP >= $P{VendorFrom} AND X.KODE_SUPP <= $P{VendorTo})]]>
	</queryString>
