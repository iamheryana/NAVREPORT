	<queryString>
		<![CDATA[SELECT	PMB.COMPANY
		, PMB.NO_INV
		, PMB.TGL_INV
		, PMB.KODE_SUPP
		, PMB.NAMA_SUPP
		, PMB.NO_INV_SUPP
		, PMB.ITEM_NO
		, PMB.ITEM_DESC
		, PMB.UOM
		, PMB.QTY
		, PMB.AMOUNT_IDR
FROM	(
			SELECT  'AUTOJAYA' AS COMPANY
					, PRH.[No_] AS NO_INV
					, PRH.[Posting Date] AS TGL_INV
					, PRH.[Pay-to Vendor No_] AS KODE_SUPP
					, PRH.[Pay-to Name] AS NAMA_SUPP
					, PRH.[Vendor Invoice No_] AS NO_INV_SUPP
					, PRLD.[No_] AS ITEM_NO
					, PRLD.[Description] AS ITEM_DESC
					, PRLD.[Unit of Measure] AS UOM
					, PRLD.[Quantity] AS QTY
					, CASE WHEN PRH.[Currency Code] = '' THEN PRLD.[Line Amount]ELSE ROUND((PRLD.[Line Amount] / PRH.[Currency Factor]),0) END AS AMOUNT_IDR
     		FROM [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Inv_ Header] PRH
			INNER JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Inv_ Line] PRLD ON PRH.[No_] = PRLD.[Document No_]
			LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Cr_ Memo Hdr_] PCMH ON PCMH.[Applies-to Doc_ No_] = PRH.[No_]
			INNER JOIN	(
							SELECT X.[Document No_]
							FROM (
							SELECT [Document No_]
									, SUM(
											CASE WHEN [Type] = 2 OR
												([Type] = 1 AND [No_] = '118.150.00')
											THEN 1 ELSE 0 END
										 ) AS JML_DTL_ITM
									,SUM(
											CASE WHEN [VAT Bus_ Posting Group] = 'VAT'
											THEN 1 ELSE 0 END
										 ) AS JML_DTL_VAT
									,SUM([Amount Including VAT]) - SUM([Amount]) AS VAT
							FROM [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Inv_ Line]
							GROUP BY [Document No_]
							) X
							WHERE X.JML_DTL_ITM > 0 AND VAT = 0 AND JML_DTL_VAT = 0 AND
								  X.[Document No_] NOT IN (
							SELECT DISTINCT PH.[No_]
							FROM [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Inv_ Line] PID
							INNER JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Inv_ Header] PH ON PH.[No_] = PID.[Document No_]
							INNER JOIN  (
											SELECT DISTINCT PIH.[No_]
													,ICA.[Applies-to Doc_ No_] AS GR_NO
													,ICA.[Applies-to Doc_ Line No_] AS GR_LINE_NO
											FROM [AUTOJAYA].[dbo].[AUTOJAYA$Item Charge Assignment (Purch)] ICA
											LEFT JOIN	(
															SELECT [Document No_] AS NO_GR
																	,[Line No_] AS GR_LINE_NO
																	,[Order No_] AS PO_NO
																	,[Order Line No_] AS PO_LINE_NO
															FROM [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Rcpt_ Line]
															WHERE [Quantity] <> 0
														) PRL ON PRL.PO_NO = ICA.[Document No_] AND PRL.PO_LINE_NO = ICA.[Document Line No_]
											LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Inv_ Line] PIL ON PIL.[Receipt No_] = PRL.NO_GR AND PIL.[Receipt Line No_] = PRL.GR_LINE_NO
											LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Inv_ Header] PIH ON PIH.[No_] = PIL.[Document No_]
											LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Cr_ Memo Hdr_] PCM ON PCM.[Applies-to Doc_ No_] = PIH.[No_]
											WHERE PCM.[No_] IS NULL
													AND PIH.[No_] IN	(
																			SELECT X.[Document No_]
																			FROM	(
																						SELECT [Document No_]
																								,MAX([Receipt No_]) AS GR_NO
																								, SUM(
																										CASE WHEN([Type] = 1 AND [No_] = '118.290.00')
																										THEN 1 ELSE 0 END
																									 ) AS JML_DTL_PNN
																								, SUM(
																										CASE WHEN[Type] = 5
																										THEN 1 ELSE 0 END
																									 ) AS JML_DTL_FREIGHT
																						FROM [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Inv_ Line]
																						GROUP BY [Document No_]
																					) X
																			WHERE X.JML_DTL_PNN > 0 AND X.JML_DTL_FREIGHT > 0
																		)
										) FRG ON FRG.GR_NO = PID.[Receipt No_] AND FRG.GR_LINE_NO = PID.[Receipt Line No_]
										)
						) PUI ON PUI.[Document No_] = PRH.[No_]
			WHERE PCMH.[No_] IS NULL AND PRLD.[No_] <> ''
			UNION ALL
			SELECT 'SOLUSI' AS COMPANY
					, PRH.[No_] AS NO_INV
					, PRH.[Posting Date] AS TGL_INV
					, PRH.[Pay-to Vendor No_] AS KODE_SUPP
					, PRH.[Pay-to Name] AS NAMA_SUPP
					, PRH.[Vendor Invoice No_] AS NO_INV_SUPP
					, PRLD.[No_] AS ITEM_NO
					, PRLD.[Description] AS ITEM_DESC
					, PRLD.[Unit of Measure] AS UOM
					, PRLD.[Quantity] AS QTY
					, CASE WHEN PRH.[Currency Code] = '' THEN PRLD.[Line Amount]ELSE ROUND((PRLD.[Line Amount] / PRH.[Currency Factor]),0) END AS AMOUNT_IDR
			FROM [AUTOJAYA].[dbo].[SOLUSI$Purch_ Inv_ Header] PRH
			INNER JOIN [AUTOJAYA].[dbo].[SOLUSI$Purch_ Inv_ Line] PRLD ON PRH.[No_] = PRLD.[Document No_]
			LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Purch_ Cr_ Memo Hdr_] PCMH ON PCMH.[Applies-to Doc_ No_] = PRH.[No_]
			INNER JOIN	(
							SELECT X.[Document No_]
							FROM (
							SELECT [Document No_]
									, SUM(
											CASE WHEN [Type] = 2 OR
												([Type] = 1 AND [No_] = '118.150.00')
											THEN 1 ELSE 0 END
										 ) AS JML_DTL_ITM
									,SUM(
											CASE WHEN [VAT Bus_ Posting Group] = 'VAT'
											THEN 1 ELSE 0 END
										 ) AS JML_DTL_VAT
									,SUM([Amount Including VAT]) - SUM([Amount]) AS VAT
							FROM [AUTOJAYA].[dbo].[SOLUSI$Purch_ Inv_ Line]
							GROUP BY [Document No_]
							) X
							WHERE X.JML_DTL_ITM > 0 AND VAT = 0 AND JML_DTL_VAT = 0 AND
								  X.[Document No_] NOT IN (
							SELECT DISTINCT PH.[No_]
							FROM [AUTOJAYA].[dbo].[SOLUSI$Purch_ Inv_ Line] PID
							INNER JOIN [AUTOJAYA].[dbo].[SOLUSI$Purch_ Inv_ Header] PH ON PH.[No_] = PID.[Document No_]
							INNER JOIN  (
											SELECT DISTINCT PIH.[No_]
													,ICA.[Applies-to Doc_ No_] AS GR_NO
													,ICA.[Applies-to Doc_ Line No_] AS GR_LINE_NO
											FROM [AUTOJAYA].[dbo].[SOLUSI$Item Charge Assignment (Purch)] ICA
											LEFT JOIN	(
															SELECT [Document No_] AS NO_GR
																	,[Line No_] AS GR_LINE_NO
																	,[Order No_] AS PO_NO
																	,[Order Line No_] AS PO_LINE_NO
															FROM [AUTOJAYA].[dbo].[SOLUSI$Purch_ Rcpt_ Line]
															WHERE [Quantity] <> 0
														) PRL ON PRL.PO_NO = ICA.[Document No_] AND PRL.PO_LINE_NO = ICA.[Document Line No_]
											LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Purch_ Inv_ Line] PIL ON PIL.[Receipt No_] = PRL.NO_GR AND PIL.[Receipt Line No_] = PRL.GR_LINE_NO
											LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Purch_ Inv_ Header] PIH ON PIH.[No_] = PIL.[Document No_]
											LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Purch_ Cr_ Memo Hdr_] PCM ON PCM.[Applies-to Doc_ No_] = PIH.[No_]
											WHERE PCM.[No_] IS NULL
													AND PIH.[No_] IN	(
																			SELECT X.[Document No_]
																			FROM	(
																						SELECT [Document No_]
																								,MAX([Receipt No_]) AS GR_NO
																								, SUM(
																										CASE WHEN([Type] = 1 AND [No_] = '118.290.00')
																										THEN 1 ELSE 0 END
																									 ) AS JML_DTL_PNN
																								, SUM(
																										CASE WHEN[Type] = 5
																										THEN 1 ELSE 0 END
																									 ) AS JML_DTL_FREIGHT
																						FROM [AUTOJAYA].[dbo].[SOLUSI$Purch_ Inv_ Line]
																						GROUP BY [Document No_]
																					) X
																			WHERE X.JML_DTL_PNN > 0 AND X.JML_DTL_FREIGHT > 0
																		)
										) FRG ON FRG.GR_NO = PID.[Receipt No_] AND FRG.GR_LINE_NO = PID.[Receipt Line No_]
										)
						) PUI ON PUI.[Document No_] = PRH.[No_]
			WHERE PCMH.[No_] IS NULL AND PRLD.[No_] <> ''
		) PMB
WHERE (PMB.COMPANY = $P{Company} OR  $P{Company} = 'ALL') AND
	(PMB.TGL_INV >= $P{TglFrom} AND PMB.TGL_INV <= $P{TglUpto})]]>
	</queryString>
