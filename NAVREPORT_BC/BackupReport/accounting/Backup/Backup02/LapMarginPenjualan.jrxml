LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Sales Cr_Memo Header] CRM ON CRM.[Applies-to Doc_ No_] = X.NO_INV
LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Responsibility Center] CAB ON CAB.[Code] = X.CABANG
) XX
WHERE XX.KODE_CUST NOT IN ('AUJA001','AUJA002' , 'SOPE001')
) XXX
WHERE (XXX.TGL_INV >= $P{TglInvFrom} AND XXX.TGL_INV <= $P{TglInvTo}) AND
	($P{Cabang} = 'ALL' OR XXX.CABANG = $P{Cabang}) AND
	(XXX.COMPANY = $P{Company})]]>
	</queryString>
