		,	CASE WHEN XX.MARGIN_PCN >= 30 THEN 'A'
			ELSE
				CASE WHEN  XX.MARGIN_PCN < 30 AND  XX.MARGIN_PCN  >= 20 THEN 'B'
				ELSE
					CASE WHEN  XX.MARGIN_PCN < 20 AND  XX.MARGIN_PCN  >= 10 THEN 'C'
					ELSE 'D'
					END
