<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="PrincipalInfo" language="groovy" pageWidth="1650" pageHeight="595" orientation="Landscape" columnWidth="1610" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="c3c56047-de54-4477-a40e-d4b81af326aa">
	<property name="ireport.zoom" value="1.7715610000000197"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="Company" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Tahun" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="VendorNo" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Principal" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ProductGroup" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="TipeKurs" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT	POP.COMPANY
		,POP.NO_PO
		,POP.VENDOR_NO
		,POP.VENDOR_NAME
		,POP.TGL_PO
		,POP.GRP_TITLE
		,POP.GRP
		,POP.TAHUN_PO
		,POP.BULAN_PO
		,POP.CURR_CODE
		,POP.DIM_PRODUCT
		,POP.PRODUCT_GROUP
		,POP.DIM_PRINCIPAL
		,POP.PRINCIPAL
		,POP.AMOUNT
		,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT END AS AMOUNT_USD
		,POP.TGL_INVOICE
		,POP.TAHUN_INV
		,POP.BULAN_INV
		,POP.NO_INVIOCE_VENDOR
		,POP.NO_INVOICE
		,POP.CURR_CODE_INV
		,POP.AMOUNT_INV
		,POP.AMOUNT_INV_JAN
		,POP.AMOUNT_INV_FEB
		,POP.AMOUNT_INV_MAR
		,POP.AMOUNT_INV_APR
		,POP.AMOUNT_INV_MAY
		,POP.AMOUNT_INV_JUN
		,POP.AMOUNT_INV_JUL
		,POP.AMOUNT_INV_AUG
		,POP.AMOUNT_INV_SEP
		,POP.AMOUNT_INV_OCT
		,POP.AMOUNT_INV_NOV
		,POP.AMOUNT_INV_DES
		,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV END AS AMOUNT_INV_USD
		,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV_JAN IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV_JAN / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV_JAN / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV_JAN END AS AMOUNT_INV_JAN_USD
		 ,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV_FEB IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV_FEB / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV_FEB / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV_FEB END AS AMOUNT_INV_FEB_USD
		 ,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV_MAR IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV_MAR / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV_MAR / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV_MAR END AS AMOUNT_INV_MAR_USD
		,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV_APR IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV_APR / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV_APR / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV_APR END AS AMOUNT_INV_APR_USD
		 ,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV_MAY IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV_MAY / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV_MAY / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV_MAY END AS AMOUNT_INV_MAY_USD
		 ,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV_JUN IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV_JUN / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV_JUN / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV_JUN END AS AMOUNT_INV_JUN_USD
		 ,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV_JUL IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV_JUL / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV_JUL / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV_JUL END AS AMOUNT_INV_JUL_USD
		 ,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV_AUG IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV_AUG / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV_AUG / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV_AUG END AS AMOUNT_INV_AUG_USD
		 ,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV_SEP IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV_SEP / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV_SEP / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV_SEP END AS AMOUNT_INV_SEP_USD
		 ,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV_OCT IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV_OCT / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV_OCT / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV_OCT END AS AMOUNT_INV_OCT_USD
		 ,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV_NOV IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV_NOV / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV_NOV / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV_NOV END AS AMOUNT_INV_NOV_USD
		 ,CASE WHEN POP.CURR_CODE = 'IDR' THEN
			CASE WHEN POP.AMOUNT_INV_DES IS NOT NULL THEN
				CASE WHEN $P{TipeKurs} = 'EOM' THEN
					ROUND((POP.AMOUNT_INV_DES / ISNULL(CER2.[Relational Exch_ Rate Amount], 1)),2)
				ELSE
					ROUND((POP.AMOUNT_INV_DES / ISNULL(CER1.[Relational Exch_ Rate Amount], 1)),2)
				END
			ELSE NULL END
		 ELSE POP.AMOUNT_INV_DES END AS AMOUNT_INV_DES_USD


FROM	(
			SELECT 'AUTOJAYA' AS COMPANY
					,POH.[No_] AS NO_PO
					,POH.[Pay-to Vendor No_] AS VENDOR_NO
					,POH.[Pay-to Name] AS VENDOR_NAME
					,POH.[Order Date] AS TGL_PO
					,	CASE WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) = $P{Tahun} THEN
							CASE WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '01' THEN 'JANUARY'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '02' THEN 'FEBRUARY'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '03' THEN 'MARCH'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '04' THEN 'APRIL'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '05' THEN 'MAY'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '06' THEN 'JUNE'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '07' THEN 'JULY'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '08' THEN 'AUGUST'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '09' THEN 'SEPTEMBER'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '10' THEN 'OCTOBER'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '11' THEN 'NOVEMBER'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '12' THEN 'DECEMBER'
								ELSE ''
							 END
						ELSE
							'EX '+SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4)
						END AS GRP_TITLE
					, CASE WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) = $P{Tahun} THEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4)+SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) ELSE SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4)+'12' END AS GRP
					,SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) AS TAHUN_PO
					,SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) AS BULAN_PO
					,CASE WHEN POH.[Currency Code] = '' THEN 'IDR' ELSE POH.[Currency Code] END AS CURR_CODE
					, (	SELECT  MAX([Starting Date]) AS EXC_DATE
						FROM [AUTOJAYA].[dbo].[AUTOJAYA$Currency Exchange Rate]
						WHERE [Currency Code] = (CASE WHEN POH.[Currency Code] = '' THEN 'USD' ELSE 'IDR' END)
								AND [Starting Date] <= POH.[Order Date]
					 ) AS EXC_DATE
					 , (	SELECT  MAX([Starting Date]) AS EXC_DATE
						FROM [AUTOJAYA].[dbo].[AUTOJAYA$Currency Exchange Rate]
						WHERE [Currency Code] = (CASE WHEN POH.[Currency Code] = '' THEN 'USD' ELSE 'IDR' END)
								AND SUBSTRING(CONVERT(varchar, [Starting Date], 105), 4,7) = SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,7)
					 ) AS EXC_DATE_EOM
					,POD.DIM_PRODUCT
					,PRD.[Description] AS PRODUCT_GROUP
					,POD.DIM_PRINCIPAL
					,PRIN.[Name] AS PRINCIPAL
					,POD.AMOUNT
					,PINV.TGL_INVOICE
					,PINV.TAHUN_INV
					,PINV.BULAN_INV
					,PINV.NO_INVIOCE_VENDOR
					,PINV.NO_INVOICE
					,PINV.CURR_CODE AS CURR_CODE_INV
					,PINV.AMOUNT AS AMOUNT_INV
					,CASE WHEN PINV.BULAN_INV  = '01' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_JAN
					,CASE WHEN PINV.BULAN_INV  = '02' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_FEB
					,CASE WHEN PINV.BULAN_INV  = '03' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_MAR
					,CASE WHEN PINV.BULAN_INV  = '04' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_APR
					,CASE WHEN PINV.BULAN_INV  = '05' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_MAY
					,CASE WHEN PINV.BULAN_INV  = '06' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_JUN
					,CASE WHEN PINV.BULAN_INV  = '07' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_JUL
					,CASE WHEN PINV.BULAN_INV  = '08' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_AUG
					,CASE WHEN PINV.BULAN_INV  = '09' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_SEP
					,CASE WHEN PINV.BULAN_INV  = '10' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_OCT
					,CASE WHEN PINV.BULAN_INV  = '11' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_NOV
					,CASE WHEN PINV.BULAN_INV  = '12' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_DES
			FROM [AUTOJAYA].[dbo].[AUTOJAYA$Purchase Header] POH
			LEFT JOIN (
						SELECT PCL.[Document No_]
								,MIN(DIM.DIM_PRODUCT) AS DIM_PRODUCT
								,MAX(DIM.DIM_PRINCIPAL) AS DIM_PRINCIPAL
								,SUM(PCL.[Amount]) AS AMOUNT
						FROM [AUTOJAYA].[dbo].[AUTOJAYA$Purchase Line] PCL
						LEFT JOIN	(
										SELECT X.[No_]
												, MAX(X.DIM_PRODUCT) AS DIM_PRODUCT
												, MAX(X.DIM_PRINCIPAL) AS DIM_PRINCIPAL
										FROM	(
													SELECT  [No_]
														  ,CASE WHEN [Dimension Code] = 'PRODUCT' THEN [Dimension Value Code] ELSE '' END AS DIM_PRODUCT
														  ,CASE WHEN [Dimension Code] = 'PRINCIPAL' THEN [Dimension Value Code] ELSE '' END AS DIM_PRINCIPAL
													FROM [AUTOJAYA].[dbo].[AUTOJAYA$Default Dimension]
													WHERE [Table ID] = 27
												) X
												GROUP BY X.[No_]
									) DIM ON DIM.[No_] = PCL.[No_]
						GROUP BY PCL.[Document No_]
					  ) POD ON POD.[Document No_] = POH.[No_]
			LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Product Group] PRD ON PRD.[Code] = POD.DIM_PRODUCT
			LEFT JOIN	(
							SELECT [Code]
								  ,[Name]
							FROM [AUTOJAYA].[dbo].[AUTOJAYA$Dimension Value]
							WHERE [Dimension Code] = 'PRINCIPAL'
						) PRIN ON PRIN.[Code] = POD.DIM_PRINCIPAL
			LEFT JOIN	(
							SELECT	INV.NO_PO
									,INV.TGL_INVOICE
									,SUBSTRING(CONVERT(varchar, INV.TGL_INVOICE, 105), 7,4) AS TAHUN_INV
									,SUBSTRING(CONVERT(varchar, INV.TGL_INVOICE, 105), 4,2) AS BULAN_INV
									,INV.NO_INVIOCE_VENDOR
									,INV.NO_INVOICE
									,INV.CURR_CODE
									,SUM(INV.AMOUNT) AS AMOUNT
							FROM	(
										SELECT 	CASE WHEN ISNULL(BPO.BPO,'') <> '' THEN
													CASE WHEN SUBSTRING(BPO.BPO,6,1) = 'P' THEN BPO.BPO ELSE PRH.[Order No_] END
													ELSE PRH.[Order No_] END AS NO_PO
												,PIH.[Posting Date] AS TGL_INVOICE
												,PIH.[Vendor Invoice No_] AS NO_INVIOCE_VENDOR
												,PIH.[No_] AS NO_INVOICE
												,CASE WHEN PIH.[Currency Code] = '' THEN 'IDR' ELSE PIH.[Currency Code] END AS CURR_CODE
												,PIL.AMOUNT
										FROM [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Inv_ Header] PIH
										LEFT JOIN	(
														SELECT [Document No_]
																,[Receipt No_]
																,SUM([Amount]) AS AMOUNT
														FROM [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Inv_ Line]
														WHERE [Amount] <> 0
														GROUP BY [Document No_], [Receipt No_]
													) PIL ON PIL.[Document No_] =  PIH.[No_]
										LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
										LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Purch_ Rcpt_ Header] PRH ON PRH.[No_] = PIL.[Receipt No_]
										LEFT JOIN	(
														SELECT PH.[No_], MAX(PD.[Blanket Order No_]) AS BPO
														FROM [AUTOJAYA].[dbo].[AUTOJAYA$Purchase Header] PH
														INNER JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Purchase Line] PD ON PD.[Document No_] = PH.[No_]
														WHERE PH.[Document Type] = 1
														GROUP BY PH.[No_]
													) BPO ON BPO.[No_] = PRH.[Order No_]
										WHERE CMH.[No_] IS NULL
									) INV
							GROUP BY INV.NO_PO ,INV.TGL_INVOICE
									,INV.NO_INVIOCE_VENDOR ,INV.NO_INVOICE
									,INV.CURR_CODE
						)PINV ON PINV.NO_PO = POH.[No_]
			WHERE ((POH.[Document Type] = 1 AND SUBSTRING(POH.[No_],5,1) <> 'P')
					OR
				  (POH.[Document Type] = 4 AND SUBSTRING(POH.[No_],6,1) = 'P'))
					AND ($P{VendorNo} = 'ALL' OR POH.[Pay-to Vendor No_] = $P{VendorNo})
					AND ($P{Principal} = 'ALL' OR POD.DIM_PRINCIPAL = $P{Principal})
					AND ($P{ProductGroup} = 'ALL' OR POD.DIM_PRODUCT = $P{ProductGroup})
					AND (	SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) = $P{Tahun}
							OR
							(SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) = ($P{Tahun}-1) AND PINV.TAHUN_INV = $P{Tahun})
						)
			UNION ALL
			SELECT 'SOLUSI' AS COMPANY
					,POH.[No_] AS NO_PO
					,POH.[Pay-to Vendor No_] AS VENDOR_NO
					,POH.[Pay-to Name] AS VENDOR_NAME
					,POH.[Order Date] AS TGL_PO
					,	CASE WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) = $P{Tahun} THEN
							CASE WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '01' THEN 'JANUARY'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '02' THEN 'FEBRUARY'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '03' THEN 'MARCH'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '04' THEN 'APRIL'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '05' THEN 'MAY'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '06' THEN 'JUNE'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '07' THEN 'JULY'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '08' THEN 'AUGUST'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '09' THEN 'SEPTEMBER'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '10' THEN 'OCTOBER'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '11' THEN 'NOVEMBER'
								WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) = '12' THEN 'DECEMBER'
								ELSE ''
							 END
						ELSE
							'EX '+SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4)
						END AS GRP_TITLE
					, CASE WHEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) = $P{Tahun} THEN SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4)+SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) ELSE SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4)+'12' END AS GRP
					,SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) AS TAHUN_PO
					,SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,2) AS BULAN_PO
					,CASE WHEN POH.[Currency Code] = '' THEN 'IDR' ELSE POH.[Currency Code] END AS CURR_CODE
					, (	SELECT  MAX([Starting Date]) AS EXC_DATE
						FROM [AUTOJAYA].[dbo].[AUTOJAYA$Currency Exchange Rate]
						WHERE [Currency Code] = (CASE WHEN POH.[Currency Code] = '' THEN 'USD' ELSE 'IDR' END)
								AND [Starting Date] <= POH.[Order Date]
					 ) AS EXC_DATE
					 , (	SELECT  MAX([Starting Date]) AS EXC_DATE
						FROM [AUTOJAYA].[dbo].[AUTOJAYA$Currency Exchange Rate]
						WHERE [Currency Code] = (CASE WHEN POH.[Currency Code] = '' THEN 'USD' ELSE 'IDR' END)
								AND SUBSTRING(CONVERT(varchar, [Starting Date], 105), 4,7) = SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 4,7)
					 ) AS EXC_DATE_EOM
					,POD.DIM_PRODUCT
					,PRD.[Description] AS PRODUCT_GROUP
					,POD.DIM_PRINCIPAL
					,PRIN.[Name] AS PRINCIPAL
					,POD.AMOUNT
					,PINV.TGL_INVOICE
					,PINV.TAHUN_INV
					,PINV.BULAN_INV
					,PINV.NO_INVIOCE_VENDOR
					,PINV.NO_INVOICE
					,PINV.CURR_CODE AS CURR_CODE_INV
					,PINV.AMOUNT AS AMOUNT_INV
					,CASE WHEN PINV.BULAN_INV  = '01' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_JAN
					,CASE WHEN PINV.BULAN_INV  = '02' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_FEB
					,CASE WHEN PINV.BULAN_INV  = '03' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_MAR
					,CASE WHEN PINV.BULAN_INV  = '04' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_APR
					,CASE WHEN PINV.BULAN_INV  = '05' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_MAY
					,CASE WHEN PINV.BULAN_INV  = '06' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_JUN
					,CASE WHEN PINV.BULAN_INV  = '07' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_JUL
					,CASE WHEN PINV.BULAN_INV  = '08' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_AUG
					,CASE WHEN PINV.BULAN_INV  = '09' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_SEP
					,CASE WHEN PINV.BULAN_INV  = '10' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_OCT
					,CASE WHEN PINV.BULAN_INV  = '11' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_NOV
					,CASE WHEN PINV.BULAN_INV  = '12' THEN PINV.AMOUNT ELSE NULL END AS AMOUNT_INV_DES
			FROM [AUTOJAYA].[dbo].[SOLUSI$Purchase Header] POH
			LEFT JOIN (
						SELECT PCL.[Document No_]
								,MIN(DIM.DIM_PRODUCT) AS DIM_PRODUCT
								,MAX(DIM.DIM_PRINCIPAL) AS DIM_PRINCIPAL
								,SUM(PCL.[Amount]) AS AMOUNT
						FROM [AUTOJAYA].[dbo].[SOLUSI$Purchase Line] PCL
						LEFT JOIN	(
										SELECT X.[No_]
												, MAX(X.DIM_PRODUCT) AS DIM_PRODUCT
												, MAX(X.DIM_PRINCIPAL) AS DIM_PRINCIPAL
										FROM	(
													SELECT  [No_]
														  ,CASE WHEN [Dimension Code] = 'PRODUCT' THEN [Dimension Value Code] ELSE '' END AS DIM_PRODUCT
														  ,CASE WHEN [Dimension Code] = 'PRINCIPAL' THEN [Dimension Value Code] ELSE '' END AS DIM_PRINCIPAL
													FROM [AUTOJAYA].[dbo].[SOLUSI$Default Dimension]
													WHERE [Table ID] = 27
												) X
												GROUP BY X.[No_]
									) DIM ON DIM.[No_] = PCL.[No_]
						GROUP BY PCL.[Document No_]
					  ) POD ON POD.[Document No_] = POH.[No_]
			LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Product Group] PRD ON PRD.[Code] = POD.DIM_PRODUCT
			LEFT JOIN	(
							SELECT [Code]
								  ,[Name]
							FROM [AUTOJAYA].[dbo].[SOLUSI$Dimension Value]
							WHERE [Dimension Code] = 'PRINCIPAL'
						) PRIN ON PRIN.[Code] = POD.DIM_PRINCIPAL
			LEFT JOIN	(
							SELECT	INV.NO_PO
									,INV.TGL_INVOICE
									,SUBSTRING(CONVERT(varchar, INV.TGL_INVOICE, 105), 7,4) AS TAHUN_INV
									,SUBSTRING(CONVERT(varchar, INV.TGL_INVOICE, 105), 4,2) AS BULAN_INV
									,INV.NO_INVIOCE_VENDOR
									,INV.NO_INVOICE
									,INV.CURR_CODE
									,SUM(INV.AMOUNT) AS AMOUNT
							FROM	(
										SELECT 	CASE WHEN ISNULL(BPO.BPO,'') <> '' THEN
													CASE WHEN SUBSTRING(BPO.BPO,6,1) = 'P' THEN BPO.BPO ELSE PRH.[Order No_] END
													ELSE PRH.[Order No_] END AS NO_PO
												,PIH.[Posting Date] AS TGL_INVOICE
												,PIH.[Vendor Invoice No_] AS NO_INVIOCE_VENDOR
												,PIH.[No_] AS NO_INVOICE
												,CASE WHEN PIH.[Currency Code] = '' THEN 'IDR' ELSE PIH.[Currency Code] END AS CURR_CODE
												,PIL.AMOUNT
										FROM [AUTOJAYA].[dbo].[SOLUSI$Purch_ Inv_ Header] PIH
										LEFT JOIN	(
														SELECT [Document No_]
																,[Receipt No_]
																,SUM([Amount]) AS AMOUNT
														FROM [AUTOJAYA].[dbo].[SOLUSI$Purch_ Inv_ Line]
														WHERE [Amount] <> 0
														GROUP BY [Document No_], [Receipt No_]
													) PIL ON PIL.[Document No_] =  PIH.[No_]
										LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Purch_ Cr_ Memo Hdr_] CMH ON CMH.[Applies-to Doc_ No_] = PIH.[No_]
										LEFT JOIN [AUTOJAYA].[dbo].[SOLUSI$Purch_ Rcpt_ Header] PRH ON PRH.[No_] = PIL.[Receipt No_]
										LEFT JOIN	(
														SELECT PH.[No_], MAX(PD.[Blanket Order No_]) AS BPO
														FROM [AUTOJAYA].[dbo].[SOLUSI$Purchase Header] PH
														INNER JOIN [AUTOJAYA].[dbo].[SOLUSI$Purchase Line] PD ON PD.[Document No_] = PH.[No_]
														WHERE PH.[Document Type] = 1
														GROUP BY PH.[No_]
													) BPO ON BPO.[No_] = PRH.[Order No_]
										WHERE CMH.[No_] IS NULL
									) INV
							GROUP BY INV.NO_PO ,INV.TGL_INVOICE
									,INV.NO_INVIOCE_VENDOR ,INV.NO_INVOICE
									,INV.CURR_CODE
						)PINV ON PINV.NO_PO = POH.[No_]
			WHERE ((POH.[Document Type] = 1 AND SUBSTRING(POH.[No_],5,1) <> 'P')
					OR
				  (POH.[Document Type] = 4 AND SUBSTRING(POH.[No_],6,1) = 'P'))
					AND ($P{VendorNo} = 'ALL' OR POH.[Pay-to Vendor No_] = $P{VendorNo})
					AND ($P{Principal} = 'ALL' OR POD.DIM_PRINCIPAL = $P{Principal})
					AND ($P{ProductGroup} = 'ALL' OR POD.DIM_PRODUCT = $P{ProductGroup})
					AND (	SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) = $P{Tahun}
							OR
							(SUBSTRING(CONVERT(varchar, POH.[Order Date], 105), 7,4) = ($P{Tahun}-1) AND PINV.TAHUN_INV = $P{Tahun})
						)
		) POP
LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Currency Exchange Rate] CER1 ON CER1.[Currency Code] = 'USD' AND  CER1.[Starting Date] = POP.EXC_DATE
LEFT JOIN [AUTOJAYA].[dbo].[AUTOJAYA$Currency Exchange Rate] CER2 ON CER2.[Currency Code] = 'USD' AND  CER2.[Starting Date] = POP.EXC_DATE_EOM
WHERE $P{Company} = 'ALL' OR POP.COMPANY = $P{Company}]]>
	</queryString>
